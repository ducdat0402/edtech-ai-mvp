INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('6a504525-c25f-406a-a879-dfef9e0f05a2', NULL, '5f36b291-05f3-4155-b8b3-0910221c4da6', 'boss', NULL, '[{"id": "q1", "options": {"A": "Bảo vệ tài chính cho gia đình trong trường hợp người được bảo hiểm không còn sống", "B": "Chi trả toàn bộ chi phí y tế", "C": "Bảo vệ tài sản khỏi thiên tai", "D": "Giảm thiểu rủi ro tài chính khi đầu tư"}, "category": "core_concept", "question": "Bảo hiểm nhân thọ có mục đích chính là gì?", "difficulty": "medium", "explanation": "Bảo hiểm nhân thọ được thiết kế để hỗ trợ tài chính cho người thân của người được bảo hiểm trong trường hợp họ qua đời.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Chi trả toàn bộ chi phí cho tất cả các tình huống", "B": "Bồi thường cho người bảo hiểm khi xảy ra sự kiện bảo hiểm", "C": "Đảm bảo lợi nhuận cho người đầu tư", "D": "Cung cấp dịch vụ y tế miễn phí"}, "category": "core_concept", "question": "Công ty bảo hiểm cam kết gì trong hợp đồng bảo hiểm?", "difficulty": "medium", "explanation": "Công ty bảo hiểm cam kết bồi thường cho người bảo hiểm khi xảy ra sự kiện bảo hiểm được quy định trong hợp đồng.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Bảo vệ tài sản khỏi thiên tai", "B": "Chi trả chi phí y tế khi ốm đau hoặc bị thương", "C": "Đảm bảo thu nhập cho người lao động", "D": "Bảo vệ trách nhiệm pháp lý"}, "category": "core_concept", "question": "Bảo hiểm y tế được thiết kế để làm gì?", "difficulty": "medium", "explanation": "Bảo hiểm y tế giúp chi trả chi phí y tế cho các dịch vụ y tế cần thiết khi người được bảo hiểm ốm đau hoặc bị thương.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Xác định nhu cầu bảo hiểm", "B": "Nghiên cứu các loại bảo hiểm", "C": "Chi trả mọi chi phí bảo hiểm ngay lập tức", "D": "Đọc kỹ hợp đồng"}, "category": "core_concept", "question": "Điều nào sau đây không phải là bước lựa chọn bảo hiểm?", "difficulty": "medium", "explanation": "Chi trả mọi chi phí bảo hiểm ngay lập tức không phải là một bước trong quá trình lựa chọn bảo hiểm, mà cần nghiên cứu và đánh giá trước.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "Khi bạn đổi số điện thoại", "B": "Khi có sự kiện lớn trong cuộc sống như kết hôn hoặc có con", "C": "Khi bạn muốn thay đổi màu xe", "D": "Khi bạn chuyển nhà"}, "category": "application", "question": "Trong trường hợp nào bạn nên xem xét lại gói bảo hiểm của mình?", "difficulty": "medium", "explanation": "Các sự kiện lớn trong cuộc sống có thể ảnh hưởng đến nhu cầu bảo hiểm của bạn và cần được xem xét lại gói bảo hiểm hiện tại.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "500 triệu đồng", "B": "1 tỷ đồng", "C": "2 tỷ đồng", "D": "300 triệu đồng"}, "category": "application", "question": "Nguyễn Văn A đã chọn bảo hiểm nhân thọ với số tiền bảo hiểm là bao nhiêu?", "difficulty": "medium", "explanation": "Nguyễn Văn A đã quyết định lựa chọn bảo hiểm nhân thọ với số tiền bảo hiểm 1 tỷ đồng để bảo vệ gia đình.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Chi phí sửa chữa xe cộ", "B": "Các khoản tiền bồi thường lớn khi gây thiệt hại cho người khác", "C": "Chi phí y tế", "D": "Mất tài sản"}, "category": "comparison", "question": "Bảo hiểm trách nhiệm dân sự giúp bạn tránh được điều gì?", "difficulty": "hard", "explanation": "Bảo hiểm trách nhiệm dân sự bảo vệ bạn khỏi những trách nhiệm pháp lý và các khoản tiền bồi thường lớn khi gây thiệt hại cho người khác.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Thời hạn bảo vệ", "B": "Mức phí bảo hiểm", "C": "Số tiền bảo hiểm", "D": "Đối tượng bảo hiểm"}, "category": "comparison", "question": "Sự khác biệt chính giữa bảo hiểm tạm thời và bảo hiểm trọn đời là gì?", "difficulty": "hard", "explanation": "Bảo hiểm tạm thời cung cấp bảo vệ trong một khoảng thời gian nhất định, trong khi bảo hiểm trọn đời bảo vệ suốt đời.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Khi bạn không có bệnh tật", "B": "Khi bạn đang khỏe mạnh", "C": "Khi bạn không có kế hoạch thăm khám", "D": "Khi bạn cần bảo vệ sức khỏe trước các chi phí y tế cao"}, "category": "application", "question": "Khi nào bạn nên mua bảo hiểm y tế?", "difficulty": "medium", "explanation": "Mua bảo hiểm y tế giúp bảo vệ bạn trước các chi phí y tế cao và không lường trước được, đặc biệt khi ốm đau hoặc bị thương.", "correctAnswer": "D"}, {"id": "q10", "options": {"A": "Chỉ chọn gói bảo hiểm rẻ nhất", "B": "Tìm hiểu kỹ về công ty bảo hiểm", "C": "Không đọc hợp đồng", "D": "Mua bảo hiểm mà không cần tham khảo ý kiến"}, "category": "core_concept", "question": "Một trong những lưu ý quan trọng khi mua bảo hiểm là gì?", "difficulty": "medium", "explanation": "Tìm hiểu kỹ về công ty bảo hiểm giúp bạn đảm bảo chọn lựa một nhà cung cấp uy tín và đáng tin cậy.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Thu nhập hàng tháng", "B": "Chi phí sinh hoạt", "C": "Sở thích cá nhân", "D": "Dự phòng cho chi phí bất ngờ"}, "category": "connection", "question": "Yếu tố nào không ảnh hưởng đến khả năng chi trả phí bảo hiểm của bạn?", "difficulty": "medium", "explanation": "Sở thích cá nhân không ảnh hưởng đến khả năng chi trả phí bảo hiểm, trong khi thu nhập, chi phí sinh hoạt và dự phòng có tác động lớn.", "correctAnswer": "C"}, {"id": "q12", "options": {"A": "Mức khấu trừ", "B": "Giá trị tài sản", "C": "Sở thích cá nhân", "D": "Mức độ nổi tiếng của công ty"}, "category": "core_concept", "question": "Khi xem xét các gói bảo hiểm, bạn nên chú ý đến điều gì trong hợp đồng?", "difficulty": "medium", "explanation": "Mức khấu trừ là số tiền mà bạn phải tự chi trả trước khi công ty bảo hiểm bắt đầu chi trả, là yếu tố quan trọng trong hợp đồng.", "correctAnswer": "A"}, {"id": "q13", "options": {"A": "Sức khỏe cá nhân", "B": "Tài sản khỏi các rủi ro như hỏa hoạn và trộm cắp", "C": "Trách nhiệm pháp lý", "D": "Chi phí y tế"}, "category": "core_concept", "question": "Bảo hiểm tài sản chủ yếu bảo vệ điều gì?", "difficulty": "medium", "explanation": "Bảo hiểm tài sản giúp bạn bảo vệ tài sản khỏi các rủi ro như hỏa hoạn, trộm cắp và thiên tai.", "correctAnswer": "B"}, {"id": "q14", "options": {"A": "Phân tích chất lượng khách hàng", "B": "Dự đoán thu nhập", "C": "Tăng phí bảo hiểm", "D": "Giảm số tiền bảo hiểm"}, "category": "connection", "question": "Đâu là một kỹ thuật phân tích rủi ro trong bảo hiểm?", "difficulty": "hard", "explanation": "Phân tích chất lượng khách hàng giúp hiểu rõ đối tượng bảo hiểm và xác định mức phí bảo hiểm phù hợp.", "correctAnswer": "A"}, {"id": "q15", "options": {"A": "Giảm thiểu rủi ro tài chính", "B": "Tạo ra lợi nhuận cho công ty bảo hiểm", "C": "Bảo vệ tài sản và sức khỏe", "D": "Chỉ bảo vệ cho cá nhân người mua bảo hiểm"}, "category": "core_concept", "question": "Nguyên tắc nào không phải là mục tiêu của bảo hiểm?", "difficulty": "hard", "explanation": "Bảo hiểm không chỉ bảo vệ cho cá nhân mà còn bảo vệ tài sản và sức khỏe cho người khác trong các tình huống cụ thể.", "correctAnswer": "D"}]', 15, 80, 'Boss Quiz: Khám Phá Thế Giới Bảo Hiểm', '2026-01-22 16:27:19.322', 'gpt-4o-mini', '2026-01-22 16:27:19.362739', '2026-01-22 16:27:19.362739');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('e71239b2-5628-4512-a7e3-5453a32c0ff9', NULL, '0c8b67ca-ab66-40ab-b34b-68f7d82b8543', 'boss', NULL, '[{"id": "q1", "options": {"A": "Kế hoạch tài chính để theo dõi thu nhập và chi tiêu", "B": "Tài khoản ngân hàng", "C": "Một loại hình đầu tư", "D": "Sổ ghi chép chi tiêu cá nhân"}, "category": "core_concept", "question": "Ngân sách là gì?", "difficulty": "medium", "explanation": "Ngân sách là một kế hoạch tài chính chi tiết nhằm theo dõi và điều chỉnh thu nhập và chi tiêu trong một khoảng thời gian nhất định.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "50% nhu cầu, 30% mong muốn, 20% tiết kiệm", "B": "30% nhu cầu, 50% mong muốn, 20% tiết kiệm", "C": "20% nhu cầu, 50% mong muốn, 30% tiết kiệm", "D": "50% tiết kiệm, 30% nhu cầu, 20% mong muốn"}, "category": "core_concept", "question": "Nguyên tắc 50/30/20 trong lập ngân sách được phân bổ như thế nào?", "difficulty": "medium", "explanation": "Nguyên tắc 50/30/20 đề xuất rằng 50% thu nhập nên dành cho nhu cầu, 30% cho mong muốn và 20% cho tiết kiệm.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "2 triệu đồng", "B": "3 triệu đồng", "C": "4 triệu đồng", "D": "5 triệu đồng"}, "category": "core_concept", "question": "Một gia đình 4 người với thu nhập 20 triệu đồng, quyết định dành bao nhiêu cho tiết kiệm?", "difficulty": "medium", "explanation": "Sau khi tổng chi tiêu là 17 triệu đồng, họ dành 3 triệu đồng còn lại để tiết kiệm.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Chi phí thuốc men", "B": "Tiền thuê nhà hàng tháng", "C": "Chi phí đi lại", "D": "Giải trí"}, "category": "core_concept", "question": "Chi phí nào là chi phí định kỳ?", "difficulty": "medium", "explanation": "Chi phí định kỳ là những khoản chi mà bạn phải trả với tần suất nhất định, như tiền thuê nhà hàng tháng.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Tiết kiệm tiền", "B": "Kiểm soát tài chính", "C": "Đặt mục tiêu tài chính", "D": "Tất cả các đáp án trên"}, "category": "connection", "question": "Ngân sách giúp bạn đạt được điều gì?", "difficulty": "medium", "explanation": "Ngân sách giúp bạn kiểm soát tài chính, tiết kiệm tiền và đặt ra các mục tiêu tài chính.", "correctAnswer": "D"}, {"id": "q6", "options": {"A": "Để chỉ biết tổng số tiền chi tiêu", "B": "Để dễ dàng dự đoán và quản lý tài chính", "C": "Không cần theo dõi các khoản chi phí", "D": "Để biết khi nào nên chi tiêu nhiều hơn"}, "category": "connection", "question": "Khi lập ngân sách, tại sao cần theo dõi chi phí định kỳ và chi phí thay đổi?", "difficulty": "medium", "explanation": "Theo dõi chi phí định kỳ và thay đổi giúp bạn dự đoán và quản lý tài chính tốt hơn.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "5 triệu đồng", "B": "7 triệu đồng", "C": "10 triệu đồng", "D": "8 triệu đồng"}, "category": "application", "question": "Nếu bạn có 25 triệu đồng thu nhập hàng tháng và tổng chi phí hàng tháng là 18 triệu đồng, bạn còn lại bao nhiêu?", "difficulty": "medium", "explanation": "25 triệu - 18 triệu = 7 triệu đồng còn lại.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Gia đình có thu nhập 20 triệu và chi tiêu 17 triệu", "B": "Gia đình có thu nhập 60 triệu và chi tiêu 24 triệu", "C": "Gia đình có thu nhập 25 triệu và chi tiêu 18 triệu", "D": "Gia đình có thu nhập 15 triệu và chi tiêu 10 triệu"}, "category": "application", "question": "Gia đình nào có ngân sách tốt nhất theo nguyên tắc 50/30/20?", "difficulty": "medium", "explanation": "Gia đình có thu nhập 60 triệu và chi tiêu 24 triệu đã phân bổ ngân sách theo nguyên tắc 50/30/20.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Chi phí thực phẩm", "B": "Chi phí bảo hiểm", "C": "Giải trí", "D": "Chi phí đi lại"}, "category": "comparison", "question": "Chi phí nào không thuộc nhóm chi phí thay đổi?", "difficulty": "hard", "explanation": "Chi phí bảo hiểm là chi phí định kỳ, trong khi thực phẩm, giải trí và đi lại là chi phí thay đổi.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Chi tiêu hàng tháng", "B": "Lợi nhuận từ đầu tư", "C": "Kế hoạch tài chính dài hạn", "D": "Giá trị tài sản"}, "category": "comparison", "question": "Ngân sách không bao gồm yếu tố nào dưới đây?", "difficulty": "hard", "explanation": "Ngân sách tập trung vào chi tiêu và thu nhập hàng tháng, không tính đến giá trị tài sản.", "correctAnswer": "D"}, {"id": "q11", "options": {"A": "Để tiêu nhiều hơn", "B": "Để theo dõi biến động tài chính", "C": "Để giảm thu nhập", "D": "Để tăng chi tiêu không cần thiết"}, "category": "connection", "question": "Tại sao cần điều chỉnh ngân sách hàng tháng?", "difficulty": "medium", "explanation": "Điều chỉnh ngân sách hàng tháng giúp bạn theo dõi biến động tài chính và điều chỉnh chi tiêu phù hợp.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Chỉ khi có vấn đề", "B": "Sau mỗi tháng", "C": "Sau mỗi 6 tháng hoặc một năm", "D": "Không cần đánh giá lại"}, "category": "comparison", "question": "Khi nào nên đánh giá lại ngân sách?", "difficulty": "medium", "explanation": "Đánh giá lại ngân sách sau mỗi 6 tháng hoặc một năm giúp bạn thích ứng với nhu cầu thay đổi.", "correctAnswer": "C"}]', 12, 80, 'Boss Quiz: Lập Ngân Sách Hàng Tháng Hiệu Quả', '2026-01-22 16:27:53.965', 'gpt-4o-mini', '2026-01-22 16:27:54.006712', '2026-01-22 16:27:54.006712');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('c703eb54-775b-49c7-b9d3-09456b0fd69c', '1cef0efb-7c27-49a7-8d8e-3fb944183db9', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Sử dụng sức mạnh từ chân và hông để tăng cường lực ném.", "B": "Chỉ sử dụng cánh tay để thực hiện cú ném.", "C": "Ném bóng từ một vị trí thấp và không cần bật lên.", "D": "Đứng nghiêng người để tăng độ chính xác."}, "category": "definition", "question": "Nguyên tắc chính của kỹ thuật ném bóng đứng là gì?", "explanation": "Đáp án A đúng vì nó nêu rõ việc sử dụng sức mạnh từ chân và hông là nguyên tắc chính trong kỹ thuật ném bóng đứng. Các đáp án khác đều không phản ánh đúng bản chất của kỹ thuật này.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Sự phối hợp giữa chân và cánh tay.", "B": "Tư thế đứng thẳng với chân rộng bằng vai.", "C": "Độ cao của bóng trước khi ném.", "D": "Sức mạnh từ chân để bật lên."}, "category": "definition", "question": "Khi thực hiện cú ném bóng đứng, yếu tố nào không quan trọng?", "explanation": "Đáp án C đúng vì độ cao của bóng trước khi ném không phải là yếu tố quan trọng trong kỹ thuật ném bóng đứng. Các đáp án khác đều là các yếu tố quan trọng trong kỹ thuật này.", "correctAnswer": "C"}, {"id": "q3", "options": {"A": "Kỹ thuật ném bóng đứng yêu cầu đứng thẳng, trong khi ném bóng chạy cần di chuyển.", "B": "Ném bóng đứng chỉ sử dụng tay, còn ném bóng chạy cần cả chân và tay.", "C": "Ném bóng đứng chỉ áp dụng cho bóng rổ, trong khi ném bóng chạy có thể áp dụng cho mọi môn thể thao.", "D": "Ném bóng đứng không cần sử dụng cơ thể."}, "category": "distinction", "question": "Kỹ thuật ném bóng đứng khác với ném bóng chạy ở điểm nào?", "explanation": "Đáp án A đúng vì kỹ thuật ném bóng đứng yêu cầu cầu thủ đứng thẳng, trong khi ném bóng chạy cần cầu thủ di chuyển. Các đáp án khác không chính xác vì chúng không phản ánh đúng sự khác biệt giữa hai kỹ thuật này.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Ném bóng ba điểm yêu cầu sử dụng lực từ chân nhiều hơn.", "B": "Kỹ thuật ném bóng đứng không cần chú ý đến đường bay của bóng.", "C": "Ném bóng ba điểm yêu cầu cầu thủ phải di chuyển liên tục.", "D": "Kỹ thuật ném bóng đứng không cần bật nhảy."}, "category": "distinction", "question": "Điểm khác biệt giữa kỹ thuật ném bóng đứng và ném bóng ba điểm là gì?", "explanation": "Đáp án A đúng vì ném bóng ba điểm yêu cầu cầu thủ sử dụng lực từ chân nhiều hơn để đạt khoảng cách xa hơn. Các đáp án khác sai vì chúng không phản ánh đúng bản chất của hai kỹ thuật này.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Lực ném từ cánh tay.", "B": "Sự phối hợp giữa các bộ phận cơ thể.", "C": "Tư thế đứng và vị trí chân.", "D": "Sự di chuyển liên tục của cầu thủ."}, "category": "distinction", "question": "Yếu tố nào không phải là yếu tố chính trong cú ném bóng đứng?", "explanation": "Đáp án D đúng vì kỹ thuật ném bóng đứng không yêu cầu cầu thủ phải di chuyển liên tục. Các yếu tố A, B và C đều là các yếu tố chính trong cú ném bóng đứng.", "correctAnswer": "D"}]', 5, 70, 'Nguyên Tắc Kỹ Thuật Ném Bóng Đứng', '2026-01-21 20:53:32.426', 'gpt-4o-mini', '2026-01-21 20:53:32.467798', '2026-01-21 20:53:32.467798');
