INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('3809bdeb-58a6-4d8f-adfd-1f3f8c9b60b8', NULL, '50b9d9a2-afee-4264-8cca-8b83a3661c45', 'boss', NULL, '[{"id": "q1", "options": {"A": "Một phương pháp chuyển tiền tự động từ tài khoản chi tiêu sang tài khoản tiết kiệm", "B": "Một cách để tiết kiệm tiền mặt tại nhà", "C": "Một hình thức đầu tư vào cổ phiếu", "D": "Một loại tài khoản ngân hàng mới"}, "category": "core_concept", "question": "Tiết kiệm tự động là gì?", "difficulty": "medium", "explanation": "Tiết kiệm tự động là phương pháp chuyển một phần thu nhập vào tài khoản tiết kiệm mà không cần phải nghĩ đến.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Phân chia thu nhập thành ba phần cho nhu cầu thiết yếu, sở thích và tiết kiệm", "B": "Chỉ tập trung vào tiết kiệm", "C": "Không cần theo dõi chi tiêu", "D": "Dùng tất cả tiền cho chi tiêu"}, "category": "core_concept", "question": "Phương pháp 50/30/20 giúp quản lý tài chính cá nhân như thế nào?", "difficulty": "medium", "explanation": "Phương pháp 50/30/20 phân chia thu nhập thành ba phần đảm bảo cân bằng giữa chi tiêu và tiết kiệm.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Giảm thiểu rủi ro tài chính", "B": "Tăng lãi suất ngân hàng", "C": "Tăng chi phí đầu tư", "D": "Tăng số lượng tài khoản ngân hàng"}, "category": "core_concept", "question": "Một trong những lợi ích của việc đa dạng hóa danh mục đầu tư là gì?", "difficulty": "medium", "explanation": "Đa dạng hóa giúp bảo vệ tài sản khỏi rủi ro từ bất kỳ một thị trường nhất định.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Mục tiêu tài chính cụ thể cho tương lai", "B": "Chỉ tiết kiệm mà không đầu tư", "C": "Chi tiêu không kiểm soát", "D": "Khám phá thị trường mới"}, "category": "core_concept", "question": "Chiến lược tiết kiệm quỹ tương lai liên quan đến điều gì?", "difficulty": "medium", "explanation": "Tiết kiệm theo quỹ tương lai đặt ra các mục tiêu tài chính cụ thể để đạt được trong tương lai.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Thiết lập mô hình tiết kiệm tự động", "B": "Chi tiêu tất cả thu nhập", "C": "Tìm kiếm tín dụng ngân hàng", "D": "Không tiết kiệm"}, "category": "connection", "question": "Gia đình Nguyễn đã áp dụng chiến lược nào để tiết kiệm cho quỹ giáo dục của các con?", "difficulty": "medium", "explanation": "Gia đình Nguyễn đã mở tài khoản tiết kiệm tự động để chuyển 20% thu nhập hàng tháng vào đó.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "50/30/20", "B": "40/40/20", "C": "60/30/10", "D": "70/20/10"}, "category": "connection", "question": "Ông Bình đã sử dụng phương pháp nào để phân chia ngân sách của mình?", "difficulty": "medium", "explanation": "Ông Bình đã áp dụng phương pháp 50/30/20 cho ngân sách hàng tháng của mình.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Đặt số tiền cần tiết kiệm", "B": "Xác định mục tiêu tài chính", "C": "Thiết lập ngân sách", "D": "Theo dõi chi tiêu"}, "category": "application", "question": "Khi xác định mục tiêu tiết kiệm, bước đầu tiên là gì?", "difficulty": "medium", "explanation": "Bước đầu tiên trong tiết kiệm theo mục tiêu là xác định các mục tiêu tài chính cụ thể.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "2 triệu đồng", "B": "3 triệu đồng", "C": "4 triệu đồng", "D": "5 triệu đồng"}, "category": "application", "question": "Nếu bạn có thu nhập 10 triệu đồng và theo phương pháp 50/30/20, bạn sẽ tiết kiệm bao nhiêu mỗi tháng?", "difficulty": "medium", "explanation": "Theo phương pháp 50/30/20, bạn sẽ tiết kiệm 20% thu nhập, tức là 2 triệu đồng.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Theo dõi chi tiêu và điều chỉnh kế hoạch", "B": "Không cần theo dõi", "C": "Chỉ tiết kiệm mà không cần đầu tư", "D": "Chỉ chi tiêu cho sở thích"}, "category": "application", "question": "Khi áp dụng chiến lược tiết kiệm, điều quan trọng là gì?", "difficulty": "medium", "explanation": "Theo dõi chi tiêu và điều chỉnh kế hoạch giúp bạn tiến gần hơn đến mục tiêu tài chính.", "correctAnswer": "A"}, {"id": "q10", "options": {"A": "Giúp hình thành thói quen tiết kiệm", "B": "Không cần theo dõi tiền tiết kiệm", "C": "Giảm căng thẳng tài chính", "D": "Tạo ra quỹ cho các mục tiêu tài chính"}, "category": "comparison", "question": "Điều nào không phải là một lợi ích của tiết kiệm tự động?", "difficulty": "hard", "explanation": "Dù tiết kiệm tự động, việc theo dõi tiến trình tiết kiệm vẫn rất quan trọng.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Không theo dõi tiến độ tiết kiệm", "B": "Đầu tư vào nhiều loại tài sản", "C": "Thiết lập mục tiêu cụ thể", "D": "Sử dụng tài khoản tiết kiệm"}, "category": "comparison", "question": "Một trong những sai lầm phổ biến khi tiết kiệm là gì?", "difficulty": "hard", "explanation": "Không theo dõi tiến độ tiết kiệm có thể dẫn đến việc không đạt được mục tiêu tài chính.", "correctAnswer": "A"}, {"id": "q12", "options": {"A": "Có mục tiêu cụ thể để tiết kiệm", "B": "Không cần lập kế hoạch", "C": "Không theo dõi chi tiêu", "D": "Chỉ tiết kiệm cho quỹ hưu trí"}, "category": "comparison", "question": "Tiết kiệm theo mục tiêu khác với tiết kiệm thông thường ở điểm nào?", "difficulty": "hard", "explanation": "Tiết kiệm theo mục tiêu yêu cầu bạn xác định rõ ràng các mục tiêu tài chính cụ thể.", "correctAnswer": "A"}]', 12, 80, 'Boss Quiz: Chiến lược tiết kiệm hiệu quả', '2026-01-22 16:29:06.906', 'gpt-4o-mini', '2026-01-22 16:29:06.947801', '2026-01-22 16:29:06.947801');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('e4062b5d-f7e7-4d4d-900c-e95f65c97083', '1b2e05f3-1497-4f29-96c1-af2d31072964', NULL, 'lesson', 'example', '[{"id": "q1", "options": {"A": "Chân đứng rộng và đầu gối khuỵu xuống.", "B": "Chân đứng hẹp và thẳng đứng.", "C": "Đứng một chân và nhảy lên.", "D": "Chân đứng rộng nhưng đầu gối thẳng."}, "category": "correct_example", "question": "Khi bạn dribble bóng, tư thế nào là đúng để giữ bóng gần mình?", "explanation": "Đáp án A đúng vì nó mô tả đúng tư thế cần thiết để giữ bóng gần và kiểm soát tốt. Các đáp án B, C, và D đều không phù hợp với tư thế dribble đúng.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Chạy nhanh qua khoảng trống và dribble bóng.", "B": "Đứng lại và chờ đồng đội đến.", "C": "Khiêu vũ thay vì chạy.", "D": "Đổi hướng và đi ra ngoài sân."}, "category": "correct_example", "question": "Khi bạn thấy khoảng trống trên sân, bạn nên:", "explanation": "Đáp án A đúng vì đó là hành động phù hợp khi có khoảng trống. Các đáp án B, C, và D không phản ánh hành động đúng trong tình huống này.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Dễ dàng kiểm soát bóng hơn.", "B": "Tránh bị cầu thủ đối phương cướp bóng.", "C": "Cả A và B.", "D": "Không cần để ý đến cầu thủ khác."}, "category": "correct_example", "question": "Khi bạn dribble, việc giữ bóng gần cơ thể sẽ giúp bạn:", "explanation": "Đáp án C là đúng vì việc giữ bóng gần cơ thể không chỉ giúp bạn kiểm soát tốt mà còn tránh bị mất bóng. Đáp án A và B cũng đúng nhưng không đầy đủ, trong khi D sai hoàn toàn.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Giữ bóng cách xa cơ thể.", "B": "Chạy lùi về phía cầu thủ đối phương.", "C": "Nhìn xung quanh để tìm đồng đội.", "D": "Thay đổi tốc độ di chuyển để đánh lừa đối thủ."}, "category": "wrong_example", "question": "Hành động nào sau đây là không đúng khi bạn đang dribble bóng?", "explanation": "Đáp án A sai vì giữ bóng cách xa cơ thể sẽ làm tăng khả năng bị cướp bóng. Các đáp án B, C và D đều là hành động hợp lý trong khi dribble.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Giữ bóng gần người và nhìn kỹ đồng đội.", "B": "Đứng im và đợi đồng đội đến gần.", "C": "Chạy ra ngoài sân và đứng chờ.", "D": "Quay lưng lại và chuyền bóng."}, "category": "wrong_example", "question": "Khi đang trong trận đấu và cần chuyền bóng, bạn nên:", "explanation": "Đáp án A đúng vì bạn cần giữ bóng gần để kiểm soát tốt và nhìn thấy đồng đội. B, C và D đều không phù hợp trong tình huống này.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Tập trung vào tư thế và hít thở sâu.", "B": "Ngừng chơi bóng.", "C": "Nói với đồng đội rằng bạn không muốn chơi.", "D": "Chạy ra khỏi sân."}, "category": "mini_case", "question": "Nếu bạn đang chuẩn bị dribble và cảm thấy không tự tin, bạn nên:", "explanation": "Đáp án A đúng vì tập trung và hít thở sâu giúp bạn tự tin hơn. Các đáp án B, C và D không phải là cách xử lý tốt.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Giữ bóng gần người và tìm cơ hội chuyền.", "B": "Chạy vào giữa đám đông và dribble.", "C": "Bỏ bóng xuống và dừng lại.", "D": "Quay lưng lại và chờ thời gian trôi qua."}, "category": "mini_case", "question": "Trong một trận đấu, bạn nhận bóng nhưng không có không gian để di chuyển. Bạn sẽ làm gì?", "explanation": "Đáp án A đúng vì giữ bóng gần và tìm cơ hội chuyền là cách tốt để xử lý tình huống. B, C và D đều không phải là giải pháp hợp lý.", "correctAnswer": "A"}]', 7, 70, 'Tư Thế Dribble Vào Lúc Chơi', '2026-01-21 20:59:09.877', 'gpt-4o-mini', '2026-01-21 20:59:10.035562', '2026-01-21 20:59:10.035562');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('0c3361b0-bb44-4d09-8236-4795faa01d37', NULL, '6fdf2752-f255-4c42-beb1-7425d8e2187c', 'boss', NULL, '[{"id": "q1", "options": {"A": "Chiến lược đầu tư mua và nắm giữ tài sản trong thời gian dài", "B": "Chiến lược đầu tư mua bán nhanh để kiếm lời", "C": "Đầu tư chỉ vào một loại tài sản duy nhất", "D": "Đầu tư chỉ trong vòng 1 năm"}, "category": "core_concept", "question": "Đầu tư dài hạn là gì?", "difficulty": "medium", "explanation": "Đầu tư dài hạn tập trung vào việc mua và nắm giữ tài sản trong thời gian dài để thu lợi nhuận từ sự tăng giá của tài sản.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Khả năng kiếm lời cao hơn nhờ vào lãi suất kép", "B": "Dễ dàng bán tài sản bất cứ lúc nào", "C": "Giảm thiểu rủi ro hoàn toàn", "D": "Chỉ cần đầu tư một lần duy nhất"}, "category": "core_concept", "question": "Lợi ích của đầu tư dài hạn so với đầu tư ngắn hạn là gì?", "difficulty": "medium", "explanation": "Đầu tư dài hạn cho phép nhà đầu tư tận dụng sức mạnh của lãi suất kép, từ đó mang lại lợi nhuận cao hơn.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Nghiên cứu thị trường và phân tích tài sản", "B": "Mua và giữ tài sản trong thời gian dài", "C": "Chỉ đầu tư vào cổ phiếu công nghệ", "D": "Tái đầu tư lợi nhuận để gia tăng giá trị tài sản"}, "category": "core_concept", "question": "Khái niệm nào sau đây không thuộc về đầu tư dài hạn?", "difficulty": "medium", "explanation": "Đầu tư dài hạn không giới hạn chỉ vào một loại tài sản mà có thể bao gồm nhiều loại khác nhau.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Giúp tránh những quyết định đầu tư sai lầm", "B": "Để có thể đầu tư vào mọi tài sản mà không cần lo lắng", "C": "Để xác định thời điểm tốt nhất để bán", "D": "Để có thể kiếm tiền nhanh chóng"}, "category": "core_concept", "question": "Tại sao nghiên cứu kỹ lưỡng trước khi đầu tư là quan trọng?", "difficulty": "medium", "explanation": "Nghiên cứu giúp nhà đầu tư đưa ra các quyết định thông minh và tránh những sai lầm có thể dẫn đến thua lỗ.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Giảm thiểu rủi ro", "B": "Tăng chi phí đầu tư", "C": "Giúp đầu tư vào một loại tài sản duy nhất", "D": "Không có ảnh hưởng gì đến lợi nhuận"}, "category": "connection", "question": "Trong chiến lược đầu tư dài hạn, việc phân bổ tài sản có vai trò gì?", "difficulty": "medium", "explanation": "Phân bổ tài sản giúp giảm thiểu rủi ro bằng cách đầu tư vào nhiều loại tài sản khác nhau.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Người dễ bị ảnh hưởng bởi biến động ngắn hạn của thị trường", "B": "Người có chiến lược đầu tư rõ ràng và kiên nhẫn", "C": "Người chỉ đầu tư vào các tài sản rủi ro cao", "D": "Người không theo dõi sự phát triển của tài sản"}, "category": "connection", "question": "Nhà đầu tư nào sau đây có khả năng nắm giữ tài sản trong thời gian dài tốt nhất?", "difficulty": "medium", "explanation": "Người có chiến lược rõ ràng và kiên nhẫn sẽ có khả năng nắm giữ tài sản tốt hơn và tránh hoảng loạn trong các tình huống biến động thị trường.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Mua ngay mà không cần tìm hiểu", "B": "Nghiên cứu thị trường và đánh giá tài sản", "C": "Chỉ xem xét giá bán", "D": "Đầu tư mà không có kế hoạch"}, "category": "application", "question": "Anna trong ví dụ đầu tư bất động sản đã thực hiện những bước nào trước khi đầu tư?", "difficulty": "hard", "explanation": "Anna đã nghiên cứu thị trường và đánh giá giá trị tài sản trước khi đưa ra quyết định đầu tư.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Nó có chi phí cao nhất", "B": "Nó có hiệu suất tốt trong lịch sử", "C": "Nó là quỹ mới thành lập", "D": "Nó không yêu cầu theo dõi thường xuyên"}, "category": "application", "question": "Michael đã chọn quỹ chỉ số S&P 500 vì lý do nào?", "difficulty": "hard", "explanation": "Michael chọn quỹ chỉ số S&P 500 vì nó đã cho thấy hiệu suất tốt trong lịch sử, phù hợp với mục tiêu đầu tư dài hạn của anh.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Cổ phiếu luôn an toàn hơn trái phiếu", "B": "Trái phiếu thường có lợi suất thấp hơn cổ phiếu", "C": "Cổ phiếu không có rủi ro", "D": "Trái phiếu không mang lại thu nhập ổn định"}, "category": "comparison", "question": "Khi so sánh đầu tư vào cổ phiếu và trái phiếu, điều nào sau đây đúng?", "difficulty": "medium", "explanation": "Trái phiếu thường có lợi suất thấp hơn cổ phiếu, nhưng cũng ít rủi ro hơn và cung cấp thu nhập ổn định.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Bán tất cả tài sản ngay lập tức", "B": "Theo dõi và đánh giá lại các khoản đầu tư", "C": "Không quan tâm đến tình hình thị trường", "D": "Chỉ đầu tư vào tiền mặt"}, "category": "comparison", "question": "Trong đầu tư dài hạn, một nhà đầu tư nên làm gì khi thị trường biến động?", "difficulty": "medium", "explanation": "Nhà đầu tư nên theo dõi và đánh giá lại các khoản đầu tư để đảm bảo rằng chúng vẫn phù hợp với mục tiêu dài hạn.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Kiên nhẫn và tự tin", "B": "Hoảng loạn khi thị trường giảm", "C": "Chỉ nghe theo người khác", "D": "Đầu tư mà không có kế hoạch"}, "category": "core_concept", "question": "Tâm lý nào sau đây là cần thiết trong đầu tư dài hạn?", "difficulty": "medium", "explanation": "Tâm lý kiên nhẫn và tự tin giúp nhà đầu tư duy trì chiến lược đầu tư dài hạn mà không bị ảnh hưởng bởi biến động ngắn hạn.", "correctAnswer": "A"}, {"id": "q12", "options": {"A": "Chỉ đầu tư vào một loại tài sản", "B": "Đánh giá và điều chỉnh định kỳ", "C": "Không cần xem xét chi phí", "D": "Chỉ đầu tư khi thị trường đang tăng"}, "category": "connection", "question": "Một nhà đầu tư muốn tối ưu hóa danh mục đầu tư cần chú ý điều gì?", "difficulty": "medium", "explanation": "Đánh giá và điều chỉnh định kỳ giúp nhà đầu tư đảm bảo rằng danh mục đầu tư vẫn phù hợp với mục tiêu tài chính.", "correctAnswer": "B"}, {"id": "q13", "options": {"A": "Rủi ro tâm lý", "B": "Rủi ro không có khả năng sinh lời", "C": "Rủi ro từ việc không theo dõi thị trường", "D": "Tất cả các lựa chọn trên"}, "category": "comparison", "question": "Rủi ro nào sau đây thường gặp trong đầu tư dài hạn?", "difficulty": "hard", "explanation": "Tất cả các yếu tố này đều có thể dẫn đến rủi ro trong đầu tư dài hạn nếu nhà đầu tư không chú ý.", "correctAnswer": "D"}, {"id": "q14", "options": {"A": "Tham gia vào nhiều nhóm đầu tư để học hỏi", "B": "Chỉ nghe theo bạn bè", "C": "Đầu tư mà không tìm hiểu", "D": "Chỉ đầu tư vào những gì họ thích"}, "category": "application", "question": "Khi đầu tư, nhà đầu tư nên làm gì để tránh các quyết định sai lầm?", "difficulty": "hard", "explanation": "Tham gia vào các nhóm đầu tư giúp nhà đầu tư học hỏi và chia sẻ kinh nghiệm, từ đó tránh được những quyết định sai lầm.", "correctAnswer": "A"}, {"id": "q15", "options": {"A": "Giúp tăng trưởng tài sản nhanh chóng", "B": "Giảm thiểu rủi ro đầu tư", "C": "Không cần theo dõi hiệu suất", "D": "Tạo ra chi phí cao hơn"}, "category": "core_concept", "question": "Tại sao việc tái đầu tư lợi nhuận là quan trọng trong đầu tư dài hạn?", "difficulty": "medium", "explanation": "Tái đầu tư lợi nhuận giúp gia tăng giá trị tài sản thông qua sức mạnh của lãi suất kép.", "correctAnswer": "A"}]', 15, 80, 'Boss Quiz: Chiến lược đầu tư dài hạn', '2026-01-22 16:29:50.221', 'gpt-4o-mini', '2026-01-22 16:29:50.259875', '2026-01-22 16:29:50.259875');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('3bdf1064-f299-4e03-95b0-c3ccafdafde4', NULL, '30dc66e2-1493-4b2e-999d-6525979015b5', 'boss', NULL, '[{"id": "q1", "options": {"A": "Quá trình chuẩn bị tài chính để sống thoải mái sau khi nghỉ hưu", "B": "Một chương trình đào tạo nghề cho người cao tuổi", "C": "Chỉ việc tiết kiệm tiền hàng tháng", "D": "Phương pháp đầu tư vào bất động sản"}, "category": "core_concept", "question": "Lập kế hoạch hưu trí là gì?", "difficulty": "medium", "explanation": "Lập kế hoạch hưu trí là quá trình chuẩn bị tài chính để đảm bảo cuộc sống ổn định và thoải mái sau khi nghỉ hưu.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Tính toán thời gian còn lại cho đến khi nghỉ hưu", "B": "Chỉ đầu tư vào một loại tài sản", "C": "Làm việc nhiều giờ hơn", "D": "Chỉ phụ thuộc vào lương hưu từ nhà nước"}, "category": "core_concept", "question": "Điều nào sau đây là một trong những bước quan trọng trong lập kế hoạch hưu trí?", "difficulty": "medium", "explanation": "Tính toán thời gian còn lại cho đến khi nghỉ hưu giúp bạn xác định cách tiết kiệm và đầu tư phù hợp.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Thị trường và nhu cầu cá nhân sẽ thay đổi theo thời gian", "B": "Để có thể tiết kiệm ít hơn", "C": "Để quên đi kế hoạch ban đầu", "D": "Để không cần phải đầu tư nữa"}, "category": "core_concept", "question": "Tại sao việc theo dõi và điều chỉnh kế hoạch hưu trí là cần thiết?", "difficulty": "medium", "explanation": "Theo dõi và điều chỉnh kế hoạch hưu trí giúp bạn đảm bảo rằng nó vẫn phù hợp với tình hình tài chính và mục tiêu của bạn.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Đánh giá chi phí sinh hoạt hàng tháng", "B": "Dự đoán tuổi thọ", "C": "Đầu tư vào vàng", "D": "Tính toán lạm phát"}, "category": "core_concept", "question": "Yếu tố nào không phải là một phần của việc tính toán nhu cầu tài chính khi nghỉ hưu?", "difficulty": "medium", "explanation": "Đầu tư vào vàng không phải là một phần của quá trình tính toán nhu cầu tài chính mà là một phương pháp đầu tư.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "Giảm khoản tiết kiệm hàng tháng", "B": "Tăng khoản tiết kiệm hàng tháng lên 50%", "C": "Ngừng đầu tư", "D": "Chuyển sang sống ở nơi khác"}, "category": "application", "question": "Ông Nguyễn đã điều chỉnh kế hoạch hưu trí của mình như thế nào khi nhận ra chi phí sinh hoạt tăng cao?", "difficulty": "medium", "explanation": "Ông Nguyễn đã quyết định tăng khoản tiết kiệm hàng tháng lên 50% để có đủ tài chính cho hưu trí.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Tiết kiệm tiền hàng tháng và đầu tư vào bất động sản", "B": "Chỉ sống dựa vào lương hưu", "C": "Không có kế hoạch gì cả", "D": "Tiêu xài phung phí"}, "category": "application", "question": "Bà Lan đã sử dụng phương pháp nào để đảm bảo tài chính cho hưu trí?", "difficulty": "medium", "explanation": "Bà Lan đã bắt đầu lập kế hoạch hưu trí từ sớm bằng cách tiết kiệm tiền và đầu tư vào bất động sản.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Rủi ro thị trường", "B": "Tăng lương", "C": "Giảm chi phí sinh hoạt", "D": "Tăng thu nhập từ công việc"}, "category": "core_concept", "question": "Điều nào sau đây là một trong những rủi ro tài chính trong kế hoạch hưu trí?", "difficulty": "medium", "explanation": "Rủi ro thị trường liên quan đến sự biến động của giá trị tài sản trên thị trường, có thể ảnh hưởng đến kế hoạch hưu trí của bạn.", "correctAnswer": "A"}, {"id": "q8", "options": {"A": "Giảm rủi ro và tối đa hóa lợi nhuận tiềm năng", "B": "Chỉ đầu tư vào một loại tài sản", "C": "Tăng chi phí đầu tư", "D": "Giúp bạn quên đi việc theo dõi đầu tư"}, "category": "comparison", "question": "Trong kế hoạch hưu trí, việc đa dạng hóa danh mục đầu tư có tác dụng gì?", "difficulty": "hard", "explanation": "Đa dạng hóa danh mục đầu tư gi