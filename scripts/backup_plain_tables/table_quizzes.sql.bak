INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('1da38501-53c0-4aa8-bd07-e09ea6d56767', NULL, 'a3d61119-14aa-4364-9789-43153104671d', 'boss', NULL, '[{"id": "q1", "options": {"A": "Sử dụng tiền một cách hiệu quả để đạt được mục tiêu tài chính", "B": "Chi tiêu nhiều hơn số tiền bạn kiếm được", "C": "Chỉ tiết kiệm mà không cần đầu tư", "D": "Mua sắm theo sở thích mà không có kế hoạch"}, "category": "core_concept", "question": "Chi tiêu thông minh chủ yếu liên quan đến việc gì?", "difficulty": "medium", "explanation": "Chi tiêu thông minh liên quan đến việc sử dụng tiền một cách hiệu quả để đạt được các mục tiêu tài chính, không chỉ đơn thuần là tiết kiệm hay chi tiêu theo sở thích.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "10%", "B": "20%", "C": "30%", "D": "50%"}, "category": "core_concept", "question": "Khi lập ngân sách, phần trăm nào thường được khuyến nghị dành cho tiết kiệm?", "difficulty": "medium", "explanation": "Nhiều chuyên gia tài chính khuyến nghị dành ít nhất 20% thu nhập cho tiết kiệm để đảm bảo an toàn tài chính.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Lập ngân sách", "B": "Tiết kiệm tiền", "C": "Xác định mục tiêu tài chính", "D": "Đánh giá chi tiêu"}, "category": "core_concept", "question": "Một trong những bước đầu tiên trong chi tiêu thông minh là gì?", "difficulty": "medium", "explanation": "Xác định mục tiêu tài chính là bước quan trọng đầu tiên trong quy trình chi tiêu thông minh, vì nó giúp bạn định hướng cách chi tiêu của mình.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Chi tiêu cần thiết chiếm 30%, không cần thiết 70%", "B": "Chi tiêu cần thiết chiếm 50%, không cần thiết 50%", "C": "Chi tiêu cần thiết chiếm 70%, không cần thiết 30%", "D": "Chi tiêu cần thiết chiếm 90%, không cần thiết 10%"}, "category": "core_concept", "question": "Chi tiêu cần thiết và chi tiêu không cần thiết nên được phân chia như thế nào trong ngân sách?", "difficulty": "medium", "explanation": "Phân chia ngân sách thường khuyến nghị rằng chi tiêu cần thiết nên chiếm khoảng 70% để đảm bảo nhu cầu cơ bản được đáp ứng.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "Giúp phát hiện các khoản chi không cần thiết", "B": "Tăng cường chi tiêu", "C": "Giảm khả năng tiết kiệm", "D": "Giúp bạn chi tiêu nhiều hơn"}, "category": "core_concept", "question": "Tại sao việc phân tích chi tiêu hàng tháng lại quan trọng?", "difficulty": "medium", "explanation": "Phân tích chi tiêu hàng tháng giúp bạn nhận ra các khoản chi không cần thiết và tìm ra cơ hội để cắt giảm chi phí.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Theo dõi tất cả các khoản chi tiêu", "B": "Chỉ ghi lại chi tiêu cần thiết", "C": "Không cần thay đổi ngân sách", "D": "Chi tiêu theo cảm xúc"}, "category": "connection", "question": "Khi lập ngân sách, việc nào sau đây là quan trọng nhất?", "difficulty": "medium", "explanation": "Theo dõi tất cả các khoản chi tiêu là cần thiết để có một cái nhìn tổng quan về tình hình tài chính và điều chỉnh ngân sách khi cần thiết.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "1 triệu VND", "B": "2 triệu VND", "C": "3 triệu VND", "D": "4 triệu VND"}, "category": "application", "question": "Nếu một người có thu nhập 10 triệu VND và muốn tiết kiệm 20% cho quỹ khẩn cấp, họ nên tiết kiệm bao nhiêu mỗi tháng?", "difficulty": "medium", "explanation": "20% của 10 triệu VND là 2 triệu VND, vì vậy họ nên tiết kiệm 2 triệu VND mỗi tháng cho quỹ khẩn cấp.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Tiêu hết", "B": "Tiết kiệm hoặc đầu tư", "C": "Vay thêm", "D": "Mua sắm đồ không cần thiết"}, "category": "application", "question": "Một sinh viên có thu nhập 5 triệu VND muốn tiết kiệm 1 triệu cho học phí, 1 triệu cho ăn uống, và 1 triệu cho chi phí khác. Số tiền còn lại nên được sử dụng như thế nào?", "difficulty": "medium", "explanation": "Số tiền còn lại nên được tiết kiệm hoặc đầu tư để đảm bảo tài chính trong tương lai, thay vì tiêu hết hoặc vay thêm.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Chi tiêu thông minh chỉ là tiết kiệm", "B": "Chi tiêu thông minh là quản lý tổng thể tài chính, tiết kiệm là một phần của nó", "C": "Chi tiêu thông minh không cần lập ngân sách", "D": "Tiết kiệm không quan trọng trong chi tiêu thông minh"}, "category": "comparison", "question": "Chi tiêu thông minh và tiết kiệm có sự khác biệt như thế nào?", "difficulty": "medium", "explanation": "Chi tiêu thông minh bao gồm việc quản lý tổng thể tài chính, trong đó tiết kiệm là một phần quan trọng để đạt được mục tiêu tài chính.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Ghi chép chính xác mọi khoản chi", "B": "Chỉ ghi lại các khoản lớn", "C": "Không cần đánh giá mức độ cần thiết", "D": "Phân loại chi tiêu không cần thiết"}, "category": "core_concept", "question": "Khi phân tích chi tiêu, điều nào sau đây là cần thiết?", "difficulty": "medium", "explanation": "Ghi chép chính xác mọi khoản chi là cần thiết để phân tích đúng đắn và tạo ra thông tin hữu ích cho việc điều chỉnh ngân sách.", "correctAnswer": "A"}, {"id": "q11", "options": {"A": "1 triệu VND", "B": "2 triệu VND", "C": "1.67 triệu VND", "D": "2.5 triệu VND"}, "category": "application", "question": "Một doanh nhân trẻ muốn tiết kiệm 100 triệu VND trong 5 năm. Họ cần tiết kiệm bao nhiêu mỗi tháng?", "difficulty": "hard", "explanation": "Để tiết kiệm 100 triệu VND trong 5 năm, doanh nhân cần tiết kiệm khoảng 1.67 triệu VND mỗi tháng.", "correctAnswer": "C"}, {"id": "q12", "options": {"A": "Tiết kiệm có rủi ro cao hơn đầu tư", "B": "Đầu tư không cần lập kế hoạch", "C": "Tiết kiệm thường có lãi suất thấp hơn đầu tư", "D": "Đầu tư không cần theo dõi"}, "category": "comparison", "question": "Khi so sánh giữa tiết kiệm và đầu tư, điều nào dưới đây là đúng?", "difficulty": "hard", "explanation": "Tiết kiệm thường có lãi suất thấp hơn so với lợi nhuận từ đầu tư, nhưng đầu tư đi kèm với rủi ro cao hơn.", "correctAnswer": "C"}, {"id": "q13", "options": {"A": "Sử dụng ứng dụng quản lý tài chính", "B": "Không cần theo dõi", "C": "Chỉ ghi lại các khoản lớn", "D": "Chi tiêu theo cảm xúc"}, "category": "connection", "question": "Khi một gia đình lập ngân sách, điều gì giúp họ theo dõi chi tiêu hiệu quả?", "difficulty": "medium", "explanation": "Sử dụng ứng dụng quản lý tài chính giúp gia đình theo dõi chi tiêu hiệu quả và dễ dàng điều chỉnh ngân sách khi cần.", "correctAnswer": "A"}, {"id": "q14", "options": {"A": "Cắt giảm chi tiêu không cần thiết", "B": "Tăng thu nhập", "C": "Vay tiền", "D": "Không thay đổi gì"}, "category": "application", "question": "Một người muốn giảm chi tiêu hàng tháng, họ nên bắt đầu từ đâu?", "difficulty": "medium", "explanation": "Cắt giảm chi tiêu không cần thiết là cách hiệu quả nhất để giảm chi tiêu hàng tháng mà không cần phải tăng thu nhập hay vay tiền.", "correctAnswer": "A"}, {"id": "q15", "options": {"A": "Chi tiêu nhiều hơn", "B": "Lập ngân sách và xác định mục tiêu tiết kiệm", "C": "Mua sắm nhiều hơn", "D": "Không cần làm gì"}, "category": "core_concept", "question": "Một sinh viên muốn tiết kiệm cho du học, họ nên làm gì trước tiên?", "difficulty": "medium", "explanation": "Lập ngân sách và xác định mục tiêu tiết kiệm là bước đầu tiên quan trọng để đạt được ước mơ du học.", "correctAnswer": "B"}]', 15, 80, 'Boss Quiz: Chi Tiêu Thông Minh: Quản Lý Tài Chính Cá Nhân', '2026-01-22 17:11:46.159', 'gpt-4o-mini', '2026-01-22 17:11:46.206118', '2026-01-22 17:11:46.206118');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('be669461-cfc5-4d75-a811-cf9aefb10e50', '884ae9c9-a3db-4d0f-a772-551a03109a8b', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Gập Đầu Gối", "B": "Chuẩn Bị", "C": "Nhấc Bóng", "D": "Thả Bóng"}, "category": "definition", "question": "Bước nào trong kỹ thuật ném bóng cơ bản liên quan đến việc tạo lực từ chân?", "explanation": "Gập Đầu Gối là bước giúp tạo ra lực từ chân để thực hiện cú ném. Các đáp án khác không liên quan đến việc tạo lực từ chân.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Không kiểm soát được lực ném", "B": "Giữ mắt trên bóng và rổ", "C": "Thả bóng quá muộn", "D": "Đứng sai tư thế"}, "category": "definition", "question": "Sai lầm thường gặp khi thực hiện kỹ thuật ném bóng cơ bản là gì?", "explanation": "Sai lầm thường gặp là không kiểm soát được lực ném, điều này dẫn đến bóng không vào rổ. Các đáp án khác không phải là sai lầm chính.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Kỹ thuật ném bóng sử dụng cả chân và tay để tạo lực", "B": "Kỹ thuật chuyền bóng chỉ sử dụng tay", "C": "Kỹ thuật ném bóng không cần sự chuẩn bị", "D": "Kỹ thuật chuyền bóng yêu cầu phải gập đầu gối"}, "category": "distinction", "question": "Kỹ thuật ném bóng cơ bản khác với kỹ thuật chuyền bóng ở điểm nào?", "explanation": "Kỹ thuật ném bóng sử dụng cả chân và tay để tạo lực, trong khi kỹ thuật chuyền bóng chủ yếu sử dụng tay. Các đáp án khác không chính xác.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Ném bóng sử dụng lực từ chân, phát bóng không cần", "B": "Ném bóng hướng lên cao, phát bóng hướng ngang", "C": "Ném bóng chỉ dành cho bóng rổ, phát bóng dành cho bóng đá", "D": "Ném bóng có nhiều bước, phát bóng chỉ có một bước"}, "category": "distinction", "question": "Điểm khác biệt giữa ném bóng và phát bóng là gì?", "explanation": "Ném bóng thường hướng lên cao trong khi phát bóng thường hướng ngang. Các đáp án khác không chính xác hoặc không đúng với thực tế.", "correctAnswer": "B"}]', 4, 70, 'Kỹ Thuật Ném Bóng Cơ Bản', '2026-01-22 09:31:04.904', 'gpt-4o-mini', '2026-01-22 09:31:04.945714', '2026-01-22 09:31:04.945714');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('b390ce22-8dfc-48be-9a22-e9d4e586cb79', NULL, '83bd90f8-4b1f-4ccf-831d-c499713a5ff7', 'boss', NULL, '[{"id": "q1", "options": {"A": "5/4", "B": "4/4", "C": "3/4", "D": "6/8"}, "category": "core_concept", "question": "Nhịp điệu chính của bản nhạc ''Take Five'' là gì?", "difficulty": "medium", "explanation": "''Take Five'' nổi bật với nhịp điệu 5/4, điều này tạo nên sự khác biệt cho bản nhạc so với các tác phẩm jazz khác.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Nhạc cụ chính", "B": "Bộ phận phản hồi", "C": "Chỉ là nhạc nền", "D": "Không có vai trò quan trọng"}, "category": "core_concept", "question": "Vai trò của piano trong jazz không chỉ là nhạc cụ phụ trợ mà còn là gì?", "difficulty": "medium", "explanation": "Piano không chỉ là nhạc cụ phụ trợ mà còn có thể đóng vai trò chính trong nhiều bản nhạc jazz.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Ngẫu hứng", "B": "Cảm xúc mạnh mẽ", "C": "Theo khuôn mẫu cứng nhắc", "D": "Hòa âm phong phú"}, "category": "core_concept", "question": "Yếu tố nào dưới đây không phải là đặc trưng của nhạc jazz?", "difficulty": "medium", "explanation": "Jazz đặc trưng bởi sự tự do và ngẫu hứng, không theo khuôn mẫu cứng nhắc.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Sự sáng tạo và kỹ thuật của nghệ sĩ", "B": "Nền tảng hòa âm", "C": "Tương tác với khán giả", "D": "Sự lặp lại giai điệu"}, "category": "core_concept", "question": "Phần solo trong ''Take Five'' chủ yếu thể hiện điều gì?", "difficulty": "medium", "explanation": "Phần solo trong ''Take Five'' thể hiện sự sáng tạo và kỹ thuật của nghệ sĩ piano, tạo ra không khí hấp dẫn.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Cả hai đều không sử dụng ngẫu hứng", "B": "Nhạc jazz không ảnh hưởng đến hip-hop", "C": "Hip-hop thường sử dụng mẫu âm thanh từ jazz", "D": "Cả hai thể loại đều không sử dụng nhạc cụ"}, "category": "connection", "question": "Mối liên hệ nào giữa nhạc jazz và nhạc hip-hop?", "difficulty": "medium", "explanation": "Nhạc hip-hop thường vay mượn các mẫu âm thanh từ nhạc jazz để tạo ra bản nhạc của mình.", "correctAnswer": "C"}, {"id": "q6", "options": {"A": "Bebop nhấn mạnh vào ngẫu hứng hơn", "B": "Swing có nhịp điệu nhanh hơn", "C": "Bebop không có cảm xúc", "D": "Swing không sử dụng nhạc cụ"}, "category": "comparison", "question": "Điểm khác biệt chính giữa ''Bebop'' và ''Swing'' trong nhạc jazz là gì?", "difficulty": "hard", "explanation": "Bebop nhấn mạnh vào sự phức tạp và ngẫu hứng hơn so với Swing, thường có tiết tấu dễ nghe hơn.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Cố định và không thay đổi", "B": "Đơn giản và dễ hiểu", "C": "Phức tạp và đa dạng", "D": "Không có cấu trúc rõ ràng"}, "category": "core_concept", "question": "Cấu trúc harmonic trong jazz có thể được mô tả như thế nào?", "difficulty": "hard", "explanation": "Cấu trúc harmonic trong jazz rất phức tạp và đa dạng, thường bao gồm nhiều loại hợp âm và tiến trình khác nhau.", "correctAnswer": "C"}, {"id": "q8", "options": {"A": "Người nghe", "B": "Nghệ sĩ piano", "C": "Nghệ sĩ saxophone", "D": "Tất cả các nhạc công"}, "category": "application", "question": "Khi áp dụng kỹ thuật call-and-response trong jazz, ai thường là người thực hiện phần call?", "difficulty": "medium", "explanation": "Trong kỹ thuật call-and-response, nghệ sĩ piano thường là người khởi xướng phần call.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Để có thể chơi theo một khuôn mẫu cứng nhắc", "B": "Để cải thiện khả năng tương tác với các nhạc công khác", "C": "Để chơi những nốt nhạc không liên quan", "D": "Hòa âm không quan trọng trong jazz"}, "category": "application", "question": "Tại sao việc hiểu về hòa âm lại quan trọng khi chơi piano trong jazz?", "difficulty": "medium", "explanation": "Việc hiểu về hòa âm giúp nghệ sĩ piano tương tác tốt hơn với các nhạc công khác trong ban nhạc.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Để giữ cho giai điệu đơn điệu", "B": "Để tạo ra sự lặp lại mà không có sự sáng tạo", "C": "Để phát triển âm nhạc qua các biến thể", "D": "Để không cần sử dụng scale"}, "category": "core_concept", "question": "Mục đích của việc sử dụng motif trong improvisation là gì?", "difficulty": "hard", "explanation": "Sử dụng motif trong improvisation cho phép nghệ sĩ phát triển âm nhạc qua các biến thể khác nhau, tạo nên chiều sâu cho phần trình diễn.", "correctAnswer": "C"}, {"id": "q11", "options": {"A": "Phong cách chơi nhanh và mạnh", "B": "Sử dụng hợp âm phức tạp và ngẫu hứng mượt mà", "C": "Chỉ chơi theo khuôn mẫu", "D": "Không có sự ngẫu hứng"}, "category": "core_concept", "question": "Bill Evans được biết đến với điều gì trong phong cách chơi piano của mình?", "difficulty": "hard", "explanation": "Bill Evans nổi tiếng với việc sử dụng hợp âm phức tạp và phong cách ngẫu hứng mượt mà, tạo ra âm thanh đặc trưng cho bản nhạc.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Hợp âm trưởng", "B": "Hợp âm thứ", "C": "Hợp âm bảy", "D": "Hợp âm đơn giản"}, "category": "core_concept", "question": "Hợp âm nào dưới đây thường được sử dụng trong nhạc jazz để tạo sự phong phú cho giai điệu?", "difficulty": "medium", "explanation": "Hợp âm bảy (major 7th, minor 7th, dominant 7th) thường được sử dụng trong jazz để tạo ra sự phong phú cho giai điệu.", "correctAnswer": "C"}, {"id": "q13", "options": {"A": "1900-1920", "B": "1930-1940", "C": "1950-1960", "D": "1970-1980"}, "category": "core_concept", "question": "Khi nào nhạc jazz bắt đầu phát triển mạnh mẽ và trở nên phổ biến trong các câu lạc bộ?", "difficulty": "medium", "explanation": "Giai đoạn Swing (1930-1940) là thời kỳ jazz phát triển mạnh mẽ và trở nên phổ biến trong các câu lạc bộ và buổi hòa nhạc.", "correctAnswer": "B"}, {"id": "q14", "options": {"A": "Nhạc jazz cổ điển không có ngẫu hứng", "B": "Nhạc jazz hiện đại có nhiều yếu tố cách tân", "C": "Nhạc jazz cổ điển không sử dụng nhạc cụ", "D": "Không có sự khác biệt nào"}, "category": "comparison", "question": "Điểm khác biệt giữa nhạc jazz cổ điển và nhạc jazz hiện đại là gì?", "difficulty": "hard", "explanation": "Nhạc jazz hiện đại thường bao gồm nhiều yếu tố cách tân và sáng tạo hơn so với nhạc jazz cổ điển.", "correctAnswer": "B"}, {"id": "q15", "options": {"A": "Chơi theo khuôn mẫu", "B": "Sử dụng scale và motif", "C": "Chỉ chơi những nốt nhạc đơn giản", "D": "Không cần luyện tập"}, "category": "application", "question": "Kỹ thuật nào giúp nghệ sĩ piano cải thiện khả năng sáng tạo trong jazz?", "difficulty": "medium", "explanation": "Sử dụng scale và motif giúp nghệ sĩ piano phát triển khả năng sáng tạo và improvisation trong jazz.", "correctAnswer": "B"}]', 15, 80, 'Boss Quiz: Khám Phá Âm Nhạc Jazz và Vai Trò của Piano', '2026-01-22 17:12:33.283', 'gpt-4o-mini', '2026-01-22 17:12:33.32908', '2026-01-22 17:12:33.32908');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('27caa2b6-2d7b-4e67-9f42-5157bf6dc2db', NULL, 'dcdc7756-1c2d-4077-8f20-19cd69d7a272', 'boss', NULL, '[{"id": "q1", "options": {"A": "Làm sáng da", "B": "Giảm nếp nhăn", "C": "Cung cấp độ ẩm", "D": "Chống lão hóa"}, "category": "core_concept", "question": "Vitamin C có tác dụng chính nào đối với làn da?", "difficulty": "medium", "explanation": "Vitamin C chủ yếu được biết đến với khả năng làm sáng da bằng cách giảm sản xuất melanin.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Vitamin C", "B": "Axit hyaluronic", "C": "Retinol", "D": "Niacinamide"}, "category": "core_concept", "question": "Thành phần hoạt chất nào dưới đây được coi là hiệu quả trong việc chống lão hóa?", "difficulty": "medium", "explanation": "Retinol là một dẫn xuất của vitamin A, nổi tiếng với khả năng kích thích sản xuất collagen và chống lão hóa.", "correctAnswer": "C"}, {"id": "q3", "options": {"A": "Làm sáng da", "B": "Cấp ẩm cho da", "C": "Giảm nếp nhăn", "D": "Chống oxy hóa"}, "category": "core_concept", "question": "Axit hyaluronic có vai trò gì trong chăm sóc da?", "difficulty": "medium", "explanation": "Axit hyaluronic chủ yếu được sử dụng để cung cấp độ ẩm cho da, giúp da luôn mềm mại và căng bóng.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Tăng cường khả năng chống nắng", "B": "Tăng cường khả năng chống oxy hóa", "C": "Cung cấp độ ẩm", "D": "Giảm nếp nhăn"}, "category": "core_concept", "question": "Sự kết hợp giữa vitamin C và E có tác dụng gì?", "difficulty": "medium", "explanation": "Sự kết hợp giữa vitamin C và E có thể tăng cường khả năng chống oxy hóa và bảo vệ da khỏi tác hại của môi trường.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Vitamin C", "B": "Axit hyaluronic", "C": "Niacinamide", "D": "Peptide"}, "category": "comparison", "question": "Thành phần nào không nên kết hợp với retinol để tránh kích ứng?", "difficulty": "hard", "explanation": "Vitamin C và retinol không nên sử dụng cùng lúc vì có thể gây kích ứng cho da.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Sử dụng vào buổi sáng", "B": "Bắt đầu với nồng độ cao", "C": "Kết hợp với kem chống nắng vào ban ngày", "D": "Theo dõi phản ứng của da"}, "category": "application", "question": "Khi sử dụng retinol, điều gì là quan trọng cần lưu ý?", "difficulty": "medium", "explanation": "Theo dõi phản ứng của da là rất quan trọng, đặc biệt khi bắt đầu sử dụng retinol, vì nó có thể gây kích ứng.", "correctAnswer": "D"}, {"id": "q7", "options": {"A": "Sử dụng vào buổi tối", "B": "Sử dụng sau khi thoa kem chống nắng", "C": "Sử dụng vào buổi sáng", "D": "Kết hợp với các loại axit"}, "category": "application", "question": "Làm thế nào để vitamin C hoạt động hiệu quả nhất?", "difficulty": "medium", "explanation": "Vitamin C nên được sử dụng vào buổi sáng để tăng cường khả năng bảo vệ da khỏi tác hại của ánh nắng mặt trời.", "correctAnswer": "C"}, {"id": "q8", "options": {"A": "Khi bạn có làn da nhờn", "B": "Khi bạn bước vào độ tuổi 30", "C": "Khi bạn không có bất kỳ vấn đề về da nào", "D": "Khi bạn có làn da nhạy cảm"}, "category": "application", "question": "Khi nào bạn nên bắt đầu sử dụng retinol?", "difficulty": "medium", "explanation": "Retinol thường được khuyến nghị bắt đầu sử dụng khi bạn bước vào độ tuổi 30, khi các dấu hiệu lão hóa bắt đầu xuất hiện.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Giúp làm sáng da", "B": "Hút ẩm từ môi trường", "C": "Kích thích sản xuất collagen", "D": "Giảm nếp nhăn"}, "category": "core_concept", "question": "Axit hyaluronic hoạt động như thế nào trong chăm sóc da?", "difficulty": "medium", "explanation": "Axit hyaluronic có khả năng hút ẩm từ môi trường và giữ ẩm cho da, giúp da luôn mềm mại và căng bóng.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Chọn sản phẩm đắt tiền", "B": "Theo dõi phản ứng của da", "C": "Sử dụng nhiều sản phẩm cùng lúc", "D": "Chọn sản phẩm có mùi thơm"}, "category": "application", "question": "Khi thử nghiệm với các sản phẩm chăm sóc da, điều gì là quan trọng nhất?", "difficulty": "medium", "explanation": "Theo dõi phản ứng của da là cần thiết để xác định sản phẩm nào phù hợp nhất với làn da của bạn.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Cung cấp độ ẩm", "B": "Tăng cường sản xuất collagen", "C": "Chống oxy hóa", "D": "Làm sáng da"}, "category": "core_concept", "question": "Chức năng chính của retinol là gì?", "difficulty": "medium", "explanation": "Retinol giúp kích thích sản xuất collagen và cải thiện tình trạng da lão hóa.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Mụn", "B": "Đốm nâu", "C": "Nếp nhăn", "D": "Da nhờn"}, "category": "core_concept", "question": "Vitamin C có thể giúp giảm sự xuất hiện của điều gì trên da?", "difficulty": "medium", "explanation": "Vitamin C có khả năng làm giảm sự xuất hiện của đốm nâu và cải thiện độ sáng da.", "correctAnswer": "B"}, {"id": "q13", "options": {"A": "Da nhờn", "B": "Da khô", "C": "Da mụn", "D": "Da nhạy cảm"}, "category": "application", "question": "Tình trạng da nào có thể được cải thiện khi sử dụng axit hyaluronic?", "difficulty": "medium", "explanation": "Axit hyaluronic rất hiệu quả trong việc cung cấp độ ẩm cho da khô, giúp da trở nên mềm mại và căng bóng hơn.", "correctAnswer": "B"}, {"id": "q14", "options": {"A": "Giảm tiết bã nhờn", "B": "Làm sáng da nhanh chóng", "C": "Kích thích tái tạo tế bào", "D": "Cấp ẩm cho da"}, "category": "comparison", "question": "So với các thành phần khác, retinol có tác dụng gì nổi bật?", "difficulty": "hard", "explanation": "Retinol nổi bật với khả năng kích thích tái tạo tế bào, mang lại làn da trẻ trung hơn.", "correctAnswer": "C"}, {"id": "q15", "options": {"A": "Giúp giảm mụn", "B": "Cung cấp độ ẩm", "C": "Chống lại gốc tự do", "D": "Tăng cường độ đàn hồi"}, "category": "core_concept", "question": "Tại sao vitamin C lại cần thiết trong quy trình chăm sóc da hàng ngày?", "difficulty": "medium", "explanation": "Vitamin C là một chất chống oxy hóa mạnh, giúp bảo vệ da khỏi tác hại của gốc tự do.", "correctAnswer": "C"}]', 15, 80, 'Boss Quiz: Hiểu Biết Về Thành Phần Hoạt Chất Trong Skincare', '2026-01-22 17:13:10.398', 'gpt-4o-mini', '2026-01-22 17:13:10.446023', '2026-01-22 17:13:10.446023');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('457ce9d3-ae85-4d18-b1d3-f26a40605493', NULL, 'f3a30319-5825-460a-86fa-045981f35738', 'boss', NULL, '[{"id": "q1", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ đường", "C": "Biểu đồ tròn", "D": "Biểu đồ tán xạ"}, "category": "core_concept", "question": "Biểu đồ nào thường được sử dụng để thể hiện tỷ lệ phần trăm của các thành phần trong một tổng thể?", "difficulty": "medium", "explanation": "Biểu đồ tròn được sử dụng để thể hiện tỷ lệ phần trăm của các thành phần trong một tổng thể, giúp so sánh các phần với nhau.", "correctAnswer": "C"}, {"id": "q2", "options": {"A": "80% thời gian cần thiết cho 20% công việc", "B": "80% kết quả đến từ 20% nguyên nhân", "C": "20% thời gian làm việc mang lại 80% lợi nhuận", "D": "Tất cả đều đúng"}, "category": "core_concept", "question": "Quy tắc 80/20 trong phân tích dữ liệu cho biết điều gì?", "difficulty": "medium", "explanation": "Quy tắc 80/20, hay nguyên lý Pareto, cho rằng 80% kết quả đến từ 20% nguyên nhân.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ đường", "C": "Biểu đồ tròn", "D": "Biểu đồ hộp"}, "category": "core_concept", "question": "Biểu đồ nào thích hợp để thể hiện sự thay đổi của dữ liệu theo thời gian?", "difficulty": "medium", "explanation": "Biểu đồ đường thường được sử dụng để thể hiện sự thay đổi của dữ liệu theo thời gian, giúp nhận diện xu hướng.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ đường", "C": "Biểu đồ tròn", "D": "Biểu đồ đường tròn"}, "category": "core_concept", "question": "Biểu đồ nào không phải là một loại biểu đồ phổ biến trong Excel?", "difficulty": "medium", "explanation": "Biểu đồ đường tròn không phải là một loại biểu đồ phổ biến trong Excel; thuật ngữ đúng là biểu đồ tròn.", "correctAnswer": "D"}, {"id": "q5", "options": {"A": "Khi cần thể hiện tỷ lệ phần trăm", "B": "Khi cần so sánh nhiều danh mục", "C": "Khi cần kiểm tra mối tương quan giữa hai biến", "D": "Khi cần thể hiện số liệu theo thời gian"}, "category": "core_concept", "question": "Khi nào nên sử dụng biểu đồ tán xạ?", "difficulty": "medium", "explanation": "Biểu đồ tán xạ là công cụ hữu ích để kiểm tra mối tương quan giữa hai biến số.", "correctAnswer": "C"}, {"id": "q6", "options": {"A": "Biểu đồ cột sử dụng cho dữ liệu liên tục, biểu đồ đường cho dữ liệu rời rạc", "B": "Biểu đồ cột thể hiện số liệu theo chiều thẳng đứng, biểu đồ đường thể hiện sự thay đổi theo thời gian", "C": "Biểu đồ cột không thể so sánh nhiều danh mục, biểu đồ đường thì có", "D": "Cả hai đều giống nhau về cách thức thể hiện"}, "category": "comparison", "question": "Biểu đồ cột và biểu đồ đường khác nhau như thế nào?", "difficulty": "medium", "explanation": "Biểu đồ cột thể hiện số liệu theo chiều thẳng đứng, còn biểu đồ đường thể hiện sự thay đổi theo thời gian.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Thể hiện mối tương quan giữa hai biến", "B": "Tóm tắt các tham số thống kê của dữ liệu", "C": "Thể hiện tỷ lệ phần trăm của các thành phần", "D": "So sánh doanh thu theo từng khu vực"}, "category": "core_concept", "question": "Mục đích chính của biểu đồ hộp là gì?", "difficulty": "medium", "explanation": "Biểu đồ hộp được sử dụng để tóm tắt các tham số thống kê của dữ liệu như trung bình, trung vị, và phương sai.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Để làm cho biểu đồ trở nên đẹp hơn", "B": "Để dễ dàng phân biệt các phần trong biểu đồ", "C": "Để thu hút sự chú ý", "D": "Để tránh nhầm lẫn và cải thiện khả năng hiểu"}, "category": "best_practice", "question": "Tại sao nên sử dụng màu sắc hợp lý trong biểu đồ?", "difficulty": "medium", "explanation": "Màu sắc nên được chọn cẩn thận để không gây nhầm lẫn và cải thiện khả năng hiểu của biểu đồ.", "correctAnswer": "D"}, {"id": "q9", "options": {"A": "Biểu đồ tròn", "B": "Biểu đồ tán xạ", "C": "Biểu đồ cột", "D": "Biểu đồ đường"}, "category": "application", "question": "Nếu bạn muốn phân tích doanh thu hàng tháng của một công ty, loại biểu đồ nào sẽ sử dụng?", "difficulty": "medium", "explanation": "Biểu đồ cột là lựa chọn tốt nhất để so sánh doanh thu hàng tháng của công ty.", "correctAnswer": "C"}, {"id": "q10", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ đường", "C": "Biểu đồ tròn", "D": "Biểu đồ hộp"}, "category": "application", "question": "Để theo dõi xu hướng sản xuất hàng tuần, bạn nên sử dụng biểu đồ nào?", "difficulty": "medium", "explanation": "Biểu đồ đường là lựa chọn tốt để theo dõi xu hướng sản xuất theo thời gian.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Khi có ít dữ liệu", "B": "Khi biểu đồ chỉ có một loại dữ liệu", "C": "Khi bạn muốn biểu đồ dễ hiểu", "D": "Khi bạn muốn làm nổi bật một phần nào đó"}, "category": "best_practice", "question": "Khi nào bạn nên tránh sử dụng quá nhiều màu sắc trong biểu đồ?", "difficulty": "medium", "explanation": "Sử dụng quá nhiều màu sắc có thể làm rối mắt và khiến biểu đồ khó hiểu.", "correctAnswer": "C"}, {"id": "q12", "options": {"A": "So sánh từng tháng với nhau", "B": "Tạo biểu đồ cột và phân tích các điểm cao và thấp", "C": "Sử dụng biểu đồ tròn để thấy tỷ lệ", "D": "Chỉ cần nhìn vào con số doanh thu"}, "category": "application", "question": "Làm thế nào để nhận diện được xu hướng trong doanh thu hàng tháng qua biểu đồ?", "difficulty": "medium", "explanation": "Tạo biểu đồ cột và phân tích các điểm cao và thấp sẽ giúp nhận diện xu hướng rõ ràng hơn.", "correctAnswer": "B"}, {"id": "q13", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ đường", "C": "Biểu đồ tròn", "D": "Biểu đồ tán xạ"}, "category": "application", "question": "Trong một dashboard, biểu đồ nào có thể được sử dụng để hiển thị tỷ lệ lỗi sản phẩm?", "difficulty": "medium", "explanation": "Biểu đồ tròn là lựa chọn tốt để thể hiện tỷ lệ lỗi sản phẩm trong tổng số sản phẩm.", "correctAnswer": "C"}, {"id": "q14", "options": {"A": "Để giảm chi phí", "B": "Để đưa ra quyết định dựa trên dữ liệu", "C": "Để tăng doanh thu", "D": "Để làm đẹp báo cáo"}, "category": "application", "question": "Tại sao việc phân tích dữ liệu bằng biểu đồ là cần thiết trong kinh doanh?", "difficulty": "hard", "explanation": "Phân tích dữ liệu bằng biểu đồ giúp đưa ra quyết định dựa trên dữ liệu chính xác và hiệu quả.", "correctAnswer": "B"}, {"id": "q15", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ tròn", "C": "Biểu đồ tán xạ", "D": "Biểu đồ hộp"}, "category": "application", "question": "Biểu đồ nào có thể được sử dụng để thể hiện các tham số thống kê mô tả như trung bình và phương sai?", "difficulty": "medium", "explanation": "Biểu đồ hộp là công cụ tuyệt vời để thể hiện các tham số thống kê mô tả của dữ liệu.", "correctAnswer": "D"}]', 15, 80, 'Boss Quiz: Tạo Biểu Đồ Trong Excel', '2026-01-22 19:07:19.317', 'gpt-4o-mini', '2026-01-22 19:07:19.361716', '2026-01-22 19:07:19.361716');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('ea31e333-7bec-4927-8854-fd0c727fe063', '7a38988d-50d6-4558-b8e7-caf6e444e400', NULL, 'lesson', 'example', '[{"id": "q1", "options": {"A": "Tìm khoảng trống để có cơ hội ném tốt hơn.", "B": "Ném bóng mà không cần quan tâm đến vị trí.", "C": "Chỉ sử dụng tay thuận để ném.", "D": "Chỉ ném bóng ngang."}, "category": "correct_example", "question": "Khi bạn gặp khó khăn trong việc ném bóng, điều gì là đúng?", "explanation": "A đúng vì việc tìm khoảng trống giúp bạn có lợi thế trong cú ném. B sai vì không quan tâm đến vị trí có thể dẫn đến thất bại. C sai vì sử dụng tay không ném có thể giúp điều chỉnh hướng bóng. D sai vì ném bóng ngang không phải lúc nào cũng là lựa chọn tốt.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Sử dụng tay không ném để điều chỉnh hướng bóng.", "B": "Ném bóng cao lên trời để lừa đối phương.", "C": "Chỉ cố gắng ném thẳng mà không thay đổi kỹ thuật.", "D": "Di chuyển một chút để tìm vị trí tốt hơn."}, "category": "wrong_example", "question": "Khi không thể ném bóng thẳng, cách nào là không phù hợp?", "explanation": "C là đáp án sai vì việc chỉ cố gắng ném thẳng mà không thay đổi kỹ thuật khiến bạn không thích ứng với tình huống khó. A, B và D đều là những kỹ thuật hợp lý để cải thiện khả năng ném trong tình huống khó.", "correctAnswer": "C"}, {"id": "q3", "options": {"A": "Thực hành ném bóng với bạn bè để cải thiện kỹ năng.", "B": "Chỉ luyện tập một lần và không cần lặp lại.", "C": "Tập trung vào các kỹ thuật khác nhau để làm quen.", "D": "Tham gia vào các trò chơi thực tế để kiểm tra kỹ năng."}, "category": "wrong_example", "question": "Kỹ thuật nào sau đây không thuộc về việc luyện tập ném bóng từ vị trí khó?", "explanation": "B là đáp án sai vì việc chỉ luyện tập một lần không đủ để thành thạo kỹ thuật. Các phương án A, C và D đều đúng vì chúng khuyến khích sự luyện tập và cải thiện kỹ năng.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Nhanh chóng di chuyển để tận dụng khoảng trống.", "B": "Đứng yên và chờ xem điều gì sẽ xảy ra.", "C": "Ném bóng mà không suy nghĩ về tình huống.", "D": "Hỏi đồng đội về cách ném bóng."}, "category": "correct_example", "question": "Bạn thấy một khoảng trống trên sân, điều nào bạn nên làm?", "explanation": "A là đúng vì bạn cần hành động nhanh chóng để tận dụng khoảng trống. B và C sai vì đứng yên hoặc ném mà không suy nghĩ sẽ làm mất cơ hội. D có thể là một ý tưởng tốt, nhưng không phải lúc nào cũng thực tế trong tình huống gấp.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Ném bóng thẳng vào đối phương.", "B": "Sử dụng tay không ném để điều chỉnh hướng.", "C": "Đợi cho đến khi đối phương rời đi.", "D": "Ném bóng cao lên trời để lừa đối phương."}, "category": "correct_example", "question": "Nếu bạn đang trong một tình huống khó khăn với đối phương chắn trước mặt, bạn nên làm gì?", "explanation": "D là đúng vì ném bóng cao lên trời là một cách lừa đối phương. A sai vì ném thẳng vào đối phương sẽ không hiệu quả. B là một lựa chọn tốt nhưng không phải là cách duy nhất. C không phải là một chiến lược tốt vì bạn có thể mất cơ hội.", "correctAnswer": "D"}, {"id": "q6", "options": {"A": "Tập trung vào việc luyện tập thường xuyên.", "B": "Ném bóng mà không xem xét tình huống.", "C": "Sử dụng các kỹ thuật khác nhau để cải thiện kỹ năng.", "D": "Tìm kiếm ý kiến từ huấn luyện viên."}, "category": "wrong_example", "question": "Trong tình huống gặp khó khăn khi ném bóng, điều nào là sai?", "explanation": "B là sai vì ném bóng mà không xem xét tình huống sẽ không mang lại hiệu quả. A, C và D đều là những hành động hợp lý để cải thiện kỹ năng ném bóng.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Chỉ ném bóng thẳng mà không thay đổi gì.", "B": "Sử dụng tay không ném để điều chỉnh.", "C": "Luyện tập ném bóng thật nhanh mà không quan tâm đến kỹ thuật.", "D": "Tìm kiếm khoảng trống để ném bóng."}, "category": "mini_case", "question": "Bạn đang luyện tập ném bóng từ một góc hẹp. Bạn sẽ chọn cách nào?", "explanation": "D là đúng vì bạn cần tìm kiếm khoảng trống để có cú ném tốt hơn. A và C sai vì không thay đổi kỹ thuật hoặc chỉ ném nhanh sẽ không giúp bạn thành công. B là một lựa chọn tốt nhưng chỉ là một phần của quá trình.", "correctAnswer": "D"}]', 7, 70, 'Kỹ Thuật Ném Bóng Từ Vị Trí Khó', '2026-01-22 09:37:58.486', 'gpt-4o-mini', '2026-01-22 09:37:58.633559', '2026-01-22 09:37:58.633559');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('79c93179-5e2f-490f-81a7-2bbe766a54a9', NULL, '00bbe76d-6014-485e-890f-1398b6100e68', 'boss', NULL, '[{"id": "q1", "options": {"A": "Khả năng ghi điểm", "B": "Chiều cao của cầu thủ", "C": "Vị trí đứng", "D": "Sức mạnh của tay"}, "category": "concept", "question": "Kỹ thuật ném bóng trong bóng rổ chủ yếu ảnh hưởng đến yếu tố nào?", "explanation": "A đúng vì kỹ thuật ném bóng quyết định đến khả năng ghi điểm của cầu thủ. Các đáp án khác không phải là yếu tố chính ảnh hưởng đến ném bóng.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Cầm bóng quá chặt", "B": "Cầm bóng bằng cả hai tay", "C": "Cảm nhận bóng", "D": "Giữ bóng ngang ngực"}, "category": "concept", "question": "Khi cầm bóng, cầu thủ nên tránh điều gì?", "explanation": "A đúng vì cầm bóng quá chặt có thể làm giảm độ chính xác và khó kiểm soát bóng. Các đáp án khác đều là cách cầm bóng đúng.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Vị trí của chân", "B": "Tư thế của tay", "C": "Cảm xúc của cầu thủ", "D": "Thời gian ném"}, "category": "concept", "question": "Yếu tố nào không ảnh hưởng đến tư thế ném bóng?", "explanation": "D đúng vì thời gian ném không phải là yếu tố quyết định đến tư thế ném; ngược lại, các yếu tố A, B, C đều ảnh hưởng đến cú ném.", "correctAnswer": "D"}, {"id": "q4", "options": {"A": "Khi cầu thủ ghi điểm", "B": "Khi cầu thủ bị phạm lỗi", "C": "Khi có bóng ra ngoài biên", "D": "Khi hết thời gian"}, "category": "concept", "question": "Trong bóng rổ, cú ném phạt thường diễn ra trong tình huống nào?", "explanation": "B đúng vì cú ném phạt thường được thực hiện khi cầu thủ bị phạm lỗi. Các đáp án khác không liên quan đến cú ném phạt.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Thực hành trong các tình huống áp lực", "B": "Tránh ném bóng", "C": "Chỉ luyện tập một mình", "D": "Không chú ý đến cảm xúc"}, "category": "concept", "question": "Cảm giác tự tin trong ném bóng có thể được cải thiện bằng cách nào?", "explanation": "A đúng vì thực hành trong tình huống áp lực giúp cầu thủ cải thiện khả năng xử lý căng thẳng. Các đáp án khác không giúp cải thiện tự tin.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Tâm lý ổn định giúp cải thiện cú ném", "B": "Cảm giác có thể ảnh hưởng đến kỹ thuật ném", "C": "Chỉ cần kỹ thuật tốt là đủ", "D": "Tự tin là yếu tố quan trọng"}, "category": "concept", "question": "Điều nào sau đây là sai khi nói về cảm giác và tâm lý trong ném bóng?", "explanation": "C sai vì chỉ có kỹ thuật tốt không đủ, tâm lý và cảm giác cũng rất quan trọng. Các đáp án khác đều đúng.", "correctAnswer": "C"}, {"id": "q7", "options": {"A": "Ném bóng cao lên", "B": "Ném bóng thẳng", "C": "Chuyền bóng cho đồng đội", "D": "Đứng yên"}, "category": "concept", "question": "Kỹ thuật ném bóng nào giúp cầu thủ ghi điểm khi bị kẹp giữa hai đối thủ?", "explanation": "A đúng vì ném bóng cao lên có thể đánh lừa đối thủ và tạo cơ hội ghi điểm. Các đáp án khác không hữu ích trong tình huống này.", "correctAnswer": "A"}, {"id": "q8", "options": {"A": "Giữ bóng bằng các ngón tay", "B": "Để bóng chạm vào lòng bàn tay", "C": "Cầm bóng quá chặt", "D": "Để bóng ở dưới cằm"}, "category": "example", "question": "Ví dụ nào sau đây là đúng về việc cầm bóng trong ném bóng?", "explanation": "A đúng vì giữ bóng bằng các ngón tay là cách đúng để kiểm soát bóng. Các đáp án khác đều sai.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Ném từ vạch 3 điểm", "B": "Ném sau khi bị phạm lỗi", "C": "Ném khi bóng ra ngoài biên", "D": "Ném trong trận đấu chính"}, "category": "example", "question": "Cú ném nào sau đây là ví dụ về cú ném phạt?", "explanation": "B đúng vì cú ném phạt xảy ra sau khi cầu thủ bị phạm lỗi. Các đáp án khác không liên quan đến cú ném phạt.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Chỉ khi tham gia trận đấu", "B": "Trong các buổi tập và tự luyện tập", "C": "Khi có bạn chơi cùng", "D": "Không cần thực hành"}, "category": "example", "question": "Khi nào bạn nên thực hành kỹ thuật ném bóng?", "explanation": "B đúng vì thực hành trong các buổi tập và tự luyện tập giúp cải thiện kỹ năng. Các đáp án khác không đúng.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Thực hành nhiều lần từ các góc khác nhau", "B": "Chỉ ném từ vị trí dễ", "C": "Không cần luyện tập", "D": "Ném bóng khi không có áp lực"}, "category": "example", "question": "Cách nào giúp bạn cải thiện cú ném từ các vị trí khó?", "explanation": "A đúng vì thực hành từ các vị trí khác nhau sẽ giúp cải thiện kỹ năng ném. Các đáp án khác không giúp ích.", "correctAnswer": "A"}, {"id": "q12", "options": {"A": "Theo dõi bóng khi ném", "B": "Ném mà không nhìn vào rổ", "C": "Giữ tay ném ở vị trí cao", "D": "Cảm nhận lực ném"}, "category": "example", "question": "Hành động nào là sai khi thực hiện cú ném?", "explanation": "B sai vì không nhìn vào rổ khi ném sẽ làm giảm khả năng ghi điểm. Các đáp án khác đều đúng.", "correctAnswer": "B"}, {"id": "q13", "options": {"A": "Tư thế cơ thể và cảm giác", "B": "Chỉ cần sức mạnh của tay", "C": "Độ cao của bóng", "D": "Thời gian ném"}, "category": "example", "question": "Khi ném bóng, bạn cần chú ý đến yếu tố nào để cải thiện tính chính xác?", "explanation": "A đúng vì tư thế cơ thể và cảm giác chính xác là rất quan trọng trong cú ném. Các đáp án khác không đủ quan trọng.", "correctAnswer": "A"}, {"id": "q14", "options": {"A": "Bị đối thủ cướp bóng", "B": "Không ghi điểm", "C": "Cảm thấy tự tin hơn", "D": "Cả A và B"}, "category": "example", "question": "Nếu kỹ thuật ném không chính xác, điều gì có thể xảy ra?", "explanation": "D đúng vì nếu ném không chính xác, cầu thủ có thể bị cướp bóng hoặc không ghi điểm. Câu A và B đều đúng.", "correctAnswer": "D"}, {"id": "q15", "options": {"A": "Hít thở sâu và giữ bình tĩnh", "B": "Chỉ tập trung vào động tác ném", "C": "Ném thật nhanh", "D": "Không cần chú ý đến tâm lý"}, "category": "example", "question": "Một cầu thủ nên thực hiện cú ném phạt như thế nào để có khả năng ghi điểm cao nhất?", "explanation": "A đúng vì việc hít thở sâu và giữ bình tĩnh giúp cầu thủ tự tin hơn trong cú ném. Các đáp án khác không giúp cải thiện khả năng ghi điểm.", "correctAnswer": "A"}, {"id": "q16", "options": {"A": "Kỹ thuật ném", "B": "Sức mạnh tay", "C": "Chiều cao cầu thủ", "D": "Tốc độ ném"}, "category": "synthesis", "question": "Kỹ thuật nào là quan trọng nhất trong ném bóng?", "explanation": "A đúng vì kỹ thuật ném là yếu tố quyết định đến khả năng ghi điểm. Các đáp án khác không quan trọng hơn kỹ thuật.", "correctAnswer": "A"}, {"id": "q17", "options": {"A": "Cầu thủ có thể ghi điểm nhiều hơn", "B": "Cầu thủ có thể trở nên tự tin hơn", "C": "Cầu thủ có thể gặp khó khăn khi ném", "D": "Cầu thủ sẽ trở thành người chơi giỏi hơn"}, "category": "synthesis", "question": "Điều gì có thể xảy ra nếu cầu thủ không luyện tập kỹ thuật ném bóng đúng cách?", "explanation": "C đúng vì không luyện tập đúng cách có thể dẫn đến khó khăn khi ném. Các đáp án khác đều sai.", "correctAnswer": "C"}, {"id": "q18", "options": {"A": "Ném trong trận đấu quyết định", "B": "Ném khi không có người theo kèm", "C": "Ném trong buổi tập", "D": "Ném khi đang vui vẻ"}, "category": "synthesis", "question": "Tình huống nào có thể gây áp lực cho cầu thủ khi thực hiện cú ném?", "explanation": "A đúng vì ném trong trận đấu quyết định thường tạo áp lực lớn cho cầu thủ. Các đáp án khác không gây áp lực tương tự.", "correctAnswer": "A"}, {"id": "q19", "options": {"A": "Cú ném có thể không chính xác", "B": "Cú ném sẽ dễ dàng hơn", "C": "Cú ném sẽ luôn vào rổ", "D": "Không có ảnh hưởng gì"}, "category": "synthesis", "question": "Nếu bạn không chú ý đến cảm giác khi ném bóng, điều gì có thể xảy ra?", "explanation": "A đúng vì không chú ý đến cảm giác có thể làm giảm tính chính xác của cú ném. Các đáp án khác đều sai.", "correctAnswer": "A"}, {"id": "q20", "options": {"A": "Luyện tập thường xuyên giúp cải thiện kỹ năng", "B": "Không cần thực hành nếu có kỹ thuật tốt", "C": "Luyện tập chỉ cần khi có áp lực", "D": "Thực hành không cần thiết cho cầu thủ"}, "category": "synthesis", "question": "Kết nối nào dưới đây là đúng giữa ném bóng và thực hành?", "explanation": "A đúng vì luyện tập thường xuyên là cách tốt nhất để cải thiện kỹ năng ném bóng. Các đáp án khác đều sai.", "correctAnswer": "A"}, {"id": "q21", "options": {"A": "Ghi điểm ít hơn", "B": "Giữ được phong độ tốt", "C": "Trở nên tự tin hơn", "D": "Không có ảnh hưởng gì"}, "category": "synthesis", "question": "Nếu không thực hiện đúng kỹ thuật ném, cầu thủ có thể gặp phải điều gì?", "explanation": "A đúng vì không thực hiện đúng kỹ thuật sẽ dẫn đến việc ghi điểm ít hơn. Các đáp án khác đều sai.", "correctAnswer": "A"}, {"id": "q22", "options": {"A": "Mất tự tin khi ném", "B": "Thực hiện cú ném sai", "C": "Ghi điểm nhiều hơn", "D": "Bị đối thủ cướp bóng"}, "category": "synthesis", "question": "Kỹ thuật ném bóng tốt có thể giúp cầu thủ tránh điều gì?", "explanation": "B đúng vì kỹ thuật ném tốt giúp cầu thủ thực hiện cú ném chính xác hơn và tránh sai lầm. Các đáp án khác không liên quan.", "correctAnswer": "B"}, {"id": "q23", "options": {"A": "Độ chính xác của cú ném", "B": "Chiều cao của cú ném", "C": "Sự cồng kềnh của bóng", "D": "Không có ảnh hưởng gì"}, "category": "synthesis", "question": "Khi ném bóng, cảm giác có thể ảnh hưởng đến điều gì?", "explanation": "A đúng vì cảm giác ảnh hưởng trực tiếp đến độ chính xác của cú ném. Các đáp án khác đều sai.", "correctAnswer": "A"}, {"id": "q24", "options": {"A": "Sự kiên nhẫn và tập trung", "B": "Chỉ cần sức mạnh tay", "C": "Chiều cao của cầu thủ", "D": "Tốc độ ném"}, "category": "synthesis", "question": "Điều nào sau đây là một trong những yếu tố quyết định thành công trong kỹ thuật ném bóng?", "explanation": "A đúng vì sự kiên nhẫn và tập trung là rất quan trọng trong kỹ thuật ném bóng. Các đáp án khác không đủ quan trọng.", "correctAnswer": "A"}, {"id": "q25", "options": {"A": "Cầu thủ có thể bị trượt kỹ năng", "B": "Cầu thủ sẽ luôn giỏi", "C": "Cầu thủ không cần luyện tập", "D": "Cầu thủ sẽ luôn ghi điểm"}, "category": "synthesis", "question": "Nếu một cầu thủ không thực hành ném bóng, điều gì có thể xảy ra?", "explanation": "A đúng vì không thực hành sẽ làm mất đi kỹ năng ném bóng. Các đáp án khác đều sai.", "correctAnswer": "A"}]', 25, 80, 'Boss Quiz: Kỹ Thuật Ném Bóng Trong Bóng Rổ', '2026-01-22 09:38:39.849', 'gpt-4o-mini', '2026-01-22 09:38:39.890971', '2026-01-22 09:38:39.890971');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('d206c4ac-0282-4908-b21b-24dfd99ef26c', NULL, 'ddc59470-7309-47b3-89d3-e28953442868', 'boss', NULL, '[{"id": "q1", "options": {"A": "Lập kế hoạch và kiểm soát thời gian cho các hoạt động", "B": "Chỉ dành thời gian cho công việc quan trọng", "C": "Tăng thời gian làm việc", "D": "Giảm thời gian làm việc"}, "category": "core_concept", "question": "Quản lý thời gian là gì?", "difficulty": "medium", "explanation": "Quản lý thời gian bao gồm cả lập kế hoạch và kiểm soát cách thức mà thời gian được sử dụng cho các hoạt động khác nhau.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Thời gian hoàn thành", "B": "Mức độ khẩn cấp và quan trọng", "C": "Địa điểm thực hiện", "D": "Số lượng người tham gia"}, "category": "core_concept", "question": "Kỹ thuật Eisenhower Matrix giúp bạn phân loại công việc dựa trên tiêu chí nào?", "difficulty": "medium", "explanation": "Eisenhower Matrix phân loại công việc dựa vào độ khẩn cấp và tầm quan trọng của chúng, giúp ưu tiên công việc hiệu quả.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "15 phút", "B": "25 phút", "C": "30 phút", "D": "50 phút"}, "category": "core_concept", "question": "Phương pháp Pomodoro khuyến khích bạn làm việc trong khoảng thời gian bao lâu?", "difficulty": "medium", "explanation": "Phương pháp Pomodoro khuyến khích làm việc trong 25 phút và sau đó nghỉ 5 phút để tăng cường sự tập trung.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Tập trung vào cá nhân thay vì quy trình", "B": "Khách hàng là trung tâm", "C": "Giảm thiểu sự cộng tác", "D": "Lập kế hoạch chi tiết từ đầu đến cuối"}, "category": "core_concept", "question": "Điều nào là một trong những nguyên tắc cơ bản của Agile trong quản lý thời gian?", "difficulty": "medium", "explanation": "Agile nhấn mạnh rằng khách hàng phải luôn là trung tâm trong việc lập kế hoạch và thực hiện các dự án.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Công việc khẩn cấp và quan trọng", "B": "Công việc không khẩn cấp nhưng quan trọng", "C": "Công việc khẩn cấp nhưng không quan trọng", "D": "Công việc không khẩn cấp và không quan trọng"}, "category": "connection", "question": "Khi áp dụng kỹ thuật Eisenhower Matrix, công việc nào nên được thực hiện trước?", "difficulty": "medium", "explanation": "Công việc khẩn cấp và quan trọng nên được thực hiện trước để đảm bảo hoàn thành các nhiệm vụ quan trọng kịp thời.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "80% thời gian dùng cho 20% công việc", "B": "20% công việc mang lại 80% kết quả", "C": "20% thời gian là đủ cho tất cả công việc", "D": "80% công việc không quan trọng"}, "category": "connection", "question": "Nguyên tắc Pareto (80/20) giúp bạn xác định điều gì trong quản lý thời gian?", "difficulty": "medium", "explanation": "Nguyên tắc Pareto cho biết 20% công việc có thể mang lại 80% kết quả, giúp bạn tập trung vào những nhiệm vụ quan trọng nhất.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Tăng thời gian làm việc liên tục", "B": "Cải thiện khả năng tập trung và năng suất", "C": "Giảm số lượng công việc hoàn thành", "D": "Tăng áp lực công việc"}, "category": "application", "question": "Trong một tình huống thực tế, một nhóm phát triển phần mềm đã áp dụng kỹ thuật Pomodoro. Họ có thể kỳ vọng điều gì từ phương pháp này?", "difficulty": "hard", "explanation": "Kỹ thuật Pomodoro giúp cải thiện khả năng tập trung và năng suất làm việc nhờ vào việc phân chia thời gian làm việc và nghỉ ngơi hợp lý.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Lập danh sách tất cả công việc cần làm", "B": "Tập trung vào công việc không quan trọng", "C": "Đánh giá công việc theo mức độ khẩn cấp và quan trọng", "D": "Thực hiện tất cả công việc cùng lúc"}, "category": "application", "question": "Một nhân viên văn phòng đang cảm thấy quá tải với khối lượng công việc. Theo kỹ thuật Eisenhower Matrix, họ nên bắt đầu từ đâu?", "difficulty": "hard", "explanation": "Nhân viên nên đánh giá công việc theo mức độ khẩn cấp và quan trọng để xác định thứ tự ưu tiên thực hiện.", "correctAnswer": "C"}, {"id": "q9", "options": {"A": "Agile yêu cầu lập kế hoạch chi tiết ngay từ đầu", "B": "Agile khuyến khích phản hồi và thay đổi linh hoạt", "C": "Quản lý thời gian truyền thống không cần ưu tiên", "D": "Agile không cần sự tham gia của khách hàng"}, "category": "comparison", "question": "So sánh giữa quản lý thời gian truyền thống và Agile, điều nào là khác biệt lớn nhất?", "difficulty": "hard", "explanation": "Agile cho phép điều chỉnh kế hoạch dựa trên phản hồi từ khách hàng, điều này khác biệt so với phương pháp quản lý thời gian truyền thống.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Theo dõi tiến độ công việc", "B": "Tăng cường khả năng giao tiếp", "C": "Giảm thời gian hoàn thành tất cả công việc", "D": "Lên lịch và nhắc nhở"}, "category": "comparison", "question": "Điều nào sau đây không phải là một lý do để sử dụng công nghệ trong quản lý thời gian?", "difficulty": "medium", "explanation": "Công nghệ giúp theo dõi tiến độ, giao tiếp và lên lịch nhưng không đảm bảo rằng tất cả công việc sẽ hoàn thành nhanh hơn.", "correctAnswer": "C"}, {"id": "q11", "options": {"A": "Tăng thời gian họp", "B": "Triển khai các công cụ quản lý dự án", "C": "Giảm số lượng nhân viên", "D": "Tăng cường các cuộc họp dài"}, "category": "application", "question": "Một công ty khởi nghiệp muốn cải thiện năng suất làm việc thông qua công nghệ. Họ nên bắt đầu từ đâu?", "difficulty": "hard", "explanation": "Triển khai các công cụ quản lý dự án sẽ giúp cải thiện theo dõi tiến độ và giao tiếp trong nhóm, từ đó tăng cường năng suất.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Tương tác giữa các thành viên nhóm", "B": "Lập kế hoạch chi tiết cho toàn bộ dự án", "C": "Thích ứng với thay đổi", "D": "Phản hồi từ khách hàng"}, "category": "comparison", "question": "Nguyên tắc nào sau đây không phải là một phần của phương pháp quản lý Agile?", "difficulty": "hard", "explanation": "Phương pháp Agile không yêu cầu lập kế hoạch chi tiết cho toàn bộ dự án mà thay vào đó tập trung vào phản hồi và điều chỉnh trong quá trình làm việc.", "correctAnswer": "B"}]', 12, 80, 'Boss Quiz: Quản lý Thời gian Hiệu quả trong Môi trường Văn phòng', '2026-01-22 19:07:59.193', 'gpt-4o-mini', '2026-01-22 19:07:59.23665', '2026-01-22 19:07:59.23665');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('fc299e28-0128-4ce2-97fa-37cf86b61e5c', NULL, '00bbe76d-6014-485e-890f-1398b6100e68', 'boss', NULL, '[{"id": "q1", "options": {"A": "Đặt bóng trên lòng bàn tay", "B": "Đặt bóng ở ngang ngực với tay không thuận", "C": "Cầm bóng quá chặt", "D": "Để bóng rơi ra khỏi tay"}, "category": "concept", "question": "Cách cầm bóng đúng cách nào sau đây là chính xác?", "explanation": "B đúng vì cách cầm bóng ở ngang ngực giúp kiểm soát tốt hơn. A sai vì bóng không nên đặt trên lòng bàn tay. C sai vì cầm bóng quá chặt làm giảm độ chính xác. D sai vì không để bóng rơi ra khỏi tay là điều cần thiết.", "correctAnswer": "B"}, {"id": "q2", "options": {"A": "Tư thế cơ thể", "B": "Cảm giác tự tin", "C": "Màu sắc của bóng", "D": "Tư thế cầm bóng"}, "category": "concept", "question": "Yếu tố nào không ảnh hưởng đến kỹ thuật ném bóng?", "explanation": "C đúng vì màu sắc của bóng không ảnh hưởng đến kỹ thuật ném. A, B, D đều là các yếu tố ảnh hưởng đến khả năng ném bóng.", "correctAnswer": "C"}, {"id": "q3", "options": {"A": "Tâm lý không ảnh hưởng đến cú ném", "B": "Tự tin là yếu tố quan trọng", "C": "Tập trung vào mục tiêu giúp cải thiện cú ném", "D": "Giữ bình tĩnh là cần thiết"}, "category": "concept", "question": "Câu phát biểu nào sau đây là sai khi nói về cảm giác và tâm lý khi ném bóng?", "explanation": "A sai vì tâm lý có ảnh hưởng lớn đến cú ném. B, C, D đều là những yếu tố đúng trong việc ném bóng.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Sử dụng động tác giả", "B": "Ném bóng thẳng mà không quan sát", "C": "Chỉ sử dụng tay ném", "D": "Cầm bóng quá chặt"}, "category": "concept", "question": "Kỹ thuật nào được sử dụng khi ném bóng từ vị trí khó?", "explanation": "A đúng vì động tác giả có thể giúp tạo khoảng trống. B sai vì việc quan sát là cần thiết. C sai vì không chỉ tay ném mà cả cơ thể cần tham gia. D sai vì cầm bóng quá chặt không giúp ích cho cú ném.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Đứng thẳng với chân gần nhau", "B": "Chân rộng bằng vai và gập nhẹ đầu gối", "C": "Ngồi xổm", "D": "Đứng trên một chân"}, "category": "concept", "question": "Khi ném bóng, tư thế cơ thể cần như thế nào?", "explanation": "B đúng vì tư thế này giúp tạo lực cho cú ném. A sai vì chân gần nhau không tạo sự ổn định. C và D đều không phải tư thế tốt khi ném bóng.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Giúp cải thiện kỹ năng mà không cần sân bóng", "B": "Không có lợi ích", "C": "Chỉ dành cho cầu thủ chuyên nghiệp", "D": "Không cần chú ý vào kỹ thuật"}, "category": "example", "question": "Việc luyện tập kỹ thuật ném bóng tại nhà có lợi ích gì?", "explanation": "A đúng vì việc luyện tập tại nhà giúp cầu thủ cải thiện kỹ năng. B và C sai vì luyện tập có lợi cho tất cả. D sai vì cần chú ý đến kỹ thuật khi luyện tập.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Nhắm mắt lại để tập trung", "B": "Thực hiện cú ném nhanh chóng", "C": "Giữ bình tĩnh và tập trung vào mục tiêu", "D": "Liên tục thay đổi tư thế"}, "category": "example", "question": "Khi thực hiện cú ném phạt, điều nào là quan trọng nhất?", "explanation": "C đúng vì giữ bình tĩnh và tập trung vào mục tiêu là rất quan trọng. A sai vì nhắm mắt là không thực tế. B sai vì cần có thời gian để chuẩn bị. D sai vì sự ổn định trong tư thế là cần thiết.", "correctAnswer": "C"}, {"id": "q8", "options": {"A": "Không kiểm soát được lực ném", "B": "Giữ mắt trên bóng và rổ", "C": "Luyện tập kỹ thuật cầm bóng", "D": "Ném bóng đúng cách"}, "category": "example", "question": "Sai lầm nào thường gặp khi ném bóng?", "explanation": "A đúng vì không kiểm soát lực ném là sai lầm phổ biến. B, C, D đều là những hành động đúng.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Khi có nhiều khoảng trống", "B": "Khi bị kẹp giữa hai đối thủ", "C": "Khi đứng ở giữa sân", "D": "Khi không có ai gần đó"}, "category": "example", "question": "Trong tình huống nào thì bạn nên sử dụng động tác giả khi ném bóng?", "explanation": "B đúng vì động tác giả giúp tạo khoảng trống trong tình huống bị kẹp. A, C, D đều không cần thiết sử dụng động tác giả.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Sử dụng sức mạnh từ chân", "B": "Thực hiện cú ném cao", "C": "Chỉ sử dụng tay thuận", "D": "Theo dõi bóng khi ném"}, "category": "example", "question": "Kỹ thuật nào không nên áp dụng khi ném bóng từ vị trí khó?", "explanation": "C sai vì cần sử dụng cả hai tay để kiểm soát bóng tốt hơn. A, B, D đều là những kỹ thuật cần thiết.", "correctAnswer": "C"}, {"id": "q11", "options": {"A": "Không có ảnh hưởng gì", "B": "Giúp cải thiện khả năng ném", "C": "Làm giảm khả năng ghi điểm", "D": "Không cần thiết phải có"}, "category": "synthesis", "question": "Khi ném bóng, sự tự tin ảnh hưởng như thế nào đến kết quả?", "explanation": "B đúng vì sự tự tin có thể nâng cao khả năng ném. A, C, D đều sai vì tự tin là yếu tố quan trọng trong ném bóng.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Cải thiện kỹ năng nhanh chóng", "B": "Nâng cao sự tự tin", "C": "Thành công trong các trận đấu", "D": "Khả năng ghi điểm giảm đi"}, "category": "synthesis", "question": "Nếu bạn không luyện tập kỹ thuật ném bóng đúng cách, điều gì có thể xảy ra?", "explanation": "D đúng vì không luyện tập sẽ dẫn đến khả năng ghi điểm kém. A, B, C đều không phản ánh thực tế.", "correctAnswer": "D"}, {"id": "q13", "options": {"A": "Phê bình đồng đội", "B": "Tìm hiểu kỹ thuật và cảm giác của từng cú ném", "C": "Quên đi các sai lầm", "D": "Chỉ nhìn vào kết quả cuối cùng"}, "category": "synthesis", "question": "Khi phân tích cú ném bóng sau trận đấu, bạn nên tập trung vào yếu tố nào?", "explanation": "B đúng vì phân tích kỹ thuật và cảm giác giúp cải thiện kỹ năng. A, C, D đều không phải là cách hiệu quả để học hỏi từ trận đấu.", "correctAnswer": "B"}, {"id": "q14", "options": {"A": "Chỉ cần ném là đủ", "B": "Kiểm soát lực ném và kỹ thuật", "C": "Không cần chú ý đến tư thế", "D": "Chỉ tập trung vào kết quả"}, "category": "synthesis", "question": "Khi luyện tập ném bóng từ xa, điều gì là quan trọng nhất?", "explanation": "B đúng vì cần kiểm soát tốt lực ném và kỹ thuật. A, C, D đều không chú trọng vào việc cải thiện kỹ năng.", "correctAnswer": "B"}, {"id": "q15", "options": {"A": "Sự hồi hộp", "B": "Sự tự tin", "C": "Sự tập trung", "D": "Sự lo lắng"}, "category": "synthesis", "question": "Cảm giác nào không nên có khi thực hiện cú ném bóng?", "explanation": "D đúng vì sự lo lắng có thể ảnh hưởng đến cú ném. A, B, C đều là cảm giác tích cực khi ném bóng.", "correctAnswer": "D"}, {"id": "q16", "options": {"A": "Cầm bóng quá chặt và không thoải mái", "B": "Giữ mắt trên bóng và rổ", "C": "Theo dõi bóng sau khi ném", "D": "Đứng đúng tư thế khi ném"}, "category": "synthesis", "question": "Tình huống nào sau đây thể hiện kỹ thuật ném bóng không chính xác?", "explanation": "A đúng vì cầm bóng quá chặt làm giảm độ chính xác. B, C, D đều là những hành động đúng.", "correctAnswer": "A"}, {"id": "q17", "options": {"A": "Cú ném sẽ chính xác hơn", "B": "Khả năng ghi điểm sẽ giảm", "C": "Bạn sẽ cảm thấy tự tin hơn", "D": "Không có ảnh hưởng gì"}, "category": "synthesis", "question": "Điều gì có thể xảy ra nếu bạn không giữ tư thế đúng khi ném bóng?", "explanation": "B đúng vì tư thế không đúng sẽ làm giảm khả năng ghi điểm. A, C, D đều không phản ánh thực tế.", "correctAnswer": "B"}, {"id": "q18", "options": {"A": "Chỉ chú ý đến kết quả", "B": "Phân tích kỹ thuật và chiến thuật", "C": "Bỏ qua các sai lầm", "D": "Chỉ học hỏi từ một cầu thủ"}, "category": "synthesis", "question": "Khi học hỏi từ các cầu thủ chuyên nghiệp, điều nào là quan trọng?", "explanation": "B đúng vì phân tích kỹ thuật và chiến thuật giúp cải thiện bản thân. A, C, D đều không tạo ra sự phát triển.", "correctAnswer": "B"}, {"id": "q19", "options": {"A": "Bạn sẽ tự tin hơn", "B": "B