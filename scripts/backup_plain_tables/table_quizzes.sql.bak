INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('4db4f12e-d2a2-4bb2-bf86-c3ac79710220', NULL, 'a1618b25-304d-43be-b2e3-bd7613475485', 'boss', NULL, '[{"id": "q1", "options": {"A": "Thể hiện cảm xúc mà không cần bản nhạc cụ thể", "B": "Chơi theo một bản nhạc đã được viết sẵn", "C": "Chơi nhạc với kỹ thuật hoàn hảo", "D": "Chỉ chơi nhạc vui tươi"}, "category": "core_concept", "question": "Khái niệm ''Chơi Nhạc Theo Cảm Hứng'' chủ yếu nhấn mạnh điều gì?", "difficulty": "medium", "explanation": "Khái niệm này nhấn mạnh việc nghệ sĩ sử dụng cảm xúc để dẫn dắt quá trình sáng tác và biểu diễn mà không cần tuân theo một bản nhạc nào.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Sự chuẩn bị kỹ lưỡng trước khi biểu diễn", "B": "Khả năng điều chỉnh và thích ứng với hoàn cảnh", "C": "Chơi theo một bản nhạc đã có sẵn", "D": "Sử dụng các công cụ điện tử trong âm nhạc"}, "category": "core_concept", "question": "Phong cách ứng tác của Keith Jarrett trong ''The Köln Concert'' được đặc trưng bởi điều gì?", "difficulty": "medium", "explanation": "Keith Jarrett đã thể hiện khả năng điều chỉnh và thích ứng với hoàn cảnh xung quanh, tạo ra âm nhạc từ cảm xúc của mình trong một tình huống không thuận lợi.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Chơi một cách hoàn hảo", "B": "Tối ưu hóa mức độ thách thức của âm nhạc", "C": "Sao chép một bản nhạc nổi tiếng", "D": "Chơi theo nhịp điệu cố định"}, "category": "core_concept", "question": "Điều gì là một yếu tố quan trọng trong việc duy trì trạng thái tinh thần khi chơi nhạc theo cảm hứng?", "difficulty": "hard", "explanation": "Tối ưu hóa mức độ thách thức của âm nhạc cùng với khả năng kỹ thuật của nghệ sĩ sẽ giúp duy trì trạng thái tinh thần tốt, cho phép họ bộc lộ cảm xúc tự nhiên.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Chỉ tập trung vào kỹ thuật chơi", "B": "Đọc được tâm trạng của khán giả và điều chỉnh giai điệu", "C": "Chơi một bản nhạc nổi tiếng", "D": "Giới thiệu các thể loại âm nhạc khác nhau"}, "category": "core_concept", "question": "Khi Doan Hoang biểu diễn, điều nào sau đây thường được ông chú trọng để tạo sự kết nối với khán giả?", "difficulty": "medium", "explanation": "Doan Hoang có khả năng đọc được tâm trạng của khán giả và linh hoạt điều chỉnh giai điệu để làm phong phú thêm trải nghiệm của họ.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Chỉ qua lời ca", "B": "Nhịp điệu và hòa âm", "C": "Chỉ qua giai điệu", "D": "Không có yếu tố nào"}, "category": "core_concept", "question": "Âm nhạc có thể truyền tải cảm xúc thông qua yếu tố nào dưới đây?", "difficulty": "hard", "explanation": "Âm nhạc có thể gợi lên các cảm xúc khác nhau thông qua các yếu tố như nhịp điệu và hòa âm, theo nghiên cứu của Juslin và Västfjäll.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Chơi theo một bản nhạc cụ thể", "B": "Chỉ chơi những nốt nhạc buồn", "C": "Để cảm xúc dẫn dắt và sử dụng những nốt nhạc tươi vui", "D": "Chơi một cách chậm rãi"}, "category": "application", "question": "Trong ''Chơi Nhạc Khi Hạnh Phúc'', điều nào đúng về cách chơi nhạc?", "difficulty": "medium", "explanation": "Người chơi được khuyến khích để cảm xúc dẫn dắt cách họ chơi, sử dụng những nốt nhạc tươi vui để phản ánh niềm vui.", "correctAnswer": "C"}, {"id": "q7", "options": {"A": "C, D, E", "B": "A, B, C", "C": "E, F, G", "D": "C, D, E, F"}, "category": "application", "question": "Khi cảm thấy buồn, nghệ sĩ nên bắt đầu với những nốt nhạc nào để thể hiện nỗi buồn?", "difficulty": "medium", "explanation": "Nghệ sĩ có thể bắt đầu với những nốt nhạc như A, B, C để phản ánh cảm xúc buồn bã một cách rõ ràng và mạnh mẽ.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Chơi theo một bản nhạc cụ thể", "B": "Khám phá và thử nghiệm với âm nhạc theo nhiều cách khác nhau", "C": "Chỉ chơi nhạc cổ điển", "D": "Tránh thử nghiệm với các thể loại mới"}, "category": "application", "question": "Khi chơi nhạc theo cảm hứng, điều gì là cần thiết để phát triển khả năng sáng tạo?", "difficulty": "hard", "explanation": "Khám phá và thử nghiệm với âm nhạc sẽ giúp nghệ sĩ phát triển khả năng sáng tạo và tìm ra phong cách riêng cho mình.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Luyện tập các bảng hợp âm", "B": "Lắng nghe âm nhạc xung quanh", "C": "Chỉ chơi theo nhịp điệu cố định", "D": "Chơi với nhịp điệu đa dạng"}, "category": "comparison", "question": "Điều nào sau đây không phải là một yếu tố trong việc ứng tác hiệu quả?", "difficulty": "medium", "explanation": "Việc chỉ chơi theo nhịp điệu cố định có thể hạn chế khả năng ứng tác và thể hiện cảm xúc tự nhiên.", "correctAnswer": "C"}, {"id": "q10", "options": {"A": "Chơi theo cảm xúc cá nhân", "B": "Chỉ cần kỹ thuật hoàn hảo", "C": "Không yêu cầu sự sáng tạo", "D": "Chỉ chơi các bản nhạc nổi tiếng"}, "category": "comparison", "question": "So với việc chơi nhạc theo bản nhạc, chơi nhạc theo cảm hứng có điểm khác biệt nào?", "difficulty": "hard", "explanation": "Chơi nhạc theo cảm hứng cho phép nghệ sĩ thể hiện cảm xúc cá nhân mà không cần phải tuân theo một bản nhạc cụ thể.", "correctAnswer": "A"}, {"id": "q11", "options": {"A": "Để biểu diễn một cách hoàn hảo", "B": "Để tạo ra không gian thoải mái cho việc thể hiện cảm xúc", "C": "Để chơi theo bản nhạc đã có sẵn", "D": "Để sao chép phong cách của người khác"}, "category": "connection", "question": "Khi ứng tác, tại sao việc tương tác với khán giả lại quan trọng?", "difficulty": "medium", "explanation": "Tương tác với khán giả tạo ra không gian thoải mái và khuyến khích sự tham gia, giúp nghệ sĩ thể hiện cảm xúc một cách tự nhiên hơn.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Kỹ năng đọc bản nhạc", "B": "Khả năng sáng tạo và lắng nghe", "C": "Chỉ cần kỹ thuật hoàn hảo", "D": "Chơi nhạc theo cách truyền thống"}, "category": "connection", "question": "Khả năng nào cần thiết để nghệ sĩ có thể chuyển đổi cảm xúc qua âm nhạc?", "difficulty": "hard", "explanation": "Khả năng sáng tạo và lắng nghe là rất quan trọng để nghệ sĩ có thể chuyển đổi cảm xúc qua âm nhạc một cách hiệu quả.", "correctAnswer": "B"}]', 12, 80, 'Boss Quiz: Chơi Nhạc Theo Cảm Hứng', '2026-01-22 16:49:26.056', 'gpt-4o-mini', '2026-01-22 16:49:26.095504', '2026-01-22 16:49:26.095504');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('8778b080-4096-48fa-b5d8-f22955f55f73', NULL, '9eb5f626-2967-4667-85fc-c96962ff50fd', 'boss', NULL, '[{"id": "q1", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ tròn", "C": "Biểu đồ đường", "D": "Biểu đồ vùng"}, "category": "core_concept", "question": "Biểu đồ nào giúp người dùng dễ dàng nhận diện xu hướng và ra quyết định dựa vào dữ liệu?", "difficulty": "medium", "explanation": "Biểu đồ cột giúp trực quan hóa dữ liệu theo từng danh mục, giúp dễ dàng nhận thấy các xu hướng rõ ràng.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Giúp giảm thiểu số liệu", "B": "Biểu diễn thông tin phức tạp dễ hiểu", "C": "Làm tăng số liệu", "D": "Không cần dùng số liệu"}, "category": "core_concept", "question": "Tại sao việc sử dụng biểu đồ lại quan trọng trong phân tích dữ liệu?", "difficulty": "medium", "explanation": "Biểu đồ giúp trình bày thông tin phức tạp một cách dễ hiểu hơn, hỗ trợ cho việc phân tích và ra quyết định.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ tròn", "C": "Biểu đồ đường", "D": "Biểu đồ khu vực"}, "category": "comparison", "question": "Biểu đồ nào không thích hợp để thể hiện doanh thu theo từng sản phẩm khi có quá nhiều sản phẩm?", "difficulty": "hard", "explanation": "Biểu đồ tròn không thích hợp khi có quá nhiều sản phẩm vì sẽ gây khó khăn trong việc phân biệt các phần.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Chỉ nên vẽ một đường", "B": "Phải có dữ liệu từ nhiều năm", "C": "Cần chú ý đến hoàn cảnh ngoại lệ", "D": "Không cần chú ý đến dữ liệu"}, "category": "core_concept", "question": "Điều gì nên được chú ý khi sử dụng biểu đồ đường để phân tích doanh thu hàng tháng?", "difficulty": "medium", "explanation": "Cần chú ý đến các hoàn cảnh ngoại lệ như mùa lễ hội có thể làm tăng đột biến doanh thu.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "Thời gian cho môn Khoa học", "B": "Thời gian cho môn Thể dục", "C": "Thời gian cho môn Toán", "D": "Thời gian cho môn Văn"}, "category": "application", "question": "Khi sử dụng biểu đồ hình tròn để theo dõi thời gian học tập, phần nào có thể được lớn hơn?", "difficulty": "medium", "explanation": "Nếu bạn dành nhiều thời gian cho môn Toán, phần của môn Toán sẽ lớn hơn trong biểu đồ hình tròn.", "correctAnswer": "C"}, {"id": "q6", "options": {"A": "Biểu đồ cột thể hiện dữ liệu theo thời gian, còn biểu đồ đường thể hiện theo danh mục", "B": "Biểu đồ cột không thể hiện xu hướng, còn biểu đồ đường thì có", "C": "Biểu đồ cột thường dùng cho số liệu tĩnh, còn biểu đồ đường dùng cho số liệu động", "D": "Biểu đồ cột trực quan hóa dữ liệu theo danh mục, còn biểu đồ đường theo thời gian"}, "category": "comparison", "question": "Sự khác biệt chính giữa biểu đồ cột và biểu đồ đường là gì?", "difficulty": "medium", "explanation": "Biểu đồ cột thường được dùng để so sánh các danh mục, trong khi biểu đồ đường thể hiện sự thay đổi theo thời gian.", "correctAnswer": "D"}, {"id": "q7", "options": {"A": "Biểu đồ nhiệt độ", "B": "Biểu đồ cột", "C": "Biểu đồ tròn", "D": "Biểu đồ đường"}, "category": "application", "question": "Khi phân tích dữ liệu thời tiết, biểu đồ nào có thể cung cấp thông tin chính xác nhất?", "difficulty": "hard", "explanation": "Biểu đồ nhiệt độ giúp người xem dễ dàng nhận diện các xu hướng nhiệt độ qua thời gian.", "correctAnswer": "A"}, {"id": "q8", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ tròn", "C": "Biểu đồ đường", "D": "Tất cả các loại biểu đồ"}, "category": "comparison", "question": "Biểu đồ nào có thể gây khó khăn trong việc hiểu nếu quá nhiều thông tin được trình bày?", "difficulty": "hard", "explanation": "Mọi loại biểu đồ có thể trở nên khó hiểu nếu chứa quá nhiều thông tin, vì vậy cần phải chọn lọc dữ liệu.", "correctAnswer": "D"}, {"id": "q9", "options": {"A": "Giảm chi phí sản xuất", "B": "Tăng cường khả năng giao tiếp", "C": "Cung cấp sản phẩm miễn phí", "D": "Tăng số lượng nhân viên"}, "category": "core_concept", "question": "Trong bối cảnh kinh tế, việc sử dụng biểu đồ giúp ích gì cho tổ chức?", "difficulty": "medium", "explanation": "Biểu đồ giúp truyền đạt thông điệp một cách rõ ràng và hiệu quả hơn giữa các nhóm và bên liên quan.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Khi bạn chỉ cần một cái nhìn tổng quan", "B": "Khi có nhiều dữ liệu cần phân tích từ nhiều góc độ", "C": "Khi bạn không cần thông tin chi tiết", "D": "Khi bạn chỉ cần một loại dữ liệu"}, "category": "application", "question": "Khi nào biểu đồ tương tác là lựa chọn tốt nhất?", "difficulty": "hard", "explanation": "Biểu đồ tương tác cho phép người dùng khám phá dữ liệu từ nhiều góc độ khác nhau, rất hữu ích khi có nhiều dữ liệu.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Chi phí và lợi ích của việc sử dụng biểu đồ", "B": "Chỉ chi phí của biểu đồ", "C": "Chỉ lợi ích của biểu đồ", "D": "Không có gì liên quan đến chi phí và lợi ích"}, "category": "core_concept", "question": "Tính chất kinh tế của biểu đồ đề cập đến điều gì?", "difficulty": "medium", "explanation": "Tính chất kinh tế của biểu đồ liên quan đến cả chi phí và lợi ích mà nó mang lại trong việc truyền đạt thông tin.", "correctAnswer": "A"}, {"id": "q12", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ tròn", "C": "Biểu đồ đường", "D": "Biểu đồ mặt phẳng"}, "category": "comparison", "question": "Biểu đồ nào là lựa chọn không phù hợp nếu bạn muốn thể hiện sự phân bổ thời gian cho nhiều hoạt động trong một ngày?", "difficulty": "hard", "explanation": "Biểu đồ đường không thích hợp để thể hiện sự phân bổ thời gian cho nhiều hoạt động, nó thường dùng để theo dõi sự thay đổi liên tục.", "correctAnswer": "C"}, {"id": "q13", "options": {"A": "Khi cần trực quan hóa dữ liệu", "B": "Khi muốn truyền đạt nhiều thông điệp", "C": "Khi muốn tránh gây rối mắt cho người xem", "D": "Khi có ít dữ liệu"}, "category": "core_concept", "question": "Khi nào nên tránh việc sử dụng quá nhiều biểu đồ trong một báo cáo?", "difficulty": "medium", "explanation": "Việc sử dụng quá nhiều biểu đồ có thể gây khó khăn và làm rối mắt cho người xem, cần phải chọn lọc thông tin.", "correctAnswer": "C"}, {"id": "q14", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ đường", "C": "Biểu đồ tròn", "D": "Biểu đồ cột nhóm"}, "category": "application", "question": "Biểu đồ nào có thể giúp bạn theo dõi doanh thu của một công ty theo thời gian?", "difficulty": "medium", "explanation": "Biểu đồ đường là công cụ tuyệt vời để theo dõi xu hướng doanh thu theo thời gian.", "correctAnswer": "B"}, {"id": "q15", "options": {"A": "Chỉ nên sử dụng biểu đồ cột", "B": "Có thể sử dụng nhiều loại biểu đồ để tạo sự sinh động", "C": "Biểu đồ tròn tốt hơn cho mọi loại dữ liệu", "D": "Biểu đồ không cần chú thích"}, "category": "comparison", "question": "Chọn câu đúng về biểu đồ trong báo cáo doanh thu.", "difficulty": "medium", "explanation": "Việc sử dụng nhiều loại biểu đồ giúp làm phong phú thêm báo cáo và truyền đạt thông tin một cách hiệu quả hơn.", "correctAnswer": "B"}]', 15, 80, 'Boss Quiz: Giới Thiệu Biểu Đồ', '2026-01-22 16:50:14.453', 'gpt-4o-mini', '2026-01-22 16:50:14.493365', '2026-01-22 16:50:14.493365');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('249864ae-f394-4f33-a886-921f0417e6b2', '4bcb48a9-f862-4043-a7a9-e598a73587ff', NULL, 'lesson', 'example', '[{"id": "q1", "options": {"A": "Dùng tay chạm nhẹ vào bóng để đẩy về phía trước.", "B": "Giữ bóng trên đầu mà không di chuyển.", "C": "Khi bóng rơi, không cần nhặt lại.", "D": "Chỉ dùng một tay và không di chuyển."}, "category": "correct_example", "question": "Khi bạn dribble bóng, động tác nào sau đây là đúng?", "explanation": "Đáp án A đúng vì đó là cách thực hiện kỹ thuật dribble. Các đáp án B, C và D đều sai vì không phù hợp với kỹ thuật dribble.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Khi có người khác cố gắng cướp bóng.", "B": "Khi bạn không muốn di chuyển.", "C": "Khi bạn đang đứng yên.", "D": "Khi bóng đã rơi ra ngoài tầm kiểm soát."}, "category": "correct_example", "question": "Khi nào bạn nên thực hiện động tác crossover trong bóng rổ?", "explanation": "Đáp án A đúng vì crossover là kỹ thuật để bảo vệ bóng khi có đối thủ. Các đáp án B, C và D không phù hợp với tình huống thực tế.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Đi bộ với bóng mà không chạm vào nó.", "B": "Chạm nhẹ vào bóng để đẩy về phía trước.", "C": "Di chuyển sang trái và phải trong khi dribble.", "D": "Giữ bóng gần sàn để kiểm soát."}, "category": "wrong_example", "question": "Đâu là một ví dụ sai về việc dribble bóng?", "explanation": "Đáp án A sai vì không có hành động dribble nào diễn ra. Các đáp án B, C và D đều là ví dụ đúng.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Lập tức dừng lại và đứng yên.", "B": "Sử dụng động tác crossover để chuyển bóng sang tay khác.", "C": "Để bóng rơi và chạy đi.", "D": "Quay lưng lại và không nhìn bóng."}, "category": "correct_example", "question": "Khi bạn đang dribble và muốn thay đổi hướng, bạn nên làm gì?", "explanation": "Đáp án B đúng vì crossover là cách hiệu quả để thay đổi hướng khi dribble. Các đáp án A, C và D đều không hợp lý.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Chạy nhanh mà không kiểm soát bóng.", "B": "Chạm nhẹ vào bóng để giữ nó bên cạnh bạn.", "C": "Di chuyển bóng lên xuống để giữ kiểm soát.", "D": "Sử dụng cả hai tay để giữ bóng."}, "category": "wrong_example", "question": "Hành động nào sau đây không phải là một phần của kỹ thuật dribble?", "explanation": "Đáp án A không phải là kỹ thuật dribble vì không có kiểm soát bóng. Các đáp án B, C và D đều thuộc về kỹ thuật dribble.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Giữ nguyên vị trí và đứng yên.", "B": "Sử dụng kỹ thuật crossover để tránh chướng ngại vật.", "C": "Để bóng rơi và bỏ chạy.", "D": "Tăng tốc mà không kiểm soát bóng."}, "category": "mini_case", "question": "Khi bạn dribble bóng, nếu bạn thấy một chướng ngại vật, bạn nên:", "explanation": "Đáp án B đúng vì crossover giúp bạn tránh chướng ngại vật trong khi vẫn giữ kiểm soát bóng. Các đáp án A, C và D không phù hợp.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Chạy thẳng vào đối thủ.", "B": "Hồi hộp và dừng lại.", "C": "Thực hiện động tác crossover để bảo vệ bóng.", "D": "Bỏ bóng và chạy đi."}, "category": "mini_case", "question": "Nếu bạn phải dribble bóng trong khi đối thủ đang cố gắng cướp bóng, bạn nên:", "explanation": "Đáp án C đúng vì crossover là cách hiệu quả để bảo vệ bóng khỏi đối thủ. Các đáp án A, B và D đều không hợp lý.", "correctAnswer": "C"}]', 7, 70, 'Dribble Ngắn Giới Thiệu', '2026-01-21 23:59:01.855', 'gpt-4o-mini', '2026-01-21 23:59:01.8993', '2026-01-21 23:59:01.8993');
