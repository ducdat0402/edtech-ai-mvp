INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('46d77c20-4b12-4494-a79d-ed72e004c36e', NULL, '076c48e4-fb7b-4f42-bcc0-56550fcf5c3b', 'boss', NULL, '[{"id": "q1", "options": {"A": "Người tham gia", "B": "Người thụ hưởng", "C": "Công ty bảo hiểm", "D": "Không ai"}, "category": "core_concept", "question": "Bảo hiểm nhân thọ cung cấp bảo vệ tài chính cho ai trong trường hợp người tham gia gặp rủi ro?", "difficulty": "medium", "explanation": "Bảo hiểm nhân thọ cung cấp quyền lợi bảo hiểm cho người thụ hưởng đã được chỉ định trong hợp đồng khi người tham gia gặp rủi ro.", "correctAnswer": "B"}, {"id": "q2", "options": {"A": "Bảo hiểm nhân thọ tạm thời", "B": "Bảo hiểm nhân thọ vĩnh viễn", "C": "Bảo hiểm nhân thọ có giá trị tài khoản", "D": "Cả B và C"}, "category": "core_concept", "question": "Loại bảo hiểm nào cung cấp bảo vệ suốt đời và có giá trị hoàn lại?", "difficulty": "medium", "explanation": "Bảo hiểm nhân thọ vĩnh viễn và bảo hiểm nhân thọ có giá trị tài khoản đều cung cấp bảo vệ suốt đời và có giá trị hoàn lại.", "correctAnswer": "D"}, {"id": "q3", "options": {"A": "Chỉ có người tham gia", "B": "Sự phụ thuộc tài chính của gia đình", "C": "Các khoản đầu tư tài chính", "D": "Chi phí sinh hoạt hàng tháng"}, "category": "core_concept", "question": "Nhu cầu bảo hiểm nhân thọ thường xuất phát từ yếu tố nào?", "difficulty": "medium", "explanation": "Nhu cầu bảo hiểm nhân thọ thường xuất phát từ việc có gia đình và những người phụ thuộc về tài chính, nhằm bảo vệ họ khi người trụ cột không còn.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Có giá trị hoàn lại", "B": "Cung cấp bảo vệ trong một khoảng thời gian nhất định", "C": "Chi phí cao hơn bảo hiểm trọn đời", "D": "Không cần xét nghiệm sức khỏe"}, "category": "core_concept", "question": "Bảo hiểm nhân thọ tạm thời có đặc điểm nào sau đây?", "difficulty": "medium", "explanation": "Bảo hiểm nhân thọ tạm thời cung cấp bảo vệ trong một khoảng thời gian nhất định và không có giá trị hoàn lại.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "500 triệu đồng", "B": "1 tỷ đồng", "C": "1.5 tỷ đồng", "D": "2 tỷ đồng"}, "category": "application", "question": "Gia đình A đã xác định nhu cầu bảo hiểm là bao nhiêu để bảo vệ tài chính?", "difficulty": "medium", "explanation": "Gia đình A đã tính toán rằng họ cần ít nhất 1 tỷ đồng để bảo vệ tài chính cho gia đình trong trường hợp không may xảy ra.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Bảo hiểm tạm thời với 1 triệu đồng", "B": "Bảo hiểm vĩnh viễn với 1.5 triệu đồng", "C": "Bảo hiểm tạm thời với 1.5 triệu đồng", "D": "Bảo hiểm vĩnh viễn với 1 triệu đồng"}, "category": "application", "question": "Gia đình B đã chọn sản phẩm bảo hiểm nhân thọ nào và với mức phí bao nhiêu?", "difficulty": "medium", "explanation": "Gia đình B đã chọn một sản phẩm bảo hiểm nhân thọ có mức phí 1.5 triệu đồng mỗi tháng với quyền lợi 1.5 tỷ đồng.", "correctAnswer": "C"}, {"id": "q7", "options": {"A": "Do không có ai trong gia đình", "B": "Do thay đổi trong tình trạng sức khỏe", "C": "Do quyết định không tham gia nữa", "D": "Do thay đổi công ty bảo hiểm"}, "category": "application", "question": "Tại sao gia đình C cần điều chỉnh hợp đồng bảo hiểm nhân thọ?", "difficulty": "medium", "explanation": "Gia đình C cần điều chỉnh hợp đồng bảo hiểm do tình trạng sức khỏe của người trụ cột đã thay đổi.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Có giá trị hoàn lại và bảo vệ suốt đời", "B": "Có giá trị hoàn lại nhưng không bảo vệ suốt đời", "C": "Không có giá trị hoàn lại và không bảo vệ suốt đời", "D": "Có giá trị thấp hơn bảo hiểm tạm thời"}, "category": "comparison", "question": "Câu hỏi nào sau đây mô tả đúng đặc điểm của bảo hiểm nhân thọ vĩnh viễn?", "difficulty": "medium", "explanation": "Bảo hiểm nhân thọ vĩnh viễn cung cấp bảo vệ suốt đời và có giá trị hoàn lại.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Khi người tham gia qua đời", "B": "Khi hợp đồng hết hạn mà không có rủi ro xảy ra", "C": "Khi người tham gia quyết định dừng hợp đồng", "D": "Khi người tham gia thay đổi công ty bảo hiểm"}, "category": "comparison", "question": "Khi nào thì bảo hiểm nhân thọ tạm thời không có giá trị hoàn lại?", "difficulty": "medium", "explanation": "Bảo hiểm nhân thọ tạm thời không có giá trị hoàn lại khi hợp đồng hết hạn mà không có rủi ro xảy ra.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Chỉ khi tham gia bảo hiểm mới", "B": "Khi có sự thay đổi trong tình trạng sức khỏe", "C": "Khi có người trong gia đình qua đời", "D": "Không cần thông báo gì"}, "category": "core_concept", "question": "Khi nào người tham gia bảo hiểm nhân thọ cần thông báo cho công ty bảo hiểm?", "difficulty": "medium", "explanation": "Người tham gia cần thông báo cho công ty bảo hiểm khi có sự thay đổi trong tình trạng sức khỏe để bảo vệ quyền lợi của mình.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Nhận tiền bảo hiểm khi qua đời", "B": "Có giá trị hoàn lại khi dừng hợp đồng", "C": "Nhận tiền bảo hiểm khi sống lâu hơn thời gian bảo hiểm", "D": "Quyền lợi bổ sung như bảo hiểm tai nạn"}, "category": "core_concept", "question": "Điều nào sau đây không phải là quyền lợi của người tham gia bảo hiểm nhân thọ?", "difficulty": "hard", "explanation": "Người tham gia không nhận lại tiền bảo hiểm khi sống lâu hơn thời gian bảo hiểm của bảo hiểm tạm thời.", "correctAnswer": "C"}, {"id": "q12", "options": {"A": "Chọn bảo hiểm dựa trên quảng cáo", "B": "Xem xét tình trạng sức khỏe và nhu cầu bảo hiểm", "C": "Chọn bảo hiểm cao nhất có thể", "D": "Không cần so sánh các sản phẩm khác nhau"}, "category": "core_concept", "question": "Lưu ý quan trọng nào về việc chọn bảo hiểm nhân thọ?", "difficulty": "hard", "explanation": "Việc xem xét tình trạng sức khỏe và nhu cầu bảo hiểm là rất quan trọng để chọn sản phẩm bảo hiểm phù hợp.", "correctAnswer": "B"}]', 12, 80, 'Boss Quiz: Khám Phá Bảo Hiểm Nhân Thọ', '2026-01-22 16:31:28.332', 'gpt-4o-mini', '2026-01-22 16:31:28.371503', '2026-01-22 16:31:28.371503');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('797f7b2f-756a-475b-88cb-91338a7c024f', NULL, '3eadfef9-f249-4920-8bd0-9151864b7c67', 'boss', NULL, '[{"id": "q1", "options": {"A": "Giúp truyền tải thông điệp rõ ràng và chuyên nghiệp.", "B": "Không có tác động gì đến thông điệp.", "C": "Chỉ quan trọng với tài liệu quảng cáo.", "D": "Giúp tiết kiệm thời gian in ấn."}, "category": "core_concept", "question": "In ấn tài liệu có tầm quan trọng như thế nào trong việc truyền tải thông điệp?", "difficulty": "medium", "explanation": "In ấn tài liệu chất lượng giúp truyền tải thông điệp một cách rõ ràng và chuyên nghiệp, nâng cao trải nghiệm người đọc.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Sử dụng giấy trắng và phông chữ rõ ràng.", "B": "In trên giấy nháp.", "C": "Sử dụng nhiều màu sắc.", "D": "In với định dạng không rõ ràng."}, "category": "core_concept", "question": "Khi in một báo cáo học tập, yếu tố nào quan trọng nhất để đảm bảo giáo viên dễ dàng đánh giá tài liệu?", "difficulty": "medium", "explanation": "Sử dụng giấy trắng và phông chữ rõ ràng giúp giáo viên dễ dàng đọc và đánh giá báo cáo.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Thiết kế tài liệu hấp dẫn và đầy đủ thông tin.", "B": "In màu sắc ngẫu nhiên.", "C": "Chỉ in tài liệu cho diễn giả.", "D": "Không cần thiết kế gì đặc biệt."}, "category": "core_concept", "question": "In ấn tài liệu cho hội thảo chuyên ngành nên chú trọng vào yếu tố nào?", "difficulty": "medium", "explanation": "Thiết kế tài liệu hấp dẫn và đầy đủ thông tin giúp người tham gia dễ dàng tiếp cận nội dung hội thảo.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "In offset.", "B": "In 3D.", "C": "In kỹ thuật số.", "D": "In bằng tay."}, "category": "core_concept", "question": "Phương pháp in nào thường được sử dụng cho sản xuất hàng loạt và giữ được chất lượng cao?", "difficulty": "medium", "explanation": "In offset là phương pháp phù hợp cho sản xuất hàng loạt với chất lượng cao nhờ vào khả năng sản xuất số lượng lớn.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Kiểm soát chất lượng định kỳ.", "B": "Chỉ cần in một lần duy nhất.", "C": "Sử dụng giấy rẻ tiền.", "D": "Không cần kiểm tra."}, "category": "core_concept", "question": "Trong quá trình in ấn tài liệu marketing, yếu tố nào là cần thiết để đảm bảo chất lượng sản phẩm?", "difficulty": "medium", "explanation": "Kiểm soát chất lượng định kỳ là cần thiết để đảm bảo rằng tài liệu in đạt tiêu chuẩn cao.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Chất lượng in ấn không ảnh hưởng đến hình ảnh thương hiệu.", "B": "Chất lượng in kém có thể hạ thấp hình ảnh thương hiệu.", "C": "Hình ảnh thương hiệu chỉ phụ thuộc vào giá cả.", "D": "Chất lượng in không quan trọng trong marketing."}, "category": "connection", "question": "Mối liên hệ giữa chất lượng in ấn và hình ảnh thương hiệu của công ty là gì?", "difficulty": "medium", "explanation": "Chất lượng in kém có thể dẫn đến hình ảnh thương hiệu bị hạ thấp và ảnh hưởng đến sự tin tưởng của khách hàng.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "In offset.", "B": "In 3D.", "C": "In kỹ thuật số.", "D": "In bằng tay."}, "category": "comparison", "question": "Giữa in offset và in kỹ thuật số, phương pháp nào phù hợp hơn cho in số lượng nhỏ hoặc tùy chỉnh?", "difficulty": "medium", "explanation": "In kỹ thuật số là lựa chọn tốt hơn cho in số lượng nhỏ hoặc tùy chỉnh do thời gian nhanh chóng và dễ dàng.", "correctAnswer": "C"}, {"id": "q8", "options": {"A": "Khi cần tạo mẫu vật và tài liệu trực quan.", "B": "Chỉ cho tài liệu văn phòng.", "C": "Chỉ cho sách hướng dẫn.", "D": "Khi không cần thiết kế."}, "category": "application", "question": "Khi nào nên sử dụng in 3D trong in ấn tài liệu?", "difficulty": "medium", "explanation": "In 3D được sử dụng để tạo ra các mẫu vật và tài liệu trực quan, giúp cải thiện trải nghiệm người dùng.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Đàm phán với khách hàng về chất lượng và giá cả.", "B": "Sử dụng giấy rẻ tiền.", "C": "In tài liệu mà không kiểm tra.", "D": "Không cần cải tiến quy trình."}, "category": "connection", "question": "Một trong những thách thức lớn nhất trong quản lý chất lượng in ấn là gì?", "difficulty": "hard", "explanation": "Đàm phán với khách hàng về sự kỳ vọng và tiêu chuẩn chất lượng là một thách thức lớn trong ngành in ấn.", "correctAnswer": "A"}, {"id": "q10", "options": {"A": "Thiết kế hấp dẫn và thông tin rõ ràng.", "B": "Sử dụng giấy nháp.", "C": "Chỉ cần gửi tin nhắn điện thoại.", "D": "Không cần chú ý đến màu sắc."}, "category": "application", "question": "Khi thiết kế thiệp mời cho bữa tiệc sinh nhật, yếu tố nào là quan trọng nhất?", "difficulty": "medium", "explanation": "Thiết kế hấp dẫn và thông tin rõ ràng giúp thu hút sự chú ý của khách mời và khiến họ cảm thấy quan trọng.", "correctAnswer": "A"}, {"id": "q11", "options": {"A": "ISO 9001 và FSC.", "B": "Chỉ cần giấy tốt.", "C": "Không cần tiêu chuẩn nào cả.", "D": "Sử dụng mực giá rẻ."}, "category": "core_concept", "question": "Các tiêu chuẩn nào là cần thiết để đảm bảo chất lượng trong in ấn?", "difficulty": "hard", "explanation": "ISO 9001 và FSC là những tiêu chuẩn quan trọng giúp đảm bảo chất lượng và tính bền vững trong in ấn.", "correctAnswer": "A"}, {"id": "q12", "options": {"A": "Tăng cường độ tin cậy và sự chú ý từ người tiêu dùng.", "B": "Không ảnh hưởng gì đến marketing.", "C": "Chỉ quan trọng với sản phẩm mới.", "D": "Giảm chi phí marketing."}, "category": "connection", "question": "In ấn tài liệu chất lượng cao có thể ảnh hưởng đến điều gì trong hoạt động marketing?", "difficulty": "hard", "explanation": "In ấn tài liệu chất lượng cao giúp tăng cường độ tin cậy và sự chú ý từ người tiêu dùng, cải thiện hiệu quả marketing.", "correctAnswer": "A"}]', 12, 80, 'Boss Quiz: Giới thiệu về In ấn Tài liệu', '2026-01-2