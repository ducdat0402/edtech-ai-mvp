INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('4f9aa0ac-4ed0-4c54-a644-7df3b9a056b4', NULL, 'd423d2c7-b79a-4eef-a960-87fd4b2784a4', 'boss', NULL, '[{"id": "q1", "options": {"A": "Tỷ lệ phần trăm của các phần trong tổng thể", "B": "Giá trị tuyệt đối của từng phần", "C": "Sự biến động theo thời gian", "D": "Các mối quan hệ giữa các biến số"}, "category": "core_concept", "question": "Biểu đồ tròn được sử dụng chủ yếu để thể hiện điều gì?", "difficulty": "medium", "explanation": "Biểu đồ tròn giúp người dùng dễ dàng nhận diện tỷ lệ phần trăm của từng phần trong tổng thể, giống như một chiếc bánh pizza.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Sử dụng màu sắc tương phản", "B": "Ghi chú tỷ lệ phần trăm cho từng phần", "C": "Tạo quá nhiều phần nhỏ", "D": "Giữ cho biểu đồ đơn giản và dễ hiểu"}, "category": "comparison", "question": "Điều gì không nên được thực hiện khi tạo biểu đồ tròn?", "difficulty": "hard", "explanation": "Tạo quá nhiều phần nhỏ trong biểu đồ tròn có thể khiến thông tin trở nên rối rắm và khó hiểu.", "correctAnswer": "C"}, {"id": "q3", "options": {"A": "Khi có từ 4 đến 6 phần", "B": "Khi các phần có tỷ lệ phần trăm khác nhau đáng kể", "C": "Khi thể hiện các phần nhỏ trong một tổng thể lớn", "D": "Khi cần so sánh nhiều giá trị khác nhau"}, "category": "core_concept", "question": "Khi nào thì biểu đồ tròn không phù hợp để sử dụng?", "difficulty": "medium", "explanation": "Biểu đồ tròn không phù hợp để so sánh nhiều giá trị khác nhau, mà nên sử dụng các loại biểu đồ khác như biểu đồ cột.", "correctAnswer": "D"}, {"id": "q4", "options": {"A": "Giá trị phần chia cho tổng giá trị nhân với 100", "B": "Tổng giá trị chia cho từng phần", "C": "Giá trị phần cộng với tổng giá trị", "D": "Tổng giá trị trừ đi giá trị phần"}, "category": "core_concept", "question": "Tỷ lệ phần trăm trong biểu đồ tròn được tính như thế nào?", "difficulty": "medium", "explanation": "Tỷ lệ phần trăm được tính bằng cách chia giá trị phần cho tổng giá trị và nhân với 100.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Hiển thị tỷ lệ ngân sách cho từng bộ phận", "B": "Dự đoán doanh thu tương lai", "C": "So sánh chi phí giữa các bộ phận", "D": "Phân tích xu hướng ngân sách qua thời gian"}, "category": "application", "question": "Khi phân bổ ngân sách cho các bộ phận công ty, biểu đồ tròn có thể giúp gì?", "difficulty": "medium", "explanation": "Biểu đồ tròn giúp người dùng dễ dàng hình dung tỷ lệ ngân sách được phân bổ cho từng bộ phận trong công ty.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Biểu đồ tròn thể hiện tỷ lệ, biểu đồ cột thể hiện giá trị tuyệt đối", "B": "Biểu đồ tròn dùng cho số liệu lớn, biểu đồ cột cho số liệu nhỏ", "C": "Biểu đồ tròn chỉ dùng cho doanh thu, biểu đồ cột cho chi phí", "D": "Biểu đồ tròn không thể có nhãn, biểu đồ cột thì có"}, "category": "comparison", "question": "Sự khác biệt giữa biểu đồ tròn và biểu đồ cột là gì?", "difficulty": "hard", "explanation": "Biểu đồ tròn thể hiện tỷ lệ phần trăm của các phần trong tổng thể, trong khi biểu đồ cột thể hiện giá trị tuyệt đối của từng phần.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Chỉ sử dụng màu sắc khác nhau cho mỗi khu vực", "B": "Nhóm các khu vực có doanh thu thấp lại với nhau", "C": "Thêm nhiều phần nhỏ vào biểu đồ", "D": "Chỉ thể hiện những khu vực có doanh thu cao"}, "category": "application", "question": "Khi bạn có doanh thu từ 3 khu vực khác nhau, cách nào để tạo biểu đồ tròn hiệu quả nhất?", "difficulty": "hard", "explanation": "Nhóm các khu vực có doanh thu thấp lại với nhau giúp biểu đồ trở nên rõ ràng hơn và tránh rối mắt.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Chọn màu sắc bắt mắt", "B": "Sử dụng các số liệu chính xác và có nguồn gốc rõ ràng", "C": "Thêm nhiều thông tin chi tiết vào biểu đồ", "D": "Làm cho biểu đồ thật lớn và nổi bật"}, "category": "core_concept", "question": "Khi tạo biểu đồ tròn, điều gì là quan trọng nhất để đảm bảo tính chính xác?", "difficulty": "medium", "explanation": "Việc sử dụng số liệu chính xác và có nguồn gốc rõ ràng đảm bảo rằng biểu đồ tròn phản ánh đúng thông tin mà nó muốn truyền đạt.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Rất lớn và dễ thấy", "B": "Rất nhỏ và khó nhận diện", "C": "Chiếm 50% diện tích biểu đồ", "D": "Có thể bị lãng quên hoàn toàn"}, "category": "application", "question": "Trong một khảo sát về sự hài lòng của khách hàng, tỷ lệ phần trăm phản hồi ''Kém'' là 5%. Nếu bạn tạo biểu đồ tròn, phần này sẽ như thế nào?", "difficulty": "medium", "explanation": "Phản hồi ''Kém'' với tỷ lệ 5% sẽ tạo thành một phần rất nhỏ trong biểu đồ tròn, khó nhận diện và có thể bị lãng quên.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Khi có quá nhiều phần trong biểu đồ", "B": "Khi bạn muốn hiển thị số liệu chính xác hơn", "C": "Khi bạn có ít dữ liệu", "D": "Khi bạn không cần người xem tương tác"}, "category": "application", "question": "Khi nào bạn nên sử dụng một công cụ tương tác cho biểu đồ tròn?", "difficulty": "hard", "explanation": "Sử dụng công cụ tương tác cho biểu đồ tròn khi có quá nhiều phần sẽ giúp người dùng có thể xem chi tiết hơn, tránh sự quá tải thông tin.", "correctAnswer": "A"}, {"id": "q11", "options": {"A": "Giúp dễ dàng so sánh các phần trong tổng thể", "B": "Giúp tăng độ chính xác của dữ liệu", "C": "Chỉ cần dùng một loại phần mềm duy nhất", "D": "Không có ý nghĩa gì trong phân tích"}, "category": "core_concept", "question": "Trong việc phân tích dữ liệu, việc sử dụng tỷ lệ phần trăm có ý nghĩa gì?", "difficulty": "medium", "explanation": "Tỷ lệ phần trăm giúp người dùng dễ dàng so sánh các phần trong tổng thể và hiểu rõ hơn về sự phân bổ dữ liệu.", "correctAnswer": "A"}, {"id": "q12", "options": {"A": "Chọn màu sắc bắt mắt", "B": "Tính toán tỷ lệ phần trăm cho từng mục", "C": "Sử dụng nhiều phần nhỏ", "D": "Ghi chú các mục không cần thiết"}, "category": "core_concept", "question": "Khi tạo biểu đồ tròn cho ngân sách hàng tháng, điều gì là cần thiết nhất?", "difficulty": "medium", "explanation": "Tính toán tỷ lệ phần trăm cho từng mục là cần thiết để tạo ra biểu đồ tròn chính xác và dễ hiểu.", "correctAnswer": "B"}, {"id": "q13", "options": {"A": "Có quá nhiều phần nhỏ", "B": "Mọi phần đều có tỷ lệ lớn", "C": "Có màu sắc khác nhau cho từng phần", "D": "Có nhãn rõ ràng cho từng phần"}, "category": "comparison", "question": "Một biểu đồ tròn có thể không truyền đạt thông điệp hiệu quả nếu:", "difficulty": "hard", "explanation": "Một biểu đồ tròn có quá nhiều phần nhỏ sẽ khiến thông điệp trở nên khó hiểu và người xem không thể nhận diện rõ các phần quan trọng.", "correctAnswer": "A"}, {"id": "q14", "options": {"A": "Màu sắc tương phản", "B": "Kích thước của biểu đồ", "C": "Số lượng phần trong biểu đồ", "D": "Hình dạng của biểu đồ"}, "category": "core_concept", "question": "Trong biểu đồ tròn, điều gì giúp người dùng dễ dàng phân biệt các phần?", "difficulty": "medium", "explanation": "Màu sắc tương phản giúp người dùng dễ dàng phân biệt các phần trong biểu đồ tròn, nâng cao khả năng nhận diện thông tin.", "correctAnswer": "A"}, {"id": "q15", "options": {"A": "Vì chúng rất dễ tạo ra", "B": "Vì chúng giúp người xem dễ dàng nhận diện tỷ lệ phần trăm", "C": "Vì chúng chỉ cần một loại dữ liệu duy nhất", "D": "Vì chúng không cần nhãn cho các phần"}, "category": "core_concept", "question": "Tại sao biếu đồ tròn lại được ưa chuộng trong các báo cáo và thuyết trình?", "difficulty": "medium", "explanation": "Biểu đồ tròn giúp người xem dễ dàng nhận diện tỷ lệ phần trăm của từng phần trong tổng thể, điều này rất hữu ích trong báo cáo và thuyết trình.", "correctAnswer": "B"}]', 15, 80, 'Boss Quiz: Tạo Biểu Đồ Tròn', '2026-01-22 17:07:51.333', 'gpt-4o-mini', '2026-01-22 17:07:51.374045', '2026-01-22 17:07:51.374045');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('d901eee2-cae7-4212-8db2-053eeb57b2c9', NULL, 'ae369e64-6c5e-4daf-b676-221dbf5c67b2', 'boss', NULL, '[{"id": "q1", "options": {"A": "Quy trình quản lý tài chính cá nhân để điều chỉnh chi tiêu và tiết kiệm", "B": "Quá trình vay mượn tiền từ ngân hàng", "C": "Phương pháp đầu tư vào cổ phiếu", "D": "Kỹ thuật lập kế hoạch cho các khoản chi tiêu hàng tháng"}, "category": "core_concept", "question": "Điều chỉnh ngân sách là gì?", "difficulty": "medium", "explanation": "Điều chỉnh ngân sách là quy trình quản lý tài chính cá nhân nhằm điều chỉnh chi tiêu và tiết kiệm cho tương lai.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Xác định mục tiêu tài chính", "B": "Đánh giá tình hình tài chính hiện tại", "C": "Tạo kế hoạch điều chỉnh ngân sách", "D": "Theo dõi và điều chỉnh thường xuyên"}, "category": "core_concept", "question": "Bước đầu tiên trong việc điều chỉnh ngân sách là gì?", "difficulty": "medium", "explanation": "Bước đầu tiên là đánh giá tình hình tài chính hiện tại để xác định thu nhập và chi tiêu.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Chỉ cần có một thời gian dài để hoàn thành", "B": "Phải cụ thể, có thể đo lường được và có thời gian thực hiện", "C": "Không cần phải cụ thể, chỉ cần là điều bạn muốn", "D": "Chỉ cần là những gì bạn nghĩ là tốt"}, "category": "core_concept", "question": "Mục tiêu tài chính cần phải có những tiêu chí nào?", "difficulty": "medium", "explanation": "Mục tiêu tài chính cần phải cụ thể, có thể đo lường được và có thời gian thực hiện để đảm bảo tính khả thi.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Bạn có thể yên tâm", "B": "Bạn cần cắt giảm chi tiêu hoặc tìm cách tăng thu nhập", "C": "Bạn không cần làm gì cả", "D": "Bạn có thể vay tiền để chi tiêu thêm"}, "category": "core_concept", "question": "Điều gì xảy ra nếu chi tiêu của bạn vượt quá thu nhập?", "difficulty": "medium", "explanation": "Nếu chi tiêu vượt quá thu nhập, bạn cần cắt giảm chi tiêu hoặc tìm cách tăng thu nhập để duy trì sự ổn định tài chính.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Biết rõ bạn đã chi tiền vào đâu", "B": "Giúp bạn quên đi các khoản nợ", "C": "Cho phép bạn chi tiêu nhiều hơn", "D": "Giúp bạn kiếm thêm tiền"}, "category": "core_concept", "question": "Theo dõi chi tiêu giúp bạn điều gì?", "difficulty": "medium", "explanation": "Theo dõi chi tiêu giúp bạn biết rõ bạn đã chi tiền vào đâu, từ đó đưa ra quyết định tài chính hợp lý hơn.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Chi tiêu theo cảm tính", "B": "Liệt kê tất cả các khoản chi phí và đánh giá sự cần thiết", "C": "Không theo dõi chi tiêu", "D": "Tăng chi tiêu cho giải trí"}, "category": "core_concept", "question": "Một trong những cách hiệu quả để tối ưu hóa chi phí hàng tháng là gì?", "difficulty": "medium", "explanation": "Liệt kê và đánh giá các khoản chi phí giúp bạn nhận diện những khoản chi không cần thiết và tối ưu hóa chi phí.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Chỉ khi bạn có nợ", "B": "Hàng tháng hoặc hàng tuần", "C": "Chỉ khi bạn muốn tiết kiệm", "D": "Không cần theo dõi"}, "category": "core_concept", "question": "Khi nào bạn nên theo dõi chi tiêu của mình?", "difficulty": "medium", "explanation": "Theo dõi chi tiêu nên được thực hiện hàng tháng hoặc hàng tuần để có cái nhìn tổng quan về tình hình tài chính.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "50% cho tiết kiệm, 30% cho nhu cầu thiết yếu, 20% cho nhu cầu cá nhân", "B": "50% cho nhu cầu thiết yếu, 30% cho nhu cầu cá nhân, 20% cho tiết kiệm và đầu tư", "C": "20% cho tiết kiệm, 50% cho nhu cầu thiết yếu, 30% cho nhu cầu cá nhân", "D": "Không có quy tắc nào cả"}, "category": "core_concept", "question": "Trong một kế hoạch tiết kiệm, bạn nên phân bổ ngân sách như thế nào theo nguyên tắc 50/30/20?",