INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('f850ec47-a187-4230-809c-94d4944a562b', '404934f0-5624-4ff1-acd6-fb98aca7084a', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Hít thở sâu để giảm căng thẳng", "B": "Tập trung vào mục tiêu", "C": "Tăng tốc độ ném bóng", "D": "Sử dụng âm thanh tích cực"}, "category": "distinction", "question": "Yếu tố nào dưới đây không phải là một kỹ thuật để cải thiện tâm lý khi ném bóng?", "explanation": "C đúng vì tăng tốc độ ném bóng không phải là một kỹ thuật tâm lý; ngược lại, A, B và D đều là kỹ thuật giúp cải thiện tâm lý trước cú ném.", "correctAnswer": "C"}, {"id": "q2", "options": {"A": "Tăng cường sức mạnh cơ bắp", "B": "Giảm căng thẳng và lo âu", "C": "Tăng tốc độ ném", "D": "Cải thiện kỹ thuật ném"}, "category": "definition", "question": "Khi cầu thủ bóng rổ hít thở sâu trước khi ném, tác động tích cực nào sau đây nó mang lại?", "explanation": "B đúng vì hít thở sâu giúp giảm căng thẳng và lo âu, điều này không liên quan đến sức mạnh cơ bắp, tốc độ ném hay cải thiện kỹ thuật.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Giúp cầu thủ quên đi sai lầm", "B": "Học hỏi từ sai lầm và thành công", "C": "Tăng cường sự chú ý vào rổ", "D": "Giảm thời gian ném bóng"}, "category": "definition", "question": "Phân tích các cú ném trước đó giúp cầu thủ bóng rổ điều chỉnh tâm lý như thế nào?", "explanation": "B đúng vì việc phân tích giúp cầu thủ học hỏi từ những sai lầm và thành công trong quá khứ, trong khi A, C, và D không liên quan trực tiếp đến tâm lý.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Tự tin là cảm giác tích cực, tự mãn là cảm giác tiêu cực", "B": "Tự tin giúp cầu thủ thể hiện tốt hơn, tự mãn có thể dẫn đến sai lầm", "C": "Tự tin chỉ cần kỹ thuật, tự mãn cần sự khích lệ", "D": "Tự tin là trạng thái tạm thời, tự mãn là trạng thái lâu dài"}, "category": "distinction", "question": "Cảm giác tự tin khi ném bóng khác với cảm giác tự mãn ở điểm nào?", "explanation": "B đúng vì tự tin giúp cầu thủ thể hiện tốt hơn, trong khi tự mãn có thể dẫn đến sự chủ quan và sai lầm. A, C và D không chính xác trong bối cảnh này.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Tập trung vào hình dung cú ném thành công", "B": "Sử dụng từ ngữ khích lệ", "C": "Tăng cường sức mạnh cơ bắp", "D": "Phân tích các cú ném trước đó"}, "category": "distinction", "question": "Điều nào dưới đây không liên quan đến việc cải thiện cảm giác khi ném bóng?", "explanation": "C đúng vì việc tăng cường sức mạnh cơ bắp không ảnh hưởng trực tiếp đến tâm lý và cảm giác khi ném bóng, trong khi A, B và D đều là kỹ thuật tâm lý quan trọng.", "correctAnswer": "C"}]', 5, 70, 'Cảm Nhận và Tâm Lý Trong Ném Bóng', '2026-01-21 23:17:48.17', 'gpt-4o-mini', '2026-01-21 23:17:48.21722', '2026-01-21 23:17:48.21722');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('0c4b00b7-5322-4187-a60f-a4ad0e5d0abb', '343b0c8e-3ad4-43b0-9947-934a75df955f', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Tư thế đứng vững chắc và sự cân bằng", "B": "Không bao giờ thay đổi tư thế", "C": "Chỉ sử dụng sức mạnh tay", "D": "Ném bóng từ xa mà không cần kỹ thuật"}, "category": "definition", "question": "Điểm nào sau đây là yếu tố quan trọng trong kỹ thuật ném bóng của cầu thủ chuyên nghiệp?", "explanation": "A đúng vì tư thế đứng vững chắc và sự cân bằng là yếu tố cốt lõi trong kỹ thuật ném bóng. Các đáp án khác sai vì chúng không phản ánh đúng bản chất của kỹ thuật ném bóng chuyên nghiệp.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Sử dụng sức mạnh từ toàn bộ cơ thể", "B": "Chỉ dựa vào sức mạnh của cánh tay", "C": "Không cần sức mạnh, chỉ cần kỹ thuật", "D": "Chỉ ném bằng một tay"}, "category": "definition", "question": "Kỹ thuật ném bóng của Stephen Curry có điểm nổi bật nào trong cách sử dụng sức mạnh?", "explanation": "A đúng vì Stephen Curry sử dụng sức mạnh từ toàn bộ cơ thể để tạo ra cú ném hiệu quả. Các đáp án khác không phản ánh đúng cách thức mà cầu thủ chuyên nghiệp tối ưu hóa sức mạnh trong cú ném.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Cầu thủ chuyên nghiệp có sự linh hoạt trong việc điều chỉnh kỹ thuật", "B": "Cầu thủ nghiệp dư hoàn toàn không có kỹ thuật", "C": "Kỹ thuật ném bóng của nghiệp dư luôn chính xác hơn", "D": "Không có sự khác biệt giữa hai loại cầu thủ"}, "category": "distinction", "question": "Điểm khác biệt nào giữa kỹ thuật ném bóng của các cầu thủ chuyên nghiệp và cầu thủ nghiệp dư?", "explanation": "A đúng vì cầu thủ chuyên nghiệp thường có khả năng điều chỉnh kỹ thuật tùy theo tình huống. Các đáp án khác sai vì chúng không phản ánh đúng sự khác biệt giữa các cấp độ cầu thủ.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Tinh thần kiên nhẫn và kiên định", "B": "Chỉ ném khi không có đối thủ", "C": "Sự linh hoạt trong kỹ thuật", "D": "Khả năng sử dụng sức mạnh hiệu quả"}, "category": "distinction", "question": "Điểm nào sau đây không phải là một khía cạnh trong kỹ thuật ném bóng của cầu thủ chuyên nghiệp?", "explanation": "B đúng vì cầu thủ chuyên nghiệp phải ném trong nhiều tình huống khác nhau, bao gồm cả khi có đối thủ. Các đáp án khác đều là những khía cạnh quan trọng trong kỹ thuật ném bóng.", "correctAnswer": "B"}]', 4, 70, 'Phân Tích Kỹ Thuật Ném Bóng Từ Các Cầu Thủ Chuyên Nghiệp', '2026-01-21 23:18:10.557', 'gpt-4o-mini', '2026-01-21 23:18:10.604222', '2026-01-21 23:18:10.604222');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('a5ff57f9-59d9-4d43-8497-fbf605900c46', '3b5150dc-d58b-40c3-ada4-ccf019ac1971', NULL, 'lesson', 'example', '[{"id": "q1", "options": {"A": "Ném bóng vào một cái rổ mini từ vị trí đứng yên.", "B": "Chỉ ném bóng mà không quan tâm đến tư thế cơ thể.", "C": "Thực hiện ném bóng mà không ghi lại video.", "D": "Ném bóng chỉ từ một góc độ duy nhất."}, "category": "correct_example", "question": "Cách nào dưới đây là một ví dụ đúng về luyện tập kỹ thuật ném bóng tại nhà?", "explanation": "A đúng vì ném bóng vào rổ mini từ vị trí đứng yên giúp cải thiện kỹ thuật cầm bóng và tư thế cơ thể. Các đáp án B, C, D đều sai vì không tập trung vào các khía cạnh quan trọng trong luyện tập.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Ném bóng vào tường để kiểm tra độ chính xác.", "B": "Chỉ ném bóng mà không chú ý đến tư thế.", "C": "Ghi lại video để tự phân tích kỹ thuật.", "D": "Thực hiện các cú ném từ nhiều góc độ khác nhau."}, "category": "wrong_example", "question": "Trong số các hoạt động dưới đây, hoạt động nào không phù hợp với việc cải thiện kỹ thuật ném bóng?", "explanation": "B là đáp án sai vì chỉ ném bóng mà không chú ý đến tư thế không giúp cải thiện kỹ thuật. Các đáp án A, C, D đều là những hoạt động phù hợp.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Tập ném bóng vào rổ mini mà không phân tích cú ném.", "B": "Thực hiện các cú ném từ nhiều góc độ khác nhau và ghi lại video.", "C": "Ném bóng vào bức tường mà không chú ý đến tư thế.", "D": "Chỉ thực hiện một bài tập ném duy nhất."}, "category": "correct_example", "question": "Ví dụ nào dưới đây thể hiện cách áp dụng đúng kỹ thuật ném bóng tại nhà?", "explanation": "B đúng vì thực hiện các cú ném từ nhiều góc độ khác nhau và ghi lại video giúp cải thiện kỹ thuật. Các đáp án A, C, D không phù hợp với nguyên tắc luyện tập hiệu quả.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Ghi lại video các cú ném để tự phân tích.", "B": "Ném bóng mà không kiểm tra tư thế cơ thể.", "C": "Thực hiện ném bóng từ nhiều góc độ khác nhau.", "D": "Sử dụng rổ mini để luyện tập."}, "category": "wrong_example", "question": "Cách nào dưới đây được coi là một ví dụ sai trong việc luyện tập ném bóng?", "explanation": "B sai vì không kiểm tra tư thế cơ thể là một sai lầm trong luyện tập. Các đáp án A, C, D đều là những cách luyện tập phù hợp.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Cầu thủ sẽ cải thiện kỹ năng ném bóng nhanh chóng.", "B": "Cầu thủ có thể không nhận diện được các điểm cần cải thiện.", "C": "Cầu thủ sẽ tự động điều chỉnh kỹ thuật của mình.", "D": "Cầu thủ sẽ dễ dàng ném bóng chính xác hơn."}, "category": "mini_case", "question": "Nếu một cầu thủ chỉ thực hiện ném bóng mà không thay đổi góc độ hoặc ghi lại video, điều này có thể dẫn đến điều gì?", "explanation": "B đúng vì không ghi lại video và không thay đổi góc độ có thể khiến cầu thủ không nhận diện được các điểm cần cải thiện. Các đáp án A, C, D không chính xác vì không phản ánh thực tế luyện tập hiệu quả.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Ném bóng thật mạnh.", "B": "Giữ tư thế cơ thể và kỹ thuật cầm bóng đúng.", "C": "Chỉ tập luyện vào buổi tối.", "D": "Ném bóng liên tục mà không nghĩ đến kỹ thuật."}, "category": "correct_example", "question": "Khi luyện tập ném bóng tại nhà, điều nào sau đây là quan trọng nhất?", "explanation": "B đúng vì giữ tư thế cơ thể và kỹ thuật cầm bóng đúng là yếu tố quan trọng nhất khi luyện tập. A, C, D không phản ánh những điều cần thiết để cải thiện kỹ năng ném bóng.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Để có thể khoe với bạn bè.", "B": "Để tự phân tích và nhận diện các điểm cần cải thiện.", "C": "Để có thêm áp lực trong luyện tập.", "D": "Để không cần chú ý đến kỹ thuật."}, "category": "correct_example", "question": "Tại sao việc ghi lại video các cú ném lại quan trọng trong quá trình luyện tập?", "explanation": "B là đáp án đúng vì ghi lại video giúp cầu thủ tự phân tích và nhận diện các điểm cần cải thiện. A, C, D không phản ánh đúng mục đích của việc ghi lại video trong luyện tập.", "correctAnswer": "B"}]', 7, 70, 'Ví Dụ: Luyện Tập Kỹ Thuật Ném Bóng Tại Nhà', '2026-01-21 23:18:46.618', 'gpt-4o-mini', '2026-01-21 23:18:46.66278', '2026-01-21 23:18:46.66278');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('f36103bd-c54e-4ac6-8b58-e88a9a06ac35', NULL, 'da4e1803-cf29-46b1-ac58-160894256860', 'boss', NULL, '[{"id": "q1", "options": {"A": "Johann Sebastian Bach", "B": "Ludwig van Beethoven", "C": "Frédéric Chopin", "D": "Wolfgang Amadeus Mozart"}, "category": "core_concept", "question": "Nhà soạn nhạc nào được xem là vị cha đẻ của nhạc cổ điển?", "difficulty": "medium", "explanation": "Johann Sebastian Bach được coi là một trong những nhà soạn nhạc vĩ đại nhất và có ảnh hưởng sâu sắc đến sự phát triển của nhạc cổ điển.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Thời kỳ Baroque", "B": "Thời kỳ Cổ điển", "C": "Thời kỳ Lãng mạn", "D": "Thời kỳ Phục hưng"}, "category": "core_concept", "question": "Trong thời kỳ nào piano được phát minh?", "difficulty": "medium", "explanation": "Piano được phát minh vào đầu thế kỷ 18, trong thời kỳ Baroque.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Prelude in C Major", "B": "Nocturne in E-flat Major", "C": "Goldberg Variations", "D": "Moonlight Sonata"}, "category": "core_concept", "question": "Tác phẩm nào của Chopin được biết đến với tính biểu cảm cao và thường được biểu diễn trong các buổi hòa nhạc?", "difficulty": "medium", "explanation": "Nocturne in E-flat Major, Op. 9, No. 2 là một trong những tác phẩm nổi bật của Chopin thể hiện sự sâu lắng và tinh tế trong cảm xúc.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Âm thanh của harpsichord to hơn", "B": "Piano cho phép điều chỉnh âm lượng tùy thuộc vào lực nhấn phím", "C": "Harpsichord có nhiều phím hơn", "D": "Piano không thể chơi các hợp âm"}, "category": "core_concept", "question": "Điểm khác biệt chính giữa harpsichord và piano là gì?", "difficulty": "medium", "explanation": "Piano cho phép người chơi điều chỉnh âm lượng tùy thuộc vào lực nhấn phím, điều này không có ở harpsichord.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Chopins sử dụng nhiều kỹ thuật phức tạp hơn", "B": "Chopin tập trung vào cảm xúc cá nhân hơn", "C": "Beethoven không viết nhạc cho piano", "D": "Chopin sống lâu hơn Beethoven"}, "category": "comparison", "question": "Chopin và Beethoven đều có những tác phẩm nổi bật cho piano. Tuy nhiên, điều gì khiến âm nhạc của Chopin khác biệt hơn trong thời kỳ Lãng mạn?", "difficulty": "medium", "explanation": "Âm nhạc của Chopin thường thể hiện cảm xúc cá nhân rất sâu sắc, điều này là đặc trưng của thời kỳ Lãng mạn.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Thể loại biến tấu", "B": "Cấu trúc ba đoạn", "C": "Cấu trúc sonata (điều kiện, phát triển, tái xuất)", "D": "Cấu trúc vòng tròn"}, "category": "core_concept", "question": "Cấu trúc nào thường được sử dụng trong các tác phẩm sonata của Beethoven?", "difficulty": "medium", "explanation": "Beethoven thường sử dụng cấu trúc sonata bao gồm điều kiện, phát triển và tái xuất trong các tác phẩm của mình.", "correctAnswer": "C"}, {"id": "q7", "options": {"A": "Nó giúp chơi nốt nhạc nhanh hơn", "B": "Nó tạo ra âm thanh mượt mà", "C": "Nó chỉ được sử dụng trong nhạc cổ điển", "D": "Nó làm cho âm nhạc trở nên phức tạp hơn"}, "category": "core_concept", "question": "Tại sao kỹ thuật arpeggio lại quan trọng trong âm nhạc piano?", "difficulty": "medium", "explanation": "Kỹ thuật arpeggio tạo ra âm thanh mượt mà khi chơi các nốt trong một hợp âm một cách liên tiếp.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Quy tắc về nhịp điệu", "B": "Quy tắc về cấu trúc sonata", "C": "Quy tắc về cách thể hiện cảm xúc", "D": "Tất cả các quy tắc trên"}, "category": "connection", "question": "Beethoven đã phá vỡ những quy tắc nào trong âm nhạc cổ điển?", "difficulty": "hard", "explanation": "Beethoven đã thử nghiệm và phá vỡ nhiều quy tắc truyền thống trong âm nhạc cổ điển, từ nhịp điệu đến cấu trúc và cách thể hiện cảm xúc.", "correctAnswer": "D"}, {"id": "q9", "options": {"A": "Âm nhạc sôi động và vui tươi", "B": "Âm nhạc đơn giản và dễ hiểu", "C": "Âm nhạc đầy cảm xúc và phong phú", "D": "Âm nhạc không có kỹ thuật phức tạp"}, "category": "application", "question": "Nếu bạn nghe một tác phẩm của Chopin, bạn có thể mong đợi điều gì từ âm nhạc của ông?", "difficulty": "medium", "explanation": "Âm nhạc của Chopin thường rất phong phú về cảm xúc và thể hiện những tâm tư sâu sắc.", "correctAnswer": "C"}, {"id": "q10", "options": {"A": "Bach tập trung vào cấu trúc âm nhạc phức tạp, trong khi Chopin chú trọng vào cảm xúc cá nhân", "B": "Chopin không sử dụng kỹ thuật phức tạp như Bach", "C": "Bach viết nhạc chủ yếu cho dàn nhạc, trong khi Chopin chỉ viết cho piano", "D": "Không có sự khác biệt nào giữa hai nhà soạn nhạc"}, "category": "comparison", "question": "Khi so sánh Bach và Chopin, điều gì là khác nhau về phong cách sáng tác của họ?", "difficulty": "hard", "explanation": "Bach thường chú trọng đến cấu trúc và kỹ thuật phức tạp, trong khi Chopin tập trung vào việc thể hiện cảm xúc cá nhân trong âm nhạc.", "correctAnswer": "A"}, {"id": "q11", "options": {"A": "Johann Sebastian Bach", "B": "Frédéric Chopin", "C": "Ludwig van Beethoven", "D": "Franz Liszt"}, "category": "core_concept", "question": "Nhà soạn nhạc nào đã sử dụng kỹ thuật pedal một cách độc đáo để tạo ra các sắc thái động lực trong âm nhạc của mình?", "difficulty": "medium", "explanation": "Franz Liszt nổi tiếng với việc sử dụng bàn đạp một cách chính xác để tạo ra các chuyển động âm nhạc mượt mà và thể hiện sự phức tạp trong tác phẩm.", "correctAnswer": "D"}, {"id": "q12", "options": {"A": "Piano có thể chơi nhiều âm thanh khác nhau", "B": "Piano là nhạc cụ duy nhất có thể biểu diễn solo", "C": "Piano không đòi hỏi kỹ thuật cao", "D": "Piano không thể chơi âm nhạc phức tạp"}, "category": "core_concept", "question": "Điều gì làm cho piano trở thành nhạc cụ quan trọng nhất trong nhạc cổ điển?", "difficulty": "medium", "explanation": "Piano có khả năng chơi nhiều âm thanh từ rất thấp đến rất cao, cho phép nó truyền tải nhiều cảm xúc khác nhau trong âm nhạc.", "correctAnswer": "A"}]', 12, 80, 'Boss Quiz: Khám Phá Nhạc Cổ Điển và Piano', '2026-01-22 16:43:57.101', 'gpt-4o-mini', '2026-01-22 16:43:57.246382', '2026-01-22 16:43:57.246382');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('eba88f1d-d39a-4fac-88cb-9d8f1c9aa0b8', NULL, '14b4bd9b-8e53-4c36-a0a9-fe2dba7623ac', 'boss', NULL, '[{"id": "q1", "options": {"A": "Hai", "B": "Ba", "C": "Bốn", "D": "Năm"}, "category": "core_concept", "question": "Cấu trúc sonata thường bao gồm mấy phần chính?", "difficulty": "medium", "explanation": "Cấu trúc sonata bao gồm ba phần chính: Exposition, Development và Recapitulation.", "correctAnswer": "B"}, {"id": "q2", "options": {"A": "Adagio", "B": "Allegro", "C": "Rondo", "D": "Exposition"}, "category": "core_concept", "question": "Phần nào trong cấu trúc concerto thường thể hiện sự tương tác mạnh mẽ giữa nhạc cụ solo và dàn nhạc?", "difficulty": "medium", "explanation": "Phần Allegro trong concerto thường mang tính năng động và thể hiện sự tương tác rõ rệt giữa nhạc cụ solo và dàn nhạc.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Nocturne", "B": "Moonlight Sonata", "C": "Concerto No. 1", "D": "Sonata K. 331"}, "category": "core_concept", "question": "Sonata No. 14 của Beethoven còn được biết đến với tên gọi nào?", "difficulty": "medium", "explanation": "Sonata No. 14 của Beethoven được biết đến với tên gọi ''Moonlight Sonata''.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Thay đổi nhịp điệu", "B": "Chuyển đổi giữa các tonalities", "C": "Tăng cường âm lượng", "D": "Giảm bớt độ phức tạp"}, "category": "core_concept", "question": "Kỹ thuật modulation trong cấu trúc sonata có tác dụng gì?", "difficulty": "hard", "explanation": "Modulation cho phép nhạc sĩ chuyển đổi giữa các tonalities, tạo ra sự phong phú và hấp dẫn trong âm nhạc.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Exposition", "B": "Development", "C": "Recapitulation", "D": "Coda"}, "category": "core_concept", "question": "Trong cấu trúc sonata, phần nào thường được nhìn nhận là phần phát triển các chủ đề?", "difficulty": "medium", "explanation": "Phần Development là nơi nhạc sĩ phát triển và biến đổi các chủ đề đã được giới thiệu ở phần Exposition.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Sonata có ba phần, concerto có bốn phần", "B": "Sonata thường dành cho một nhạc cụ solo, concerto có dàn nhạc đi kèm", "C": "Sonata không có phần phát triển, concerto có", "D": "Sonata thường nhanh hơn concerto"}, "category": "comparison", "question": "So sánh cấu trúc sonata và concerto, điều gì là khác biệt chính giữa chúng?", "difficulty": "medium", "explanation": "Cấu trúc sonata thường dành cho một nhạc cụ solo, trong khi concerto là sự kết hợp giữa nhạc cụ solo và dàn nhạc.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Beethoven sử dụng giai điệu vui tươi, Mozart sử dụng giai điệu trầm buồn", "B": "Beethoven thường thể hiện cảm xúc sâu sắc, Mozart lại tập trung vào sự hài hòa", "C": "Beethoven không sử dụng cấu trúc sonata, Mozart sử dụng hoàn toàn", "D": "Beethoven viết nhạc cho dàn nhạc, Mozart viết cho solo"}, "category": "comparison", "question": "Điểm khác biệt nào giữa phong cách âm nhạc của Beethoven và Mozart trong sonata?", "difficulty": "hard", "explanation": "Beethoven thường thể hiện cảm xúc nội tâm sâu sắc, trong khi Mozart hướng đến sự hài hòa và vui tươi trong âm nhạc.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Chơi toàn bộ tác phẩm", "B": "Xác định các chủ đề chính trong Exposition", "C": "Chuyển sang Development", "D": "Tìm hiểu về tác giả"}, "category": "application", "question": "Nếu bạn muốn phân tích một tác phẩm sonata, điều gì là bước đầu tiên bạn cần làm?", "difficulty": "medium", "explanation": "Bước đầu tiên để phân tích một tác phẩm sonata là xác định các chủ đề chính trong phần Exposition.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Sôi động và vui vẻ", "B": "Trầm lắng và sâu lắng", "C": "Nhẹ nhàng và vui tươi", "D": "Căng thẳng và kịch tính"}, "category": "application", "question": "Khi bạn nghe phần Adagio trong concerto, cảm xúc nào thường được thể hiện?", "difficulty": "medium", "explanation": "Phần Adagio thường mang tính chất sâu lắng, thể hiện sự cảm xúc và lãng mạn.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Giai điệu mạnh mẽ", "B": "Giai điệu nhẹ nhàng và thanh thoát", "C": "Giai điệu nhanh và vui tươi", "D": "Giai điệu buồn bã"}, "category": "core_concept", "question": "Trong cấu trúc nocturne, phần A thường thể hiện điều gì?", "difficulty": "medium", "explanation": "Phần A trong nocturne thường là một giai điệu đẹp và dễ nhớ, thể hiện cảm xúc nhẹ nhàng và thanh thoát.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Phần nhẹ nhàng và sâu lắng trong sonata", "B": "Phần vui tươi và nhịp điệu 3/4", "C": "Phần chậm rãi và trầm buồn", "D": "Phần mạnh mẽ và kêu gọi"}, "category": "core_concept", "question": "Chọn một ví dụ phản ánh cấu trúc scherzo?", "difficulty": "medium", "explanation": "Scherzo thường có cấu trúc A-B-A, với phần A thể hiện giai điệu vui tươi và nhịp điệu 3/4.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Chỉ nghe tác phẩm mà không phân tích", "B": "Sáng tác một đoạn nhạc theo cấu trúc sonata", "C": "Chơi nhạc mà không cần hiểu cấu trúc", "D": "Chỉ tập trung vào kỹ thuật chơi nhạc"}, "category": "application", "question": "Chọn một cách ứng dụng thực tế liên quan đến việc nắm rõ cấu trúc sonata.", "difficulty": "hard", "explanation": "Việc sáng tác một đoạn nhạc theo cấu trúc sonata giúp người học áp dụng kiến thức và hiểu rõ hơn về cấu trúc này.", "correctAnswer": "B"}]', 12, 80, 'Boss Quiz: Khám Phá Cấu Trúc Tác Phẩm Piano', '2026-01-22 16:44:41.302', 'gpt-4o-mini', '2026-01-22 16:44:41.344858', '2026-01-22 16:44:41.344858');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('a30cb381-e172-4758-8700-8ce79be06b8c', NULL, '051808b5-36fa-4d63-b08b-7c036c3903ca', 'boss', NULL, '[{"id": "q1", "options": {"A": "Sự sắp xếp của âm thanh và im lặng trong thời gian.", "B": "Sự thay đổi của âm sắc trong một bản nhạc.", "C": "Cách mà một bản nhạc được sáng tác.", "D": "Sự phối hợp giữa các nhạc cụ."}, "category": "core_concept", "question": "Nhịp điệu trong âm nhạc được định nghĩa là gì?", "difficulty": "medium", "explanation": "Nhịp điệu là sự sắp xếp của âm thanh và im lặng trong thời gian, tạo nên cấu trúc cho âm nhạc.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Có bốn nhịp trong mỗi khuôn nhạc.", "B": "Có ba nhịp trong mỗi khuôn nhạc.", "C": "Có hai nhịp trong mỗi khuôn nhạc.", "D": "Có sáu nhịp trong mỗi khuôn nhạc."}, "category": "core_concept", "question": "Cấu trúc nhịp 4/4 cho biết điều gì?", "difficulty": "medium", "explanation": "Nhịp 4/4 có bốn nhịp trong mỗi khuôn, là cấu trúc phổ biến trong nhiều thể loại âm nhạc.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Một kỹ thuật nhịp điệu trong đó nhịp được chơi ở những vị trí không ngờ.", "B": "Một loại nhạc cụ trong nhạc jazz.", "C": "Một phương pháp để giữ nhịp trong nhạc cổ điển.", "D": "Một hình thức nhạc dân gian."}, "category": "core_concept", "question": "Syncopation là gì?", "difficulty": "medium", "explanation": "Syncopation là kỹ thuật nhịp điệu tạo ra sự bất ngờ trong âm nhạc, thường thấy trong jazz và funk.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Cảm xúc mà bản nhạc truyền tải.", "B": "Số lượng nhạc cụ sử dụng trong bản nhạc.", "C": "Cách mà bản nhạc được sáng tác.", "D": "Độ dài của bản nhạc."}, "category": "core_concept", "question": "Tempo của một bản nhạc ảnh hưởng đến điều gì?", "difficulty": "medium", "explanation": "Tempo ảnh hưởng đến cảm xúc của bản nhạc, ví dụ như tempo nhanh có thể tạo ra cảm giác vui vẻ.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Nó tạo ra một cảm giác không bình thường và độc đáo.", "B": "Nó là một nhịp 4/4 thông thường.", "C": "Nó không sử dụng bất kỳ nhịp nào.", "D": "Nó tương tự như nhịp 2/4."}, "category": "connection", "question": "Điều gì đặc biệt về nhịp 5/4 trong tác phẩm ''Take Five'' của Dave Brubeck?", "difficulty": "hard", "explanation": "Nhịp 5/4 tạo ra một cảm giác độc đáo và bất ngờ, khác biệt với nhịp phổ biến hơn như 4/4.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Chơi theo nhịp và thử sử dụng syncopation.", "B": "Chỉ nghe nhạc mà không chơi.", "C": "Chơi nhạc cổ điển trước.", "D": "Giữ nhịp bằng cách không di chuyển."}, "category": "application", "question": "Cách nào tốt nhất để cảm nhận nhịp điệu trong nhạc jazz?", "difficulty": "medium", "explanation": "Thực hành chơi theo nhịp và sử dụng syncopation sẽ giúp bạn cảm nhận nhịp điệu jazz tốt hơn.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Vỗ tay hoặc nhảy theo nhịp.", "B": "Sử dụng nhạc cụ điện tử.", "C": "Chơi một cách ngẫu hứng.", "D": "Chỉ nghe mà không biểu diễn."}, "category": "application", "question": "Khi chơi một bản nhạc dân gian, điều nào sau đây là quan trọng nhất để giữ nhịp điệu?", "difficulty": "medium", "explanation": "Vỗ tay hoặc nhảy theo nhịp sẽ giúp bạn cảm nhận nhịp điệu rõ ràng hơn trong nhạc dân gian.", "correctAnswer": "A"}, {"id": "q8", "options": {"A": "Nhịp đơn giản dễ cảm nhận hơn và thường có cấu trúc cố định.", "B": "Nhịp phức tạp bao giờ cũng nhanh hơn nhịp đơn giản.", "C": "Nhịp đơn giản không thể sử dụng trong nhạc jazz.", "D": "Không có sự khác biệt nào."}, "category": "comparison", "question": "Sự khác biệt chính giữa nhịp đơn giản và nhịp phức tạp là gì?", "difficulty": "medium", "explanation": "Nhịp đơn giản như 4/4 dễ cảm nhận hơn và thường có cấu trúc cố định, trong khi nhịp phức tạp có thể tạo thách thức cho người nghe.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Tempo xác định tốc độ, trong khi meter xác định cấu trúc nhịp.", "B": "Tempo và meter hoàn toàn giống nhau.", "C": "Meter không ảnh hưởng đến cảm xúc của bản nhạc.", "D": "Tempo không cần thiết trong nhạc cổ điển."}, "category": "comparison", "question": "Khi so sánh giữa tempo và meter, điều nào sau đây là đúng?", "difficulty": "medium", "explanation": "Tempo là tốc độ của nhịp điệu, còn meter là cấu trúc nhịp của bản nhạc.", "correctAnswer": "A"}, {"id": "q10", "options": {"A": "Kết hợp nhảy múa hoặc vỗ tay theo nhịp.", "B": "Chỉ ngồi yên khi nghe nhạc.", "C": "Không cần phải di chuyển.", "D": "Chỉ cần nghe mà không cần thực hành."}, "category": "application", "question": "Khi cảm nhận nhịp điệu qua vận động, điều nào sau đây là quan trọng?", "difficulty": "medium", "explanation": "Kết hợp nhảy múa hoặc vỗ tay theo nhịp sẽ giúp bạn cảm nhận nhịp điệu mạnh mẽ hơn.", "correctAnswer": "A"}, {"id": "q11", "options": {"A": "Nó giúp bạn cảm nhận sự chuyển động và cảm xúc của bản nhạc.", "B": "Bạn không cần chú ý đến nhịp điệu.", "C": "Chỉ cần chơi một cách ngẫu hứng.", "D": "Không cần cảm nhận nhịp điệu."}, "category": "application", "question": "Cảm nhận nhịp điệu trong một bản nhạc cổ điển như ''Für Elise'' có gì đặc biệt?", "difficulty": "hard", "explanation": "Cảm nhận nhịp điệu giúp bạn hiểu rõ hơn về cảm xúc và chuyển động của bản nhạc cổ điển.", "correctAnswer": "A"}]', 11, 80, 'Boss Quiz: Khám Phá Nhịp Điệu Trong Âm Nhạc', '2026-01-22 16:45:22.776', 'gpt-4o-mini', '2026-01-22 16:45:22.928402', '2026-01-22 16:45:22.928402');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('baff9e1c-6ce6-4d2a-8bb3-e2c65e9fac97', NULL, '9f04f3a2-d5a2-4f72-b8fe-309ea8790d33', 'boss', NULL, '[{"id": "q1", "options": {"A": "Bảng cân đối tài sản và báo cáo thu nhập", "B": "Chỉ có báo cáo thu nhập", "C": "Chỉ có bảng cân đối tài sản", "D": "Chỉ có chi tiêu hàng tháng"}, "category": "core_concept", "question": "Báo cáo tài chính cá nhân bao gồm những phần nào?", "difficulty": "medium", "explanation": "Báo cáo tài chính cá nhân bao gồm bảng cân đối tài sản và báo cáo thu nhập, giúp cá nhân theo dõi tình hình tài chính tổng thể.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Tài sản - Nợ", "B": "Tài sản + Nợ", "C": "Tài sản / Nợ", "D": "Nợ - Tài sản"}, "category": "core_concept", "question": "Tài sản ròng được tính như thế nào?", "difficulty": "medium", "explanation": "Tài sản ròng là phần chênh lệch giữa tổng tài sản và tổng nợ, giúp đánh giá tình hình tài chính cá nhân.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Chi tiêu hàng tháng", "B": "Thu nhập từ đầu tư", "C": "Thu nhập hàng tháng", "D": "Tài sản cố định"}, "category": "core_concept", "question": "Khi lập báo cáo thu nhập, yếu tố nào không được đưa vào?", "difficulty": "medium", "explanation": "Báo cáo thu nhập chỉ tập trung vào thu nhập và chi tiêu, không bao gồm tài sản cố định.", "correctAnswer": "D"}, {"id": "q4", "options": {"A": "Quản lý tiền bạc và đưa ra quyết định tài chính hợp lý", "B": "Tăng thu nhập mà không cần chi tiêu", "C": "Không cần theo dõi chi tiêu hàng tháng", "D": "Bỏ qua các khoản nợ"}, "category": "core_concept", "question": "Báo cáo tài chính cá nhân giúp cá nhân làm gì?", "difficulty": "medium", "explanation": "Báo cáo tài chính cá nhân giúp cá nhân quản lý tiền bạc và đưa ra quyết định tài chính hợp lý hơn.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Bảng cân đối tài sản không liên quan đến báo cáo thu nhập", "B": "Tài sản ròng từ bảng cân đối tài sản ảnh hưởng đến khả năng tiết kiệm trong báo cáo thu nhập", "C": "Báo cáo thu nhập quyết định tổng tài sản", "D": "Không có mối liên hệ nào giữa hai phần này"}, "category": "connection", "question": "Mối liên hệ giữa bảng cân đối tài sản và báo cáo thu nhập là gì?", "difficulty": "medium", "explanation": "Tài sản ròng từ bảng cân đối tài sản ảnh hưởng đến khả năng tiết kiệm trong báo cáo thu nhập, cho thấy mối liên hệ giữa hai phần.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Dòng tiền dương", "B": "Dòng tiền âm", "C": "Tiết kiệm gia tăng", "D": "Không ảnh hưởng đến tài chính"}, "category": "connection", "question": "Điều gì xảy ra khi tổng chi tiêu vượt quá tổng thu nhập?", "difficulty": "medium", "explanation": "Khi tổng chi tiêu vượt quá tổng thu nhập, cá nhân sẽ rơi vào trạng thái dòng tiền âm, có thể dẫn đến nợ nần.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "5 triệu VNĐ", "B": "30 triệu VNĐ", "C": "25 triệu VNĐ", "D": "Không tiết kiệm được"}, "category": "application", "question": "Nếu một cá nhân có thu nhập hàng tháng là 30 triệu VNĐ và chi tiêu 25 triệu VNĐ, họ sẽ tiết kiệm được bao nhiêu?", "difficulty": "medium", "explanation": "Tiết kiệm được tính bằng cách lấy thu nhập trừ đi chi tiêu, trong trường hợp này là 30 triệu - 25 triệu = 5 triệu VNĐ.", "correctAnswer": "A"}, {"id": "q8", "options": {"A": "Tăng ngân sách cho giải trí", "B": "Cắt giảm các khoản chi tiêu không cần thiết", "C": "Không thay đổi gì", "D": "Chỉ tập trung vào tiết kiệm"}, "category": "application", "question": "Một cá nhân nhận thấy họ chi tiêu quá nhiều cho giải trí. Họ nên làm gì để cải thiện tài chính?", "difficulty": "medium", "explanation": "Cắt giảm các khoản chi tiêu không cần thiết là một cách hiệu quả để cải thiện tình hình tài chính cá nhân.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Chi tiêu cố định thay đổi hàng tháng, chi tiêu biến đổi không thay đổi", "B": "Chi tiêu cố định là những khoản chi không thay đổi, trong khi chi tiêu biến đổi là những khoản chi có thể thay đổi theo thời gian", "C": "Không có sự khác biệt nào", "D": "Chi tiêu cố định không quan trọng, chỉ cần chi tiêu biến đổi"}, "category": "comparison", "question": "Sự khác biệt giữa chi tiêu cố định và chi tiêu biến đổi là gì?", "difficulty": "medium", "explanation": "Chi tiêu cố định là những khoản chi không thay đổi hàng tháng như tiền thuê nhà, trong khi chi tiêu biến đổi như ăn uống có thể thay đổi.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Xác định mục tiêu tài chính", "B": "Theo dõi chi tiêu hàng tháng", "C": "Cắt giảm tất cả các khoản chi tiêu", "D": "Phân nhóm các khoản chi"}, "category": "comparison", "question": "Khi lập ngân sách cá nhân, điều nào là không cần thiết?", "difficulty": "medium", "explanation": "Mặc dù cắt giảm chi tiêu là quan trọng, không cần thiết phải cắt giảm tất cả các khoản chi tiêu; cần phải cân nhắc để duy trì một lối sống hợp lý.", "correctAnswer": "C"}, {"id": "q11", "options": {"A": "150 triệu VNĐ", "B": "250 triệu VNĐ", "C": "200 triệu VNĐ", "D": "100 triệu VNĐ"}, "category": "application", "question": "Nếu một cá nhân có 200 triệu VNĐ tài sản và 50 triệu VNĐ nợ, tài sản ròng của họ là bao nhiêu?", "difficulty": "medium", "explanation": "Tài sản ròng được tính bằng cách lấy tài sản trừ đi nợ: 200 triệu - 50 triệu = 150 triệu VNĐ.", "correctAnswer": "A"}, {"id": "q12", "options": {"A": "Chỉ khi có thay đổi lớn trong thu nhập", "B": "Mỗi tháng hoặc mỗi quý", "C": "Chỉ khi có khoản nợ mới", "D": "Không cần cập nhật thường xuyên"}, "category": "core_concept", "question": "Khi nào nên cập nhật báo cáo tài chính cá nhân?", "difficulty": "medium", "explanation": "Nên cập nhật báo cáo tài chính hàng tháng hoặc hàng quý để theo dõi tình hình tài chính và điều chỉnh khi cần thiết.", "correctAnswer": "B"}]', 12, 80, 'Boss Quiz: Báo cáo tài chính cá nhân: Hướng dẫn toàn diện', '2026-01-22 16:45:59.154', 'gpt-4o-mini', '2026-01-22 16:45:59.195693', '2026-01-22 16:45:59.195693');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('38994e77-4626-4c9f-8bdd-ed640de8ee6c', NULL, '05e95475-8353-4a5e-b9cb-e481c1e73870', 'boss', NULL, '[{"id": "q1", "options": {"A": "Đoạn nhạc, câu nhạc, hình thức tổng thể", "B": "Giai điệu, hòa âm, nhịp điệu", "C": "Âm sắc, âm lượng, cao độ", "D": "Kỹ thuật chơi, biểu cảm, phong cách"}, "category": "core_concept", "question": "Cấu trúc nhạc thường được chia thành những phần nào?", "difficulty": "medium", "explanation": "Cấu trúc nhạc thường được chia thành các phần chính như đoạn nhạc, câu nhạc và hình thức tổng thể để hiểu rõ hơn về tổ chức âm nhạc.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Một đoạn nhạc có thể được lặp lại", "B": "Phần đầu của bản nhạc", "C": "Phần nhạc có những cảm xúc mạnh mẽ", "D": "Một đoạn nhạc không được lặp lại"}, "category": "core_concept", "question": "Điều nào là câu nhạc chính trong một tác phẩm?", "difficulty": "medium", "explanation": "Câu nhạc chính thường là một giai điệu dễ nhớ và có thể được lặp lại nhiều lần trong tác phẩm.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Hình thức tổng thể không ảnh hưởng đến cảm xúc", "B": "Hình thức tổng thể giúp tổ chức ý tưởng âm nhạc", "C": "Có một hình thức duy nhất cho mỗi bản nhạc", "D": "Hình thức tổng thể chỉ là lặp lại đoạn nhạc"}, "category": "core_concept", "question": "Câu nào sau đây đúng về hình thức tổng thể trong nhạc?", "difficulty": "medium", "explanation": "Hình thức tổng thể giúp tổ chức ý tưởng âm nhạc và tạo ra sự rõ ràng cho người nghe.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Phần mở đầu dễ nhớ", "B": "Phần kết thúc", "C": "Phần giữa có giai điệu phức tạp", "D": "Phần có cảm xúc thấp"}, "category": "core_concept", "question": "Trong bản nhạc ''Für Elise'', phần nào thường được nhận diện như một đoạn nhạc chính?", "difficulty": "medium", "explanation": "''Für Elise'' bắt đầu với một phần mở đầu dễ nhớ, tạo nên ấn tượng đầu tiên cho người nghe.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Hợp âm tạo ra cảm giác cần phải giải quyết", "B": "Hợp âm là nơi bắt đầu và kết thúc", "C": "Hợp âm tạo ra sự chuyển tiếp", "D": "Hợp âm chứa nhiều nốt nhạc"}, "category": "core_concept", "question": "Trong phân tích hòa âm, chức năng nào được gọi là Tonic?", "difficulty": "hard", "explanation": "Tonic là hợp âm cảm giác như nằm ở trung tâm, thường là nơi bắt đầu và kết thúc trong một phần âm nhạc.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Một sự ngắt quãng", "B": "Sự chuyển động và phát triển ý tưởng", "C": "Nhịp điệu không đều", "D": "Chỉ là lặp lại đoạn nhạc"}, "category": "connection", "question": "Sự chuyển giao giữa các đoạn nhạc thường thể hiện điều gì trong âm nhạc?", "difficulty": "medium", "explanation": "Sự chuyển giao giữa các đoạn nhạc thể hiện sự chuyển động và phát triển của ý tưởng âm nhạc.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Sự yên bình", "B": "Cảm xúc mạnh mẽ và căng thẳng", "C": "Sự tĩnh lặng", "D": "Một cảm xúc vui vẻ"}, "category": "connection", "question": "Trong bản nhạc ''Clair de Lune'', cảm xúc nào thường được truyền tải qua phần giữa?", "difficulty": "medium", "explanation": "Phần giữa của ''Clair de Lune'' mang lại cảm xúc mạnh mẽ và căng thẳng, thể hiện sự phát triển của âm nhạc.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Chơi theo cảm hứng mà không phân tích", "B": "Phân tích cấu trúc và luyện tập", "C": "Chỉ chú ý đến nhịp điệu", "D": "Chơi nhanh để tạo cảm xúc"}, "category": "application", "question": "Cách nào tốt nhất để hiểu rõ về một bản nhạc khi chơi piano?", "difficulty": "medium", "explanation": "Phân tích cấu trúc và luyện tập sẽ giúp bạn hiểu rõ hơn về bản nhạc và cải thiện khả năng biểu diễn.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Chỉ cần lặp lại câu nhạc", "B": "Tìm hiểu ý nghĩa và cảm xúc", "C": "Chỉ chú ý đến nhịp điệu", "D": "Phân tích hòa âm"}, "category": "application", "question": "Khi phân tích câu nhạc, điều gì bạn nên chú ý nhất?", "difficulty": "medium", "explanation": "Khi phân tích câu nhạc, việc tìm hiểu ý nghĩa và cảm xúc sẽ giúp bạn diễn đạt tốt hơn khi chơi.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Sự khác biệt về nhịp điệu", "B": "Sự lặp lại và giai điệu dễ nhớ", "C": "Thời gian của bản nhạc", "D": "Số lượng nốt nhạc trong đoạn"}, "category": "comparison", "question": "Để phân biệt giữa đoạn nhạc chính và phụ trong một bản nhạc, bạn cần chú ý đến điều gì?", "difficulty": "hard", "explanation": "Đoạn nhạc chính thường có giai điệu dễ nhớ và có thể được lặp lại, trong khi đoạn phụ tạo ra sự tương phản.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Đoạn nhạc là một phần nhỏ hơn câu nhạc", "B": "Câu nhạc không có giai điệu", "C": "Đoạn nhạc không thể lặp lại", "D": "Câu nhạc không thể chứa đoạn nhạc"}, "category": "comparison", "question": "Điểm khác biệt lớn nhất giữa đoạn nhạc và câu nhạc là gì?", "difficulty": "medium", "explanation": "Đoạn nhạc là một phần nhỏ của bản nhạc, thường kết thúc bằng một điểm dừng, trong khi câu nhạc là một tập hợp các đoạn nhạc kết hợp lại.", "correctAnswer": "A"}, {"id": "q12", "options": {"A": "Chơi với tốc độ nhanh hơn", "B": "Sử dụng kỹ thuật biểu cảm", "C": "Giữ nguyên nhịp điệu", "D": "Bỏ qua các đoạn nhạc phụ"}, "category": "application", "question": "Một trong những cách để thể hiện sự chuyển giao giữa các đoạn nhạc là gì?", "difficulty": "hard", "explanation": "Sử dụng kỹ thuật biểu cảm sẽ giúp thể hiện rõ ràng sự chuyển giao giữa các đoạn nhạc và tạo cảm xúc cho người nghe.", "correctAnswer": "B"}]', 12, 80, 'Boss Quiz: Phân Tích Cấu Trúc Bản Nhạc', '2026-01-22 16:46:44.436', 'gpt-4o-mini', '2026-01-22 16:46:44.589343', '2026-01-22 16:46:44.589343');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('a0b659b7-bd8e-487b-ac6a-825b86f5c2a6', NULL, 'b889e673-7eb0-4577-9f57-dd4a569aa654', 'boss', NULL, '[{"id": "q1", "options": {"A": "Tối ưu hóa ngân sách", "B": "Tăng thu nhập", "C": "Giảm chi phí cố định", "D": "Mua sắm nhiều hơn"}, "category": "core_concept", "question": "Phân tích chi tiêu hàng tháng chủ yếu nhằm mục đích gì?", "difficulty": "medium", "explanation": "Mục đích chính của phân tích chi tiêu là để tối ưu hóa ngân sách và tìm ra những khu vực có thể tiết kiệm.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Lập kế hoạch ngân sách mới", "B": "Ghi chép tất cả các khoản chi tiêu", "C": "Giảm chi phí giải trí", "D": "Tăng cường tiết kiệm"}, "category": "core_concept", "question": "Một trong những bước đầu tiên trong phân tích chi tiêu hàng tháng là gì?", "difficulty": "medium", "explanation": "Ghi chép tất cả các khoản chi tiêu là bước đầu tiên để có cái nhìn tổng quan về tài chính.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Số tiền bạn đã tiết kiệm", "B": "Xu hướng chi tiêu của bản thân", "C": "Cách tăng thu nhập", "D": "Tổng số tiền bạn đã chi cho giải trí"}, "category": "core_concept", "question": "Phân tích dữ liệu chi tiêu giúp bạn nhận diện điều gì?", "difficulty": "medium", "explanation": "Phân tích dữ liệu giúp nhận diện xu hướng chi tiêu để từ đó có thể điều chỉnh ngân sách.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Tiền thuê nhà", "B": "Hóa đơn điện nước", "C": "Giải trí", "D": "Thực phẩm"}, "category": "core_concept", "question": "Trong số các khoản chi tiêu, khoản nào thường được coi là chi phí không thiết yếu?", "difficulty": "medium", "explanation": "Chi phí giải trí không phải là khoản chi thiết yếu và có thể giảm bớt khi cần tiết kiệm.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "Để tăng số tiền tiết kiệm", "B": "Để biết được khoản nào có thể cắt giảm", "C": "Để giảm chi phí cố định", "D": "Để tìm kiếm các khoản đầu tư"}, "category": "connection", "question": "Khi phân tích chi tiêu, tại sao việc phân loại các khoản chi lại quan trọng?", "difficulty": "medium", "explanation": "Phân loại các khoản chi giúp nhận diện được những khoản có thể cắt giảm nhằm tối ưu hóa ngân sách.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Tăng thu nhập", "B": "Cắt giảm chi phí không thiết yếu", "C": "Giảm chi phí cố định", "D": "Không cần làm gì"}, "category": "application", "question": "Gia đình A có thu nhập 20 triệu VND, với tổng chi tiêu hàng tháng là 20 triệu VND. Họ nên làm gì để tiết kiệm hơn?", "difficulty": "hard", "explanation": "Cắt giảm chi phí không thiết yếu là cách tốt nhất để tiết kiệm hơn trong ngân sách hiện tại.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Tìm cách kiếm thêm tiền", "B": "Nấu ăn tại nhà nhiều hơn", "C": "Giảm chi tiêu cho thực phẩm", "D": "Tăng chi tiêu cho thực phẩm"}, "category": "application", "question": "Nếu một người phát hiện mình chi quá nhiều cho ăn uống ngoài hàng, họ nên làm gì để cải thiện tình hình tài chính?", "difficulty": "medium", "explanation": "Nấu ăn tại nhà thường giúp tiết kiệm hơn so với việc ăn ngoài hàng.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Giúp dễ dàng mua sắm hơn", "B": "Tăng số tiền tiết kiệm hàng tháng", "C": "Nhận diện được xu hướng chi tiêu", "D": "Giảm chi phí sinh hoạt"}, "category": "core_concept", "question": "Một trong những lợi ích của việc ghi chép chi tiêu hàng tháng là gì?", "difficulty": "medium", "explanation": "Ghi chép chi tiêu giúp nhận diện xu hướng chi tiêu, từ đó có thể điều chỉnh ngân sách.", "correctAnswer": "C"}, {"id": "q9", "options": {"A": "Để tăng lợi nhuận", "B": "Để hiểu rõ hơn về thói quen tiêu dùng", "C": "Để tìm kiếm các khoản vay", "D": "Để giảm thiểu chi phí cố định"}, "category": "core_concept", "question": "Tại sao việc phân tích chi tiêu hàng tháng lại quan trọng đối với cá nhân hoặc gia đình?", "difficulty": "medium", "explanation": "Phân tích chi tiêu giúp cá nhân hoặc gia đình hiểu rõ hơn về thói quen tiêu dùng và từ đó có kế hoạch tài chính hợp lý.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Giải trí và du lịch", "B": "Tiền thuê nhà và thực phẩm", "C": "Mua sắm cá nhân", "D": "Chi phí học hành cho con cái"}, "category": "comparison", "question": "Chi tiêu thiết yếu bao gồm những khoản nào?", "difficulty": "medium", "explanation": "Chi tiêu thiết yếu bao gồm các khoản cần thiết cho cuộc sống hàng ngày như tiền thuê nhà và thực phẩm.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Chỉ khi có vấn đề tài chính", "B": "Mỗi tháng một lần", "C": "Định kỳ mỗi năm", "D": "Khi cảm thấy cần thiết"}, "category": "core_concept", "question": "Khi nào là thời điểm tốt nhất để thực hiện phân tích chi tiêu?", "difficulty": "medium", "explanation": "Thực hiện phân tích chi tiêu hàng tháng giúp bạn luôn cập nhật tình hình tài chính và có những điều chỉnh kịp thời.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Thiếu thông tin", "B": "Thói quen tiêu dùng không tốt", "C": "Không có kế hoạch chi tiêu", "D": "Không biết cách ghi chép"}, "category": "comparison", "question": "Một trong những lý do tại sao người tiêu dùng không tuân thủ kế hoạch chi tiêu của mình là gì?", "difficulty": "hard", "explanation": "Thói quen tiêu dùng không tốt có thể khiến người tiêu dùng không tuân thủ kế hoạch chi tiêu của mình.", "correctAnswer": "B"}, {"id": "q13", "options": {"A": "Chi tiêu thiết yếu là bắt buộc, còn không thiết yếu là tùy chọn", "B": "Chi tiêu thiết yếu là cho sản phẩm cao cấp, không thiết yếu là cho sản phẩm bình dân", "C": "Chi tiêu thiết yếu chỉ liên quan đến thực phẩm, không thiết yếu thì không", "D": "Không có sự khác biệt"}, "category": "comparison", "question": "Sự khác biệt giữa chi tiêu thiết yếu và không thiết yếu là gì?", "difficulty": "medium", "explanation": "Chi tiêu thiết yếu là các khoản chi cần thiết cho sự sống, trong khi chi tiêu không thiết yếu là những khoản chi không bắt buộc.", "correctAnswer": "A"}, {"id": "q14", "options": {"A": "Để dễ dàng hơn trong mua sắm", "B": "Để tiết kiệm thời gian và tăng độ chính xác", "C": "Để tăng chi tiêu", "D": "Để tìm kiếm các khoản vay"}, "category": "connection", "question": "Tại sao nên sử dụng công cụ hỗ trợ trong việc ghi chép và phân tích chi tiêu?", "difficulty": "medium", "explanation": "Sử dụng công cụ hỗ trợ giúp bạn ghi chép và phân tích chi tiêu dễ dàng hơn, tiết kiệm thời gian và tăng độ chính xác.", "correctAnswer": "B"}, {"id": "q15", "options": {"A": "Tăng thu nhập", "B": "Cắt giảm chi tiêu không cần thiết", "C": "Tăng chi tiêu cho các khoản đầu tư", "D": "Không cần làm gì"}, "category": "application", "question": "Nếu bạn muốn tiết kiệm nhiều hơn hàng tháng, bạn nên bắt đầu từ đâu?", "difficulty": "hard", "explanation": "Cắt giảm các khoản chi tiêu không cần thiết là cách hiệu quả nhất để tiết kiệm nhiều hơn mỗi tháng.", "correctAnswer": "B"}]', 15, 80, 'Boss Quiz: Phân Tích Chi Tiêu Hàng Tháng', '2026-01-22 16:47:40.264', 'gpt-4o-mini', '2026-01-22 16:47:40.307113', '2026-01-22 16:47:40.307113');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('89e6f44f-544c-463c-93df-061fda68bc76', '4f273462-fab8-48ca-9751-6ce0914fb5c3', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Giúp người chơi di chuyển linh hoạt trong khi kiểm soát bóng", "B": "Giúp người chơi ném bóng chính xác hơn", "C": "Giúp người chơi tạo ra âm thanh khi bóng chạm đất", "D": "Giúp người chơi dừng lại nhanh chóng"}, "category": "definition", "question": "Dribble trong bóng rổ chủ yếu có tác dụng gì?", "explanation": "A đúng vì dribble cho phép người chơi di chuyển linh hoạt trong khi vẫn kiểm soát bóng. Các đáp án khác không phản ánh đúng chức năng của dribble trong bóng rổ.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Dùng tay chạm vào bóng và đẩy nó ra phía trước", "B": "Ném bóng về phía đối thủ", "C": "Giữ bóng trong tay và chạy nhanh", "D": "Đứng yên và chuyền bóng cho đồng đội"}, "category": "definition", "question": "Khái niệm nào sau đây mô tả đúng kỹ thuật dribble?", "explanation": "A đúng vì nó mô tả hành động chính trong dribble. Các đáp án khác không liên quan đến kỹ thuật dribble.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Không để bóng rơi vào tay đối thủ", "B": "Tạo khoảng trống để ghi điểm", "C": "Giữ bóng trong tay khi chạy", "D": "Di chuyển nhanh chóng trên sân"}, "category": "distinction", "question": "Điều nào sau đây không phải là mục tiêu của kỹ thuật dribble?", "explanation": "C là đáp án sai vì dribble không phải là việc giữ bóng trong tay mà là kiểm soát bóng khi nó chạm đất. Các đáp án khác đều là mục tiêu của dribble.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Dribble cho phép di chuyển trong khi kiểm soát bóng", "B": "Chuyền bóng là kỹ thuật duy nhất để ghi điểm", "C": "Dribble không yêu cầu người chơi phải di chuyển", "D": "Chuyền bóng không liên quan đến việc ki