INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('b915b050-50fc-49c7-a68a-6784e6c01243', NULL, '8919c949-8d38-4436-92d6-137f600ea20c', 'boss', NULL, '[{"id": "q1", "options": {"A": "Brochure", "B": "Sách", "C": "Giấy in văn phòng", "D": "Giấy nhám"}, "category": "core_concept", "question": "Giấy bóng mịn thường được sử dụng cho loại tài liệu nào?", "difficulty": "medium", "explanation": "Giấy bóng mịn giúp hình ảnh nổi bật và thu hút sự chú ý, rất phù hợp cho brochure quảng cáo.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Brochure gấp", "B": "Thiệp mời", "C": "Hợp đồng", "D": "Báo cáo"}, "category": "core_concept", "question": "Kích thước A5 thường được sử dụng cho loại tài liệu nào?", "difficulty": "medium", "explanation": "Kích thước A5 thường được chọn cho thiệp mời vì tính nhỏ gọn và dễ dàng mang theo.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Có bề mặt bóng loáng", "B": "Thích hợp cho việc viết tay", "C": "Giá rẻ nhất", "D": "Không thể in màu sắc"}, "category": "core_concept", "question": "Giấy nhám có đặc điểm gì nổi bật?", "difficulty": "medium", "explanation": "Giấy nhám có bề mặt không bóng, rất thích hợp cho việc viết tay và tạo cảm giác tự nhiên.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "A3", "B": "A4 gấp thành A5", "C": "A5", "D": "Letter"}, "category": "core_concept", "question": "Một brochure quảng cáo thường nên có kích thước nào để tối ưu hóa không gian và thông tin?", "difficulty": "medium", "explanation": "Kích thước A4 gấp thành A5 giúp đạt được sự cân bằng giữa không gian và thông tin trình bày.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "In ảnh nghệ thuật", "B": "In báo cáo cho tổ chức phi lợi nhuận", "C": "In brochure quảng cáo", "D": "In danh thiếp"}, "category": "application", "question": "Giấy tái chế thường được sử dụng trong trường 