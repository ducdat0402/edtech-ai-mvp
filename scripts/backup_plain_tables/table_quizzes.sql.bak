INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('1a5e7697-d301-42d2-8afa-5de4c7ad0e3c', NULL, '3c97a877-b240-475b-89dc-474d0a2b17b5', 'boss', NULL, '[{"id": "q1", "options": {"A": "Ngăn chặn đối thủ ghi điểm", "B": "Tăng cường khả năng ghi điểm của đội mình", "C": "Cải thiện sức bền cho cầu thủ", "D": "Tăng tốc độ di chuyển của bóng"}, "category": "core_concept", "question": "Kỹ thuật phòng thủ trong bóng rổ chủ yếu nhằm mục đích gì?", "difficulty": "medium", "explanation": "Kỹ thuật phòng thủ trong bóng rổ chủ yếu nhằm ngăn chặn đối thủ ghi điểm, đây là mục tiêu cốt lõi của phòng thủ.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Theo dõi bóng", "B": "Sử dụng tay để chặn đường chuyền", "C": "Thực hiện cú ném ba điểm", "D": "Giao tiếp với đồng đội"}, "category": "core_concept", "question": "Yếu tố nào không phải là một kỹ thuật phòng thủ cơ bản trong bóng rổ?", "difficulty": "medium", "explanation": "Kỹ thuật thực hiện cú ném ba điểm không liên quan đến phòng thủ, mà là một phần của kỹ thuật tấn công.", "correctAnswer": "C"}, {"id": "q3", "options": {"A": "Tạo áp lực lên đối thủ", "B": "Thực hiện các cú ném chính xác", "C": "Tập trung vào việc bắt bóng", "D": "Chỉ theo dõi bóng"}, "category": "core_concept", "question": "Điều gì là cần thiết khi thực hiện kỹ thuật phòng thủ one-on-one?", "difficulty": "medium", "explanation": "Tạo áp lực lên đối thủ là yếu tố quan trọng trong phòng thủ one-on-one để ngăn chặn họ ghi điểm.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Phòng thủ khu vực", "B": "Phòng thủ one-on-one", "C": "Phòng thủ đội nhóm", "D": "Phòng thủ phản công"}, "category": "core_concept", "question": "Kawhi Leonard nổi tiếng với kỹ thuật phòng thủ nào?", "difficulty": "hard", "explanation": "Kawhi Leonard nổi tiếng với kỹ thuật phòng thủ one-on-one, giúp anh hạn chế điểm số của những cầu thủ tấn công hàng đầu.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Mỗi cầu thủ chỉ cần chú ý đến vị trí của mình", "B": "Giao tiếp và phối hợp giữa các cầu thủ", "C": "Cố gắng ghi điểm nhiều hơn đối thủ", "D": "Đứng yên tại vị trí của mình"}, "category": "core_concept", "question": "Trong phòng thủ đội nhóm, điều gì là quan trọng nhất?", "difficulty": "medium", "explanation": "Giao tiếp và phối hợp giữa các cầu thủ là yếu tố then chốt trong phòng thủ đội nhóm để ngăn chặn đối phương ghi điểm.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Đối đầu với một cầu thủ tấn công mạnh", "B": "Khi đối thủ ném cú ba điểm", "C": "Khi đội bóng cần ghi điểm", "D": "Khi có tình huống phản công"}, "category": "connection", "question": "Kỹ thuật phòng thủ zoning defense thường được áp dụng trong tình huống nào?", "difficulty": "medium", "explanation": "Kỹ thuật phòng thủ zoning defense thường được sử dụng để đối phó với các cầu thủ tấn công mạnh, tạo ra sự phối hợp hiệu quả trong phòng thủ.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Khả năng đọc tình huống và phản ứng nhanh chóng", "B": "Khả năng ghi điểm cao", "C": "Sức mạnh thể chất vượt trội", "D": "Kỹ năng dẫn bóng tốt"}, "category": "application", "question": "Trong tình huống phòng thủ one-on-one, điều gì làm cho một cầu thủ phòng thủ có lợi thế?", "difficulty": "medium", "explanation": "Khả năng đọc tình huống và phản ứng nhanh chóng giúp cầu thủ phòng thủ tạo ra lợi thế trong các tình huống one-on-one.", "correctAnswer": "A"}, {"id": "q8", "options": {"A": "Khi đối thủ không có bóng", "B": "Khi đối thủ đang ở vị trí nguy hiểm", "C": "Khi bóng rổ đang ở giữa sân", "D": "Khi đồng đội không thể hỗ trợ"}, "category": "application", "question": "Khi nào thì cầu thủ phòng thủ nên sử dụng kỹ thuật bẫy (trap)?", "difficulty": "hard", "explanation": "Kỹ thuật bẫy nên được sử dụng khi đối thủ đang ở vị trí nguy hiểm, giúp tạo áp lực cao và buộc đối thủ phải mắc lỗi.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Phòng thủ cá nhân chỉ cần một người, trong khi phòng thủ đội nhóm cần nhiều người", "B": "Phòng thủ cá nhân không cần giao tiếp", "C": "Phòng thủ đội nhóm không cần theo dõi đối thủ", "D": "Phòng thủ cá nhân chỉ tập trung vào một cầu thủ"}, "category": "comparison", "question": "Điểm khác biệt chính giữa phòng thủ cá nhân và phòng thủ đội nhóm là gì?", "difficulty": "medium", "explanation": "Phòng thủ cá nhân tập trung vào một cầu thủ trong khi phòng thủ đội nhóm yêu cầu sự phối hợp và giao tiếp giữa nhiều cầu thủ.", "correctAnswer": "A"}, {"id": "q10", "options": {"A": "Tăng cường khả năng ghi điểm", "B": "Bảo vệ khu vực gần rổ và giảm khả năng ném xa của đối thủ", "C": "Chỉ tập trung vào một cầu thủ tấn công", "D": "Giúp cầu thủ di chuyển nhanh hơn"}, "category": "comparison", "question": "Chiến thuật 3-2 trong phòng thủ zoning defense có ưu điểm gì?", "difficulty": "hard", "explanation": "Chiến thuật 3-2 trong phòng thủ zoning defense giúp bảo vệ khu vực gần rổ và giảm khả năng ném xa của đối thủ, tạo ra sự phòng thủ chặt chẽ.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Chỉ theo dõi bóng", "B": "Giữ khoảng cách hợp lý và theo sát đối thủ", "C": "Chỉ chú ý đến cầu thủ đang dẫn bóng", "D": "Tập trung vào việc ghi điểm"}, "category": "application", "question": "Khi thực hiện phòng thủ cá nhân, cầu thủ nên chú ý đến điều gì?", "difficulty": "medium", "explanation": "Cầu thủ cần giữ khoảng cách hợp lý và theo sát đối thủ để không cho họ có cơ hội ghi điểm.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Đứng thụ động", "B": "Giao tiếp với đồng đội", "C": "Theo dõi bóng", "D": "Duy trì tư thế thấp"}, "category": "anti-patterns", "question": "Một trong những điều cần tránh khi phòng thủ là gì?", "difficulty": "medium", "explanation": "Đứng thụ động sẽ tạo cơ hội cho đối thủ dễ dàng ghi điểm, vì vậy cầu thủ cần luôn chủ động trong phòng thủ.", "correctAnswer": "A"}]', 12, 80, 'Boss Quiz: Kỹ Thuật Phòng Thủ Trong Bóng Rổ', '2026-01-22 16:15:12.321', 'gpt-4o-mini', '2026-01-22 16:15:12.363468', '2026-01-22 16:15:12.363468');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('d3fca056-5d9e-43ab-ae6b-22bacdeaa366', NULL, '6456d6c3-ddb5-4719-b480-68c6f12638ab', 'boss', NULL, '[{"id": "q1", "options": {"A": "Duy trì kiểm soát bóng", "B": "Chỉ để ghi điểm", "C": "Tăng cường thể lực", "D": "Làm rối loạn đối thủ"}, "category": "core_concept", "question": "Chuyền bóng trong bóng rổ có vai trò gì chính yếu?", "difficulty": "medium", "explanation": "Chuyền bóng là kỹ thuật cơ bản giúp duy trì kiểm soát bóng và tạo ra cơ hội ghi điểm cho đội.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Chuyền tay, chuyền bóng cao, chuyền bóng dưới", "B": "Chuyền tay, chuyền bóng nảy, chuyền bóng chéo", "C": "Chuyền tay, chuyền chéo, chuyền lật", "D": "Chuyền bóng cao, chuyền tay, chuyền bóng đá"}, "category": "core_concept", "question": "Ba loại chuyền bóng chính trong bóng rổ là gì?", "difficulty": "medium", "explanation": "Ba loại chuyền bóng chính là chuyền tay, chuyền bóng cao và chuyền bóng dưới, mỗi loại có ứng dụng riêng.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Để thực hiện chuyền bóng bằng một tay", "B": "Để lựa chọn loại chuyền bóng phù hợp", "C": "Để ghi điểm nhanh hơn", "D": "Để tránh bị cướp bóng"}, "category": "core_concept", "question": "Tại sao việc quan sát đồng đội là quan trọng trong kỹ thuật chuyền bóng?", "difficulty": "medium", "explanation": "Quan sát đồng đội giúp cầu thủ lựa chọn loại chuyền bóng phù hợp, tối ưu hóa cơ hội ghi điểm.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Khi đồng đội gần bạn", "B": "Khi đồng đội ở xa hoặc bị chắn", "C": "Khi có không gian thoáng", "D": "Khi bạn muốn ném bóng vào rổ"}, "category": "core_concept", "question": "Khi nào nên sử dụng chuyền bóng cao?", "difficulty": "medium", "explanation": "Chuyền bóng cao được sử dụng khi đồng đội ở xa hoặc có nhiều cầu thủ đối phương chắn bóng.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Chuyền bóng bằng tay thuận", "B": "Chuyền bóng bằng tay không thuận", "C": "Chuyền bóng qua đất", "D": "Chuyền bóng cao"}, "category": "core_concept", "question": "Chip Pass là kỹ thuật chuyền bóng nào?", "difficulty": "hard", "explanation": "Chip Pass là kỹ thuật chuyền bóng bằng tay không thuận, thường được dùng khi cầu thủ đứng gần đồng đội.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Khi có nhiều cầu thủ đối phương chắn trên cao", "B": "Khi đồng đội ở gần bạn", "C": "Khi bạn cần chuyền bóng cao", "D": "Khi bạn muốn ghi điểm nhanh"}, "category": "core_concept", "question": "Trong tình huống nào chuyền bóng dưới thường được sử dụng?", "difficulty": "medium", "explanation": "Chuyền bóng dưới được sử dụng khi cầu thủ muốn chuyền cho đồng đội ở vị trí thấp hơn hoặc khi có cầu thủ đối phương chắn trên cao.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Đứng xa đồng đội", "B": "Nắm bóng chắc chắn và đẩy bóng đúng hướng", "C": "Chỉ sử dụng tay thuận", "D": "Đẩy bóng mạnh hơn"}, "category": "connection", "question": "Để thực hiện chuyền bóng tay chính xác, cầu thủ cần lưu ý điều gì?", "difficulty": "medium", "explanation": "Nắm bóng chắc chắn và đẩy bóng đúng hướng là yếu tố quan trọng để thực hiện chuyền bóng tay hiệu quả.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Khi đối thủ không cản trở", "B": "Khi đồng đội đứng gần bạn", "C": "Khi đối thủ đang chắn bóng", "D": "Khi bạn muốn ghi điểm nhanh"}, "category": "connection", "question": "Khi nào thì Bounce Pass trở thành lựa chọn tốt nhất?", "difficulty": "medium", "explanation": "Bounce Pass là lựa chọn tốt khi có đối thủ can thiệp, giúp bóng tránh bị cướp.", "correctAnswer": "C"}, {"id": "q9", "options": {"A": "Chuyền tay", "B": "Chip Pass", "C": "Bounce Pass", "D": "Chuyền bóng cao"}, "category": "application", "question": "Nếu cầu thủ A muốn chuyền bóng cho cầu thủ B đang chạy về phía rổ, kỹ thuật nào nên được sử dụng?", "difficulty": "medium", "explanation": "Bounce Pass sẽ giúp bóng dễ dàng đến tay cầu thủ B đang chạy mà không bị chắn.", "correctAnswer": "C"}, {"id": "q10", "options": {"A": "Chuyền tay", "B": "Chuyền bóng cao", "C": "Chip Pass", "D": "Bounce Pass"}, "category": "application", "question": "Nếu có hai cầu thủ đối phương đang chắn đường bóng, cách chuyền nào là tốt nhất?", "difficulty": "hard", "explanation": "Bounce Pass giúp bóng chạm đất trước khi đến tay đồng đội, tránh bị chắn bởi đối phương.", "correctAnswer": "D"}, {"id": "q11", "options": {"A": "Chuyền bóng mạnh", "B": "Chuyền bóng nhanh chóng và chính xác", "C": "Chuyền bóng chỉ khi cần thiết", "D": "Chuyền bóng theo kiểu ngẫu hứng"}, "category": "connection", "question": "Khi cầu thủ A chuyền bóng cho cầu thủ B, điều gì cần thiết để duy trì kiểm soát trận đấu?", "difficulty": "medium", "explanation": "Chuyền bóng nhanh chóng và chính xác giúp đội duy trì kiểm soát trận đấu và tạo cơ hội ghi điểm.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Chip Pass không cần chạm đất", "B": "Chip Pass nhanh hơn", "C": "Bounce Pass khó hơn", "D": "Chip Pass chỉ dùng tay thuận"}, "category": "comparison", "question": "Điểm khác biệt chính giữa Chip Pass và Bounce Pass là gì?", "difficulty": "hard", "explanation": "Chip Pass không cần chạm đất, trong khi Bounce Pass thì bóng phải chạm đất trước khi đến tay đồng đội.", "correctAnswer": "A"}, {"id": "q13", "options": {"A": "Chuyền tay", "B": "Bounce Pass", "C": "Chuyền bóng cao", "D": "Chip Pass"}, "category": "application", "question": "Trong một tình huống phòng ngự, kỹ thuật nào giúp chuyển từ phòng ngự sang tấn công nhanh nhất?", "difficulty": "hard", "explanation": "Bounce Pass giúp bóng tiếp cận đồng đội mà không bị chắn, cho phép đội nhanh chóng chuyển từ phòng ngự sang tấn công.", "correctAnswer": "B"}]', 13, 80, 'Boss Quiz: Kỹ Thuật Chuyền Bóng Trong Bóng Rổ', '2026-01-22 16:15:49.54', 'gpt-4o-mini', '2026-01-22 16:15:49.581638', '2026-01-22 16:15:49.581638');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('2e620a27-1c9e-4a2d-93b2-b6311f7887da', '11cb163a-b88e-4003-93b7-aabf03a7e24b', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Giữ bóng và di chuyển mà không bị mất kiểm soát", "B": "Ném bóng vào rổ", "C": "Chuyển bóng cho đồng đội", "D": "Đứng yên và quan sát"}, "category": "definition", "question": "Dribble trong bóng rổ chủ yếu được sử dụng để làm gì?", "explanation": "A đúng vì dribble là kỹ thuật giúp người chơi kiểm soát bóng khi di chuyển. Các đáp án khác sai vì không phản ánh đúng chức năng chính của dribble.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Crossover", "B": "Jump shot", "C": "Behind-the-back", "D": "Spin move"}, "category": "distinction", "question": "Yếu tố nào dưới đây KHÔNG phải là một kỹ thuật trong dribble?", "explanation": "B đúng vì jump shot là một kỹ thuật ném bóng, không phải là kỹ thuật dribble. Các đáp án khác đều là kỹ thuật dribble.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Chân đứng rộng, gối hơi khuỵu", "B": "Đứng thẳng, chân khép lại", "C": "Ngồi xổm xuống", "D": "Đứng một chân"}, "category": "definition", "question": "Khi thực hiện dribble, người chơi nên giữ tư thế như thế nào?", "explanation": "A đúng vì tư thế chân đứng rộng và gối khuỵu giúp người chơi giữ thăng bằng và dễ dàng di chuyển. Các đáp án khác không phù hợp với tư thế cần thiết khi dribble.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Dribble là kỹ thuật di chuyển với bóng, trong khi kỹ thuật ném bóng là để ghi điểm", "B": "Dribble chỉ sử dụng một tay, trong khi kỹ thuật khác sử dụng cả hai tay", "C": "Dribble là kỹ thuật phòng thủ, trong khi kỹ thuật ném bóng là tấn công", "D": "Dribble không cần quan sát, kỹ thuật khác thì cần"}, "category": "distinction", "question": "Dribble khác với kỹ thuật nào dưới đây chủ yếu ở điểm nào?", "explanation": "A đúng vì dribble là kỹ thuật di chuyển với bóng, trong khi ném bóng là để ghi điểm. Các đáp án khác không chính xác.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Sử dụng ngón tay để đập bóng", "B": "Nhìn xuống bóng để theo dõi", "C": "Giữ bóng bằng lòng bàn tay", "D": "Đập bóng càng mạnh càng tốt"}, "category": "definition", "question": "Khi dribble, điều gì là quan trọng nhất để kiểm soát bóng?", "explanation": "A đúng vì việc sử dụng ngón tay để đập bóng giúp tăng độ chính xác và kiểm soát. Các đáp án khác không đúng với kỹ thuật dribble hiệu quả.", "correctAnswer": "A"}]', 5, 70, 'Khái niệm dribble', '2026-01-21 19:15:35.713', 'gpt-4o-mini', '2026-01-21 19:15:35.862878', '2026-01-21 19:15:35.862878');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('0abfad7a-8232-441e-8c2a-fe6e3db0e6f7', '0ac3ad84-1103-4276-b000-af84a6526703', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Ngón tay cái nên ở phía dưới bóng và các ngón tay quanh bóng.", "B": "Lòng bàn tay nên chạm vào bóng để kiểm soát tốt hơn.", "C": "Cầm bóng bằng cả hai tay và đặt nó trên đầu.", "D": "Chỉ sử dụng một ngón tay để giữ bóng."}, "category": "definition", "question": "Khi cầm bóng rổ, điều nào sau đây là đúng?", "explanation": "A đúng vì nó mô tả cách cầm bóng chính xác. B sai vì lòng bàn tay không nên chạm vào bóng. C sai vì cầm bóng trên đầu không phải là tư thế đúng. D sai vì cầm bóng chỉ bằng một ngón tay không đủ an toàn.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Nó giúp bạn kiểm soát bóng khi ném.", "B": "Nó giúp bạn giữ bóng chắc chắn hơn.", "C": "Nó không có ảnh hưởng gì.", "D": "Nó làm cho bóng nặng hơn."}, "category": "definition", "question": "Tại sao tay không ném lại quan trọng khi cầm bóng rổ?", "explanation": "A đúng vì tay không ném giúp kiểm soát bóng. B sai vì tay không ném không giúp giữ bóng chắc hơn. C sai vì tay không ném có ảnh hưởng lớn đến khả năng ném. D sai vì tay không ném không làm bóng nặng hơn.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Đứng thẳng, đầu gối hơi khuỵu, giữ bóng gần đầu.", "B": "Ngồi xổm và giữ bóng ở dưới đất.", "C": "Đứng thẳng với hai tay giơ lên cao.", "D": "Chỉ cần đứng tự do mà không cần tư thế cụ thể."}, "category": "definition", "question": "Khi bạn cầm bóng rổ, tư thế đứng nào là đúng?", "explanation": "A đúng vì tư thế này giúp quan sát rổ dễ dàng hơn. B sai vì ngồi xổm không thích hợp khi ném bóng. C sai vì giơ tay lên cao không phải là tư thế cầm bóng đúng. D sai vì tư thế cụ thể là cần thiết để ném bóng hiệu quả.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Cách cầm bóng rổ yêu cầu sử dụng ngón tay để kiểm soát, trong khi bóng chuyền không cần.", "B": "Bóng rổ luôn phải được cầm bằng cả hai tay.", "C": "Bóng chuyền không cần phải giữ bóng gần đầu.", "D": "Cả hai đều yêu cầu lòng bàn tay chạm vào bóng."}, "category": "distinction", "question": "Khác biệt chính giữa cách cầm bóng rổ và cách cầm bóng chuyền là gì?", "explanation": "A đúng vì cách cầm bóng rổ khác biệt ở điểm cần sử dụng ngón tay để kiểm soát. B sai vì bóng rổ có thể cầm bằng một tay. C sai vì cả hai đều cần sự kiểm soát. D sai vì bóng chuyền không yêu cầu lòng bàn tay chạm vào bóng như bóng rổ.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Bóng rổ cần cầm bằng tay để kiểm soát, trong khi bóng bầu dục có thể ném bằng vai.", "B": "Bóng rổ luôn phải được ném từ trên cao.", "C": "Bóng bầu dục không cần kiểm soát bằng tay.", "D": "Không có sự khác biệt nào trong cách ném."}, "category": "distinction", "question": "Điểm khác biệt nào giữa việc ném bóng rổ và ném bóng bầu dục?", "explanation": "A đúng vì cách cầm và ném bóng rổ và bóng bầu dục hoàn toàn khác nhau. B sai vì bóng rổ không nhất thiết phải ném từ trên cao. C sai vì bóng bầu dục cũng cần kiểm soát bằng tay. D sai vì có nhiều điểm khác biệt trong cách ném.", "correctAnswer": "A"}]', 5, 70, 'Cách Cầm Bóng Đúng Cách', '2026-01-21 19:16:01.868', 'gpt-4o-mini', '2026-01-21 19:16:01.911243', '2026-01-21 19:16:01.911243');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('c6197c52-fc68-400d-bea5-f6c06e9f6da9', NULL, '655ee4c3-c649-4744-a97e-8d5d402940a9', 'boss', NULL, '[{"id": "q1", "options": {"A": "Tạo ra khoảng trống cho cầu thủ tấn công", "B": "Chỉ định một cầu thủ phòng ngự", "C": "Tăng cường phòng ngự", "D": "Thay đổi cầu thủ trên sân"}, "category": "core_concept", "question": "Khái niệm Pick and Roll trong bóng rổ chủ yếu liên quan đến hoạt động nào?", "difficulty": "medium", "explanation": "Pick and Roll là một chiến thuật giúp tạo không gian tấn công cho cầu thủ có bóng.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Khi hàng phòng ngự đối phương chưa sắp xếp lại", "B": "Khi đội tấn công đang bị dẫn điểm", "C": "Khi tất cả các cầu thủ đều đã ở vị trí tốt", "D": "Khi trận đấu đã kết thúc"}, "category": "core_concept", "question": "Chiến thuật Fast Break thường được thực hiện trong tình huống nào?", "difficulty": "medium", "explanation": "Fast Break được thực hiện khi hàng phòng ngự chưa kịp tổ chức lại, tạo cơ hội cho đội tấn công ghi điểm nhanh.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Kỹ năng cá nhân của cầu thủ", "B": "Tổ chức đội hình chặt chẽ", "C": "Tạo ra các màn chắn", "D": "Chuyền bóng liên tục"}, "category": "core_concept", "question": "Isolation là chiến thuật chủ yếu dựa vào điều gì?", "difficulty": "medium", "explanation": "Isolation tập trung vào khả năng cá nhân của cầu thủ, cho phép họ hoạt động một đối một với đối thủ.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Giao tiếp giữa các cầu thủ", "B": "Sự ngẫu nhiên trong tấn công", "C": "Không cần phải di chuyển", "D": "Chỉ có cầu thủ dẫn bóng cần phải di chuyển"}, "category": "core_concept", "question": "Một trong những yếu tố quan trọng trong chiến thuật Pick and Roll là gì?", "difficulty": "medium", "explanation": "Giao tiếp tốt giữa các cầu thủ là yếu tố quan trọng để thực hiện chiến thuật Pick and Roll hiệu quả.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Pick and Roll sử dụng nhiều cầu thủ hơn", "B": "Isolation không cần tạo màn chắn", "C": "Pick and Roll chỉ áp dụng cho cầu thủ có bóng", "D": "Isolation là chiến thuật phòng ngự"}, "category": "comparison", "question": "Giữa Pick and Roll và Isolation, điểm khác biệt chính là gì?", "difficulty": "hard", "explanation": "Isolation không cần tạo màn chắn, mà tập trung vào khả năng cá nhân của một cầu thủ để ghi điểm.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Cầu thủ phòng ngự", "B": "Cầu thủ cầm bóng", "C": "Cầu thủ tạo màn chắn", "D": "Cầu thủ không có bóng"}, "category": "application", "question": "Khi thực hiện Fast Break, cầu thủ nào cần phải quyết định nhanh chóng về việc ghi điểm hay chuyền bóng?", "difficulty": "medium", "explanation": "Cầu thủ cầm bóng trong Fast Break cần phải quyết định nhanh chóng để tận dụng cơ hội ghi điểm.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Cần phải giao tiếp tốt", "B": "Cầu thủ tạo màn chắn phải đứng vững", "C": "Cầu thủ dẫn bóng không cần phải quyết định nhanh", "D": "Cần tạo khoảng trống cho các cầu thủ khác"}, "category": "core_concept", "question": "Điều gì không phải là một lưu ý khi thực hiện chiến thuật Pick and Roll?", "difficulty": "hard", "explanation": "Cầu thủ dẫn bóng cần phải quyết định nhanh chóng dựa trên tình huống để tạo cơ hội tốt nhất.", "correctAnswer": "C"}, {"id": "q8", "options": {"A": "Chạy theo cầu thủ cầm bóng để tạo hỗ trợ", "B": "Đứng yên ở vị trí của mình", "C": "Chạy ngược về phía rổ phòng ngự", "D": "Chỉ theo dõi cầu thủ cầm bóng"}, "category": "application", "question": "Trong một tình huống Fast Break, các cầu thủ hỗ trợ nên làm gì?", "difficulty": "medium", "explanation": "Các cầu thủ hỗ trợ cần chạy theo cầu thủ cầm bóng để tạo ra sự hỗ trợ và cơ hội ghi điểm.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Cầu thủ chính phải có kỹ năng dribble tốt", "B": "Tất cả cầu thủ phải tham gia", "C": "Cầu thủ chính cần phải chuyền bóng ngay lập tức", "D": "Không cần tạo khoảng trống"}, "category": "core_concept", "question": "Khi thực hiện Isolation, điều quan trọng nhất là gì?", "difficulty": "hard", "explanation": "Kỹ năng dribble tốt là rất quan trọng để cầu thủ chính có thể vượt qua hậu vệ một đối một.", "correctAnswer": "A"}, {"id": "q10", "options": {"A": "Đọc tình huống và phản ứng nhanh", "B": "Chỉ cần một cầu thủ có kỹ năng", "C": "Không cần phối hợp với các cầu thủ khác", "D": "Chỉ thực hiện một lần trong trận"}, "category": "connection", "question": "Điều gì là cần thiết để tối ưu hóa chiến thuật Pick and Roll?", "difficulty": "hard", "explanation": "Đọc tình huống và phản ứng nhanh là cần thiết để tối ưu hóa hiệu quả của Pick and Roll.", "correctAnswer": "A"}, {"id": "q11", "options": {"A": "Giữ bóng lại", "B": "Chuyền cho cầu thủ đang ở vị trí tốt hơn", "C": "Cố gắng ghi điểm bất chấp áp lực", "D": "Lùi lại để tổ chức lại"}, "category": "application", "question": "Trong chiến thuật Fast Break, nếu cầu thủ cầm bóng gặp phải áp lực từ phòng ngự, họ nên làm gì?", "difficulty": "medium", "explanation": "Nếu gặp áp lực, cầu thủ cầm bóng nên nhanh chóng chuyền cho đồng đội đang ở vị trí thuận lợi.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Tăng cường phòng ngự", "B": "Tạo ra cơ hội ghi điểm cho cả hai cầu thủ", "C": "Chỉ tập trung vào một cầu thủ", "D": "Giảm thiểu thời gian tấn công"}, "category": "core_concept", "question": "Điểm mạnh chính của chiến thuật Pick and Roll là gì?", "difficulty": "medium", "explanation": "Pick and Roll tạo ra cơ hội ghi điểm cho cả người điều khiển bóng và người tạo màn chắn.", "correctAnswer": "B"}, {"id": "q13", "options": {"A": "Khi đội bóng cần ghi điểm trong những tình huống căng thẳng", "B": "Khi hàng phòng ngự đã sẵn sàng", "C": "Khi cầu thủ không có kỹ năng", "D": "Khi không có cơ hội tấn công"}, "category": "application", "question": "Khi nào thì chiến thuật Isolation thường được áp dụng?", "difficulty": "hard", "explanation": "Isolation thường được áp dụng trong các tình huống căng thẳng khi đội bóng cần ghi điểm nhanh chóng.", "correctAnswer": "A"}, {"id": "q14", "options": {"A": "Tốc độ di chuyển", "B": "Sự hỗ trợ từ đồng đội", "C": "Thời gian tấn công", "D": "Khả năng phòng ngự"}, "category": "comparison", "question": "Đâu là yếu tố không cần thiết khi thực hiện một Fast Break thành công?", "difficulty": "medium", "explanation": "Khả năng phòng ngự không phải là yếu tố cần thiết trong một Fast Break, mà là tốc độ và sự hỗ trợ từ đồng đội.", "correctAnswer": "D"}, {"id": "q15", "options": {"A": "Fast Break tập trung vào tốc độ, trong khi Pick and Roll dựa vào phối hợp", "B": "Cả hai đều yêu cầu một cầu thủ duy nhất", "C": "Fast Break cần ít cầu thủ hơn", "D": "Pick and Roll không yêu cầu tốc độ"}, "category": "comparison", "question": "Điểm khác biệt giữa Fast Break và Pick and Roll là gì?", "difficulty": "hard", "explanation": "Fast Break tập trung vào tốc độ và tận dụng cơ hội, trong khi Pick and Roll dựa vào phối hợp giữa hai cầu thủ.", "correctAnswer": "A"}]', 15, 80, 'Boss Quiz: Phân Tích Chiến Thuật Tấn Công Trong Bóng Rổ', '2026-01-22 16:16:25.474', 'gpt-4o-mini', '2026-01-22 16:16:25.515633', '2026-01-22 16:16:25.515633');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('6deaf8ca-78d4-480b-8083-5d0a2416491c', '0bc2a120-d678-4e04-8ffa-4a33274e265f', NULL, 'lesson', 'example', '[{"id": "q1", "options": {"A": "Sự chuẩn bị tâm lý vững vàng", "B": "Kỹ thuật ném hoàn hảo", "C": "Động tác nhảy sai kỹ thuật", "D": "Khả năng đọc tình huống đúng"}, "category": "wrong_example", "question": "Yếu tố nào sau đây không đóng góp vào thành công của cú ném 3 điểm trong tình huống căng thẳng?", "explanation": "C là đáp án đúng vì động tác nhảy sai kỹ thuật sẽ ảnh hưởng tiêu cực đến cú ném. A, B và D đều là các yếu tố quan trọng góp phần vào thành công của cú ném.", "correctAnswer": "C"}, {"id": "q2", "options": {"A": "Sự tự tin và chuẩn bị tâm lý", "B": "Chỉ cần đứng yên và ném", "C": "Bỏ qua việc chọn thời điểm ném", "D": "Phụ thuộc hoàn toàn vào sức mạnh tay"}, "category": "correct_example", "question": "Trong cú ném 3 điểm, yếu tố nào sau đây là cần thiết để thực hiện cú ném hiệu quả?", "explanation": "A là đáp án đúng vì sự tự tin và chuẩn bị tâm lý là rất quan trọng trong những tình huống áp lực. B, C và D đều không thể hiện yếu tố cần thiết trong cú ném hiệu quả.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Sử dụng sức mạnh từ chân để hỗ trợ cú ném", "B": "Chuẩn bị tâm lý trước khi thực hiện cú ném", "C": "Chọn thời điểm ném mà không quan tâm đến tình huống", "D": "Động tác tay đúng cách trong quá trình ném"}, "category": "wrong_example", "question": "Chọn ví dụ nào dưới đây là sai khi phân tích kỹ thuật ném bóng thành công?", "explanation": "C là đáp án đúng vì chọn thời điểm ném mà không quan tâm đến tình huống là không phù hợp. A, B và D đều là các yếu tố đúng trong việc phân tích kỹ thuật ném bóng.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Động tác tay không cần chính xác", "B": "Sự tập trung và quyết định đúng lúc", "C": "Chỉ cần có sức mạnh để ném", "D": "Ném mà không cần quan sát đối thủ"}, "category": "correct_example", "question": "Khi thực hiện cú ném 3 điểm trong trận đấu, điều nào sau đây là yếu tố quyết định thành công?", "explanation": "B là đáp án đúng vì sự tập trung và quyết định đúng lúc là rất quan trọng trong tình huống căng thẳng. A, C và D đều không đúng và thiếu sót trong việc đánh giá thành công của cú ném.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Hướng dẫn từ huấn luyện viên về lựa chọn thời điểm", "B": "Ném bóng khi không có sự chuẩn bị tâm lý", "C": "Thực hành động tác tay và chân trước trận đấu", "D": "Tập trung vào mục tiêu trước khi ném"}, "category": "wrong_example", "question": "Tình huống nào sau đây là không phù hợp với việc thực hiện cú ném 3 điểm trong trận đấu?", "explanation": "B là đáp án đúng vì việc ném bóng mà không có sự chuẩn bị tâm lý sẽ dẫn đến cú ném không hiệu quả. A, C và D đều là những hành động đúng đắn trong việc chuẩn bị cho cú ném.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Thực hiện cú ném mà không quan tâm đến áp lực", "B": "Sử dụng kỹ thuật ném tốt và giữ tâm lý vững vàng", "C": "Chỉ tập trung vào sức mạnh tay khi ném", "D": "Không cần chuẩn bị tâm lý trước trận đấu"}, "category": "correct_example", "question": "Yếu tố nào sau đây thể hiện sự kết hợp giữa kỹ thuật và tâm lý trong việc ném bóng?", "explanation": "B là đáp án đúng vì việc sử dụng kỹ thuật ném tốt và giữ tâm lý vững vàng là sự kết hợp hoàn hảo. A, C và D đều không thể hiện sự kết hợp này.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Sự chuẩn bị tâm lý rất quan trọng", "B": "Kỹ thuật ném chính xác là cần thiết", "C": "Chỉ cần ném bóng mà không cần nghĩ ngợi", "D": "Thời điểm ném phải được lựa chọn kỹ lưỡng"}, "category": "wrong_example", "question": "Khi phân tích cú ném 3 điểm của Stephen Curry, yếu tố nào là không phù hợp?", "explanation": "C là đáp án đúng vì việc ném bóng mà không cần suy nghĩ hay phân tích tình huống là không phù hợp. A, B và D đều là các yếu tố quan trọng trong cú ném thành công.", "correctAnswer": "C"}]', 7, 70, 'Case Study: Cú Ném Quyết Định Trong Trận Chung Kết', '2026-01-21 19:20:09.097', 'gpt-4o-mini', '2026-01-21 19:20:09.142659', '2026-01-21 19:20:09.142659');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('09a15815-252e-4aed-9e29-9055d271f37a', NULL, '66f15918-baf4-4eb8-a780-da5d7b570c8f', 'boss', NULL, '[{"id": "q1", "options": {"A": "Cải thiện sức mạnh và sức bền", "B": "Chỉ để giảm cân", "C": "Làm quen với bóng rổ", "D": "Chỉ để thi đấu"}, "category": "core_concept", "question": "Lập kế hoạch tập luyện thể lực cho bóng rổ nhằm mục đích gì?", "difficulty": "medium", "explanation": "Mục đích chính của lập kế hoạch tập luyện là cải thiện sức mạnh và sức bền, giúp tối ưu hóa hiệu suất thi đấu.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Specificity", "B": "Progressive Overload", "C": "Periodization", "D": "Recovery"}, "category": "core_concept", "question": "Nguyên tắc nào trong lập kế hoạch tập luyện chỉ ra rằng vận động viên nên dần dần tăng cường độ và tần suất tập luyện?", "difficulty": "medium", "explanation": "Nguyên tắc Progressive Overload yêu cầu vận động viên gia tăng dần cường độ tập luyện để cải thiện thể lực.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Thời gian tập luyện", "B": "Kinh nghiệm thi đấu", "C": "Màu sắc của giày", "D": "Mục tiêu cá nhân"}, "category": "core_concept", "question": "Yếu tố nào không ảnh hưởng đến kế hoạch tập luyện của vận động viên bóng rổ?", "difficulty": "hard", "explanation": "Màu sắc của giày không ảnh hưởng đến kế hoạch tập luyện, trong khi thời gian, kinh nghiệm và mục tiêu cá nhân đều là những yếu tố quan trọng.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Giai đoạn thi đấu", "B": "Giai đoạn tối ưu", "C": "Giai đoạn căn bản", "D": "Giai đoạn khôi phục"}, "category": "core_concept", "question": "Trong giai đoạn nào của kế hoạch tập luyện, vận động viên nên tập trung vào việc phát triển sức bền cơ bản?", "difficulty": "medium", "explanation": "Giai đoạn căn bản là thời điểm mà vận động viên nên tập trung vào việc phát triển sức bền cơ bản.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "1 giờ mỗi tuần", "B": "2-3 giờ mỗi tuần", "C": "5-6 giờ mỗi tuần", "D": "Không cần thực hành"}, "category": "application", "question": "Nếu một vận động viên bóng rổ muốn cải thiện khả năng ném bóng, họ nên dành bao nhiêu thời gian mỗi tuần để thực hành ném bóng?", "difficulty": "medium", "explanation": "Để cải thiện khả năng ném bóng, vận động viên nên dành từ 2-3 giờ mỗi tuần để thực hành.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Đánh giá tình trạng thể lực hiện tại", "B": "Chọn bài tập phù hợp", "C": "Thiết lập lịch tập luyện", "D": "Xác định mục tiêu tập luyện"}, "category": "core_concept", "question": "Khi lập kế hoạch tập luyện, yếu tố nào cần được xem xét đầu tiên?", "difficulty": "medium", "explanation": "Xác định mục tiêu tập luyện là bước đầu tiên quan trọng nhất trong việc lập kế hoạch tập luyện.", "correctAnswer": "D"}, {"id": "q7", "options": {"A": "Chỉ tập luyện khi có người hướng dẫn", "B": "Ghi nhật ký tập luyện", "C": "Tập luyện theo cách ngẫu nhiên", "D": "Không cần theo dõi tiến độ"}, "category": "application", "question": "Để duy trì động lực trong quá trình tập luyện, vận động viên nên làm gì?", "difficulty": "medium", "explanation": "Ghi nhật ký tập luyện giúp vận động viên theo dõi sự tiến bộ và duy trì động lực.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Chỉ tập trung vào bài tập sức mạnh", "B": "Tăng cường độ và khối lượng bài tập", "C": "Giảm số ngày tập luyện", "D": "Không cần theo dõi tiến độ"}, "category": "comparison", "question": "So với giai đoạn căn bản, giai đoạn tối ưu trong kế hoạch tập luyện sẽ có đặc điểm gì khác biệt?", "difficulty": "hard", "explanation": "Giai đoạn tối ưu yêu cầu tăng cường độ và khối lượng bài tập để đạt được hiệu suất tốt nhất.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Nghỉ ngơi", "B": "Giãn cơ và kéo giãn", "C": "Tăng cường độ tập luyện ngay lập tức", "D": "Massage"}, "category": "comparison", "question": "Yếu tố nào không thuộc về kỹ thuật hồi phục sau tập luyện?", "difficulty": "hard", "explanation": "Tăng cường độ tập luyện ngay lập tức không phải là một phương pháp hồi phục, mà ngược lại, cần thời gian để hồi phục.", "correctAnswer": "C"}, {"id": "q10", "options": {"A": "Tăng cường tập luyện mà không nghỉ", "B": "Thực hiện yoga hoặc thiền", "C": "Bỏ qua các bài tập hồi phục", "D": "Ngừng tập luyện hoàn toàn"}, "category": "application", "question": "Trong trường hợp một vận động viên bóng rổ gặp khó khăn về tâm lý, họ nên làm gì để cải thiện tình trạng này?", "difficulty": "medium", "explanation": "Thực hiện yoga hoặc thiền có thể giúp vận động viên cải thiện tâm lý và giảm căng thẳng.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Không quan trọng", "B": "Chỉ cần ăn bất kỳ thực phẩm nào", "C": "Hỗ trợ phục hồi và tối ưu hóa hiệu suất", "D": "Chỉ cần uống nước là đủ"}, "category": "core_concept", "question": "Chế độ dinh dưỡng đóng vai trò gì trong kế hoạch tập luyện cho bóng rổ?", "difficulty": "medium", "explanation": "Chế độ dinh dưỡng hợp lý hỗ trợ phục hồi nhanh chóng và tối ưu hóa hiệu suất thi đấu cho vận động viên.", "correctAnswer": "C"}, {"id": "q12", "options": {"A": "Tối ưu hóa sự phát triển thể lực theo giai đoạn", "B": "Chỉ tập luyện một loại bài tập duy nhất", "C": "Đảm bảo không có thời gian nghỉ ngơi", "D": "Tăng cường tập luyện liên tục mà không thay đổi"}, "category": "core_concept", "question": "Mục tiêu của phân kỳ huấn luyện là gì?", "difficulty": "hard", "explanation": "Phân kỳ huấn luyện nhằm tối ưu hóa sự phát triển thể lực của vận động viên trong từng giai đoạn.", "correctAnswer": "A"}]', 12, 80, 'Boss Quiz: Lập kế hoạch tập luyện thể lực cho bóng rổ', '2026-01-22 16:17:02.586', 'gpt-4o-mini', '2026-01-22 16:17:02.628099', '2026-01-22 16:17:02.628099');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('bb20c6fc-e3d7-4fbd-97af-b913d74c4a30', NULL, '70fd8ebf-4ad8-4041-a696-1e04d14e3ff5', 'boss', NULL, '[{"id": "q1", "options": {"A": "Nó giúp cơ thể sản xuất hormone tăng trưởng.", "B": "Nó làm giảm sức đề kháng của cơ thể.", "C": "Nó không có tác động đến quá trình phục hồi.", "D": "Nó chỉ cần thiết cho các vận động viên chuyên nghiệp."}, "category": "core_concept", "question": "Tại sao việc nghỉ ngơi lại quan trọng trong phục hồi sau tập luyện?", "difficulty": "medium", "explanation": "Nghỉ ngơi giúp cơ thể sản xuất hormone tăng trưởng, một yếu tố quan trọng trong việc tái tạo và phục hồi cơ bắp.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Sử dụng bồn nước nóng.", "B": "Tập luyện liên tục mà không nghỉ.", "C": "Giãn cơ.", "D": "Dinh dưỡng hợp lý."}, "category": "core_concept", "question": "Phương pháp nào dưới đây không phải là một kỹ thuật phục hồi sau tập luyện?", "difficulty": "medium", "explanation": "Tập luyện liên tục mà không nghỉ không phải là một kỹ thuật phục hồi, nó có thể dẫn đến chấn thương.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Nó làm tăng sức mạnh cơ bắp.", "B": "Nó giúp giảm đau nhức và tăng cường lưu thông máu.", "C": "Nó là một thiết bị tập luyện.", "D": "Nó không có lợi ích gì."}, "category": "core_concept", "question": "Lợi ích chính của việc sử dụng foam roller trong phục hồi là gì?", "difficulty": "medium", "explanation": "Foam roller giúp giảm đau nhức và cải thiện lưu thông máu, hỗ trợ quá trình phục hồi cơ bắp.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Chỉ cần nghỉ ngơi và không làm gì.", "B": "Kết hợp giữa nghỉ ngơi, dinh dưỡng và giãn cơ.", "C": "Tập luyện thêm để tăng sức bền.", "D": "Chỉ ăn thực phẩm giàu protein."}, "category": "core_concept", "question": "Cách nào tốt nhất để tối ưu hóa quá trình phục hồi sau khi tập luyện?", "difficulty": "medium", "explanation": "Tối ưu hóa quá trình phục hồi cần kết hợp giữa nghỉ ngơi, dinh dưỡng hợp lý và các bài tập giãn cơ.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Dinh dưỡng không ảnh hưởng đến phục hồi.", "B": "Dinh dưỡng chỉ quan trọng cho sức khỏe tổng quát.", "C": "Dinh dưỡng giúp cung cấp năng lượng và chất dinh dưỡng cần thiết cho phục hồi cơ bắp.", "D": "Dinh dưỡng chỉ cần thiết trước khi tập luyện."}, "category": "connection", "question": "Mối liên hệ giữa dinh dưỡng và phục hồi là gì?", "difficulty": "medium", "explanation": "Dinh dưỡng đóng vai trò quan trọng trong việc cung cấp năng lượng và các chất dinh dưỡng thiết yếu cho quá trình phục hồi cơ bắp.", "correctAnswer": "C"}, {"id": "q6", "options": {"A": "Tiếp tục thi đấu mà không nghỉ.", "B": "Tham khảo ý kiến bác sĩ và thực hiện các bài tập phục hồi được chỉ định.", "C": "Chỉ tập yoga.", "D": "Ngủ ít hơn để tăng thời gian tập luyện."}, "category": "application", "question": "Cách nào dưới đây giúp cầu thủ bóng rổ phục hồi nhanh chóng sau một chấn thương?", "difficulty": "hard", "explanation": "Tham khảo ý kiến bác sĩ và thực hiện các bài tập phục hồi giúp cầu thủ tối ưu hóa quá trình hồi phục và đảm bảo an toàn cho sức khỏe.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Tự mình suy nghĩ tiêu cực.", "B": "Thực hành thiền và yoga để giảm căng thẳng.", "C": "Không nói chuyện với ai.", "D": "Tránh tất cả các hoạt động thể chất."}, "category": "application", "question": "Đâu là phương pháp tốt nhất để cải thiện tâm lý trong quá trình phục hồi?", "difficulty": "medium", "explanation": "Thực hành thiền và yoga giúp giảm căng thẳng và cải thiện tâm lý, hỗ trợ quá trình phục hồi.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Giãn cơ là một phần của khởi động.", "B": "Khởi động chỉ bao gồm các bài tập thể lực.", "C": "Giãn cơ không cần thiết trước khi tập luyện.", "D": "Khởi động chỉ là một hoạt động thể chất nhẹ."}, "category": "comparison", "question": "Để phân biệt giữa giãn cơ và khởi động, bạn có thể nói rằng:", "difficulty": "medium", "explanation": "Giãn cơ là một phần quan trọng trong quá trình khởi động, giúp làm nóng và chuẩn bị cơ thể trước khi tập luyện.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Dinh dưỡng trước tập chỉ cần cung cấp năng lượng, sau tập cần phục hồi.", "B": "Dinh dưỡng không quan trọng trước và sau tập.", "C": "Cả hai đều giống nhau và không cần phân biệt.", "D": "Dinh dưỡng sau tập không cần thiết."}, "category": "comparison", "question": "Sự khác biệt giữa dinh dưỡng trước và sau tập luyện là gì?", "difficulty": "medium", "explanation": "Dinh dưỡng trước tập luyện cần cung cấp năng lượng, trong khi dinh dưỡng sau tập luyện chủ yếu nhằm phục hồi và tái tạo cơ bắp.", "correctAnswer": "A"}, {"id": "q10", "options": {"A": "Chỉ tập luyện mà không nghỉ.", "B": "Một kế hoạch phục hồi toàn diện bao gồm nghỉ ngơi, dinh dưỡng và giãn cơ.", "C": "Tập luyện liên tục mặc kệ chấn thương.", "D": "Duy trì chế độ ăn uống bình thường."}, "category": "application", "question": "Khi một cầu thủ bóng rổ phải đối mặt với chấn thương, yếu tố nào là quan trọng nhất trong kế hoạch phục hồi?", "difficulty": "hard", "explanation": "Kế hoạch phục hồi toàn diện giúp cầu thủ hồi phục an toàn và nhanh chóng, bao gồm các yếu tố như nghỉ ngơi, dinh dưỡng và giãn cơ.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Nó làm tăng nhiệt độ cơ thể.", "B": "Nó giúp giảm viêm và mệt mỏi cơ bắp.", "C": "Nó không có tác dụng gì.", "D": "Nó chỉ cần thiết cho các vận động viên chuyên nghiệp."}, "category": "core_concept", "question": "Tại sao sử dụng thiết bị phục hồi như bồn nước lạnh lại hiệu quả?", "difficulty": "medium", "explanation": "Bồn nước lạnh giúp giảm viêm và mệt mỏi cơ bắp, giúp cải thiện quá trình phục hồi.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Nó có thể làm tăng cortisol và gây hại.", "B": "Nó không có ảnh hưởng gì.", "C": "Nó giúp duy trì động lực và thúc đẩy tiến bộ phục hồi.", "D": "Nó chỉ quan trọng trong các tình huống thi đấu."}, "category": "connection", "question": "Tâm lý tích cực có tác động như thế nào đến quá trình phục hồi?", "difficulty": "hard", "explanation": "Tâm lý tích cực giúp duy trì động lực và thúc đẩy tiến bộ phục hồi, ảnh hưởng đến khả năng phục hồi tổng thể.", "correctAnswer": "C"}]', 12, 80, 'Boss Quiz: Kỹ Thuật Phục Hồi Sau Tập Luyện', '2026-01-22 16:17:42.941', 'gpt-4o-mini', '2026-01-22 16:17:43.093569', '2026-01-22 16:17:43.093569');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('0c1fe1dc-58ee-4a35-9cf9-ff8d3c1b14c8', NULL, '4d65529b-adb2-410a-99ad-0c8ad3fb7867', 'boss', NULL, '[{"id": "q1", "options": {"A": "Bình tĩnh và tự tin", "B": "Lo lắng về áp lực", "C": "Chỉ tập trung vào kỹ thuật", "D": "Ngại ngùng trước khán giả"}, "category": "core_concept", "question": "Kỹ thuật sút phạt trong bóng rổ yêu cầu cầu thủ phải giữ tâm lý như thế nào?", "difficulty": "medium", "explanation": "Tâm lý bình tĩnh và tự tin sẽ giúp cầu thủ thực hiện cú sút phạt chính xác hơn.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Chân đứng rộng hơn vai", "B": "Chân đứng hẹp hơn vai", "C": "Chân rộng bằng vai", "D": "Chân đứng không có quy tắc"}, "category": "core_concept", "question": "Tư thế đứng đúng khi thực hiện sút phạt là gì?", "difficulty": "medium", "explanation": "Tư thế chân rộng bằng vai giúp cầu thủ giữ thăng bằng và ổn định trước cú sút.", "correctAnswer": "C"}, {"id": "q3", "options": {"A": "Điểm bóng chạm vào rổ", "B": "Điểm bóng được tách ra khỏi tay cầu thủ", "C": "Điểm cầu thủ bắt bóng", "D": "Điểm cầu thủ nhảy lên"}, "category": "core_concept", "question": "Điểm rơi (release point) trong cú sút phạt là gì?", "difficulty": "medium", "explanation": "Điểm rơi là thời điểm bóng được tách ra khỏi tay cầu thủ, ảnh hưởng lớn đến độ chính xác của cú sút.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Tự tin", "B": "Tập trung", "C": "Kỹ thuật cầm bóng", "D": "Quản lý cảm xúc"}, "category": "core_concept", "question": "Yếu tố nào không thuộc về tâm lý khi thực hiện sút phạt?", "difficulty": "medium", "explanation": "Kỹ thuật cầm bóng là yếu tố kỹ thuật, không phải yếu tố tâm lý.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "Sự ổn định tâm lý", "B": "Sự nhanh nhẹn", "C": "Sức mạnh cơ bắp", "D": "Kỹ năng giao tiếp"}, "category": "connection", "question": "Khi thực hiện cú sút phạt, điều nào sau đây là quan trọng nhất?", "difficulty": "hard", "explanation": "Ổn định tâm lý là yếu tố then chốt giúp cầu thủ thực hiện cú sút chính xác hơn trong những tình huống căng thẳng.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Kỹ thuật thở không ảnh hưởng đến sút phạt", "B": "Kỹ thuật thở giúp giảm căng thẳng và tăng tập trung", "C": "Kỹ thuật thở chỉ cần thiết khi không có áp lực", "D": "Kỹ thuật thở chỉ sử dụng khi khán giả không xem"}, "category": "connection", "question": "Mối liên hệ giữa kỹ thuật thở và sút phạt là gì?", "difficulty": "medium", "explanation": "Kỹ thuật thở giúp cầu thủ giảm căng thẳng và nâng cao khả năng tập trung trước cú sút.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Hít thở sâu và hình dung cú sút thành công", "B": "Không nghĩ gì và chỉ sút", "C": "Nghe lời khuyên của khán giả", "D": "Chỉ chú ý vào sức mạnh cú sút"}, "category": "application", "question": "Khi cầu thủ A sút phạt thành công trong tình huống áp lực, điều gì có thể giúp nâng cao khả năng thành công?", "difficulty": "hard", "explanation": "Hít thở sâu và hình dung cú sút thành công sẽ giúp cầu thủ tự tin hơn, từ đó nâng cao khả năng ghi điểm.", "correctAnswer": "A"}, {"id": "q8", "options": {"A": "Thể hiện sự tự tin", "B": "Sút quá mạnh để chắc chắn ghi điểm", "C": "Phân tâm vào áp lực từ khán giả", "D": "Chỉ chú ý vào kỹ thuật mà không cần tâm lý"}, "category": "application", "question": "Trong một trận đấu quyết định, nếu cầu thủ bị phạm lỗi và được sút phạt, điều gì cần thiết nhất?", "difficulty": "hard", "explanation": "Thể hiện sự tự tin là yếu tố quan trọng để ghi điểm thành công trong những tình huống quyết định.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Điểm rơi cao giúp bóng dễ bị chặn", "B": "Điểm rơi thấp giúp bóng dễ đi vào rổ", "C": "Điểm rơi cao giúp cầu thủ có thời gian điều chỉnh cú sút tốt hơn", "D": "Không có sự khác biệt"}, "category": "comparison", "question": "Phân biệt giữa điểm rơi cao và thấp trong cú sút phạt sẽ ảnh hưởng như thế nào đến cú sút?", "difficulty": "medium", "explanation": "Điểm rơi cao giúp cầu thủ có thời gian điều chỉnh cú sút và tăng độ chính xác.", "correctAnswer": "C"}, {"id": "q10", "options": {"A": "Cầm bóng quá chặt", "B": "Cầm bóng bằng một tay và để tay kia hỗ trợ", "C": "Cầm bóng bằng cả hai tay nhưng không hỗ trợ", "D": "Cầm bóng bằng tay không có ngón tay"}, "category": "core_concept", "question": "Kỹ thuật cầm bóng để thực hiện cú sút phạt cần chú ý điều gì?", "difficulty": "medium", "explanation": "Cầm bóng bằng một tay trong khi tay còn lại hỗ trợ giúp tạo ra sự kiểm soát tốt hơn cho cú sút.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Tình huống sút phạt yêu cầu chú ý cao hơn", "B": "Tình huống khác cần chú ý hơn", "C": "Cả hai tình huống đều giống nhau", "D": "Không cần chú ý trong tình huống nào"}, "category": "comparison", "question": "So sánh sự chú ý của cầu thủ trong tình huống sút phạt và trong các tình huống khác. Điều gì là khác biệt?", "difficulty": "hard", "explanation": "Tình huống sút phạt thường yêu cầu cầu thủ tập trung cao độ hơn do áp lực từ khán giả và tính chất quyết định của tình huống.", "correctAnswer": "A"}, {"id": "q12", "options": {"A": "Tập trung vào mục tiêu", "B": "Hít thở sâu", "C": "Bị phân tâm bởi khán giả", "D": "Giữ tư thế đứng vững"}, "category": "comparison", "question": "Khi cầu thủ thực hiện cú sút phạt, điều gì không nên làm?", "difficulty": "medium", "explanation": "Bị phân tâm bởi khán giả có thể làm giảm khả năng thành công của cú sút phạt.", "correctAnswer": "C"}, {"id": "q13", "options": {"A": "Tăng sự tự tin", "B": "Giảm sự tập trung", "C": "Không ảnh hưởng gì", "D": "Giúp cầu thủ sút tốt hơn"}, "category": "application", "question": "Trong tình huống sút phạt, áp lực từ khán giả có thể ảnh hưởng như thế nào đến tâm lý cầu thủ?", "difficulty": "hard", "explanation": "Áp lực từ khán giả có thể làm giảm sự tập trung và tự tin của cầu thủ, ảnh hưởng đến hiệu suất sút phạt.", "correctAnswer": "B"}, {"id": "q14", "options": {"A": "Chỉ dựa vào kỹ thuật cầm bóng", "B": "Tâm lý và chuẩn bị kỹ lưỡng", "C": "Sức mạnh cơ bắp", "D": "May mắn trong tình huống"}, "category": "connection", "question": "Phân tích cú sút phạt của một cầu thủ nổi tiếng như Stephen Curry cho thấy điều gì quan trọng nhất?", "difficulty": "hard", "explanation": "Cú sút phạt thành công không chỉ dựa vào kỹ thuật mà còn cần tâm lý ổn định và sự chuẩn bị kỹ lưỡng.", "correctAnswer": "B"}, {"id": "q15", "options": {"A": "Tập trung vào khán giả", "B": "Hít thở và hình dung thành công", "C": "Chỉ nghĩ đến áp lực", "D": "Không cần kỹ thuật"}, "category": "application", "question": "Kỹ thuật nào giúp cầu thủ giữ bình tĩnh và tự tin trong tình huống sút phạt?", "difficulty": "hard", "explanation": "Hít thở và hình dung thành công giúp cầu thủ giữ bình tĩnh và tự tin trong tình huống áp lực.", "correctAnswer": "B"}]', 15, 80, 'Boss Quiz: Kỹ Thuật Sút Phạt Trong Bóng Rổ', '2026-01-22 16:18:32.074', 'gpt-4o-mini', '2026-01-22 16:18:32.115118', '2026-01-22 16:18:32.115118');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('d985245a-e92a-4a87-8bc5-59181f87b68e', NULL, '12b4d5d2-630e-48c5-8f8e-9c5563030ef4', 'boss', NULL, '[{"id": "q1", "options": {"A": "File", "B": "Edit", "C": "View", "D": "Insert"}, "category": "core_concept", "question": "Chức năng nào trong Microsoft Word cho phép người dùng bắt đầu một tài liệu mới?", "difficulty": "medium", "explanation": "Chức năng ''File'' cho phép người dùng chọn ''New'' để tạo tài liệu mới.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Nhấn vào tab ''Insert''", "B": "Nhấn vào tab ''Home''", "C": "Sử dụng phím tắt Ctrl + P", "D": "Chọn ''File''"}, "category": "core_concept", "question": "Khi chèn hình ảnh vào tài liệu, bước nào là cần thiết?", "difficulty": "medium", "explanation": "Để chèn hình ảnh, người dùng phải nhấn vào tab ''Insert'' và chọn ''Pictures''.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Khi bạn muốn lưu tài liệu với một tên mới", "B": "Khi bạn muốn in tài liệu", "C": "Khi bạn muốn đóng tài liệu", "D": "Khi bạn muốn mở tài liệu"}, "category": "core_concept", "question": "Khi nào bạn nên sử dụng tính năng ''Save As'' thay vì ''Save''?", "difficulty": "medium", "explanation": "''Save As'' cho phép bạn lưu tài liệu với tên hoặc định dạng mới mà không làm mất bản gốc.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Để làm cho văn bản trông đẹp hơn", "B": "Để dễ dàng đọc và truyền đạt thông điệp", "C": "Để tiết kiệm không gian", "D": "Để thêm hình ảnh"}, "category": "core_concept", "question": "Tại sao việc định dạng văn bản lại quan trọng trong tài liệu?", "difficulty": "medium", "explanation": "Định dạng văn bản giúp tài liệu dễ đọc và thông điệp được truyền đạt hiệu quả hơn.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Mở Microsoft Word và chọn ''New''", "B": "Nhập nội dung báo cáo", "C": "Lưu báo cáo ngay lập tức", "D": "Định dạng văn bản"}, "category": "connection", "question": "Nếu bạn muốn tạo một báo cáo từ một mẫu có sẵn, bước đầu tiên bạn nên làm là gì?", "difficulty": "medium", "explanation": "Bạn cần mở Microsoft Word và chọn ''New'' để tìm kiếm các mẫu có sẵn.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Style", "B": "Insert", "C": "Layout", "D": "Review"}, "category": "connection", "question": "Để định dạng tiêu đề trong báo cáo, bạn nên sử dụng tính năng nào?", "difficulty": "medium", "explanation": "Tính năng ''Style'' cho phép bạn chọn kiểu định dạng cho tiêu đề và các phần khác trong tài liệu.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Để tiết kiệm thời gian", "B": "Để tránh mất dữ liệu", "C": "Để làm cho tài liệu trông đẹp hơn", "D": "Để chia sẻ tài liệu nhanh chóng"}, "category": "connection", "question": "Nếu bạn thường xuyên làm việc trên một tài liệu lớn, tại sao bạn nên sử dụng tính năng tự động lưu?", "difficulty": "medium", "explanation": "Tính năng tự động lưu giúp bảo vệ dữ liệu của bạn và giảm thiểu rủi ro mất mát thông tin.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Khi liệt kê thông tin quan trọng", "B": "Khi viết đoạn văn dài", "C": "Khi chèn hình ảnh", "D": "Khi lưu tài liệu"}, "category": "application", "question": "Khi nào bạn nên sử dụng bullet points trong báo cáo?", "difficulty": "medium", "explanation": "Bullet points giúp làm nổi bật các ý chính và giúp tài liệu dễ đọc hơn.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Mẫu tài liệu thường nhanh hơn và giúp tiết kiệm thời gian", "B": "Tạo từ đầu luôn trông đẹp hơn", "C": "Mẫu tài liệu không thể chỉnh sửa", "D": "Tạo từ đầu dễ dàng hơn"}, "category": "comparison", "question": "Nếu bạn muốn so sánh giữa việc sử dụng mẫu tài liệu và tạo tài liệu từ đầu, điểm khác biệt chính là gì?", "difficulty": "medium", "explanation": "Sử dụng mẫu tài liệu giúp tiết kiệm thời gian và nỗ lực trong việc thiết kế và định dạng tài liệu.", "correctAnswer": "A"}, {"id": "q10", "options": {"A": "Định dạng văn bản liên quan đến kiểu chữ, trong khi định dạng đoạn liên quan đến bố cục", "B": "Định dạng đoạn không thể thay đổi kiểu chữ", "C": "Định dạng văn bản chỉ áp dụng cho tiêu đề", "D": "Định dạng văn bản không cần thiết trong tài liệu"}, "category": "comparison", "question": "Điểm khác biệt nào sau đây giữa định dạng văn bản và định dạng đoạn?", "difficulty": "medium", "explanation": "Định dạng văn bản liên quan đến kiểu chữ, kích thước và màu sắc, trong khi định dạng đoạn liên quan đến bố cục và cách căn chỉnh.", "correctAnswer": "A"}]', 10, 80, 'Boss Quiz: Chức Năng Cơ Bản Trong Microsoft Word', '2026-01-22 16:19:06.496', 'gpt-4o-mini', '2026-01-22 16:19:06.539551', '2026-01-22 16:19:06.539551');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('add68b2c-53db-40c1-bf37-bdd6e8ea8db4', NULL, '4641d048-bdd6-4a6b-bc90-24da439ed95b', 'boss', NULL, '[{"id": "q1", "options": {"A": "Cách trình bày văn bản", "B": "Nội dung văn bản", "C": "Số lượng văn bản", "D": "Độ dài văn bản"}, "category": "core_concept", "question": "Định dạng văn bản trong Microsoft Word chủ yếu liên quan đến việc thay đổi điều gì?", "difficulty": "medium", "explanation": "Định dạng văn bản chủ yếu liên quan đến cách trình bày văn bản, bao gồm kiểu chữ, kích thước và màu sắc.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Kiểu chữ", "B": "Kích thước chữ", "C": "Màu sắc", "D": "Nội dung"}, "category": "core_concept", "question": "Yếu tố nào không phải là một phần của định dạng văn bản?", "difficulty": "medium", "explanation": "Nội dung là thông tin trong văn bản, không phải là yếu tố định dạng.", "correctAnswer": "D"}, {"id": "q3", "options": {"A": "8pt", "B": "10-12pt", "C": "14-16pt", "D": "18pt"}, "category": "core_concept", "question": "Kích thước chữ thông thường cho văn bản chính trong các tài liệu là bao nhiêu?", "difficulty": "medium", "explanation": "Kích thước chữ từ 10 đến 12pt là thông thường cho văn bản chính trong tài liệu.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Tăng độ dài văn bản", "B": "Tạo sự thu hút và nhấn mạnh các phần quan trọng", "C": "Giảm tốc độ đọc", "D": "Tăng số lượng từ"}, "category": "core_concept", "question": "Việc sử dụng màu sắc trong định dạng văn bản có tác dụng gì?", "difficulty": "medium", "explanation": "Màu sắc giúp làm nổi bật các phần quan trọng và tạo sự thu hút cho tài liệu.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Khi bạn cần sự trang trọng", "B": "Khi bạn muốn tài liệu có cảm giác hiện đại hơn", "C": "Khi bạn viết một bức thư chính thức", "D": "Khi bạn cầ