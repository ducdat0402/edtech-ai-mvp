INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('19380847-c66c-436d-8163-bc0d1430f647', '600179e3-0704-4947-be20-4be6ba345f3f', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Đập bóng xuống mặt đất với một tay, giữ đầu cao và tập trung vào hướng đi.", "B": "Xoay người về phía tay còn lại trong khi giữ bóng gần cơ thể.", "C": "Đập bóng từ tay này sang tay kia một cách nhanh chóng.", "D": "Đập bóng ra sau lưng với một tay và lấy bóng bằng tay còn lại."}, "category": "definition", "question": "Đặc điểm chính của dribble đơn giản là gì?", "explanation": "A đúng vì nó mô tả chính xác cách thực hiện dribble đơn giản. B, C, và D mô tả các loại dribble khác không phải là dribble đơn giản.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Khi muốn đổi hướng đột ngột và đánh lừa đối thủ.", "B": "Khi không có áp lực từ đối thủ.", "C": "Khi bị đối thủ áp sát từ trước.", "D": "Khi bạn muốn thoát ra khỏi tình huống bị chặn."}, "category": "definition", "question": "Khi nào nên sử dụng kỹ thuật crossover?", "explanation": "A đúng vì crossover thường được sử dụng để tạo ra sự bất ngờ và đổi hướng. B, C, và D là các tình huống phù hợp cho các kỹ thuật khác như dribble đơn giản, behind-the-back, và spin move.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Behind-the-back dùng để vượt qua đối thủ áp sát từ trước, còn spin move dùng để thoát khỏi tình huống bị chặn.", "B": "Spin move là đập bóng ra sau lưng, còn behind-the-back là xoay người.", "C": "Cả hai đều thực hiện bằng cách xoay người.", "D": "Không có sự khác biệt giữa chúng."}, "category": "distinction", "question": "Điểm khác biệt giữa behind-the-back và spin move là gì?", "explanation": "A đúng vì nó chính xác chỉ ra cách sử dụng của từng kỹ thuật. B sai vì mô tả không đúng về cách thực hiện. C và D cũng không đúng vì có sự khác biệt rõ ràng giữa hai kỹ thuật.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Dribble đơn giản được sử dụng trong các tình huống di chuyển thẳng, còn crossover là để tạo ra sự bất ngờ khi đổi hướng.", "B": "Crossover là kỹ thuật dễ thực hiện hơn so với dribble đơn giản.", "C": "Dribble đơn giản chỉ thực hiện bằng một tay, còn crossover phải sử dụng cả hai tay.", "D": "Crossover không thể thực hiện trong không gian hẹp."}, "category": "distinction", "question": "Dribble đơn giản và crossover khác nhau ở điểm nào?", "explanation": "A đúng vì nó mô tả rõ ràng sự khác biệt trong ứng dụng của hai kỹ thuật. B, C, và D không chính xác và không phản ánh đúng bản chất của các kỹ thuật này.", "correctAnswer": "A"}]', 4, 70, 'Các loại dribble cơ bản', '2026-01-22 01:11:09.456', 'gpt-4o-mini', '2026-01-22 01:11:09.498089', '2026-01-22 01:11:09.498089');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('53e35f69-e667-4350-a950-5b4061608ad0', NULL, 'ac610c24-e58c-4189-bda5-66876bcbdf7e', 'boss', NULL, '[{"id": "q1", "options": {"A": "Khoản tiền dự trữ cho các tình huống khẩn cấp", "B": "Tiền tiết kiệm cho du lịch", "C": "Khoản tiền đầu tư vào cổ phiếu", "D": "Tiền dùng để tiêu xài hàng ngày"}, "category": "core_concept", "question": "Quỹ khẩn cấp là gì?", "difficulty": "medium", "explanation": "Quỹ khẩn cấp là khoản tiền dự phòng nhằm đáp ứng các nhu cầu tài chính không lường trước được như mất việc làm, bệnh tật hay sửa chữa khẩn cấp.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "1 tháng", "B": "2 tháng", "C": "3-6 tháng", "D": "12 tháng"}, "category": "core_concept", "question": "Mức quỹ khẩn cấp được khuyến nghị nên có ít nhất bao nhiêu tháng chi phí sinh hoạt?", "difficulty": "medium", "explanation": "Quỹ khẩn cấp thường được khuyến nghị nên có từ 3 đến 6 tháng chi phí sinh hoạt để đảm bảo an toàn tài chính trong trường hợp khẩn cấp.", "correctAnswer": "C"}, {"id": "q3", "options": {"A": "Giúp bạn chi tiêu thoải mái", "B": "Tránh được các khoản vay mượn", "C": "Tăng thu nhập", "D": "Giúp bạn đầu tư nhiều hơn"}, "category": "core_concept", "question": "Tại sao quỹ khẩn cấp giúp bạn giảm áp lực tài chính?", "difficulty": "medium", "explanation": "Quỹ khẩn cấp cung cấp nguồn tài chính khi cần thiết, giúp bạn tránh phải vay mượn và giảm áp lực tài chính trong thời điểm khó khăn.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Xác định chi phí sinh hoạt hàng tháng", "B": "Tiết kiệm tiền hàng tháng", "C": "Tiêu xài số tiền đã tiết kiệm", "D": "Mở tài khoản tiết kiệm riêng"}, "category": "core_concept", "question": "Điều nào sau đây không phải là một bước để tạo ra quỹ khẩn cấp?", "difficulty": "medium", "explanation": "Tiêu xài số tiền đã tiết kiệm không phải là bước để tạo ra quỹ khẩn cấp, mà ngược lại, bạn cần tiết kiệm số tiền này.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "Khi có kế hoạch du lịch", "B": "Khi gặp tai nạn và cần chi trả chi phí y tế", "C": "Khi muốn mua sắm", "D": "Khi có tiền thưởng"}, "category": "application", "question": "Khi nào bạn cần sử dụng quỹ khẩn cấp?", "difficulty": "medium", "explanation": "Quỹ khẩn cấp được sử dụng trong các tình huống khẩn cấp như tai nạn, mất việc hoặc chi phí y tế bất ngờ.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Để đầu tư vào cổ phiếu", "B": "Chi trả cho các chi phí thiết yếu như tiền thuê nhà và thực phẩm", "C": "Chi tiêu cho du lịch", "D": "Mua sắm đồ điện tử"}, "category": "application", "question": "Gia đình Nguyễn đã sử dụng quỹ khẩn cấp của mình như thế nào trong thời gian đại dịch?", "difficulty": "medium", "explanation": "Gia đình Nguyễn đã sử dụng quỹ khẩn cấp để chi trả cho các chi phí thiết yếu trong thời gian khó khăn do đại dịch.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Không cần làm gì cả", "B": "Lên kế hoạch để tích lũy lại số tiền đã sử dụng", "C": "Tiêu xài hết số tiền còn lại", "D": "Đầu tư vào các cổ phiếu rủi ro"}, "category": "application", "question": "Nếu bạn phải sử dụng một phần quỹ khẩn cấp, bạn nên làm gì tiếp theo?", "difficulty": "medium", "explanation": "Sau khi sử dụng quỹ khẩn cấp, bạn nên lên kế hoạch để tích lũy lại số tiền đã sử dụng để đảm bảo quỹ luôn đầy đủ.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Quỹ khẩn cấp cơ bản dành cho mọi tình huống, quỹ y tế chỉ dành cho chi phí y tế", "B": "Quỹ khẩn cấp cơ bản lớn hơn quỹ y tế", "C": "Quỹ khẩn cấp y tế không cần thiết", "D": "Không có sự khác biệt"}, "category": "comparison", "question": "Điểm khác biệt giữa quỹ khẩn cấp cơ bản và quỹ khẩn cấp cho y tế?", "difficulty": "hard", "explanation": "Quỹ khẩn cấp cơ bản là khoản tiền dự trữ cho mọi tình huống khẩn cấp, trong khi quỹ y tế chỉ dành riêng cho các chi phí y tế bất ngờ.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Khi bạn muốn tiêu tiền", "B": "Khi có thay đổi trong chi phí sinh hoạt", "C": "Chỉ khi có khủng hoảng tài chính", "D": "Không cần điều chỉnh"}, "category": "connection", "question": "Khi nào bạn nên điều chỉnh số tiền trong quỹ khẩn cấp?", "difficulty": "medium", "explanation": "Bạn nên điều chỉnh số tiền trong quỹ khẩn cấp khi có thay đổi trong chi phí sinh hoạt để đảm bảo quỹ luôn đầy đủ.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Bạn không thể đi du lịch", "B": "Bạn sẽ phải vay mượn với lãi suất cao", "C": "Bạn sẽ kiếm được ít tiền hơn", "D": "Không có tác động nào"}, "category": "comparison", "question": "Tại sao việc không có quỹ khẩn cấp có thể dẫn đến rủi ro tài chính cao?", "difficulty": "hard", "explanation": "Không có quỹ khẩn cấp có thể khiến bạn phải vay mượn với lãi suất cao, dẫn đến gánh nặng tài chính trong tương lai.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Giúp bạn tiết kiệm tiền hơn", "B": "Giúp bạn an tâm hơn trong cuộc sống", "C": "Giúp bạn tiêu xài thoải mái", "D": "Giúp bạn đầu tư nhiều hơn"}, "category": "core_concept", "question": "Điều gì là một trong những lợi ích lớn nhất của việc duy trì quỹ khẩn cấp?", "difficulty": "medium", "explanation": "Quỹ khẩn cấp mang lại sự an tâm bởi bạn biết rằng có một nguồn tài chính để đối phó với các tình huống bất ngờ.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Khi bạn có tiền thưởng", "B": "Ngay khi có thu nhập", "C": "Chỉ khi có khủng hoảng", "D": "Khi bạn đã tiết kiệm đủ tiền"}, "category": "core_concept", "question": "Khi nào bạn nên bắt đầu tạo ra quỹ khẩn cấp?", "difficulty": "medium", "explanation": "Nên bắt đầu tạo ra quỹ khẩn cấp ngay khi bạn có thu nhập để đảm bảo bạn luôn có tài chính dự phòng trong trường hợp khẩn cấp.", "correctAnswer": "B"}]', 12, 80, 'Boss Quiz: Tạo và Duy trì Quỹ Khẩn Cấp', '2026-01-22 16:59:48.36', 'gpt-4o-mini', '2026-01-22 16:59:48.40161', '2026-01-22 16:59:48.40161');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('64b5174b-cca5-422f-b084-0e3ec299d2d2', '5ff4b9a4-3e88-4e34-a223-fe0234ce2260', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Nhìn xuống bóng", "B": "Sử dụng cả hai tay", "C": "Khuỵu gối", "D": "Đập bóng nhẹ"}, "category": "definition", "question": "Sai lầm nào dưới đây có thể khiến bạn không quan sát được tình huống xung quanh khi dribble?", "explanation": "A đúng vì nhìn xuống bóng sẽ khiến cầu thủ không quan sát được các đối thủ và tình huống xung quanh. Các đáp án khác không liên quan đến việc quan sát tình huống xung quanh.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Mất kiểm soát bóng", "B": "Tăng tốc độ di chuyển", "C": "Giảm sức ép từ đối thủ", "D": "Nâng cao kỹ thuật cá nhân"}, "category": "definition", "question": "Đập bóng quá mạnh hoặc quá nhẹ có thể dẫn đến điều gì?", "explanation": "A đúng vì đập bóng quá mạnh có thể khiến bóng bay ra xa, trong khi đập quá nhẹ có thể khiến cầu thủ mất kiểm soát. Các đáp án khác không phản ánh được vấn đề thực tế khi dribble.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Đứng thẳng khi dribble", "B": "Nhìn xuống bóng", "C": "Không điều chỉnh lực đập bóng", "D": "Sử dụng một tay"}, "category": "distinction", "question": "Sai lầm nào sau đây thường bị nhầm lẫn với việc không sử dụng cả hai tay khi dribble?", "explanation": "D đúng vì không sử dụng cả hai tay có thể đồng nghĩa với việc chỉ sử dụng một tay. Các đáp án khác liên quan đến các vấn đề khác trong kỹ thuật dribble.", "correctAnswer": "D"}, {"id": "q4", "options": {"A": "Giảm khả năng linh hoạt", "B": "Tăng sức mạnh", "C": "Cải thiện kiểm soát bóng", "D": "Tăng tốc độ di chuyển"}, "category": "definition", "question": "Tư thế đứng thẳng khi dribble có ảnh hưởng như thế nào đến kỹ thuật?", "explanation": "A đúng vì đứng thẳng không giúp cầu thủ giữ thăng bằng và linh hoạt, điều này làm giảm hiệu quả khi dribble. Các đáp án khác không phản ánh đúng ảnh hưởng của tư thế đứng thẳng.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Giữ đầu cao", "B": "Thực hành điều chỉnh lực đập bóng", "C": "Luyện tập với cả hai tay", "D": "Luôn đập bóng mạnh"}, "category": "distinction", "question": "Điều nào dưới đây không phải là một giải pháp để cải thiện kỹ thuật dribble?", "explanation": "D đúng vì việc luôn đập bóng mạnh không phải là giải pháp để cải thiện kỹ thuật dribble mà có thể gây mất kiểm soát. Các đáp án khác đều là những giải pháp hợp lý để cải thiện kỹ thuật dribble.", "correctAnswer": "D"}]', 5, 70, 'Những sai lầm thường gặp khi dribble', '2026-01-22 01:19:07.615', 'gpt-4o-mini', '2026-01-22 01:19:07.656927', '2026-01-22 01:19:07.656927');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('a1142c4c-d6cc-4806-918e-cfd2db352bba', NULL, 'd36bd957-a3e0-4f13-8847-41e19dfcd6bf', 'boss', NULL, '[{"id": "q1", "options": {"A": "Duy trì độ ẩm", "B": "Tăng cường sản xuất dầu", "C": "Giảm nhiệt độ cơ thể", "D": "Tăng cường sắc tố da"}, "category": "core_concept", "question": "Vai trò chính của nước trong việc chăm sóc da là gì?", "difficulty": "medium", "explanation": "Nước đóng vai trò quan trọng trong việc duy trì độ ẩm cho da, giúp da không bị khô và bong tróc.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Sự thải độc cơ thể", "B": "Cung cấp nước cho cơ thể", "C": "Tăng cường tiêu hóa", "D": "Giảm căng thẳng"}, "category": "core_concept", "question": "Hydration có nghĩa là gì?", "difficulty": "medium", "explanation": "Hydration có nghĩa là sự cung cấp nước cho cơ thể, đặc biệt là làn da.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Giúp da sáng màu hơn", "B": "Giúp da giữ ẩm và đàn hồi", "C": "Giảm cảm giác ngứa", "D": "Tăng cường sản xuất tế bào da"}, "category": "core_concept", "question": "Tại sao uống đủ nước hàng ngày lại quan trọng cho da?", "difficulty": "medium", "explanation": "Uống đủ nước giúp da giữ ẩm và tăng cường độ đàn hồi, từ đó làm cho da khỏe mạnh hơn.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Da sáng hơn", "B": "Da khô và xỉn màu", "C": "Da mềm mại hơn", "D": "Da không thay đổi"}, "category": "connection", "question": "Một người đàn ông thường xuyên uống bia và cà phê, da của anh ta sẽ gặp vấn đề gì?", "difficulty": "hard", "explanation": "Uống nhiều bia và cà phê có thể gây mất nước, dẫn đến tình trạng da khô và xỉn màu.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Chất tẩy rửa", "B": "Sản phẩm chứa hyaluronic acid", "C": "Nước hoa", "D": "Sản phẩm chống nắng"}, "category": "connection", "question": "Các sản phẩm nào chứa thành phần giúp duy trì độ ẩm cho da?", "difficulty": "medium", "explanation": "Sản phẩm chứa hyaluronic acid có khả năng giữ nước gấp 1000 lần trọng lượng của nó, giúp da luôn ẩm mượt.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Uống ít nước hơn", "B": "Sử dụng kem dưỡng ẩm thường xuyên", "C": "Tắm nước nóng hàng ngày", "D": "Tránh sử dụng sản phẩm dưỡng da"}, "category": "application", "question": "Khi thời tiết khô, người ta nên làm gì để duy trì độ ẩm cho da?", "difficulty": "medium", "explanation": "Trong thời tiết khô, việc sử dụng kem dưỡng ẩm thường xuyên giúp duy trì độ ẩm cho da.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Da trở nên mềm mại hơn", "B": "Da khô và bong tróc", "C": "Da trở nên sáng hơn", "D": "Da không có thay đổi nào"}, "category": "core_concept", "question": "Khi thiếu nước, làn da có thể gặp vấn đề gì?", "difficulty": "medium", "explanation": "Thiếu nước làm cho da trở nên khô, gây ra tình trạng bong tróc và nhăn nheo.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Da khô bị bong tróc, da ẩm mềm mại", "B": "Da khô sáng hơn, da ẩm xỉn màu", "C": "Da khô không cần chăm sóc, da ẩm cần chăm sóc", "D": "Không có sự khác biệt"}, "category": "comparison", "question": "Sự khác biệt giữa da khô và da ẩm là gì?", "difficulty": "hard", "explanation": "Da khô thường bị bong tróc và cảm giác căng, trong khi da ẩm thì mềm mại và mịn màng.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Dựa vào màu sắc của da", "B": "Dựa vào cảm giác của da sau khi rửa mặt", "C": "Dựa vào tuổi tác", "D": "Dựa vào sản phẩm quảng cáo"}, "category": "application", "question": "Làm thế nào để xác định loại da của bạn khi chọn sản phẩm dưỡng ẩm?", "difficulty": "medium", "explanation": "Cảm giác của da sau khi rửa mặt sẽ giúp bạn xác định loại da của mình và chọn sản phẩm dưỡng ẩm phù hợp.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Giảm bớt sản phẩm dưỡng ẩm", "B": "Tăng cường sử dụng sản phẩm chứa cồn", "C": "Tránh uống nước", "D": "Sử dụng sản phẩm dưỡng ẩm nhiều hơn"}, "category": "application", "question": "Khi môi trường ẩm, bạn nên làm gì để chăm sóc da?", "difficulty": "medium", "explanation": "Trong môi trường ẩm, bạn có thể giảm bớt sản phẩm dưỡng ẩm vì da sẽ tự nhiên giữ ẩm tốt hơn.", "correctAnswer": "A"}, {"id": "q11", "options": {"A": "Tăng cường sắc tố da", "B": "Giúp da giữ nước", "C": "Giảm nếp nhăn", "D": "Tăng cường sản xuất dầu"}, "category": "core_concept", "question": "Hyaluronic Acid có vai trò gì trong chăm sóc da?", "difficulty": "medium", "explanation": "Hyaluronic Acid giúp da giữ nước, từ đó cải thiện độ ẩm và độ đàn hồi của da.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Chế độ ăn uống, tuổi tác, môi trường", "B": "Chỉ tuổi tác", "C": "Chỉ chế độ ăn uống", "D": "Không có yếu tố nào ảnh hưởng"}, "category": "connection", "question": "Những yếu tố nào ảnh hưởng đến khả năng giữ nước của da?", "difficulty": "hard", "explanation": "Chế độ ăn uống, tuổi tác và môi trường đều có ảnh hưởng đến khả năng giữ nước của da.", "correctAnswer": "A"}]', 12, 80, 'Boss Quiz: Vai Trò Của Nước Trong Chăm Sóc Da', '2026-01-22 17:00:24.42', 'gpt-4o-mini', '2026-01-22 17:00:24.461347', '2026-01-22 17:00:24.461347');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('abc44429-d855-4c8f-9903-451a0b3e7087', NULL, 'd772e784-c579-4914-a5e1-3777fff5963b', 'boss', NULL, '[{"id": "q1", "options": {"A": "Xu hướng dữ liệu theo thời gian", "B": "Giá trị trung bình của dữ liệu", "C": "Tổng số lượng dữ liệu", "D": "Tỷ lệ phần trăm của dữ liệu"}, "category": "core_concept", "question": "Biểu đồ đường chủ yếu được sử dụng để thể hiện điều gì?", "difficulty": "medium", "explanation": "Biểu đồ đường thể hiện sự thay đổi của dữ liệu theo thời gian, giúp nhận diện xu hướng tăng trưởng hoặc suy giảm.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Trục x", "B": "Trục y", "C": "Điểm dữ liệu", "D": "Giá trị trung bình"}, "category": "core_concept", "question": "Điểm nào sau đây không phải là một thành phần của biểu đồ đường?", "difficulty": "medium", "explanation": "Biểu đồ đường không có thành phần nào gọi là giá trị trung bình; nó có trục x, trục y và điểm dữ liệu để thể hiện xu hướng.", "correctAnswer": "D"}, {"id": "q3", "options": {"A": "Khi có quá nhiều dữ liệu", "B": "Khi dữ liệu có sự biến động lớn", "C": "Khi dữ liệu không có trục thời gian", "D": "Khi dữ liệu là định tính"}, "category": "core_concept", "question": "Khi nào thì biểu đồ đường không thể hiện được xu hướng một cách rõ ràng?", "difficulty": "hard", "explanation": "Biểu đồ đường yêu cầu có một trục thời gian rõ ràng để thể hiện xu hướng, nếu không có, nó không thể hiện được xu hướng dữ liệu.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Biểu đồ cột", "B": "Biểu đồ hình tròn", "C": "Biểu đồ đường", "D": "Biểu đồ phân tán"}, "category": "core_concept", "question": "Biểu đồ đường nào dưới đây tốt nhất để theo dõi doanh thu theo tháng?", "difficulty": "medium", "explanation": "Biểu đồ đường là công cụ tốt nhất để theo dõi xu hướng doanh thu theo tháng vì nó thể hiện sự thay đổi theo thời gian.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "Giá trị trung bình của doanh thu", "B": "Xu hướng tăng trưởng và suy giảm", "C": "Sự phân bố của doanh thu", "D": "Tổng doanh thu qua các năm"}, "category": "core_concept", "question": "Biểu đồ đường có thể giúp bạn nhận diện điều gì trong dữ liệu về doanh thu?", "difficulty": "medium", "explanation": "Biểu đồ đường rất hiệu quả trong việc thể hiện xu hướng tăng trưởng và suy giảm theo thời gian.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Chọn dữ liệu", "B": "Mở công cụ vẽ biểu đồ", "C": "Thêm dữ liệu vào bảng", "D": "Chọn loại biểu đồ đường"}, "category": "connection", "question": "Thao tác nào sau đây không phải là bước để tạo biểu đồ đường?", "difficulty": "medium", "explanation": "Việc thêm dữ liệu vào bảng không phải là bước trong quá trình tạo biểu đồ đường, mà trước đó bạn cần chuẩn bị dữ liệu.", "correctAnswer": "C"}, {"id": "q7", "options": {"A": "Chỉ số giá tiêu dùng", "B": "Sự kiện kinh tế quan trọng", "C": "Màu sắc của đường biểu đồ", "D": "Độ dày của đường"}, "category": "connection", "question": "Khi phân tích biểu đồ đường của giá cổ phiếu, bạn cần chú ý đến điều gì?", "difficulty": "hard", "explanation": "Sự kiện kinh tế quan trọng có thể ảnh hưởng đến giá cổ phiếu và cần được chú ý khi phân tích biểu đồ.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Dữ liệu bị sai lệch", "B": "Xu hướng tăng trưởng", "C": "Không có gì đặc biệt", "D": "Thời gian không chính xác"}, "category": "connection", "question": "Nếu bạn phát hiện một điểm dữ liệu bất thường trên biểu đồ đường, điều này có thể chỉ ra điều gì?", "difficulty": "hard", "explanation": "Điểm dữ liệu bất thường có thể chỉ ra rằng dữ liệu đó bị sai lệch hoặc có những yếu tố ngoài dự kiến ảnh hưởng đến nó.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Theo dõi doanh thu hàng tháng", "B": "Phân tích sự thay đổi nhiệt độ hàng ngày", "C": "So sánh tỷ lệ phần trăm giữa các nhóm", "D": "Theo dõi biến động giá cổ phiếu"}, "category": "comparison", "question": "Trong trường hợp nào thì biểu đồ đường không nên được sử dụng?", "difficulty": "medium", "explanation": "Biểu đồ đường không phù hợp cho việc so sánh tỷ lệ phần trăm giữa các nhóm; biểu đồ cột hoặc biểu đồ tròn sẽ phù hợp hơn.", "correctAnswer": "C"}, {"id": "q10", "options": {"A": "Tổng doanh thu của từng sản phẩm", "B": "Thời gian biểu diễn của cả hai sản phẩm", "C": "Màu sắc của đường biểu đồ", "D": "Giá bán của từng sản phẩm"}, "category": "comparison", "question": "Khi so sánh biểu đồ đường của hai sản phẩm khác nhau, bạn cần chú ý đến điều gì?", "difficulty": "hard", "explanation": "Thời gian biểu diễn của cả hai sản phẩm cần phải tương đồng để việc so sánh có ý nghĩa và chính xác.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Để làm cho biểu đồ đẹp hơn", "B": "Để người xem dễ hiểu hơn", "C": "Để tăng cường độ khó cho biểu đồ", "D": "Để giảm thời gian tạo biểu đồ"}, "category": "application", "question": "Tại sao việc tùy chỉnh biểu đồ đường là quan trọng?", "difficulty": "medium", "explanation": "Việc tùy chỉnh biểu đồ giúp người xem dễ dàng hiểu thông điệp mà biểu đồ truyền tải.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Thời gian và số lượng sản phẩm bán ra", "B": "Thời gian và doanh thu", "C": "Số lượng khách hàng và doanh thu", "D": "Giá sản phẩm và doanh thu"}, "category": "application", "question": "Dữ liệu nào là cần thiết để tạo biểu đồ đường cho doanh thu hàng tháng?", "difficulty": "medium", "explanation": "Cần có thông tin về thời gian và doanh thu để tạo biểu đồ đường thể hiện xu hướng doanh thu hàng tháng.", "correctAnswer": "B"}, {"id": "q13", "options": {"A": "Khi dữ liệu không thay đổi", "B": "Khi có sự kiện bất thường xảy ra", "C": "Khi biểu đồ quá đơn giản", "D": "Khi bạn muốn thêm nhiều màu sắc"}, "category": "application", "question": "Khi nào thì bạn nên xem xét điều chỉnh lại biểu đồ đường?", "difficulty": "hard", "explanation": "Khi có sự kiện bất thường, cần xem xét điều chỉnh lại biểu đồ để phản ánh chính xác tình hình dữ liệu.", "correctAnswer": "B"}, {"id": "q14", "options": {"A": "Dễ hiểu hơn", "B": "Thể hiện xu hướng theo thời gian tốt hơn", "C": "Thể hiện giá trị tuyệt đối tốt hơn", "D": "Có thể so sánh nhiều bộ dữ liệu cùng lúc"}, "category": "comparison", "question": "So với biểu đồ cột, biểu đồ đường có ưu điểm gì?", "difficulty": "hard", "explanation": "Biểu đồ đường thể hiện xu hướng theo thời gian tốt hơn so với biểu đồ cột, giúp nhận diện các xu hướng tăng trưởng hoặc suy giảm.", "correctAnswer": "B"}, {"id": "q15", "options": {"A": "Màu sắc đường", "B": "Độ dày đường", "C": "Sự rõ ràng của nhãn trục", "D": "Tất cả các yếu tố trên"}, "category": "core_concept", "question": "Điểm nào sau đây là yếu tố quan trọng khi tạo biểu đồ đường?", "difficulty": "medium", "explanation": "Tất cả các yếu tố như màu sắc, độ dày và sự rõ ràng của nhãn đều quan trọng để tạo ra một biểu đồ đường dễ hiểu và trực quan.", "correctAnswer": "D"}]', 15, 80, 'Boss Quiz: Tạo Biểu Đồ Đường', '2026-01-22 17:01:10.043', 'gpt-4o-mini', '2026-01-22 17:01:10.082429', '2026-01-22 17:01:10.082429');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('a6a55246-8d92-4d0d-bf4a-4c018d9132a5', '73e17cfc-7b9b-4fd3-8fed-e5dcf5e5400b', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Stationary Dribble", "B": "Crossover Dribble", "C": "Speed Dribble", "D": "Reverse Dribble"}, "category": "definition", "question": "Loại dribble nào được thực hiện khi cầu thủ đứng tại chỗ và dribble bóng lên xuống quanh người?", "explanation": "A đúng vì Stationary Dribble là kỹ thuật đứng tại chỗ và dribble bóng quanh người. B, C, và D không đúng vì chúng mô tả các kỹ thuật khác. Crossover Dribble liên quan đến việc chuyển bóng giữa hai tay, Speed Dribble là chạy nhanh trong khi dribble, và Reverse Dribble không được đề cập trong nội dung.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Stationary Dribble", "B": "Crossover Dribble", "C": "Speed Dribble", "D": "Bounce Pass"}, "category": "definition", "question": "Kỹ thuật nào được sử dụng chủ yếu để đánh lừa đối thủ bằng cách di chuyển bóng từ tay này sang tay khác?", "explanation": "B đúng vì Crossover Dribble là kỹ thuật chuyển bóng giữa hai tay, giúp cầu thủ đánh lừa đối thủ. A là kỹ thuật đứng yên, C là chạy nhanh trong khi dribble, còn D là một phương pháp chuyền bóng, không liên quan đến dribble.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Speed Dribble cần di chuyển nhanh", "B": "Stationary Dribble không cần bóng", "C": "Speed Dribble không sử dụng tay", "D": "Stationary Dribble chỉ dành cho người mới"}, "category": "distinction", "question": "Điểm khác biệt chính giữa Speed Dribble và Stationary Dribble là gì?", "explanation": "A đúng vì Speed Dribble yêu cầu cầu thủ di chuyển nhanh trong khi dribble, trong khi Stationary Dribble là đứng tại chỗ. B và D không đúng vì Stationary Dribble vẫn sử dụng bóng và không chỉ dành cho người mới. C sai vì cả hai đều sử dụng tay.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Khi bạn cần di chuyển nhanh qua đối thủ", "B": "Khi bạn muốn kiểm soát bóng tốt hơn", "C": "Khi bạn đứng yên và không di chuyển", "D": "Khi bạn muốn đánh lừa đối thủ"}, "category": "distinction", "question": "Khi nào thì bạn nên sử dụng Speed Dribble thay vì Crossover Dribble?", "explanation": "A đúng vì Speed Dribble là kỹ thuật được sử dụng khi cần di chuyển nhanh qua đối thủ. B không đúng vì Crossover Dribble thường giúp kiểm soát bóng trong tình huống đánh lừa, C sai vì Speed Dribble không được sử dụng khi đứng yên, và D là mục tiêu của Crossover Dribble.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Stationary Dribble", "B": "Crossover Dribble", "C": "Speed Dribble", "D": "All of the above"}, "category": "definition", "question": "Trong các loại dribble, loại nào giúp người chơi cải thiện khả năng di chuyển nhanh chóng trên sân?", "explanation": "C đúng vì Speed Dribble giúp người chơi di chuyển nhanh chóng trên sân. A và B không đúng vì Stationary Dribble không liên quan đến di chuyển nhanh và Crossover Dribble chủ yếu dùng để đánh lừa chứ không phải để di chuyển nhanh.", "correctAnswer": "C"}]', 5, 70, 'Các Loại Dribble Cơ Bản', '2026-01-22 01:28:58.902', 'gpt-4o-mini', '2026-01-22 01:28:58.942413', '2026-01-22 01:28:58.942413');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('5a0fae22-2692-4c42-acd7-6ffc526fa13b', NULL, '320c50fc-7675-4eb0-b835-3ae88e246f60', 'boss', NULL, '[{"id": "q1", "options": {"A": "Intro, Verse, Chorus, Bridge, Outro", "B": "Intro, Verse, Solo, Outro", "C": "Chorus, Verse, Outro, Interlude", "D": "Verse, Chorus, Bridge"}, "category": "core_concept", "question": "Cấu trúc bài hát thường bao gồm những phần nào?", "difficulty": "medium", "explanation": "Cấu trúc bài hát chuẩn thường bao gồm Intro, Verse, Chorus, Bridge và Outro, tạo ra một trải nghiệm âm nhạc toàn diện.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Kể chuyện về nội dung", "B": "Truyền tải thông điệp chính và dễ nhớ", "C": "Tạo sự chuyển tiếp", "D": "Thể hiện cảm xúc sâu sắc"}, "category": "core_concept", "question": "Điệp khúc (Chorus) trong một bài hát thường có chức năng gì?", "difficulty": "medium", "explanation": "Điệp khúc là phần nổi bật nhất trong bài hát, thường mang thông điệp chính và dễ nhớ để người nghe có thể hát theo.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Mở đầu bài hát", "B": "Kết thúc bài hát", "C": "Tạo sự bất ngờ và chuyển tiếp", "D": "Diễn tả nội dung chính"}, "category": "core_concept", "question": "Cầu nối (Bridge) trong cấu trúc bài hát thường có vai trò gì?", "difficulty": "medium", "explanation": "Cầu nối tạo ra sự bất ngờ và thay đổi cảm xúc trước khi trở lại với điệp khúc, giúp làm mới bài hát.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Giúp bài hát trở nên dài hơn", "B": "Tạo ra một tổng thể hài hòa và ấn tượng", "C": "Làm cho bài hát khó hiểu hơn", "D": "Chỉ là yếu tố trang trí"}, "category": "connection", "question": "Trong phân tích cấu trúc bài hát, việc liên kết giữa các phần có ý nghĩa gì?", "difficulty": "medium", "explanation": "Mối liên kết giữa các phần giúp tạo ra một tổng thể hài hòa và ấn tượng, làm cho bài hát trở nên dễ tiếp nhận hơn.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Intro", "B": "Verse", "C": "Chorus", "D": "Bridge"}, "category": "application", "question": "Khi phân tích bài hát ''Shape of You'', phần nào được xem là nổi bật và dễ nhớ nhất?", "difficulty": "medium", "explanation": "Phần điệp khúc (Chorus) trong ''Shape of You'' là phần nổi bật và dễ nhớ nhất, thường được lặp lại nhiều lần.", "correctAnswer": "C"}, {"id": "q6", "options": {"A": "Sự buồn bã", "B": "Sự bất ngờ", "C": "Sự lạc quan", "D": "Sự nhàn nhã"}, "category": "core_concept", "question": "Cảm xúc mà phần Bridge trong bài hát thường truyền tải là gì?", "difficulty": "medium", "explanation": "Phần Bridge thường tạo ra sự bất ngờ, giúp làm mới cảm xúc trước khi trở lại với điệp khúc.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Verse", "B": "Chorus", "C": "Bridge", "D": "Outro"}, "category": "comparison", "question": "Khi so sánh giữa phần Verse và Chorus, phần nào thường dài hơn và kể chuyện nhiều hơn?", "difficulty": "medium", "explanation": "Phần Verse thường dài hơn và tập trung vào việc kể chuyện, trong khi Chorus ngắn hơn và dễ nhớ hơn.", "correctAnswer": "A"}, {"id": "q8", "options": {"A": "Intro", "B": "Verse", "C": "Chorus", "D": "Outro"}, "category": "application", "question": "Trong bài hát ''Someone Like You'', phần nào thể hiện rõ nhất nỗi đau và sự mất mát?", "difficulty": "medium", "explanation": "Phần Verse trong ''Someone Like You'' thể hiện rõ ràng nỗi đau và sự mất mát của nhân vật.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Verse", "B": "Chorus", "C": "Bridge", "D": "Outro"}, "category": "core_concept", "question": "Phần nào trong cấu trúc bài hát thường được sử dụng để tạo sự chuyển tiếp giữa các phần?", "difficulty": "medium", "explanation": "Phần Bridge thường được sử dụng để tạo sự chuyển tiếp và làm mới cảm xúc trong bài hát.", "correctAnswer": "C"}, {"id": "q10", "options": {"A": "Giúp cho bài hát trở nên dài hơn", "B": "Tăng cường cảm xúc và sự thu hút", "C": "Giúp dễ dàng nhớ lời bài hát", "D": "Không có tác dụng gì"}, "category": "core_concept", "question": "Khi phân tích cấu trúc bài hát, việc hiểu Logic Harmonique có tác dụng gì?", "difficulty": "hard", "explanation": "Logic Harmonique giúp tăng cường cảm xúc và sự thu hút của bài hát thông qua việc lựa chọn hợp âm và sự chuyển tiếp giữa các phần.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Tình tiết câu chuyện", "B": "Chất lượng âm thanh", "C": "Sự thay đổi giai điệu", "D": "Nội dung lời bài hát"}, "category": "connection", "question": "Một trong những yếu tố quan trọng nhất để tạo sự liên kết giữa các phần trong bài hát là gì?", "difficulty": "hard", "explanation": "Sự thay đổi giai điệu giữa các phần là yếu tố quan trọng để tạo ra sự liên kết và duy trì sự chú ý của người nghe.", "correctAnswer": "C"}, {"id": "q12", "options": {"A": "Để bài hát trở nên dài hơn", "B": "Để tạo ra sự mới mẻ và hấp dẫn", "C": "Để dễ dàng nhớ lời bài hát", "D": "Để tăng thời gian biểu diễn"}, "category": "application", "question": "Khi viết một bài hát, tại sao việc thử nghiệm với cấu trúc khác nhau là quan trọng?", "difficulty": "hard", "explanation": "Thử nghiệm với cấu trúc khác nhau giúp tạo ra sự mới mẻ và hấp dẫn cho bài hát, thu hút người nghe hơn.", "correctAnswer": "B"}]', 12, 80, 'Boss Quiz: Phân Tích Cấu Trúc Bài Hát: Khám Phá Sáng Tạo Âm Nhạc', '2026-01-22 17:02:03.208', 'gpt-4o-mini', '2026-01-22 17:02:03.246642', '2026-01-22 17:02:03.246642');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('1b189719-20a6-4246-9c20-a6c32fcdf32e', NULL, '9177fd5b-8026-4bf6-97a4-829f05d77a16', 'boss', NULL, '[{"id": "q1", "options": {"A": "Quá trình kết hợp các âm thanh khác nhau để tạo thành một tổng thể âm nhạc hài hòa.", "B": "Chỉ đơn giản là chơi nhiều nốt nhạc cùng một lúc.", "C": "Chỉ sử dụng các hợp âm đơn giản trong một bài hát.", "D": "Một thể loại nhạc cụ trong âm nhạc."}, "category": "core_concept", "question": "Hòa âm là gì?", "difficulty": "medium", "explanation": "Hòa âm không chỉ là việc chơi nhiều nốt nhạc cùng một lúc, mà còn là cách các âm thanh tương tác với nhau để tạo ra cảm xúc và màu sắc âm nhạc.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "C, G, Am, F", "B": "D, A, E, B", "C": "C#, G#, A#", "D": "F#, B, D#,"}, "category": "core_concept", "question": "Các hợp âm nào thường được sử dụng trong sáng tác một bản ballad đơn giản?", "difficulty": "medium", "explanation": "C, G, Am, và F là những hợp âm đơn giản, phổ biến trong việc viết ballad và dễ nhớ cho người nghe.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Verse, Chorus, Bridge", "B": "Solo, Refrain, Outro", "C": "Intro, Verse, Solo", "D": "Verse, Pre-Chorus, Coda"}, "category": "core_concept", "question": "Cấu trúc hòa âm trong một bài ballad thường bao gồm những phần nào?", "difficulty": "medium", "explanation": "Một bài ballad điển hình thường có cấu trúc gồm Verse (câu chuyện), Chorus (điệp khúc), và có thể có một Bridge (phần chuyển tiếp).", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Sử dụng nhiều nhạc cụ khác nhau", "B": "Chỉ sử dụng một nhạc cụ duy nhất", "C": "Không thay đổi nhạc cụ trong toàn bộ bài hát", "D": "Luôn giữ cùng một nhịp điệu"}, "category": "core_concept", "question": "Kỹ thuật nào có thể giúp tạo ra âm thanh phong phú khi phối khí?", "dif