INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('394555eb-97c2-4c96-a483-8032ff6dedaf', NULL, '6dc1e2dc-af82-4233-8f65-255cd6fdf4dd', 'boss', NULL, '[{"id": "q1", "options": {"A": "So sánh các giá trị số", "B": "Hiển thị dữ liệu dạng văn bản", "C": "Phân tích dữ liệu hình ảnh", "D": "Lưu trữ dữ liệu"}, "category": "core_concept", "question": "Biểu đồ cột thường được sử dụng để làm gì?", "difficulty": "medium", "explanation": "Biểu đồ cột được sử dụng chủ yếu để so sánh các giá trị số giữa các danh mục khác nhau, cho phép người xem dễ dàng nhận diện sự khác biệt.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Khi muốn thể hiện xu hướng theo thời gian", "B": "Khi cần so sánh số lượng giữa các danh mục", "C": "Khi muốn thể hiện tỷ lệ phần trăm", "D": "Khi dữ liệu quá lớn"}, "category": "core_concept", "question": "Khi nào bạn nên sử dụng biểu đồ cột thay vì biểu đồ đường?", "difficulty": "medium", "explanation": "Biểu đồ cột rất hiệu quả trong việc so sánh số lượng giữa các danh mục, trong khi biểu đồ đường thường dùng để thể hiện xu hướng theo thời gian.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Giúp biểu đồ trở nên đẹp hơn", "B": "Cung cấp thêm thông tin cho người xem", "C": "Giảm bớt số lượng cột cần thiết", "D": "Tăng kích thước cột"}, "category": "core_concept", "question": "Một trong những lợi ích của việc thêm nhãn dữ liệu vào biểu đồ cột là gì?", "difficulty": "medium", "explanation": "Nhãn dữ liệu cung cấp giá trị cụ thể trên mỗi cột, giúp người xem dễ dàng hiểu và phân tích dữ liệu.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Chọn màu sắc đẹp nhất", "B": "Đảm bảo thông tin dễ hiểu và rõ ràng", "C": "Sử dụng nhiều loại cột khác nhau", "D": "Thêm nhiều nhãn không cần thiết"}, "category": "core_concept", "question": "Khi tùy chỉnh biểu đồ cột, điều gì là quan trọng nhất?", "difficulty": "medium", "explanation": "Khi tùy chỉnh biểu đồ, điều quan trọng nhất là đảm bảo thông tin được truyền tải một cách dễ hiểu và rõ ràng cho người xem.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Chỉ cần một cột cho doanh thu", "B": "Hai cột: một cho tháng và một cho doanh thu", "C": "Một cột duy nhất cho tất cả dữ liệu", "D": "Dữ liệu không cần được chuẩn bị"}, "category": "application", "question": "Khi bạn tạo một biểu đồ cột để thể hiện doanh thu hàng tháng, bạn cần chuẩn bị dữ liệu như thế nào?", "difficulty": "medium", "explanation": "Để tạo biểu đồ cột, bạn cần có một cột cho tên tháng và một cột cho doanh thu tương ứng.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Màu sắc nhất quán", "B": "Nhãn rõ ràng", "C": "Dữ liệu không liên quan", "D": "Kích thước cột hợp lý"}, "category": "comparison", "question": "Trong biểu đồ cột, yếu tố nào không góp phần làm cho biểu đồ trở nên hiệu quả?", "difficulty": "hard", "explanation": "Dữ liệu không liên quan sẽ làm cho biểu đồ trở nên khó hiểu và không chính xác, làm mất đi hiệu quả của việc phân tích.", "correctAnswer": "C"}, {"id": "q7", "options": {"A": "Biểu đồ sẽ trở nên dễ hiểu hơn", "B": "Người xem sẽ bị nhầm lẫn", "C": "Không có ảnh hưởng gì", "D": "Biểu đồ sẽ trông đẹp hơn"}, "category": "comparison", "question": "Điều gì xảy ra nếu bạn sử dụng quá nhiều màu sắc trong một biểu đồ cột?", "difficulty": "hard", "explanation": "Sử dụng quá nhiều màu sắc có thể gây nhầm lẫn cho người xem, làm cho thông tin trở nên khó hiểu.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Sử dụng biểu đồ cột để hiển thị số lượng khách hàng", "B": "Sử dụng biểu đồ tròn", "C": "Chỉ sử dụng bảng số liệu", "D": "Không cần biểu đồ"}, "category": "application", "question": "Nếu bạn cần so sánh số lượng khách hàng giữa các tháng, bạn sẽ làm gì?", "difficulty": "medium", "explanation": "Biểu đồ cột là công cụ hiệu quả để so sánh số lượng khách hàng giữa các tháng một cách trực quan.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Cho thấy doanh thu từng tháng", "B": "Cho thấy xu hướng doanh thu theo năm", "C": "So sánh doanh thu giữa các sản phẩm", "D": "Tất cả các đáp án trên"}, "category": "connection", "question": "Biểu đồ cột có thể giúp bạn điều gì trong việc phân tích doanh thu hàng tháng?", "difficulty": "medium", "explanation": "Biểu đồ cột có thể cho bạn cái nhìn tổng quan về doanh thu hàng tháng, xu hướng theo năm và so sánh giữa các sản phẩm.", "correctAnswer": "D"}, {"id": "q10", "options": {"A": "Phân tích xu hướng doanh thu và số lượng khách hàng", "B": "Lên kế hoạch cho các chương trình khuyến mãi", "C": "Đánh giá hiệu quả của các sản phẩm", "D": "Tất cả các đáp án trên"}, "category": "application", "question": "Điều gì có thể hỗ trợ tốt nhất trong việc quyết định chiến lược kinh doanh dựa trên biểu đồ cột?", "difficulty": "hard", "explanation": "Tất cả các yếu tố này đều có thể hỗ trợ quyết định chiến lược kinh doanh dựa trên thông tin từ biểu đồ cột.", "correctAnswer": "D"}, {"id": "q11", "options": {"A": "Để tiết kiệm thời gian", "B": "Để đảm bảo thông tin được truyền tải chính xác", "C": "Để làm cho biểu đồ đẹp hơn", "D": "Không quan trọng lắm"}, "category": "core_concept", "question": "Tại sao việc kiểm tra độ chính xác của dữ liệu là quan trọng khi tạo biểu đồ cột?", "difficulty": "hard", "explanation": "Độ chính xác của dữ liệu là rất quan trọng để đảm bảo rằng thông tin được truyền tải một cách chính xác và không gây nhầm lẫn.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Thay đổi màu sắc cho cột thấp hơn", "B": "Thêm nhãn dữ liệu cho cột cao nhất và thấp nhất", "C": "Tăng kích thước cột cao nhất", "D": "Tất cả các đáp án trên"}, "category": "application", "question": "Nếu bạn muốn thể hiện sự khác biệt giữa doanh thu cao nhất và thấp nhất trong một biểu đồ cột, bạn sẽ làm gì?", "difficulty": "hard", "explanation": "Tất cả các biện pháp này đều có thể giúp người xem dễ dàng nhận diện sự khác biệt giữa doanh thu cao nhất và thấp nhất.", "correctAnswer": "D"}]', 12, 80, 'Boss Quiz: Tạo Biểu Đồ Cột', '2026-01-22 16:51:08.531', 'gpt-4o-mini', '2026-01-22 16:51:08.572077', '2026-01-22 16:51:08.572077');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('ab115a89-993d-4f68-b5fa-2adadcd1a6fb', '4ed65126-e645-4293-bda2-d8b92e34b9ad', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Đánh giá vị trí của đối thủ và khoảng cách đến rổ", "B": "Thực hiện cú ném ngay lập tức", "C": "Chỉ sử dụng tay thuận để ném", "D": "Tìm cách tránh xa đối thủ"}, "category": "definition", "question": "Kỹ thuật ném bóng từ vị trí khó yêu cầu cầu thủ cần phải làm gì đầu tiên?", "explanation": "A đúng vì việc đánh giá vị trí của đối thủ và khoảng cách đến rổ là bước quan trọng đầu tiên trong kỹ thuật ném bóng từ vị trí khó. Các đáp án khác không phù hợp với quy trình thực hiện kỹ thuật này.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Sử dụng động tác giả để đánh lừa đối thủ", "B": "Cố gắng ném bóng ngay lập tức", "C": "Kêu gọi đồng đội hỗ trợ", "D": "Bỏ bóng và rời khỏi vị trí"}, "category": "definition", "question": "Trong tình huống bị kẹp giữa hai cầu thủ, cầu thủ nên làm gì để tạo khoảng trống?", "explanation": "A đúng vì sử dụng động tác giả là một kỹ thuật hiệu quả để tạo khoảng trống trong tình huống này. Các đáp án khác không liên quan hoặc không hiệu quả trong việc tạo khoảng trống.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Kỹ thuật ném bóng từ vị trí khó đòi hỏi sự linh hoạt và nhanh chóng hơn", "B": "Kỹ thuật ném bóng thông thường không cần đánh giá vị trí đối thủ", "C": "Kỹ thuật ném bóng từ vị trí khó chỉ sử dụng một tay", "D": "Kỹ thuật ném bóng thông thường không cần theo dõi đường đi của bóng"}, "category": "distinction", "question": "Kỹ thuật ném bóng từ vị trí khó khác với kỹ thuật ném bóng thông thường như thế nào?", "explanation": "A đúng vì kỹ thuật ném bóng từ vị trí khó yêu cầu cầu thủ phải linh hoạt và nhanh chóng hơn so với ném bóng thông thường, nơi có thể dễ dàng hơn. Các đáp án khác không chính xác hoặc không phản ánh đúng bản chất của hai kỹ thuật này.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Ném bóng từ vị trí khó thường diễn ra trong tình huống bị áp lực", "B": "Ném bóng ba điểm không bao giờ cần đánh lừa đối thủ", "C": "Ném bóng từ vị trí khó yêu cầu phải có hai tay để ném", "D": "Ném bóng ba điểm không cần chú ý đến khoảng cách"}, "category": "distinction", "question": "Kỹ thuật ném bóng từ vị trí khó có điểm khác biệt nào so với việc ném bóng ba điểm?", "explanation": "A đúng vì kỹ thuật ném bóng từ vị trí khó thường xảy ra trong tình huống cầu thủ bị áp lực từ đối thủ, trong khi ném bóng ba điểm có thể thực hiện trong điều kiện thuận lợi hơn. Các đáp án khác không đúng hoặc không liên quan đến sự khác biệt giữa hai kỹ thuật này.", "correctAnswer": "A"}]', 4, 70, 'Kỹ Thuật Ném Bóng Từ Vị Trí Khó', '2026-01-22 00:04:05.377', 'gpt-4o-mini', '2026-01-22 00:04:05.527224', '2026-01-22 00:04:05.527224');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('565dbdd4-59e3-4321-94db-460a2d1a35ca', NULL, '9a86344a-83ea-473f-82fb-0ce95a20d903', 'boss', NULL, '[{"id": "q1", "options": {"A": "Tăng tính hấp dẫn và trực quan cho nội dung", "B": "Giảm dung lượng tệp PowerPoint", "C": "Tăng thời gian trình bày", "D": "Giảm số lượng slide cần thiết"}, "category": "core_concept", "question": "Chèn đối tượng trong PowerPoint giúp gì cho bài thuyết trình?", "difficulty": "medium", "explanation": "Chèn đối tượng như hình ảnh, video và âm thanh làm cho bài thuyết trình trở nên sinh động và hấp dẫn hơn, giúp người xem dễ dàng tiếp thu thông tin.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Kích thước và độ phân giải của hình ảnh", "B": "Màu sắc của hình ảnh", "C": "Nguồn gốc của hình ảnh", "D": "Thời gian chèn hình ảnh"}, "category": "core_concept", "question": "Khi chèn hình ảnh vào slide, bạn cần chú ý điều gì nhất?", "difficulty": "medium", "explanation": "Kích thước và độ phân giải của hình ảnh đảm bảo rằng hình ảnh sẽ hiển thị rõ ràng và không bị méo khi trình chiếu.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Âm thanh phải được phát tự động", "B": "Âm thanh phải có định dạng tương thích với PowerPoint", "C": "Âm thanh phải được ghi âm lại", "D": "Âm thanh phải được chèn vào tất cả các slide"}, "category": "core_concept", "question": "Điều gì là cần thiết để âm thanh chèn vào slide hoạt động hiệu quả?", "difficulty": "medium", "explanation": "Âm thanh cần có định dạng tương thích với PowerPoint (như .mp3) để phát đúng cách trong bài thuyết trình.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Khi bạn muốn giải thích một khái niệm phức tạp", "B": "Khi bạn không có đủ thông tin để trình bày", "C": "Khi bạn muốn tiết kiệm thời gian", "D": "Khi bạn muốn thay thế hoàn toàn phần văn bản"}, "category": "core_concept", "question": "Khi nào bạn nên sử dụng video trong một bài thuyết trình?", "difficulty": "medium", "explanation": "Video có thể giúp giải thích một khái niệm phức tạp một cách trực quan hơn, giúp người xem dễ hiểu hơn.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Hình ảnh thường không cần kiểm tra âm thanh, video thì cần", "B": "Hình ảnh chỉ cần chèn vào một slide, video thì không", "C": "Hình ảnh không có định dạng, video thì có", "D": "Hình ảnh không thể điều chỉnh kích thước, video thì có"}, "category": "comparison", "question": "Hình ảnh và video có điểm gì khác nhau khi chèn vào PowerPoint?", "difficulty": "medium", "explanation": "Khi chèn video, bạn cần đảm bảo rằng âm thanh hoạt động tốt, trong khi hình ảnh không có yếu tố âm thanh đi kèm.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Khi bạn muốn di chuyển chúng dễ dàng cùng nhau", "B": "Khi bạn muốn thay đổi kích thước từng đối tượng", "C": "Khi bạn muốn xóa một đối tượng", "D": "Khi bạn muốn tạo một slide mới"}, "category": "core_concept", "question": "Khi nào bạn nên nhóm các đối tượng trong PowerPoint?", "difficulty": "medium", "explanation": "Nhóm các đối tượng giúp bạn dễ dàng di chuyển và sắp xếp chúng mà không làm thay đổi vị trí của các đối tượng khác.", "correctAnswer": "A"}, {"id": "q7", "options": {"A": "Phải có kết nối internet ổn định", "B": "Video phải có độ dài ngắn hơn 1 phút", "C": "Video phải có âm thanh", "D": "Video phải có định dạng .mp4"}, "category": "application", "question": "Khi chèn video từ internet vào PowerPoint, điều gì cần được lưu ý?", "difficulty": "medium", "explanation": "Nếu video chèn từ internet, bạn cần đảm bảo kết nối internet ổn định để video có thể phát trong suốt bài thuyết trình.", "correctAnswer": "A"}, {"id": "q8", "options": {"A": "Khi bài thuyết trình dài và có nhiều thông tin", "B": "Khi bạn muốn tạo không khí thoải mái cho người nghe", "C": "Khi bạn không có đủ hình ảnh và video", "D": "Khi bạn muốn tăng thời gian trình bày"}, "category": "application", "question": "Trong trường hợp nào bạn nên sử dụng âm thanh nền trong bài thuyết trình?", "difficulty": "medium", "explanation": "Âm thanh nền có thể tạo ra một không khí thoải mái, giúp người nghe dễ dàng tiếp thu thông tin hơn.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Đảm bảo tất cả các đối tượng đều cùng một màu sắc", "B": "Sắp xếp các đối tượng một cách hợp lý và dễ nhìn", "C": "Chọn các đối tượng có kích thước lớn", "D": "Giảm số lượng đối tượng trên slide"}, "category": "application", "question": "Khi tổ chức các đối tượng trên slide, điều gì là quan trọng nhất?", "difficulty": "medium", "explanation": "Sắp xếp các đối tượng một cách hợp lý giúp tạo ra một bố cục dễ nhìn và thu hút người xem.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Chọn hình ảnh có độ phân giải thấp để tiết kiệm dung lượng", "B": "Sử dụng hình ảnh có bản quyền để tránh vi phạm", "C": "Chèn nhiều hình ảnh cùng lúc mà không kiểm tra", "D": "Chọn hình ảnh có màu sắc rực rỡ để thu hút sự chú ý"}, "category": "best_practices", "question": "Điều nào là một thực tiễn tốt nhất khi chèn hình ảnh vào PowerPoint?", "difficulty": "hard", "explanation": "Sử dụng hình ảnh có bản quyền là cần thiết để tránh các vấn đề pháp lý liên quan đến quyền sở hữu trí tuệ.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Hình ảnh có thể tạo ra không gian âm thanh", "B": "Hình ảnh giúp người xem dễ dàng ghi nhớ thông tin hơn", "C": "Hình ảnh không cần kiểm tra âm lượng", "D": "Hình ảnh luôn thu hút sự chú ý hơn âm thanh"}, "category": "comparison", "question": "So với âm thanh, hình ảnh có ưu điểm gì khi chèn vào slide?", "difficulty": "medium", "explanation": "Hình ảnh không có yếu tố âm thanh đi kèm, do đó không cần kiểm tra âm lượng khi trình bày.", "correctAnswer": "C"}, {"id": "q12", "options": {"A": "Kiểm tra trước video trên nhiều thiết bị", "B": "Chọn video có chất lượng kém", "C": "Không cần kiểm tra vì video đã được chèn", "D": "Chỉ chọn video ngắn hơn 30 giây"}, "category": "best_practices", "question": "Làm thế nào để đảm bảo video chèn vào slide phát mượt mà trong bài thuyết trình?", "difficulty": "medium", "explanation": "Kiểm tra video trên nhiều thiết bị giúp đảm bảo tính tương thích và chất lượng khi trình bày.", "correctAnswer": "A"}, {"id": "q13", "options": {"A": "Để hình ảnh có thể chiếm toàn bộ slide", "B": "Để hình ảnh không bị méo và phù hợp với bố cục", "C": "Để hình ảnh có thể chèn vào nhiều slide khác nhau", "D": "Để hình ảnh có thể được nhóm với các đối tượng khác"}, "category": "core_concept", "question": "Tại sao việc điều chỉnh kích thước hình ảnh là quan trọng trong PowerPoint?", "difficulty": "medium", "explanation": "Điều chỉnh kích thước hình ảnh giúp đảm bảo hình ảnh không bị méo và phù hợp với bố cục tổng thể của slide.", "correctAnswer": "B"}, {"id": "q14", "options": {"A": "Khi video có âm thanh", "B": "Khi video cần tạo sự chú ý đặc biệt", "C": "Khi video dài hơn 5 phút", "D": "Khi video không có hình ảnh"}, "category": "application", "question": "Khi nào nên sử dụng hiệu ứng chuyển tiếp cho video trong PowerPoint?", "difficulty": "medium", "explanation": "Sử dụng hiệu ứng chuyển tiếp giúp tạo sự chú ý đặc biệt cho video trong bài thuyết trình.", "correctAnswer": "B"}, {"id": "q15", "options": {"A": "Tổ chức theo hàng lối rõ ràng", "B": "Chồng chéo các đối tượng lên nhau", "C": "Sử dụng màu sắc phù hợp", "D": "Giữ số lượng đối tượng vừa phải"}, "category": "comparison", "question": "Khi tổ chức đối tượng trong slide, điều gì có thể gây rối mắt cho người xem?", "difficulty": "medium", "explanation": "Chồng chéo các đối tượng lên nhau có thể khiến người xem khó chịu và không thể theo dõi nội dung một cách dễ dàng.", "correctAnswer": "B"}]', 15, 80, 'Boss Quiz: Chèn Đối Tượng Trong PowerPoint', '2026-01-22 16:52:03.03', 'gpt-4o-mini', '2026-01-22 16:52:03.072913', '2026-01-22 16:52:03.072913');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('abe79731-d49b-407a-ab05-5dbc619a8c2c', NULL, 'd2b414fb-5274-4213-a74a-33a08abcf0ef', 'boss', NULL, '[{"id": "q1", "options": {"A": "Khả năng mất mát tài chính từ các khoản đầu tư", "B": "Khả năng kiếm lợi nhuận từ các khoản đầu tư", "C": "Khả năng tăng giá trị tài sản", "D": "Khả năng nhận được lãi suất cao"}, "category": "core_concept", "question": "Rủi ro đầu tư là gì?", "difficulty": "medium", "explanation": "Rủi ro đầu tư đề cập đến khả năng mất mát tài chính trong quá trình đầu tư, đây là bản chất của rủi ro đầu tư.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Sự sụp đổ của thị trường chứng khoán", "B": "Sự phát triển không bền vững của thị trường bất động sản", "C": "Sự gia tăng tỷ lệ thất nghiệp", "D": "Sự thay đổi trong chính sách tiền tệ"}, "category": "core_concept", "question": "Khủng hoảng tài chính 2008 chủ yếu do yếu tố nào gây ra?", "difficulty": "medium", "explanation": "Khủng hoảng tài chính 2008 bắt nguồn chủ yếu từ sự phát triển không bền vững của thị trường bất động sản và các khoản vay thế chấp không bảo đảm.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Rủi ro thị trường", "B": "Rủi ro tín dụng", "C": "Rủi ro lạm phát", "D": "Rủi ro tăng trưởng"}, "category": "core_concept", "question": "Điều nào sau đây không phải là loại rủi ro đầu tư?", "difficulty": "medium", "explanation": "Rủi ro tăng trưởng không phải là một loại rủi ro đầu tư được công nhận, trong khi rủi ro thị trường, tín dụng và lạm phát đều là các loại rủi ro thực tế.", "correctAnswer": "D"}, {"id": "q4", "options": {"A": "Rủi ro hệ thống có thể loại bỏ bằng đa dạng hóa", "B": "Rủi ro hệ thống ảnh hưởng đến toàn bộ thị trường", "C": "Rủi ro phi hệ thống không có ảnh hưởng tài chính", "D": "Rủi ro phi hệ thống là rủi ro của toàn bộ hệ thống tài chính"}, "cat