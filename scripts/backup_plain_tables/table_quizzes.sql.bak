INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('ae411644-445a-4e15-b657-2fe2b2a3303c', '33bac6d5-8582-43fc-8035-b89d1d53b9a8', NULL, 'lesson', 'concept', '[{"id": "q1", "options": {"A": "Tự tin vào khả năng của bản thân", "B": "Đợi cho đến khi có sự hỗ trợ từ đồng đội", "C": "Chỉ tập trung vào kỹ thuật", "D": "Tránh mọi áp lực từ bên ngoài"}, "category": "definition", "question": "Yếu tố nào sau đây là cần thiết để cải thiện kỹ thuật ném bóng của bạn?", "explanation": "A đúng vì tự tin là yếu tố quan trọng giúp cầu thủ thực hiện cú ném tốt hơn. Các đáp án khác không trực tiếp hỗ trợ trong việc nâng cao khả năng ném bóng.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Không suy nghĩ gì và chỉ ném", "B": "Tập luyện trong các tình huống áp lực", "C": "Từ bỏ nếu cảm thấy lo lắng", "D": "Chỉ chú ý vào khán giả"}, "category": "definition", "question": "Khi ném bóng dưới áp lực, cầu thủ nên làm gì để giữ bình tĩnh?", "explanation": "B đúng vì tập luyện trong áp lực giúp cầu thủ nâng cao khả năng kiểm soát cảm xúc. Các đáp án khác không hỗ trợ cho việc cải thiện tâm lý khi thi đấu.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Tập trung là về cảm xúc, trong khi tự tin là về kỹ thuật", "B": "Tập trung liên quan đến việc định hướng mục tiêu, tự tin là niềm tin vào khả năng", "C": "Tập trung là cần thiết hơn so với tự tin", "D": "Tự tin là cảm giác, tập trung là hành động"}, "category": "distinction", "question": "Khác biệt chính giữa ''tập trung'' và ''tự tin'' là gì?", "explanation": "B đúng vì tập trung liên quan đến việc chú ý vào mục tiêu trong khi tự tin liên quan đến niềm tin vào khả năng của bản thân. Các đáp án khác không chính xác trong việc phân biệt rõ ràng hai khái niệm này.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Kiểm soát cảm xúc", "B": "Kỹ thuật ném", "C": "Tự tin", "D": "Tập trung"}, "category": "distinction", "question": "Điều nào sau đây không phải là một yếu tố tâm lý trong ném bóng?", "explanation": "B đúng vì kỹ thuật ném là khía cạnh kỹ thuật, không phải yếu tố tâm lý. Các đáp án khác đều là các yếu tố tâm lý quan trọng trong quá trình ném bóng.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Sự hồi hộp", "B": "Sự phấn khích", "C": "Sự tự mãn", "D": "Sự chán nản"}, "category": "distinction", "question": "Cảm xúc nào sau đây cần phải được kiểm soát khi thực hiện cú ném dưới áp lực?", "explanation": "A đúng vì sự hồi hộp có thể ảnh hưởng tiêu cực đến cú ném. Các cảm xúc khác cũng có thể gây xao nhãng nhưng không phải là yếu tố chính cần kiểm soát trong tình huống áp lực.", "correctAnswer": "A"}]', 5, 70, 'Cảm Giác Và Tâm Lý Khi Ném Bóng', '2026-01-21 22:42:38.844', 'gpt-4o-mini', '2026-01-21 22:42:38.886288', '2026-01-21 22:42:38.886288');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('e29cc6c4-bf8e-45ff-b2cb-b73ae89846a3', NULL, 'be67c408-3381-498d-9fb4-1939e30a70ba', 'boss', NULL, '[{"id": "q1", "options": {"A": "Theo dõi một cầu thủ đối phương cụ thể.", "B": "Bảo vệ một khu vực trên sân.", "C": "Tạo áp lực lên toàn bộ đội đối phương.", "D": "Chỉ tập trung vào việc ghi điểm."}, "category": "core_concept", "question": "Phòng thủ cá nhân (man-to-man defense) yêu cầu cầu thủ phòng thủ phải làm gì?", "difficulty": "medium", "explanation": "Phòng thủ cá nhân yêu cầu cầu thủ phòng thủ theo dõi và cản phá một cầu thủ cụ thể của đối phương.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Dễ dàng phản ứng với các tình huống tấn công.", "B": "Giảm thiểu cú ném từ xa khi đối phương không có khả năng ném tốt.", "C": "Tăng cường khả năng phòng thủ cá nhân.", "D": "Giúp cầu thủ phòng thủ tự do di chuyển."}, "category": "core_concept", "question": "Ưu điểm lớn nhất của phòng thủ vùng là gì?", "difficulty": "medium", "explanation": "Phòng thủ vùng giúp bảo vệ rổ và giảm thiểu cú ném từ xa hiệu quả nếu đối phương không có khả năng ném tốt.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Có thể tạo ra khoảng trống nếu không phối hợp tốt.", "B": "Đòi hỏi cầu thủ phải có kỹ năng theo dõi tốt.", "C": "Khó phản ứng với các tình huống tấn công bất ngờ.", "D": "Có thể dễ dàng bị vượt qua nếu cầu thủ phòng thủ không tập trung."}, "category": "core_concept", "question": "Điều nào sau đây không phải là một nhược điểm của phòng thủ cá nhân?", "difficulty": "medium", "explanation": "Phòng thủ cá nhân thường có khả năng phản ứng tốt với tình huống tấn công bất ngờ nếu cầu thủ phòng thủ nắm bắt tình huống nhanh chóng.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Theo dõi cầu thủ đối phương cá nhân.", "B": "Giao tiếp tốt với các đồng đội.", "C": "Chỉ tập trung vào các cú ném gần rổ.", "D": "Luôn đứng yên tại vị trí."}, "category": "core_concept", "question": "Phòng thủ vùng yêu cầu cầu thủ phải làm gì để duy trì hiệu quả?", "difficulty": "medium", "explanation": "Giao tiếp giữa các cầu thủ là rất quan trọng trong phòng thủ vùng để đảm bảo không có khoảng trống nào bị bỏ sót.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "A1 cần cải thiện khả năng phòng ngự.", "B": "B1 là cầu thủ rất giỏi.", "C": "Phòng thủ cá nhân không hiệu quả.", "D": "Không có giải pháp cho tình huống này."}, "category": "application", "question": "Trong một tình huống phòng thủ cá nhân, nếu cầu thủ A1 bị cầu thủ B1 lừa, điều đó cho thấy điều gì?", "difficulty": "hard", "explanation": "Việc bị lừa cho thấy A1 cần cải thiện khả năng đọc tình huống và phản ứng nhanh hơn.", "correctAnswer": "A"}, {"id": "q6", "options": {"A": "Các cầu thủ phòng thủ cần đứng yên.", "B": "Cầu thủ phòng thủ cần nhanh chóng chuyển động để bảo vệ khu vực.", "C": "Chỉ một cầu thủ nên theo sát B2.", "D": "Các cầu thủ phòng thủ cần quay lưng lại với B2."}, "category": "application", "question": "Khi đội A áp dụng phòng thủ vùng và cầu thủ B2 nhận bóng, điều gì nên xảy ra tiếp theo?", "difficulty": "hard", "explanation": "Các cầu thủ phòng thủ cần nhanh chóng di chuyển để bảo vệ khu vực khi cầu thủ đối phương nhận bóng.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Phòng thủ cá nhân theo dõi từng cầu thủ, trong khi phòng thủ vùng bảo vệ khu vực.", "B": "Phòng thủ vùng không cần giao tiếp, trong khi phòng thủ cá nhân thì cần.", "C": "Phòng thủ cá nhân không yêu cầu kỹ năng, trong khi phòng thủ vùng thì có.", "D": "Phòng thủ vùng chỉ dành cho các cầu thủ tấn công."}, "category": "comparison", "question": "Câu hỏi nào sau đây mô tả đúng về sự khác biệt giữa phòng thủ cá nhân và phòng thủ vùng?", "difficulty": "medium", "explanation": "Phòng thủ cá nhân theo dõi từng cầu thủ đối phương, trong khi phòng thủ vùng bảo vệ các khu vực trên sân.", "correctAnswer": "A"}, {"id": "q8", "options": {"A": "Tạo ra khoảng trống cho các cú ném từ xa.", "B": "Khó khăn trong việc theo dõi cầu thủ đối phương.", "C": "Dễ dàng phản ứng với các tình huống tấn công bất ngờ.", "D": "Giảm khả năng bảo vệ rổ."}, "category": "comparison", "question": "Điểm yếu nào sau đây có thể xảy ra khi áp dụng phòng thủ vùng?", "difficulty": "hard", "explanation": "Phòng thủ vùng có thể tạo ra khoảng trống nếu không được tổ chức tốt, đặc biệt là với các cú ném từ xa.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Luôn đứng xa đối thủ.", "B": "Theo dõi và giữ khoảng cách an toàn với cầu thủ đối phương.", "C": "Chỉ tập trung vào việc ghi điểm.", "D": "Không cần di chuyển nhiều."}, "category": "application", "question": "Điều nào sau đây là một trong những cách thực hiện phòng thủ cá nhân hiệu quả?", "difficulty": "medium", "explanation": "Theo dõi và giữ khoảng cách an toàn là rất quan trọng trong phòng thủ cá nhân để ngăn cản các cú ném và di chuyển của đối thủ.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Tăng cường giao tiếp giữa các cầu thủ.", "B": "Đứng yên và chờ đợi.", "C": "Không cần quan tâm đến khu vực khác.", "D": "Chỉ một cầu thủ cần phản ứng."}, "category": "application", "question": "Trong phòng thủ vùng, điều gì quan trọng nhất khi cầu thủ đối phương có bóng?", "difficulty": "hard", "explanation": "Giao tiếp giữa các cầu thủ là cần thiết để tổ chức lại đội hình và bảo vệ rổ hiệu quả khi đối phương có bóng.", "correctAnswer": "A"}, {"id": "q11", "options": {"A": "Bởi vì mỗi cầu thủ có trách nhiệm với một cầu thủ cụ thể.", "B": "Vì phòng thủ cá nhân không cần di chuyển.", "C": "Vì phòng thủ vùng là chiến thuật cũ.", "D": "Không có sự khác biệt nào."}, "category": "comparison", "question": "Tại sao phòng thủ cá nhân lại dễ dàng phản ứng với các tình huống tấn công bất ngờ hơn so với phòng thủ vùng?", "difficulty": "hard", "explanation": "Trong phòng thủ cá nhân, mỗi cầu thủ có trách nhiệm với một cầu thủ đối phương, cho phép phản ứng nhanh hơn với các tình huống tấn công bất ngờ.", "correctAnswer": "A"}, {"id": "q12", "options": {"A": "Giữ khoảng cách gần với cầu thủ đối phương.", "B": "Luôn nhìn về phía rổ.", "C": "Chỉ tập trung vào việc ghi điểm.", "D": "Thực hiện các cú nhảy cao."}, "category": "core_concept", "question": "Khi áp dụng phòng thủ cá nhân, cầu thủ phòng thủ cần chú ý điều gì nhất?", "difficulty": "medium", "explanation": "Giữ khoảng cách gần với cầu thủ đối phương là rất cần thiết để có thể ngăn cản các cú ném và di chuyển của họ.", "correctAnswer": "A"}]', 12, 80, 'Boss Quiz: Phân Tích Chiến Thuật Phòng Thủ Trong Bóng Rổ', '2026-01-22 16:39:15.062', 'gpt-4o-mini', '2026-01-22 16:39:15.222787', '2026-01-22 16:39:15.222787');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('9a4c7330-1e41-4f49-93b3-c6fd889e2722', NULL, '9a370239-b7fc-4ac9-8bbc-3def509c7ead', 'boss', NULL, '[{"id": "q1", "options": {"A": "Sự tắc nghẽn lỗ chân lông", "B": "Thiếu vitamin C", "C": "Sử dụng kem chống nắng", "D": "Thay đổi thời tiết"}, "category": "core_concept", "question": "Mụn thường hình thành do nguyên nhân nào sau đây?", "difficulty": "medium", "explanation": "Mụn hình thành do sự tắc nghẽn của lỗ chân lông bởi bã nhờn, tế bào chết và vi khuẩn.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Hormone không ảnh hưởng đến da", "B": "Hormone làm giảm sản xuất bã nhờn", "C": "Hormone kích thích sản xuất bã nhờn", "D": "Hormone chỉ ảnh hưởng đến tóc"}, "category": "core_concept", "question": "Tại sao hormone có thể gây ra tình trạng mụn?", "difficulty": "medium", "explanation": "Sự thay đổi hormone, đặc biệt trong giai đoạn dậy thì và kinh nguyệt, thường làm tăng sản xuất bã nhờn, dẫn đến mụn.", "correctAnswer": "C"}, {"id": "q3", "options": {"A": "Mụn đầu đen", "B": "Mụn đầu trắng", "C": "Mụn viêm", "D": "Mụn trứng cá"}, "category": "core_concept", "question": "Loại mụn nào thường gây đau và sưng?", "difficulty": "medium", "explanation": "Mụn viêm, bao gồm mụn bọc và mụn mủ, thường gây đau và sưng.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Thiếu nước", "B": "Sử dụng sản phẩm không phù hợp", "C": "Ánh nắng mặt trời", "D": "Sự thay đổi nhiệt độ"}, "category": "core_concept", "question": "Nám da chủ yếu xuất hiện do nguyên nhân nào?", "difficulty": "medium", "explanation": "Ánh nắng mặt trời có thể kích thích sản xuất melanin, dẫn đến nám da.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "Sử dụng sản phẩm chứa axit salicylic", "B": "Rửa mặt hàng ngày", "C": "Sử dụng chế độ ăn chứa nhiều đường", "D": "Tham khảo ý kiến bác sĩ d