INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('55e66b88-2be4-4e3d-a709-51503d2d572f', NULL, 'd825d720-549c-4003-9315-0c5443034518', 'boss', NULL, '[{"id": "q1", "options": {"A": "Hyaluronic Acid", "B": "Vitamin C", "C": "Glycerin", "D": "Salicylic Acid"}, "category": "core_concept", "question": "Thành phần nào dưới đây được coi là chất chống oxy hóa mạnh mẽ trong sản phẩm chăm sóc da?", "difficulty": "medium", "explanation": "Vitamin C là một trong những chất chống oxy hóa mạnh mẽ nhất, giúp bảo vệ da khỏi tác hại của môi trường và cải thiện sự sáng của da.", "correctAnswer": "B"}, {"id": "q2", "options": {"A": "Octinoxate", "B": "Zinc Oxide", "C": "Hyaluronic Acid", "D": "Niacinamide"}, "category": "core_concept", "question": "Chất nào thường được sử dụng trong sản phẩm kem chống nắng để tạo ra một lớp bảo vệ vật lý trên da?", "difficulty": "medium", "explanation": "Zinc Oxide là một loại khoáng chất được sử dụng trong kem chống nắng như một chất bảo vệ vật lý, ngăn chặn tia UVA và UVB.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Vitamin E", "B": "Salicylic Acid", "C": "Glycerin", "D": "Retinol"}, "category": "core_concept", "question": "Thành phần nào được xếp vào nhóm chất giữ ẩm trong sản phẩm chăm sóc da?", "difficulty": "medium", "explanation": "Glycerin là một chất giữ ẩm hiệu quả, có khả năng hút nước từ không khí và giữ lại trong da.", "correctAnswer": "C"}, {"id": "q4", "options": {"A": "Để biết giá sản phẩm", "B": "Để tìm hiểu công dụng của sản phẩm", "C": "Để tránh các thành phần gây dị ứng", "D": "Để biết thương hiệu sản phẩm"}, "category": "core_concept", "question": "Tại sao cần đọc danh sách thành phần trên nhãn sản phẩm chăm sóc da?", "difficulty": "medium", "explanation": "Việc đọc danh sách thành phần giúp người tiêu dùng nhận biết các chất có thể gây dị ứng và lựa chọn sản phẩm an toàn cho mình.", "correctAnswer": "C"}, {"id": "q5", "options": {"A": "Sản phẩm chứa hương liệu", "B": "Sản phẩm không chứa paraben", "C": "Sản phẩm chứa nhiều chất bảo quản", "D": "Sản phẩm chứa AHA"}, "category": "application", "question": "Nếu bạn có làn da nhạy cảm, bạn nên chọn sản phẩm nào dưới đây?", "difficulty": "medium", "explanation": "Sản phẩm không chứa paraben thường an toàn hơn cho làn da nhạy cảm, trong khi các chất khác có thể gây kích ứng.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Nó giúp làm sáng da", "B": "Nó có thể gây kích ứng nếu sử dụng quá mức", "C": "Nó là chất giữ ẩm", "D": "Nó không được khuyến cáo cho da dầu"}, "category": "core_concept", "question": "Khi bạn thấy thành phần ''Retinol'' trong một sản phẩm, điều gì sau đây là đúng?", "difficulty": "hard", "explanation": "Retinol là một dẫn xuất của Vitamin A, có khả năng thúc đẩy tái tạo tế bào nhưng có thể gây kích ứng nếu không được sử dụng đúng cách.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Vitamin C là chất giữ ẩm, Niacinamide là chất chống oxy hóa", "B": "Vitamin C làm sáng da, Niacinamide cải thiện tông màu da", "C": "Niacinamide không có lợi cho da nhờn", "D": "Vitamin C không có tác dụng chống lão hóa"}, "category": "comparison", "question": "Điểm khác biệt nào giữa Vitamin C và Niacinamide trong sản phẩm chăm sóc da?", "difficulty": "hard", "explanation": "Vitamin C giúp làm sáng da và chống oxy hóa, trong khi Niacinamide giúp cải thiện tông màu da và giữ ẩm.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Mức độ bảo vệ khỏi tia UVA", "B": "Mức độ bảo vệ khỏi tia UVB", "C": "Thời gian sử dụng sản phẩm", "D": "Khả năng chống nước của sản phẩm"}, "category": "core_concept", "question": "Khi phân tích nhãn sản phẩm kem chống nắng, chỉ số SPF cho biết điều gì?", "difficulty": "medium", "explanation": "Chỉ số SPF cho biết mức độ bảo vệ của sản phẩm chống lại tia UVB, giúp ngăn ngừa cháy nắng.", "correctAnswer": "B"}, {"id": "q9", "options": {"A": "Hyaluronic Acid", "B": "Vitamin C", "C": "Glycerin", "D": "Peptides"}, "category": "comparison", "question": "Thành phần nào dưới đây thường được khuyến cáo không nên kết hợp với Retinol?", "difficulty": "hard", "explanation": "Sự kết hợp giữa Retinol và Vitamin C có thể gây kích ứng do khác biệt về pH và khả năng gây khô da.", "correctAnswer": "B"}, {"id": "q10", "options": {"A": "Trước khi mua sản phẩm", "B": "Sau khi sử dụng một lần", "C": "Trước khi sử dụng trên toàn bộ mặt", "D": "Khi sản phẩm có hương liệu"}, "category": "application", "question": "Khi nào bạn nên thử nghiệm một sản phẩm mới trên một vùng da nhỏ?", "difficulty": "medium", "explanation": "Thử nghiệm trên một vùng da nhỏ trước khi sử dụng trên toàn bộ mặt giúp xác định xem sản phẩm có gây dị ứng hay không.", "correctAnswer": "C"}, {"id": "q11", "options": {"A": "Mức độ hiệu quả của sản phẩm", "B": "Cách sử dụng sản phẩm để đạt hiệu quả tối ưu", "C": "Giá của sản phẩm", "D": "Thành phần chính của sản phẩm"}, "category": "core_concept", "question": "Hướng dẫn sử dụng trên nhãn sản phẩm thường cung cấp thông tin gì?", "difficulty": "medium", "explanation": "Hướng dẫn sử dụng cung cấp thông tin về cách sử dụng sản phẩm hiệu quả nhất.", "correctAnswer": "B"}, {"id": "q12", "options": {"A": "Sản phẩm chỉ bảo vệ chống lại tia UVB", "B": "Sản phẩm bảo vệ chống lại cả tia UVA và UVB", "C": "Sản phẩm không gây dị ứng", "D": "Sản phẩm có thể sử dụng cho mọi loại da"}, "category": "core_concept", "question": "Khi bạn thấy ''Broad Spectrum'' trên nhãn kem chống nắng, điều đó có nghĩa là gì?", "difficulty": "medium", "explanation": "Broad Spectrum chỉ ra rằng sản phẩm cung cấp bảo vệ chống lại cả tia UVA và UVB, rất quan trọng cho việc bảo vệ da.", "correctAnswer": "B"}]', 12, 80, 'Boss Quiz: Hiểu Biết Về Nhãn Sản Phẩm Chăm Sóc Da', '2026-01-22 16:42:39.366', 'gpt-4o-mini', '2026-01-22 16:42:39.409216', '2026-01-22 16:42:39.409216');
