INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('b447a466-356b-4af3-9a85-055483abc15c', NULL, '55ffda79-3855-4d3d-a344-0a48fcab1d04', 'boss', NULL, '[{"id": "q1", "options": {"A": "3", "B": "4", "C": "5", "D": "2"}, "category": "core_concept", "question": "Giấc ngủ được chia thành bao nhiêu giai đoạn chính?", "difficulty": "medium", "explanation": "Giấc ngủ được chia thành 4 giai đoạn chính: Giấc ngủ nông (NREM), Giấc ngủ sâu, Giấc ngủ REM và giai đoạn chuyển tiếp.", "correctAnswer": "B"}, {"id": "q2", "options": {"A": "Cortisol", "B": "Serotonin", "C": "Hormone tăng trưởng", "D": "Melatonin"}, "category": "core_concept", "question": "Hormone nào được sản xuất nhiều hơn trong giấc ngủ sâu và có vai trò quan trọng trong việc tái tạo tế bào da?", "difficulty": "medium", "explanation": "Hormone tăng trưởng được tiết ra trong giấc ngủ sâu, giúp thúc đẩy sự hồi phục tế bào và tái tạo da.", "correctAnswer": "C"}, {"id": "q3", "options": {"A": "Làn da sáng bóng", "B": "Mụn trứng cá", "C": "Da đều màu", "D": "Da khỏe mạnh"}, "category": "core_concept", "question": "Thiếu ngủ có thể dẫn đến vấn đề gì về da?", "difficulty": "medium", "explanation": "Thiếu ngủ làm gia tăng hormone cortisol, dẫn đến tăng tiết dầu và mụn trứng cá.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Không ảnh hưởng", "B": "Giúp cân bằng hormone", "C": "Chỉ ảnh hưởng đến cortisol", "D": "Tăng cường sản xuất testosterone"}, "category": "core_concept", "question": "Giấc ngủ có ảnh hưởng như thế nào đến hormone trong cơ thể?", "difficulty": "medium", "explanation": "Giấc ngủ giúp duy trì sự cân bằng hormone trong cơ thể, ảnh hưởng tích cực đến sức khỏe da.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Tăng cường sức đề kháng", "B": "Giảm nguy cơ viêm da", "C": "Làm xỉn màu da", "D": "Cải thiện độ đàn hồi da"}, "category": "connec