INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('f6468d61-eb9c-4ecb-a709-b77f029b391b', NULL, 'ba24d13e-ca49-4e63-ac35-d7726870c8f1', 'boss', NULL, '[{"id": "q1", "options": {"A": "Tập hợp các tài sản tài chính mà một cá nhân hoặc tổ chức nắm giữ.", "B": "Một loại tài sản duy nhất.", "C": "Chỉ bao gồm cổ phiếu.", "D": "Một kế hoạch tiết kiệm."}, "category": "core_concept", "question": "Danh mục đầu tư là gì?", "difficulty": "medium", "explanation": "Danh mục đầu tư được định nghĩa là tập hợp các tài sản tài chính mà một cá nhân hoặc tổ chức nắm giữ, bao gồm nhiều loại tài sản khác nhau để tối ưu hóa lợi nhuận và giảm thiểu rủi ro.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Đầu tư vào một loại tài sản duy nhất.", "B": "Đầu tư vào nhiều loại tài sản khác nhau để giảm thiểu rủi ro.", "C": "Chỉ đầu tư vào các cổ phiếu có giá cao.", "D": "Không theo dõi hiệu suất danh mục đầu tư."}, "category": "core_concept", "question": "Phân tán rủi ro trong đầu tư có nghĩa là gì?", "difficulty": "medium", "explanation": "Phân tán rủi ro có nghĩa là đầu tư vào nhiều loại tài sản khác nhau để giảm thiểu tác động của sự biến động giá trị của một loại tài sản duy nhất đến toàn bộ danh mục.", "correctAnswer": "B"}, {"id": "q3", "options": {"A": "Để biết giá trị danh mục đầu tư của bạn.", "B": "Để điều chỉnh danh mục và tối ưu hóa lợi nhuận.", "C": "Để giảm thiểu thuế.", "D": "Để mua thêm cổ phiếu."}, "category": "core_concept", "question": "Tại sao việc theo dõi hiệu suất danh mục đầu tư là cần thiết?", "difficulty": "medium", "explanation": "Theo dõi hiệu suất danh mục đầu tư giúp nhà đầu tư đánh giá và điều chỉnh danh mục để tối ưu hóa lợi nhuận và đảm bảo rằng nó phù hợp với mục tiêu tài chính.", "correctAnswer": "B"}, {"id": "q4", "options": {"A": "Tỷ suất sinh lợi (ROI)", "B": "Chỉ số Sharpe", "C": "Biến động", "D": "Tỷ lệ nợ trên vốn."}, "category": "core_concept", "question": "Chỉ số nào được sử dụng để đo lường mức độ sinh lời so với rủi ro trong danh mục đầu tư?", "difficulty": "hard", "explanation": "Chỉ số Sharpe được sử dụng để đo lường mức sinh lời của một danh mục đầu tư so với rủi ro mà nó mang lại, giúp nhà đầu tư đánh giá hiệu quả đầu tư.", "correctAnswer": "B"}, {"id": "q5", "options": {"A": "Không theo dõi gì cả.", "B": "Sử dụng ứng dụng tài chính để theo dõi hiệu suất hàng tháng.", "C": "Chỉ kiểm tra một lần mỗi năm.", "D": "Chỉ theo dõi cổ phiếu."}, "category": "application", "question": "Nhà đầu tư A đã thực hiện giải pháp nào để theo dõi hiệu suất danh mục đầu tư của mình?", "difficulty": "medium", "explanation": "Nhà đầu tư A đã sử dụng một ứng dụng tài chính để theo dõi hiệu suất danh mục hàng tháng, điều này giúp anh ta quản lý tốt hơn danh mục đầu tư của mình.", "correctAnswer": "B"}, {"id": "q6", "options": {"A": "Đầu tư vào một cổ phiếu là cách tốt nhất để kiếm tiền.", "B": "Phân tán rủi ro là rất quan trọng để bảo vệ tài sản.", "C": "Chỉ cần theo dõi cổ phiếu hàng ngày.", "D": "Không cần điều chỉnh danh mục đầu tư."}, "category": "application", "question": "Nhà đầu tư B đã học được bài học gì từ việc đầu tư vào một cổ phiếu duy nhất?", "difficulty": "medium", "explanation": "Nhà đầu tư B nhận ra rằng việc không phân tán rủi ro đã dẫn đến tổn thất lớn và từ đó quyết định phải đa dạng hóa danh mục đầu tư trong tương lai.", "correctAnswer": "B"}, {"id": "q7", "options": {"A": "Bạn sẽ luôn có lợi nhuận.", "B": "Bạn có thể không đạt được mục tiêu đầu tư.", "C": "Bạn sẽ không cần theo dõi danh mục đầu tư.", "D": "Bạn sẽ có một danh mục đầu tư hoàn hảo mãi mãi."}, "category": "application", "question": "Điều gì có thể xảy ra nếu bạn không điều chỉnh danh mục đầu tư của mình?", "difficulty": "hard", "explanation": "Nếu không điều chỉnh danh mục đầu tư, bạn có thể không đạt được mục tiêu đầu tư vì tỷ lệ các loại tài sản có thể không còn phù hợp với mức độ rủi ro và lợi nhuận mong muốn.", "correctAnswer": "B"}, {"id": "q8", "options": {"A": "Không phân bổ vốn vào trái phiếu.", "B": "Đầu tư vào nhiều loại tài sản.", "C": "Theo dõi hiệu suất danh mục đầu tư hàng tháng.", "D": "Đầu tư vào quỹ chỉ số."}, "category": "comparison", "question": "So với nhà đầu tư A, nhà đầu tư B đã gặp phải vấn đề gì trong danh mục đầu tư?", "difficulty": "medium", "explanation": "Nhà đầu tư B đã đầu tư 200 triệu đồng vào một cổ phiếu duy nhất mà không có sự phân bổ vào trái phiếu, điều này dẫn đến tổn thất lớn khi cổ phiếu giảm giá.", "correctAnswer": "A"}, {"id": "q9", "options": {"A": "Mua và giữ.", "B": "Đầu tư vào một loại tài sản duy nhất.", "C": "Phân tán rủi ro.", "D": "Không đầu tư."}, "category": "comparison", "question": "Chiến lược nào được sử dụng để bảo vệ tài sản khi thị trường biến động?", "difficulty": "hard", "explanation": "Phân tán rủi ro là chiến lược được sử dụng để bảo vệ tài sản trong những thời điểm thị trường biến động, giúp giảm thiểu rủi ro từ sự giảm giá của một loại tài sản nhất định.", "correctAnswer": "C"}, {"id": "q10", "options": {"A": "Chỉ điều chỉnh khi có thua lỗ.", "B": "Điều chỉnh hàng năm hoặc khi tỷ lệ các loại tài sản vượt quá ngưỡng nhất định.", "C": "Không cần điều chỉnh.", "D": "Điều chỉnh mỗi tháng."}, "category": "application", "question": "Làm thế nào để xác định thời điểm điều chỉnh danh mục đầu tư?", "difficulty": "medium", "explanation": "Để duy trì mức độ rủi ro và lợi nhuận mong muốn, nhà đầu tư nên điều chỉnh danh mục hàng năm hoặc khi tỷ lệ các loại tài sản vượt quá ngưỡng nhất định.", "correctAnswer": "B"}, {"id": "q11", "options": {"A": "Giữ nguyên danh mục đầu tư.", "B": "Điều chỉnh danh mục đầu tư cho phù hợp với mục tiêu mới.", "C": "Bán tất cả tài sản.", "D": "Không cần làm gì cả."}, "category": "comparison", "question": "Một nhà đầu tư nên làm gì khi mục tiêu đầu tư của họ thay đổi?", "difficulty": "medium", "explanation": "Khi mục tiêu đầu tư thay đổi, nhà đầu tư cần điều chỉnh danh mục đầu tư để đảm bảo rằng nó vẫn phù hợp với mục tiêu tài chính mới.", "correctAnswer": "B"}]', 11, 80, 'Boss Quiz: Quản lý danh mục đầu tư', '2026-01-22 16:57:45.343', 'gpt-4o-mini', '2026-01-22 16:57:45.384891', '2026-01-22 16:57:45.384891');
INSERT INTO public.quizzes (id, "contentItemId", "learningNodeId", type, "contentType", questions, "totalQuestions", "passingScore", title, "generatedAt", "generationModel", "createdAt", "updatedAt") VALUES ('ca0e34c9-b3b0-424d-964c-f38e2202797b', NULL, '22db2630-70bb-4ef0-9e8f-51f654775cd4', 'boss', NULL, '[{"id": "q1", "options": {"A": "Thế kỷ 18", "B": "Thế kỷ 19", "C": "Thế kỷ 20", "D": "Thế kỷ 21"}, "category": "core_concept", "question": "Piano đã được phát minh vào thế kỷ nào?", "difficulty": "medium", "explanation": "Piano được phát minh bởi Bartolomeo Cristofori vào thế kỷ 18.", "correctAnswer": "A"}, {"id": "q2", "options": {"A": "Verse-chorus", "B": "AABA", "C": "12-bar blues", "D": "Rondo"}, "category": "core_concept", "question": "Bài hát ''Someone Like You'' của Adele sử dụng cấu trúc âm nhạc nào?", "difficulty": "medium", "explanation": "Bài hát ''Someone Like You'' theo định dạng verse-chorus, giúp người nghe dễ dàng nhớ và kết nối với bài hát.", "correctAnswer": "A"}, {"id": "q3", "options": {"A": "Sampling", "B": "Acoustic recording", "C": "Analog synthesis", "D": "Folk music fusion"}, "category": "core_concept", "question": "Kỹ thuật nào được sử dụng để tạo ra âm thanh piano trong nhạc điện tử?", "difficulty": "medium", "explanation": "Kỹ thuật sample piano cho phép các nghệ sĩ tạo ra nhiều âm sắc khác nhau từ âm thanh cổ điển đến âm thanh hiện đại.", "correctAnswer": "A"}, {"id": "q4", "options": {"A": "Tạo giai điệu chính", "B": "Thay thế giọng hát", "C": "Chỉ làm nền", "D": "Không có vai trò rõ ràng"}, "category": "core_concept", "question": "Âm thanh của piano trong bài hát ''Titanium'' của David Guetta có vai trò gì?", "difficulty": "medium", "explanation": "Âm thanh piano trong bài hát ''Titanium'' được sử dụng để tạo ra giai điệu chính, giúp bài hát trở nên thu hút và dễ nhớ hơn.", "correctAnswer": "A"}, {"id": "q5", "options": {"A": "Điệp khúc mạnh mẽ", "B": "Hợp âm phức tạp", "C": "Nền harmonization nhẹ nhàng", "D": "Sử dụng nhạc cụ khác"