INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('02d8712d-d294-4baf-b7d8-6efa00782da5', 'f809ae85-28d5-443b-83de-ee5cefcf69f2', 'concept', 'Hiểu Về Tia UV', 'Tia UV là bức xạ từ mặt trời có thể gây hại cho da. Có hai loại tia UV chính: UVA và UVB. Tia UVA có thể gây lão hóa da sớm, trong khi tia UVB có thể gây cháy nắng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa tia UV chiếu xuống mặt đất, với các biểu tượng phân loại UVA và UVB.", "videoScript": "Chào mừng bạn đến với bài học về tia UV. Trong video này, chúng ta sẽ khám phá những điều cần biết về tia UV và tác động của chúng đến sức khỏe làn da.", "imageDescription": "Một bức tranh mô tả tia UV từ mặt trời, với các mũi tên chỉ vào các loại tia UVA và UVB.", "videoDescription": "Video giải thích về tia UV, các loại của chúng và tác động đối với da."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:14:15.646153', '2026-01-27 13:14:15.646153', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tia UV là gì và tại sao chúng ta cần bảo vệ da khỏi chúng?", "detailed": "Tia UV là bức xạ từ mặt trời có thể gây hại cho da. Có hai loại tia UV chính: UVA và UVB. Tia UVA có thể gây lão hóa da sớm, trong khi tia UVB có thể gây cháy nắng.", "comprehensive": "Tia UV (Ultraviolet) từ mặt trời có hai loại chính là UVA và UVB. Tia UVA có khả năng xuyên sâu vào da, gây ra lão hóa và các vấn đề như nám. Tia UVB thường gây ra các tình trạng da như cháy nắng và có thể dẫn đến ung thư da. Việc hiểu rõ về tia UV sẽ giúp chúng ta có biện pháp bảo vệ hiệu quả hơn."}');
