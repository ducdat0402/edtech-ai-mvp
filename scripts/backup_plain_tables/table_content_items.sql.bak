INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7730111a-2174-436d-b308-bbfd60cfa9df', 'c15caccd-cd02-4caa-8282-c520cd1680ca', 'boss_quiz', 'Boss Quiz: Thói quen dưỡng ẩm hàng ngày', 'Kiểm tra kiến thức về Thói quen dưỡng ẩm hàng ngày', NULL, 100, '{"xp": 50, "coin": 10}', '{"options": ["A. Sau khi ăn", "B. Sau khi làm sạch da", "C. Khi đi ra ngoài", "D. Trước khi ngủ"], "question": "Khi nào là thời điểm tốt nhất để sử dụng kem dưỡng ẩm?", "explanation": "Sử dụng kem dưỡng ẩm sau khi làm sạch da sẽ giúp khóa ẩm tốt hơn.", "correctAnswer": 1}', '2026-01-27 21:09:28.537697', '2026-01-27 21:09:28.537697', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f6eebdbd-859b-4c6f-accd-07f6594b05a3', 'c28c9245-826f-4ddf-aa17-09815d63a233', 'concept', 'Nhận diện âm thanh từ các phím', 'Mỗi phím trên piano đại diện cho một nốt nhạc khác nhau. Khi nhấn phím, âm thanh phát ra sẽ tương ứng với tần số cụ thể của nốt đó.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh cận cảnh các phím trắng và đen trên piano.", "videoScript": "Chúng ta sẽ lắng nghe âm thanh từ từng phím trên piano và nhận diện các nốt nhạc.", "imageDescription": "Một cái nhìn gần hơn vào các phím của piano, cho thấy sự phân bố giữa các phím trắng và đen.", "videoDescription": "Video hướng dẫn người xem cách nhận diện âm thanh từ các phím trên piano bằng cách chơi từng nốt một."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:48:52.656154', '2026-01-27 12:48:52.656154', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mỗi phím trên piano tạo ra một âm thanh khác nhau.", "detailed": "Mỗi phím trên piano đại diện cho một nốt nhạc khác nhau. Khi nhấn phím, âm thanh phát ra sẽ tương ứng với tần số cụ thể của nốt đó.", "comprehensive": "Trên piano, có 88 phím, trong đó có 52 phím trắng và 36 phím đen. Mỗi phím trắng tương ứng với một nốt nhạc trong thang âm C major, trong khi các phím đen là các nốt tăng và giảm. Khi bạn nhấn một phím, búa bên trong sẽ gõ vào dây, tạo ra âm thanh với tần số đặc trưng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cd216648-a87e-459c-9d1c-cbf1de59e833', 'c28c9245-826f-4ddf-aa17-09815d63a233', 'concept', 'Khám phá âm sắc của piano', 'Âm sắc của piano không chỉ phụ thuộc vào nốt mà còn phụ thuộc vào lực nhấn và tốc độ của ngón tay khi chơi.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhạc công đang chơi piano với nhiều cảm xúc.", "videoScript": "Trong video này, chúng ta sẽ khám phá cách âm sắc của piano thay đổi khi bạn điều chỉnh lực nhấn.", "imageDescription": "Một nhạc công thể hiện cảm xúc qua từng nốt nhạc trên piano.", "videoDescription": "Video minh họa sự thay đổi âm sắc trên piano khi chơi với các kỹ thuật khác nhau."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:48:52.659426', '2026-01-27 12:48:52.659426', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Âm sắc của piano có thể thay đổi tùy thuộc vào cách chơi.", "detailed": "Âm sắc của piano không chỉ phụ thuộc vào nốt mà còn phụ thuộc vào lực nhấn và tốc độ của ngón tay khi chơi.", "comprehensive": "Âm sắc của piano được xác định bởi nhiều yếu tố, bao gồm lực nhấn, tốc độ gõ phím và kỹ thuật chơi. Khi bạn gõ phím nhẹ nhàng, âm thanh sẽ mềm mại và nhẹ nhàng, trong khi gõ mạnh sẽ tạo ra âm thanh to và đầy đặn. Điều này cho phép người chơi tạo ra nhiều sắc thái âm nhạc khác nhau."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('77a5ab05-716c-4fb5-941d-7d0bd603c72a', '3eadfef9-f249-4920-8bd0-9151864b7c67', 'concept', 'Giới thiệu về In ấn Tài liệu', 'In ấn tài liệu bao gồm các bước như chuẩn bị nội dung, chọn loại giấy, định dạng tài liệu và sử dụng máy in phù hợp.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một máy in đa chức năng đang in tài liệu.", "videoScript": "Chào mừng các bạn đến với bài học về in ấn tài liệu. Hôm nay, chúng ta sẽ tìm hiểu quy trình in ấn tài liệu từ đầu đến cuối.", "imageDescription": "Một máy in đang hoạt động, với tài liệu đang được in ra từ khay giấy.", "videoDescription": "Video hướng dẫn quy trình in ấn tài liệu, từ việc chuẩn bị nội dung cho đến sản phẩm cuối cùng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:29:11.96705', '2026-01-27 12:29:11.96705', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "In ấn tài liệu là quá trình chuyển đổi thông tin từ định dạng kỹ thuật số sang giấy.", "detailed": "In ấn tài liệu bao gồm các bước như chuẩn bị nội dung, chọn loại giấy, định dạng tài liệu và sử dụng máy in phù hợp.", "comprehensive": "In ấn tài liệu là một quy trình quan trọng trong việc truyền đạt thông tin. Nó bao gồm việc thiết kế tài liệu, chọn loại giấy, định dạng và in ấn. Các công nghệ in hiện đại như in kỹ thuật số và in offset giúp nâng cao chất lượng và hiệu suất in ấn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('67c9dc17-6cdb-4bda-9129-3d0ed0477782', '3eadfef9-f249-4920-8bd0-9151864b7c67', 'concept', 'Các loại máy in trong In ấn Tài liệu', 'Máy in phun thích hợp cho in màu sắc sống động, máy in laser cho tài liệu đen trắng nhanh và máy in kim thường dùng cho hóa đơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại máy in khác nhau.", "videoScript": "Hãy cùng tìm hiểu các loại máy in phổ biến trong in ấn tài liệu và cách thức hoạt động của chúng.", "imageDescription": "Một bức ảnh mô tả ba loại máy in: máy in phun, máy in laser và máy in kim.", "videoDescription": "Video giới thiệu các loại máy in và ứng dụng của chúng trong in ấn tài liệu."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:29:11.976261', '2026-01-27 12:29:11.976261', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại máy in như máy in phun, máy in laser và máy in kim.", "detailed": "Máy in phun thích hợp cho in màu sắc sống động, máy in laser cho tài liệu đen trắng nhanh và máy in kim thường dùng cho hóa đơn.", "comprehensive": "Trong lĩnh vực in ấn tài liệu, có các loại máy in chính: máy in phun, máy in laser, và máy in kim. Máy in phun tạo ra chất lượng màu sắc tốt, máy in laser có tốc độ in nhanh và độ bền cao, trong khi máy in kim được sử dụng chủ yếu cho các tài liệu cần in liên tục như hóa đơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cbb60edd-d535-4436-8fca-1ac1efa652ec', 'c28c9245-826f-4ddf-aa17-09815d63a233', 'concept', 'Nhận diện âm thanh qua hợp âm', 'Hợp âm được tạo ra khi hai hoặc nhiều nốt nhạc được chơi cùng lúc. Điều này tạo ra âm thanh phong phú hơn so với chơi một nốt đơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một bản nhạc với các hợp âm được ghi chú.", "videoScript": "Chúng ta sẽ cùng nhau chơi một số hợp âm cơ bản trên piano và lắng nghe âm thanh của chúng.", "imageDescription": "Một trang nhạc với các hợp âm rõ ràng, cho thấy cách chúng được kết hợp trong một bản nhạc.", "videoDescription": "Video giới thiệu các hợp âm cơ bản trên piano và cách chúng phát ra âm thanh khi chơi cùng nhau."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:48:52.663888', '2026-01-27 12:48:52.663888', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm là sự kết hợp của nhiều nốt nhạc.", "detailed": "Hợp âm được tạo ra khi hai hoặc nhiều nốt nhạc được chơi cùng lúc. Điều này tạo ra âm thanh phong phú hơn so với chơi một nốt đơn.", "comprehensive": "Hợp âm trên piano là sự kết hợp của ít nhất hai nốt nhạc cùng một lúc. Có nhiều loại hợp âm, chẳng hạn như hợp âm trưởng, hợp âm thứ và hợp âm bảy. Mỗi loại hợp âm mang lại một cảm xúc và sắc thái âm nhạc khác nhau. Việc nhận diện âm thanh của các hợp âm giúp người chơi hiểu rõ hơn về cấu trúc âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3e007f87-37ef-4d92-8d17-bf00c7edde1e', 'c28c9245-826f-4ddf-aa17-09815d63a233', 'concept', 'Thực hành nhận diện âm thanh', 'Để nhận diện âm thanh piano tốt hơn, bạn nên thực hành chơi và nghe các nốt và hợp âm khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhạc công đang thực hành piano.", "videoScript": "Trong video này, chúng ta sẽ thực hành nhận diện âm thanh qua các bài tập đơn giản.", "imageDescription": "Một nhạc công chăm chú thực hành trên piano, thể hiện sự tập trung và nghiêm túc.", "videoDescription": "Video hướng dẫn thực hành nhận diện âm thanh trên piano với các bài tập cụ thể."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:48:52.667289', '2026-01-27 12:48:52.667289', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thực hành là cách tốt nhất để nhận diện âm thanh.", "detailed": "Để nhận diện âm thanh piano tốt hơn, bạn nên thực hành chơi và nghe các nốt và hợp âm khác nhau.", "comprehensive": "Việc thực hành nhận diện âm thanh là rất quan trọng trong việc học piano. Bằng cách nghe và chơi các nốt nhạc và hợp âm, người học có thể phát triển khả năng nghe nhạc và cải thiện kỹ năng chơi đàn. Hãy thử chơi từng nốt và hợp âm, sau đó nghe lại để hiểu rõ hơn về âm thanh mà bạn tạo ra."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('97293b8b-0fce-46ac-b9c8-cff54fe8dd75', '980807c3-7f72-4f43-ad22-d0670b74a227', 'concept', 'Giới Thiệu Về Kỹ Thuật Tay Trái', 'Kỹ thuật tay trái trong piano không chỉ là việc chơi nốt đơn lẻ mà còn bao gồm việc phối hợp với tay phải để tạo ra một bản nhạc hòa quyện.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nghệ sĩ piano đang chơi với tay trái rõ ràng, thể hiện kỹ thuật ngón tay.", "videoScript": "Xin chào các bạn! Hôm nay, chúng ta sẽ khám phá kỹ thuật tay trái trong piano. Hãy cùng nhau tìm hiểu cách để chơi một cách hiệu quả.", "imageDescription": "Một nghệ sĩ piano đang thể hiện kỹ thuật tay trái, với ngón tay đặt đúng trên phím đàn.", "videoDescription": "Video hướng dẫn về kỹ thuật tay trái trong piano, bao gồm các mẹo và bài tập để cải thiện."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:46.232213', '2026-01-27 12:52:46.232213', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kỹ thuật tay trái là rất quan trọng trong piano. Nó giúp tạo ra âm thanh đầy đủ hơn.", "detailed": "Kỹ thuật tay trái trong piano không chỉ là việc chơi nốt đơn lẻ mà còn bao gồm việc phối hợp với tay phải để tạo ra một bản nhạc hòa quyện.", "comprehensive": "Kỹ thuật tay trái trong piano bao gồm các khía cạnh như vị trí ngón tay, cách sử dụng sức mạnh, và khả năng phối hợp với tay phải. Việc luyện tập tay trái sẽ giúp tạo ra âm thanh phong phú và cân bằng cho bản nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6b32d945-47a8-4cc6-a000-f8afefad2e26', '3eadfef9-f249-4920-8bd0-9151864b7c67', 'concept', 'Chọn giấy in phù hợp', 'Khi chọn giấy in, cần xem xét độ dày, độ bóng và loại giấy tùy thuộc vào mục đích sử dụng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại giấy in khác nhau.", "videoScript": "Trong phần này, chúng ta sẽ thảo luận về cách chọn giấy in phù hợp cho từng loại tài liệu.", "imageDescription": "Một bức ảnh hiển thị các loại giấy in khác nhau, bao gồm giấy A4, giấy mỹ thuật và giấy photocopy.", "videoDescription": "Video giải thích sự khác biệt giữa các loại giấy in và cách chọn lựa chúng cho các nhu cầu in ấn khác nhau."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:29:11.981651', '2026-01-27 12:29:11.981651', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giấy in có nhiều loại như giấy A4, giấy mỹ thuật và giấy photocopy.", "detailed": "Khi chọn giấy in, cần xem xét độ dày, độ bóng và loại giấy tùy thuộc vào mục đích sử dụng.", "comprehensive": "Việc chọn giấy in phù hợp rất quan trọng trong in ấn tài liệu. Giấy A4 thường dùng cho văn bản thông thường, giấy mỹ thuật thích hợp cho in ấn tài liệu quảng cáo, trong khi giấy photocopy là lựa chọn phổ biến cho in ấn hàng ngày. Cần xem xét các yếu tố như độ dày, độ bóng và khả năng tương thích với máy in."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('23d8b591-6ff3-43fa-8c3e-e916805bb515', '3eadfef9-f249-4920-8bd0-9151864b7c67', 'concept', 'Kỹ thuật in ấn cơ bản', 'In một mặt là in trên một bên của trang, trong khi in hai mặt là in trên cả hai bên.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một tài liệu được in một mặt và hai mặt.", "videoScript": "Chúng ta sẽ xem xét các kỹ thuật in ấn cơ bản và cách thực hiện chúng.", "imageDescription": "Bức ảnh mô tả sự khác biệt giữa tài liệu in một mặt và tài liệu in hai mặt.", "videoDescription": "Video hướng dẫn về các kỹ thuật in ấn cơ bản và cách thiết lập máy in."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:29:11.985941', '2026-01-27 12:29:11.985941', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kỹ thuật in ấn cơ bản bao gồm in một mặt và in hai mặt.", "detailed": "In một mặt là in trên một bên của trang, trong khi in hai mặt là in trên cả hai bên.", "comprehensive": "Kỹ thuật in ấn cơ bản bao gồm nhiều phương pháp như in một mặt và in hai mặt. In một mặt thường dùng cho tài liệu không cần thiết phải in cả hai bên, trong khi in hai mặt tiết kiệm giấy và thường được sử dụng cho sách hoặc tài liệu dài. Cần hiểu rõ cách thiết lập máy in để thực hiện các kỹ thuật này hiệu quả."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6bfac8ee-1ef4-4510-a957-edea624f46da', '3eadfef9-f249-4920-8bd0-9151864b7c67', 'concept', 'Tối ưu hóa chất lượng in', 'Để có chất lượng in tốt, cần điều chỉnh độ phân giải của tài liệu và chọn chế độ in phù hợp.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh cài đặt máy in với các tùy chọn chất lượng.", "videoScript": "Để có chất lượng in tốt nhất, hãy cùng tìm hiểu cách tối ưu hóa các cài đặt máy in.", "imageDescription": "Một bức ảnh hiển thị màn hình cài đặt máy in với các tùy chọn chất lượng khác nhau.", "videoDescription": "Video hướng dẫn cách tối ưu hóa chất lượng in thông qua các cài đặt máy in và điều chỉnh tài liệu."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:29:11.991276', '2026-01-27 12:29:11.991276', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tối ưu hóa chất lượng in là điều chỉnh độ phân giải và cài đặt máy in.", "detailed": "Để có chất lượng in tốt, cần điều chỉnh độ phân giải của tài liệu và chọn chế độ in phù hợp.", "comprehensive": "Tối ưu hóa chất lượng in là một bước quan trọng trong quy trình in tài liệu. Điều này bao gồm việc điều chỉnh độ phân giải của tài liệu trước khi in, chọn chế độ in (chẳng hạn như chế độ chất lượng cao hoặc thông thường) và kiểm tra các cài đặt máy in để đảm bảo rằng màu sắc và độ rõ nét được tối ưu hóa."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f79ffeb3-7d59-4c2b-b3cb-edfe6728b426', '9177fd5b-8026-4bf6-97a4-829f05d77a16', 'concept', 'Giới thiệu về Hòa Âm', 'Hòa âm là quá trình sắp xếp các âm thanh theo cách mà chúng có thể tương tác và hỗ trợ lẫn nhau, tạo nên một bản nhạc có chiều sâu và cảm xúc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bức tranh về một nhạc sĩ đang sáng tác hòa âm trên đàn piano.", "videoScript": "Chào mừng đến với bài học về hòa âm. Hôm nay, chúng ta sẽ khám phá những khía cạnh cơ bản của nghệ thuật phối hợp âm thanh.", "imageDescription": "Một nhạc sĩ ngồi bên cây đàn piano, xung quanh là nốt nhạc và các bản hòa âm đang được viết.", "videoDescription": "Video giới thiệu về hòa âm trong âm nhạc, giải thích khái niệm và tầm quan trọng của nó."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:49:21.940258', '2026-01-27 12:49:21.940258', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hòa âm là nghệ thuật kết hợp các âm thanh lại với nhau.", "detailed": "Hòa âm là quá trình sắp xếp các âm thanh theo cách mà chúng có thể tương tác và hỗ trợ lẫn nhau, tạo nên một bản nhạc có chiều sâu và cảm xúc.", "comprehensive": "Hòa âm không chỉ đơn thuần là việc kết hợp các âm thanh mà còn là nghệ thuật tạo ra sự hòa quyện giữa các giai điệu, nhịp điệu và sắc thái. Nó bao gồm việc hiểu biết về các hợp âm, cấu trúc nhạc, và cách mà các âm thanh có thể tương tác để tạo ra một trải nghiệm âm nhạc phong phú."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d231b0eb-6640-4ad7-a26b-5f38ba6089c7', '12b4d5d2-630e-48c5-8f8e-9c5563030ef4', 'concept', 'Giới thiệu về Microsoft Word', 'Microsoft Word là một ứng dụng xử lý văn bản do Microsoft phát triển, cho phép người dùng tạo, chỉnh sửa và định dạng văn bản một cách dễ dàng và hiệu quả.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giao diện chính của Microsoft Word.", "videoScript": "Giới thiệu về Microsoft Word và các chức năng cơ bản của nó.", "imageDescription": "Giao diện người dùng của Microsoft Word với các công cụ và menu.", "videoDescription": "Video hướng dẫn giới thiệu về Microsoft Word, bao gồm các chức năng chính và cách sử dụng cơ bản."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:29:36.291392', '2026-01-27 12:29:36.291392', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Microsoft Word là một phần mềm xử lý văn bản phổ biến.", "detailed": "Microsoft Word là một ứng dụng xử lý văn bản do Microsoft phát triển, cho phép người dùng tạo, chỉnh sửa và định dạng văn bản một cách dễ dàng và hiệu quả.", "comprehensive": "Microsoft Word là phần mềm xử lý văn bản hàng đầu được sử dụng rộng rãi trong các lĩnh vực giáo dục, kinh doanh và cá nhân. Nó cung cấp nhiều tính năng mạnh mẽ như định dạng văn bản, chèn hình ảnh, tạo bảng và nhiều công cụ hỗ trợ khác giúp người dùng tạo ra các tài liệu chuyên nghiệp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f023962b-4d6b-411f-9a16-355b975ad435', '12b4d5d2-630e-48c5-8f8e-9c5563030ef4', 'concept', 'Cách tạo và lưu tài liệu mới', 'Để tạo tài liệu mới trong Microsoft Word, bạn chỉ cần vào menu ''File'', chọn ''New'', và sau đó chọn mẫu hoặc tài liệu trống. Đừng quên lưu tài liệu sau khi hoàn thành.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh hướng dẫn tạo tài liệu mới trong Microsoft Word.", "videoScript": "Hướng dẫn cách tạo và lưu tài liệu mới trong Microsoft Word.", "imageDescription": "Các bước để tạo tài liệu mới trong giao diện Microsoft Word.", "videoDescription": "Video hướng dẫn chi tiết về cách tạo và lưu tài liệu mới trong Microsoft Word."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:29:36.300164', '2026-01-27 12:29:36.300164', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể tạo tài liệu mới bằng cách chọn ''File'' và sau đó ''New''.", "detailed": "Để tạo tài liệu mới trong Microsoft Word, bạn chỉ cần vào menu ''File'', chọn ''New'', và sau đó chọn mẫu hoặc tài liệu trống. Đừng quên lưu tài liệu sau khi hoàn thành.", "comprehensive": "Để tạo một tài liệu mới trong Microsoft Word, trước tiên, bạn mở ứng dụng và vào menu ''File''. Tại đây, bạn chọn ''New'' và có thể chọn từ các mẫu có sẵn hoặc tạo một tài liệu trống. Sau khi soạn thảo xong, hãy nhớ lưu tài liệu bằng cách chọn ''Save'' hoặc ''Save As'' để đặt tên và chọn vị trí lưu trữ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3d74aff6-e62d-45ae-abbc-4f5d767980fe', '12b4d5d2-630e-48c5-8f8e-9c5563030ef4', 'concept', 'Định dạng văn bản cơ bản', 'Định dạng văn bản trong Microsoft Word bao gồm việc thay đổi kiểu chữ, kích thước, màu sắc và các thuộc tính khác như đậm, nghiêng hay gạch chân.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh mô tả các công cụ định dạng văn bản trong Microsoft Word.", "videoScript": "Hướng dẫn định dạng văn bản cơ bản trong Microsoft Word.", "imageDescription": "Các công cụ định dạng văn bản trong thanh công cụ của Microsoft Word.", "videoDescription": "Video hướng dẫn cách định dạng văn bản cơ bản trong Microsoft Word, bao gồm các kiểu chữ, kích thước, và màu sắc."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:29:36.303729', '2026-01-27 12:29:36.303729', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể thay đổi kiểu chữ, kích thước và màu sắc văn bản.", "detailed": "Định dạng văn bản trong Microsoft Word bao gồm việc thay đổi kiểu chữ, kích thước, màu sắc và các thuộc tính khác như đậm, nghiêng hay gạch chân.", "comprehensive": "Microsoft Word cung cấp nhiều công cụ để định dạng văn bản. Bạn có thể thay đổi kiểu chữ, kích thước, màu sắc, và thêm các hiệu ứng như đậm, nghiêng, hoặc gạch chân. Các tính năng này giúp tài liệu của bạn trở nên nổi bật và dễ đọc hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cccb8fd8-d3f2-4b1c-89c8-62b002547532', '12b4d5d2-630e-48c5-8f8e-9c5563030ef4', 'concept', 'Chèn hình ảnh và đối tượng', 'Để chèn hình ảnh vào tài liệu, vào tab ''Insert'', sau đó chọn ''Pictures'' để thêm hình ảnh từ máy tính hoặc chọn ''Online Pictures'' để tải hình ảnh từ internet.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh hướng dẫn chèn hình ảnh vào tài liệu trong Microsoft Word.", "videoScript": "Hướng dẫn chèn hình ảnh và đối tượng trong Microsoft Word.", "imageDescription": "Các bước để chèn hình ảnh vào tài liệu trong Microsoft Word.", "videoDescription": "Video hướng dẫn cách chèn hình ảnh và các đối tượng khác vào tài liệu trong Microsoft Word."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:29:36.307526', '2026-01-27 12:29:36.307526', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể chèn hình ảnh vào tài liệu bằng cách chọn ''Insert'' và sau đó ''Pictures''.", "detailed": "Để chèn hình ảnh vào tài liệu, vào tab ''Insert'', sau đó chọn ''Pictures'' để thêm hình ảnh từ máy tính hoặc chọn ''Online Pictures'' để tải hình ảnh từ internet.", "comprehensive": "Chèn hình ảnh vào tài liệu trong Microsoft Word rất đơn giản. Bạn chỉ cần vào tab ''Insert'', chọn ''Pictures'' để tải hình ảnh từ máy tính hoặc ''Online Pictures'' để tìm kiếm và chèn hình ảnh từ internet. Sau khi chèn, bạn có thể định dạng và điều chỉnh kích thước hình ảnh theo ý muốn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e8254cd3-8122-4e03-be2d-ec84a6c760d5', '9177fd5b-8026-4bf6-97a4-829f05d77a16', 'concept', 'Cấu Trúc Hòa Âm Cơ Bản', 'Cấu trúc hòa âm cơ bản bao gồm các hợp âm lớn, nhỏ và các nốt nhạc giai điệu. Việc hiểu rõ cách sắp xếp các hợp âm này là rất quan trọng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Sơ đồ cấu trúc hợp âm trên đàn piano.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu về cách cấu trúc các hợp âm cơ bản trên đàn piano và ứng dụng của chúng trong việc sáng tác.", "imageDescription": "Một sơ đồ hiển thị cách chơi các loại hợp âm cơ bản trên đàn piano.", "videoDescription": "Video hướng dẫn cách chơi và kết hợp các hợp âm cơ bản trên đàn piano."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:49:21.948139', '2026-01-27 12:49:21.948139', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cấu trúc hòa âm cơ bản bao gồm hợp âm và giai điệu.", "detailed": "Cấu trúc hòa âm cơ bản bao gồm các hợp âm lớn, nhỏ và các nốt nhạc giai điệu. Việc hiểu rõ cách sắp xếp các hợp âm này là rất quan trọng.", "comprehensive": "Cấu trúc hòa âm cơ bản bao gồm các hợp âm lớn, nhỏ, và hợp âm 7. Mỗi loại hợp âm có những tính chất riêng và có ảnh hưởng đến cảm xúc của bản nhạc. Việc kết hợp các hợp âm này với giai điệu thích hợp sẽ tạo ra một nền tảng vững chắc cho bất kỳ bản nhạc nào."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8be77746-28e1-43bc-8c66-619cf9d3a755', '9177fd5b-8026-4bf6-97a4-829f05d77a16', 'concept', 'Tạo Giai Điệu Hòa Âm', 'Giai điệu hòa âm là phần âm thanh chính mà người nghe có thể nhận diện. Nó cần được phát triển một cách sáng tạo để phù hợp với các hợp âm đã chọn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nhạc sĩ đang viết giai điệu trên bản nhạc.", "videoScript": "Hãy cùng khám phá cách tạo ra giai điệu hòa âm và những nguyên tắc cơ bản cần nhớ khi viết giai điệu.", "imageDescription": "Hình ảnh của một nhạc sĩ ngồi viết giai điệu trên bản nhạc với cây đàn piano bên cạnh.", "videoDescription": "Video hướng dẫn cách viết và phát triển giai điệu hòa âm trong âm nhạc."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:49:21.951816', '2026-01-27 12:49:21.951816', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giai điệu hòa âm là phần âm thanh chính trong bản nhạc.", "detailed": "Giai điệu hòa âm là phần âm thanh chính mà người nghe có thể nhận diện. Nó cần được phát triển một cách sáng tạo để phù hợp với các hợp âm đã chọn.", "comprehensive": "Giai điệu hòa âm là phần quan trọng nhất trong một tác phẩm âm nhạc. Để tạo ra một giai điệu hấp dẫn, người sáng tác cần phải hiểu rõ cách mà giai điệu tương tác với các hợp âm. Việc sử dụng các kỹ thuật như lặp lại, biến đổi và phát triển sẽ giúp tạo ra những giai điệu độc đáo và đáng nhớ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('09da17fa-f3f3-488a-abc8-490aec970a36', '9177fd5b-8026-4bf6-97a4-829f05d77a16', 'concept', 'Kỹ Thuật Phối Hợp Âm Thanh', 'Các kỹ thuật phối hợp âm thanh như sử dụng hợp âm bổ sung, chuyển tiếp và các yếu tố nhịp điệu có thể làm cho bản nhạc trở nên thú vị hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nhạc sĩ đang thử nghiệm các kỹ thuật hòa âm trên đàn piano.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu về các kỹ thuật phối hợp âm thanh và cách áp dụng chúng vào sáng tác.", "imageDescription": "Nhạc sĩ đang thực hành các kỹ thuật phối hợp âm thanh trên đàn piano với một bản nhạc mở trước mặt.", "videoDescription": "Video hướng dẫn các kỹ thuật phối hợp âm thanh trong sáng tác âm nhạc."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:49:21.955489', '2026-01-27 12:49:21.955489', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kỹ thuật phối hợp âm thanh giúp tạo ra sự phong phú trong âm nhạc.", "detailed": "Các kỹ thuật phối hợp âm thanh như sử dụng hợp âm bổ sung, chuyển tiếp và các yếu tố nhịp điệu có thể làm cho bản nhạc trở nên thú vị hơn.", "comprehensive": "Kỹ thuật phối hợp âm thanh là một phần quan trọng trong sáng tác âm nhạc. Việc sử dụng các hợp âm bổ sung, các chuyển tiếp mượt mà và sự kết hợp của các yếu tố nhịp điệu sẽ tạo ra những sắc thái phong phú cho bản nhạc. Các kỹ thuật này không chỉ làm phong phú thêm bản nhạc mà còn giúp người nghe cảm nhận được sự chuyển động và cảm xúc trong âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b2113a66-7c16-4487-9977-1bf286a49eab', '12b4d5d2-630e-48c5-8f8e-9c5563030ef4', 'concept', 'Tạo bảng trong tài liệu', 'Để tạo bảng trong tài liệu, bạn vào tab ''Insert'', chọn ''Table'' và sau đó xác định số hàng và cột mà bạn muốn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh hướng dẫn tạo bảng trong Microsoft Word.", "videoScript": "Hướng dẫn tạo bảng trong tài liệu Microsoft Word.", "imageDescription": "Giao diện tạo bảng trong Microsoft Word với các tùy chọn.", "videoDescription": "Video hướng dẫn cách tạo và định dạng bảng trong tài liệu Microsoft Word."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:29:36.311318', '2026-01-27 12:29:36.311318', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể tạo bảng bằng cách chọn ''Insert'' và sau đó ''Table''.", "detailed": "Để tạo bảng trong tài liệu, bạn vào tab ''Insert'', chọn ''Table'' và sau đó xác định số hàng và cột mà bạn muốn.", "comprehensive": "Tạo bảng trong Microsoft Word là một chức năng hữu ích giúp bạn tổ chức thông tin một cách rõ ràng. Để tạo bảng, bạn vào tab ''Insert'', chọn ''Table'', sau đó kéo chuột để chọn số hàng và cột mà bạn cần. Bạn cũng có thể tùy chỉnh kiểu bảng và định dạng các ô trong bảng theo ý muốn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('48ad1144-5585-4dcd-92cf-00fa659bbe09', '9177fd5b-8026-4bf6-97a4-829f05d77a16', 'concept', 'Phát Triển Bản Hòa Âm', 'Phát triển bản hòa âm bao gồm việc mở rộng ý tưởng âm nhạc ban đầu và làm cho nó trở nên phong phú hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nhạc sĩ đang hoàn thiện bản hòa âm của mình.", "videoScript": "Trong bài học cuối cùng này, chúng ta sẽ khám phá cách phát triển một bản hòa âm từ ý tưởng ban đầu đến một tác phẩm hoàn chỉnh.", "imageDescription": "Nhạc sĩ đang ngồi bên đàn piano, chăm chú vào việc hoàn thiện bản hòa âm của mình với bản nhạc trước mặt.", "videoDescription": "Video hướng dẫn cách phát triển bản hòa âm trong sáng tác âm nhạc."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:49:21.95885', '2026-01-27 12:49:21.95885', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phát triển bản hòa âm là bước quan trọng trong sáng tác.", "detailed": "Phát triển bản hòa âm bao gồm việc mở rộng ý tưởng âm nhạc ban đầu và làm cho nó trở nên phong phú hơn.", "comprehensive": "Phát triển bản hòa âm là quá trình mở rộng và biến đổi các ý tưởng âm nhạc ban đầu để tạo ra một tác phẩm hoàn chỉnh. Điều này có thể bao gồm việc thay đổi nhịp điệu, thay đổi giai điệu, hoặc thêm các phần mới để tạo ra sự đa dạng và chiều sâu cho bản nhạc. Việc phát triển bản hòa âm cần sự sáng tạo và hiểu biết sâu sắc về âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f791d822-e284-4daf-a8cc-dcabf98494ea', 'f809ae85-28d5-443b-83de-ee5cefcf69f2', 'concept', 'Cách Chọn Kem Chống Nắng', 'Khi chọn kem chống nắng, hãy tìm sản phẩm có SPF từ 30 trở lên và khả năng chống cả UVA và UVB. Nên thoa kem trước khi ra ngoài khoảng 30 phút.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại kem chống nắng với nhãn hiệu về SPF.", "videoScript": "Trong video này, chúng ta sẽ cùng nhau tìm hiểu cách chọn kem chống nắng phù hợp với loại da và nhu cầu cá nhân.", "imageDescription": "Một bức ảnh chứa nhiều sản phẩm kem chống nắng khác nhau với các chỉ số SPF nổi bật.", "videoDescription": "Video hướng dẫn cách chọn kem chống nắng, giải thích về chỉ số SPF và các thành phần quan trọng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:14:15.655724', '2026-01-27 13:14:15.655724', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn kem chống nắng có SPF cao là cách bảo vệ da hiệu quả.", "detailed": "Khi chọn kem chống nắng, hãy tìm sản phẩm có SPF từ 30 trở lên và khả năng chống cả UVA và UVB. Nên thoa kem trước khi ra ngoài khoảng 30 phút.", "comprehensive": "Kem chống nắng là sản phẩm không thể thiếu để bảo vệ da khỏi tác hại của tia UV. Nên chọn kem có chỉ số SPF 30 trở lên và có ghi rõ khả năng chống UVA và UVB. Ngoài ra, cần nhớ thoa lại mỗi hai giờ, đặc biệt là khi bơi hoặc đổ mồ hôi. Một số thành phần như Zinc Oxide và Titanium Dioxide cũng rất hiệu quả trong việc bảo vệ da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9ff38c5e-57f1-4841-9642-d682e01da486', 'f809ae85-28d5-443b-83de-ee5cefcf69f2', 'concept', 'Thời Điểm Thoa Kem Chống Nắng', 'Nên thoa kem chống nắng khoảng 30 phút trước khi ra ngoài và thoa lại sau mỗi 2 giờ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh đồng hồ báo thức và kem chống nắng.", "videoScript": "Chào mừng các bạn! Hôm nay chúng ta sẽ bàn về thời điểm thoa kem chống nắng để đạt được hiệu quả tốt nhất cho làn da.", "imageDescription": "Một bức ảnh với đồng hồ và kem chống nắng thể hiện thời gian lý tưởng để thoa sản phẩm.", "videoDescription": "Video giải thích về thời điểm lý tưởng để thoa kem chống nắng và cách thức thoa đúng cách."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:14:15.66004', '2026-01-27 13:14:15.66004', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thoa kem chống nắng trước khi ra ngoài.", "detailed": "Nên thoa kem chống nắng khoảng 30 phút trước khi ra ngoài và thoa lại sau mỗi 2 giờ.", "comprehensive": "Một trong những yếu tố quan trọng để kem chống nắng phát huy hiệu quả là thời gian thoa. Nên thoa kem chống nắng ít nhất 30 phút trước khi ra ngoài để sản phẩm có đủ thời gian để thẩm thấu vào da. Đặc biệt, cần thoa lại kem sau mỗi 2 giờ, hoặc ngay sau khi bơi hoặc đổ mồ hôi. Việc này giúp duy trì hiệu quả bảo vệ da trong suốt thời gian tiếp xúc với tia UV."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('43a8681c-f6c4-4131-a3c2-4a3aabf59ebe', 'f809ae85-28d5-443b-83de-ee5cefcf69f2', 'concept', 'Các Biện Pháp Bảo Vệ Da Khác', 'Ngoài kem chống nắng, bạn có thể sử dụng quần áo bảo vệ, kính râm và đội mũ rộng vành để bảo vệ da khỏi tia UV.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người mặc áo dài tay, đội mũ và đeo kính râm dưới ánh nắng.", "videoScript": "Trong video này, chúng ta sẽ cùng khám phá những biện pháp bảo vệ da ngoài kem chống nắng để giữ cho làn da luôn khỏe mạnh.", "imageDescription": "Một bức tranh thể hiện người bảo vệ da bằng cách mặc quần áo dài tay và sử dụng các phụ kiện chống nắng.", "videoDescription": "Video giới thiệu các biện pháp bảo vệ da khác bên cạnh việc sử dụng kem chống nắng."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:14:15.664131', '2026-01-27 13:14:15.664131', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mặc áo dài tay và đội mũ khi ra ngoài.", "detailed": "Ngoài kem chống nắng, bạn có thể sử dụng quần áo bảo vệ, kính râm và đội mũ rộng vành để bảo vệ da khỏi tia UV.", "comprehensive": "Để bảo vệ da khỏi tia UV, bên cạnh việc sử dụng kem chống nắng, bạn cũng nên áp dụng các biện pháp bảo vệ khác như mặc quần áo dài tay, đội mũ rộng vành, và đeo kính râm. Quần áo có chỉ số UPF (Ultraviolet Protection Factor) cao có thể cung cấp thêm sự bảo vệ. Hãy tìm cách ở trong bóng râm khi có thể, đặc biệt từ 10 giờ sáng đến 4 giờ chiều khi tia UV mạnh nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9114e0bf-d26d-4d40-afd0-a294bf288562', '4641d048-bdd6-4a6b-bc90-24da439ed95b', 'concept', 'Giới Thiệu Về Định Dạng Văn Bản', 'Định dạng văn bản bao gồm các thuộc tính như font, kích thước chữ, màu sắc và kiểu chữ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giao diện Word với các công cụ định dạng văn bản.", "videoScript": "Chào mừng đến với bài học về định dạng văn bản trong Word. Hôm nay, chúng ta sẽ tìm hiểu cách sử dụng các công cụ để làm cho văn bản của bạn nổi bật hơn.", "imageDescription": "Giao diện Microsoft Word hiển thị các công cụ định dạng văn bản như font, cỡ chữ, màu sắc.", "videoDescription": "Video hướng dẫn cách sử dụng các tính năng định dạng văn bản trong Microsoft Word."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:30:04.236095', '2026-01-27 12:30:04.236095', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Định dạng văn bản giúp văn bản trở nên dễ đọc hơn.", "detailed": "Định dạng văn bản bao gồm các thuộc tính như font, kích thước chữ, màu sắc và kiểu chữ.", "comprehensive": "Định dạng văn bản trong Word là quá trình thay đổi cách trình bày văn bản để nó trở nên dễ nhìn và thu hút hơn. Các yếu tố như kiểu chữ, kích thước, màu sắc, và khoảng cách giữa các dòng đều có thể được điều chỉnh để cải thiện tính thẩm mỹ và khả năng đọc của tài liệu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3dfe00c7-1481-44a4-aa87-f1b5d584d446', 'f809ae85-28d5-443b-83de-ee5cefcf69f2', 'concept', 'Nhận Biết Tổn Thương Da Do Tia UV', 'Các dấu hiệu tổn thương da do tia UV bao gồm nám, tàn nhang, và da khô sần.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh trước và sau của làn da bị tổn thương do tia UV.", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ cùng nhau tìm hiểu về các dấu hiệu tổn thương da do tia UV và cách nhận biết chúng.", "imageDescription": "Một bức tranh so sánh làn da khỏe mạnh và làn da bị tổn thương do tia UV.", "videoDescription": "Video giải thích về các dấu hiệu tổn thương da do tia UV và cách phòng tránh."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:14:15.668969', '2026-01-27 13:14:15.668969', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhận biết các dấu hiệu tổn thương da để điều trị kịp thời.", "detailed": "Các dấu hiệu tổn thương da do tia UV bao gồm nám, tàn nhang, và da khô sần.", "comprehensive": "Tia UV có thể gây ra nhiều vấn đề cho da như lão hóa sớm, nám, tàn nhang và các dấu hiệu cháy nắng. Các dấu hiệu này có thể bao gồm sự xuất hiện của các đốm nâu, da khô sần và nếp nhăn. Việc nhận biết sớm các dấu hiệu này giúp bạn có thể điều trị kịp thời và bảo vệ da hiệu quả hơn. Hãy thường xuyên kiểm tra làn da của bạn để phát hiện bất kỳ thay đổi nào."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4c3edf27-be3f-4670-ad8e-d6bab0b5a709', 'd4c52247-5e59-474e-81d2-16996c679847', 'concept', 'Khái Niệm Về Bố Trí Hợp Âm Nâng Cao', 'Bố trí hợp âm nâng cao không chỉ đơn thuần là việc đánh hợp âm mà còn bao gồm cách sắp xếp và thêm các nốt bổ sung để tạo ra âm thanh đa dạng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một bản nhạc piano với các hợp âm nâng cao được đánh dấu.", "videoScript": "Chào mừng đến với bài học về bố trí hợp âm nâng cao. Hôm nay, chúng ta sẽ khám phá cách sử dụng các hợp âm phức tạp để tạo ra âm thanh phong phú hơn.", "imageDescription": "Một bản nhạc piano hiển thị các hợp âm nâng cao với các nốt bổ sung.", "videoDescription": "Video hướng dẫn về cách bố trí hợp âm nâng cao trên piano, bao gồm lý thuyết và các ví dụ thực hành."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:49:48.756302', '2026-01-27 12:49:48.756302', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bố trí hợp âm nâng cao giúp tạo ra âm thanh phong phú hơn.", "detailed": "Bố trí hợp âm nâng cao không chỉ đơn thuần là việc đánh hợp âm mà còn bao gồm cách sắp xếp và thêm các nốt bổ sung để tạo ra âm thanh đa dạng.", "comprehensive": "Bố trí hợp âm nâng cao là phương pháp sử dụng các hợp âm phức tạp hơn, bao gồm việc thêm các nốt như 7th, 9th, và các hợp âm biến thể, nhằm tạo ra âm thanh sâu sắc và hấp dẫn hơn trong âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0737deae-b863-4c36-9330-928baf6d1fe7', '4641d048-bdd6-4a6b-bc90-24da439ed95b', 'concept', 'Sử Dụng Font Chữ Trong Word', 'Trong Word, bạn có thể chọn từ nhiều loại font chữ khác nhau để phù hợp với nội dung và đối tượng người đọc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh danh sách các font chữ trong Word.", "videoScript": "Hãy cùng khám phá cách chọn font chữ trong Word. Chúng ta sẽ xem xét các tùy chọn và cách thức áp dụng chúng vào văn bản.", "imageDescription": "Danh sách các font chữ có sẵn trong Microsoft Word với các mẫu hiển thị.", "videoDescription": "Video hướng dẫn cách chọn và thay đổi font chữ trong Microsoft Word."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:30:04.244473', '2026-01-27 12:30:04.244473', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Font chữ quyết định kiểu dáng của văn bản.", "detailed": "Trong Word, bạn có thể chọn từ nhiều loại font chữ khác nhau để phù hợp với nội dung và đối tượng người đọc.", "comprehensive": "Việc chọn font chữ là một yếu tố quan trọng trong thiết kế tài liệu. Word cung cấp nhiều lựa chọn font chữ từ truyền thống đến hiện đại. Bạn nên cân nhắc đến sự phù hợp của font chữ với nội dung và đối tượng độc giả khi lựa chọn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8aca2494-3a33-4c56-9195-a4b57f9c09d8', '9d747bf1-17bc-4150-8d46-740e6739b499', 'concept', 'Giới thiệu về Dưỡng Ẩm', 'Dưỡng ẩm là bước quan trọng trong quy trình chăm sóc da, giúp da không bị khô và duy trì độ đàn hồi.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người phụ nữ đang thoa kem dưỡng ẩm lên mặt.", "videoScript": "Chào mừng bạn đến với bài học về dưỡng ẩm. Hôm nay, chúng ta sẽ khám phá tầm quan trọng của dưỡng ẩm trong quy trình chăm sóc da.", "imageDescription": "Hình ảnh mô tả một người phụ nữ với làn da sáng bóng đang chăm sóc da mặt bằng kem dưỡng ẩm.", "videoDescription": "Video hướng dẫn về cách chọn lựa sản phẩm dưỡng ẩm phù hợp với loại da và cách sử dụng đúng cách."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:14:44.154103', '2026-01-27 13:14:44.154103', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Dưỡng ẩm giúp da giữ nước và mềm mại.", "detailed": "Dưỡng ẩm là bước quan trọng trong quy trình chăm sóc da, giúp da không bị khô và duy trì độ đàn hồi.", "comprehensive": "Dưỡng ẩm không chỉ giúp da duy trì độ ẩm cần thiết mà còn bảo vệ da khỏi các tác động xấu từ môi trường. Trong bài học này, chúng ta sẽ tìm hiểu về các loại sản phẩm dưỡng ẩm và cách chọn sản phẩm phù hợp với từng loại da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('09fa58dd-4756-4273-ab8c-60886255875f', '9d747bf1-17bc-4150-8d46-740e6739b499', 'concept', 'Các Loại Sản Phẩm Dưỡng Ẩm', 'Sản phẩm dưỡng ẩm có thể ở dạng kem, gel hoặc lotion. Mỗi loại có công thức và công dụng riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bộ sản phẩm dưỡng ẩm gồm kem, gel và lotion.", "videoScript": "Trong video này, chúng ta sẽ xem xét từng loại sản phẩm dưỡng ẩm và ưu nhược điểm của chúng.", "imageDescription": "Bức ảnh minh họa các loại sản phẩm dưỡng ẩm khác nhau trên một bàn trang điểm.", "videoDescription": "Video giới thiệu về các loại sản phẩm dưỡng ẩm, cách chúng hoạt động và khi nào nên sử dụng từng loại."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:14:44.163082', '2026-01-27 13:14:44.163082', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại sản phẩm như kem, gel, và lotion.", "detailed": "Sản phẩm dưỡng ẩm có thể ở dạng kem, gel hoặc lotion. Mỗi loại có công thức và công dụng riêng.", "comprehensive": "Các sản phẩm dưỡng ẩm thường được chia thành ba loại chính: kem dưỡng ẩm có độ đặc cao cho da khô, gel dưỡng ẩm nhẹ cho da dầu, và lotion cho làn da nhạy cảm. Việc hiểu rõ về các loại sản phẩm này sẽ giúp bạn chọn được sản phẩm phù hợp nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a60065cc-295a-4d32-a708-bc952df86bf3', '4641d048-bdd6-4a6b-bc90-24da439ed95b', 'concept', 'Định Dạng Kích Thước Chữ', 'Bạn có thể thay đổi kích thước chữ để nhấn mạnh các phần quan trọng trong tài liệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh thanh công cụ định dạng với tùy chọn kích thước chữ.", "videoScript": "Trong phần này, chúng ta sẽ học cách thay đổi kích thước chữ trong Word để làm cho tài liệu của chúng ta thu hút hơn.", "imageDescription": "Thanh công cụ định dạng trong Word với các lựa chọn kích thước chữ khác nhau.", "videoDescription": "Video hướng dẫn cách điều chỉnh kích thước chữ trong Microsoft Word."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:30:04.250135', '2026-01-27 12:30:04.250135', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kích thước chữ ảnh hưởng đến sự nổi bật của văn bản.", "detailed": "Bạn có thể thay đổi kích thước chữ để nhấn mạnh các phần quan trọng trong tài liệu.", "comprehensive": "Kích thước chữ là yếu tố quyết định mức độ nổi bật của thông tin trong tài liệu. Trong Word, bạn có thể dễ dàng điều chỉnh kích thước chữ bằng cách sử dụng thanh công cụ định dạng. Việc chọn kích thước phù hợp giúp người đọc dễ dàng nhận diện và tiếp thu thông tin."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e327bc78-82ac-4bec-975e-6122f67c3c81', '4641d048-bdd6-4a6b-bc90-24da439ed95b', 'concept', 'Sử Dụng Màu Sắc Trong Định Dạng Văn Bản', 'Sử dụng màu sắc có thể tạo ra sự tương phản và thu hút sự chú ý của người đọc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh bảng màu trong Word.", "videoScript": "Chúng ta sẽ khám phá cách sử dụng màu sắc để định dạng văn bản trong Word. Màu sắc có thể làm cho tài liệu của bạn nổi bật và thu hút hơn.", "imageDescription": "Bảng màu trong Microsoft Word với các lựa chọn màu sắc cho văn bản.", "videoDescription": "Video hướng dẫn cách áp dụng màu sắc cho văn bản trong Microsoft Word."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:30:04.25423', '2026-01-27 12:30:04.25423', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Màu sắc giúp làm nổi bật văn bản.", "detailed": "Sử dụng màu sắc có thể tạo ra sự tương phản và thu hút sự chú ý của người đọc.", "comprehensive": "Màu sắc là một công cụ mạnh mẽ trong việc định dạng văn bản. Khi sử dụng màu sắc, bạn có thể tạo ra sự tương phản và nhấn mạnh các phần quan trọng của tài liệu. Word cho phép bạn chọn từ bảng màu đa dạng để phù hợp với nội dung và phong cách của tài liệu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3e9ab161-7107-42cc-ac79-c195f582e2cb', '9d747bf1-17bc-4150-8d46-740e6739b499', 'concept', 'Cách Sử Dụng Sản Phẩm Dưỡng Ẩm', 'Sau khi rửa mặt, hãy thoa sản phẩm dưỡng ẩm lên da trong vòng 1-2 phút để giữ ẩm tốt nhất.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Người phụ nữ thoa kem dưỡng ẩm sau khi rửa mặt.", "videoScript": "Hãy cùng theo dõi cách thoa sản phẩm dưỡng ẩm để đảm bảo da bạn nhận được đầy đủ độ ẩm cần thiết.", "imageDescription": "Hình ảnh một người phụ nữ đang thoa kem dưỡng ẩm lên da mặt sau khi đã rửa sạch.", "videoDescription": "Video hướng dẫn chi tiết cách sử dụng sản phẩm dưỡng ẩm để tối ưu hóa hiệu quả."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:14:44.166227', '2026-01-27 13:14:44.166227', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thoa sản phẩm lên da mặt sau khi rửa sạch.", "detailed": "Sau khi rửa mặt, hãy thoa sản phẩm dưỡng ẩm lên da trong vòng 1-2 phút để giữ ẩm tốt nhất.", "comprehensive": "Để đạt hiệu quả tối ưu, bạn nên sử dụng sản phẩm dưỡng ẩm ngay sau khi làm sạch da. Hãy thoa một lượng vừa đủ lên mặt và cổ, kết hợp với các động tác massage nhẹ nhàng để sản phẩm thẩm thấu nhanh hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b4f48b23-5476-4eb8-ac1f-39f369494564', '9d747bf1-17bc-4150-8d46-740e6739b499', 'concept', 'Lợi Ích Của Việc Dưỡng Ẩm Đều Đặn', 'Dưỡng ẩm thường xuyên giúp cải thiện kết cấu da, ngăn ngừa tình trạng khô da và lão hóa sớm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Da mặt căng bóng và khỏe mạnh.", "videoScript": "Chúng ta sẽ cùng khám phá những lợi ích vô giá của việc dưỡng ẩm đều đặn cho làn da của bạn.", "imageDescription": "Bức ảnh một người với làn da bóng khỏe, thể hiện kết quả của việc dưỡng ẩm đều đặn.", "videoDescription": "Video trình bày các lợi ích của việc dưỡng ẩm thường xuyên và cách nó ảnh hưởng tích cực đến sức khỏe làn da."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:14:44.170029', '2026-01-27 13:14:44.170029', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giúp da khỏe mạnh và ngăn ngừa lão hóa.", "detailed": "Dưỡng ẩm thường xuyên giúp cải thiện kết cấu da, ngăn ngừa tình trạng khô da và lão hóa sớm.", "comprehensive": "Việc dưỡng ẩm đều đặn không chỉ bảo vệ da khỏi các tác nhân gây hại mà còn giúp da duy trì độ đàn hồi, làm giảm sự xuất hiện của nếp nhăn và mang lại làn da sáng khỏe. Hãy cùng tìm hiểu thêm về lợi ích của việc dưỡng ẩm trong bài học này."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('997148f1-044c-4d8f-a6ae-bbf8b2c4b49c', '9d747bf1-17bc-4150-8d46-740e6739b499', 'concept', 'Chọn Sản Phẩm Dưỡng Ẩm Phù Hợp', 'Mỗi loại da có những yêu cầu đặc biệt. Hãy chọn sản phẩm dưỡng ẩm phù hợp với loại da của bạn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Các sản phẩm dưỡng ẩm với nhãn hiệu khác nhau.", "videoScript": "Hãy cùng chúng tôi tìm hiểu cách chọn lựa sản phẩm dưỡng ẩm phù hợp với từng loại da.", "imageDescription": "Hình ảnh mô tả nhiều sản phẩm dưỡng ẩm với nhãn hiệu và thành phần khác nhau.", "videoDescription": "Video hướng dẫn cách xác định loại da và hướng dẫn chọn sản phẩm dưỡng ẩm phù hợp cho từng loại da."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:14:44.173821', '2026-01-27 13:14:44.173821', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn sản phẩm dựa trên loại da của bạn.", "detailed": "Mỗi loại da có những yêu cầu đặc biệt. Hãy chọn sản phẩm dưỡng ẩm phù hợp với loại da của bạn.", "comprehensive": "Việc lựa chọn sản phẩm dưỡng ẩm phù hợp rất quan trọng. Da dầu có thể cần gel nhẹ, trong khi da khô cần kem đặc hơn. Hãy tìm hiểu cách xác định loại da và chọn sản phẩm phù hợp trong bài học này."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3a151580-5d2b-4a27-aecb-d66b76f3a5a9', '4641d048-bdd6-4a6b-bc90-24da439ed95b', 'concept', 'Căn Lề Và Khoảng Cách Giữa Các Dòng', 'Căn lề và khoảng cách giữa các dòng có ảnh hưởng lớn đến cấu trúc và sự rõ ràng của tài liệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh tùy chọn căn lề và khoảng cách dòng trong Word.", "videoScript": "Trong phần này, chúng ta sẽ tìm hiểu cách căn lề và điều chỉnh khoảng cách giữa các dòng để tạo ra một tài liệu chuyên nghiệp hơn.", "imageDescription": "Giao diện Microsoft Word hiển thị các tùy chọn căn lề và khoảng cách giữa các dòng.", "videoDescription": "Video hướng dẫn cách căn lề và điều chỉnh khoảng cách giữa các dòng trong Microsoft Word."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:30:04.259313', '2026-01-27 12:30:04.259313', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Căn lề và khoảng cách giúp văn bản dễ đọc hơn.", "detailed": "Căn lề và khoảng cách giữa các dòng có ảnh hưởng lớn đến cấu trúc và sự rõ ràng của tài liệu.", "comprehensive": "Căn lề và khoảng cách giữa các dòng là những yếu tố quan trọng trong việc tạo ra một tài liệu dễ đọc. Trong Word, bạn có thể điều chỉnh các thiết lập này để cải thiện cấu trúc và sự rõ ràng của văn bản. Việc căn lề hợp lý và khoảng cách giữa các dòng giúp người đọc dễ dàng theo dõi nội dung."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0f9c5c0c-c2fc-4819-aae5-64550df5b281', '980807c3-7f72-4f43-ad22-d0670b74a227', 'concept', 'Luyện Tập Các Nốt Đơn Lẻ Bằng Tay Trái', 'Hãy thử chơi các nốt đơn lẻ bằng tay trái để cải thiện độ chính xác và tốc độ. Bắt đầu với nốt C, D, E, và tiếp tục với các nốt khác.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bàn tay trái đang chơi nốt C trên piano.", "videoScript": "Trong video này, chúng ta sẽ luyện tập từng nốt đơn lẻ bằng tay trái. Hãy theo dõi và cùng nhau thực hành nhé!", "imageDescription": "Bàn tay trái đang đặt trên phím C của đàn piano, thể hiện kỹ thuật chơi nốt đơn.", "videoDescription": "Video hướng dẫn cách luyện tập nốt đơn lẻ bằng tay trái trên piano."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:46.240552', '2026-01-27 12:52:46.240552', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bắt đầu bằng cách luyện tập từng nốt đơn lẻ với tay trái.", "detailed": "Hãy thử chơi các nốt đơn lẻ bằng tay trái để cải thiện độ chính xác và tốc độ. Bắt đầu với nốt C, D, E, và tiếp tục với các nốt khác.", "comprehensive": "Luyện tập các nốt đơn lẻ là bước đầu tiên trong việc phát triển kỹ thuật tay trái. Bắt đầu từ những nốt cơ bản như C, D, E, và sử dụng metronome để tăng dần tốc độ. Chú ý đến độ chính xác và cảm xúc trong từng nốt."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('815636d3-898e-45d3-a639-ad885d708403', 'd0d968fd-ca4d-4835-9585-5c9d6a12a1a9', 'concept', 'Giới thiệu về việc chèn hình ảnh trong Word', 'Chèn hình ảnh vào tài liệu Word giúp minh họa cho nội dung và tạo sự thu hút cho người đọc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang chèn hình ảnh vào tài liệu Word.", "videoScript": "Xin chào! Hôm nay chúng ta sẽ học cách chèn hình ảnh vào tài liệu Word. Đầu tiên, bạn mở tài liệu Word của mình, sau đó vào tab ''Insert'' và chọn ''Pictures''.", "imageDescription": "Một màn hình máy tính với giao diện Word đang mở và một hình ảnh được kéo vào tài liệu.", "videoDescription": "Video hướng dẫn chèn hình ảnh vào Microsoft Word."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:30:33.475608', '2026-01-27 12:30:33.475608', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hình ảnh có thể làm cho tài liệu của bạn sinh động hơn.", "detailed": "Chèn hình ảnh vào tài liệu Word giúp minh họa cho nội dung và tạo sự thu hút cho người đọc.", "comprehensive": "Việc sử dụng hình ảnh trong tài liệu Word không chỉ làm cho văn bản trở nên hấp dẫn hơn mà còn giúp truyền đạt thông tin một cách trực quan. Bạn có thể chèn hình ảnh từ máy tính hoặc từ Internet."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('66b4aa31-c70d-4e47-9605-6d5949b2c57a', '858bb936-8ac9-49b7-9166-74b02c840ff4', 'concept', 'Giới Thiệu về Toning', 'Toning là một bước trong quy trình chăm sóc da giúp loại bỏ bụi bẩn và bã nhờn còn sót lại sau khi rửa mặt, từ đó cân bằng độ pH cho làn da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh về các loại toner khác nhau, bao gồm toner nước, gel và lotion.", "videoScript": "Chào mừng đến với bài học về Toning. Hôm nay, chúng ta sẽ tìm hiểu về tầm quan trọng của bước này trong quy trình chăm sóc da.", "imageDescription": "Một bức ảnh bao gồm nhiều loại toner với thiết kế chai khác nhau trên nền trắng.", "videoDescription": "Video giới thiệu về toning, cách sử dụng và lợi ích của nó trong chăm sóc da."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:15:12.507556', '2026-01-27 13:15:12.507556', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Toning là bước quan trọng trong quy trình chăm sóc da giúp cân bằng độ pH.", "detailed": "Toning là một bước trong quy trình chăm sóc da giúp loại bỏ bụi bẩn và bã nhờn còn sót lại sau khi rửa mặt, từ đó cân bằng độ pH cho làn da.", "comprehensive": "Toning không chỉ giúp làm sạch sâu mà còn chuẩn bị cho da tiếp nhận các sản phẩm dưỡng khác. Bước này giúp cân bằng độ pH của da, điều chỉnh lượng dầu và cung cấp độ ẩm cần thiết cho làn da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3ef51717-8e5d-4b1f-a09f-50284aff0296', '858bb936-8ac9-49b7-9166-74b02c840ff4', 'concept', 'Tại Sao Cần Cân Bằng Độ pH?', 'Độ pH của da ảnh hưởng đến sức khỏe của da. Nếu độ pH bị mất cân bằng, da có thể trở nên nhạy cảm và dễ bị mụn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ độ pH của da và các sản phẩm chăm sóc da.", "videoScript": "Trong video này, chúng ta sẽ khám phá lý do tại sao việc cân bằng độ pH là cần thiết cho làn da khỏe mạnh.", "imageDescription": "Một biểu đồ thể hiện độ pH lý tưởng của da và các tác động của việc mất cân bằng độ pH.", "videoDescription": "Video giải thích về độ pH của da và cách toning giúp duy trì độ pH cân bằng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:15:12.51495', '2026-01-27 13:15:12.51495', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cân bằng độ pH giúp da khỏe mạnh và ngăn ngừa các vấn đề da.", "detailed": "Độ pH của da ảnh hưởng đến sức khỏe của da. Nếu độ pH bị mất cân bằng, da có thể trở nên nhạy cảm và dễ bị mụn.", "comprehensive": "Khi độ pH của da không được cân bằng, các vấn đề như mụn, khô da và viêm có thể xuất hiện. Sử dụng toner giúp phục hồi độ pH tự nhiên, duy trì hàng rào bảo vệ da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0553c45c-c7bb-4e2a-9b09-d73fbde70cb0', '5f36b291-05f3-4155-b8b3-0910221c4da6', 'concept', 'Lợi Ích Của Bảo Hiểm', 'Lợi ích lớn nhất của bảo hiểm là giảm thiểu rủi ro tài chính và cung cấp sự hỗ trợ trong thời điểm khó khăn. Nó cũng giúp cá nhân lập kế hoạch tài chính tốt hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang cầm hợp đồng bảo hiểm với nụ cười trên khuôn mặt.", "videoScript": "Hãy cùng khám phá những lợi ích mà bảo hiểm mang lại cho bạn và gia đình, từ sự an tâm đến bảo vệ tài chính.", "imageDescription": "Một bức ảnh của một cá nhân vui vẻ đang đọc hợp đồng bảo hiểm, biểu thị sự an tâm và tự tin.", "videoDescription": "Video này sẽ giải thích các lợi ích của bảo hiểm về mặt tài chính, tinh thần và cách nó giúp bạn chuẩn bị cho tương lai."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:06:25.86449', '2026-01-27 13:06:25.86449', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bảo hiểm giúp giảm thiểu rủi ro tài chính và mang lại sự an tâm.", "detailed": "Lợi ích lớn nhất của bảo hiểm là giảm thiểu rủi ro tài chính và cung cấp sự hỗ trợ trong thời điểm khó khăn. Nó cũng giúp cá nhân lập kế hoạch tài chính tốt hơn.", "comprehensive": "Bảo hiểm không chỉ bảo vệ khỏi những rủi ro tài chính mà còn giúp cải thiện cảm giác an toàn và ổn định cho cá nhân và gia đình. Khi có bảo hiểm, bạn có thể yên tâm hơn trong các quyết định tài chính và đầu tư, vì bạn đã có một lớp bảo vệ khỏi những sự kiện không mong muốn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('503dc75e-5811-460d-8ca3-71d03af11017', '858bb936-8ac9-49b7-9166-74b02c840ff4', 'concept', 'Làm Sạch Sâu với Toning', 'Sử dụng toner giúp làm sạch sâu, loại bỏ tạp chất và bã nhờn mà sữa rửa mặt không thể làm sạch hoàn toàn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang sử dụng bông tẩy trang với toner.", "videoScript": "Hãy cùng xem cách sử dụng toner để làm sạch sâu cho làn da bạn. Đầu tiên, hãy thấm toner vào bông tẩy trang và nhẹ nhàng lau lên mặt.", "imageDescription": "Một bức ảnh của một người đang nhẹ nhàng thoa toner lên mặt bằng bông tẩy trang.", "videoDescription": "Video hướng dẫn cách sử dụng toner để làm sạch sâu và chuẩn bị cho các bước dưỡng da tiếp theo."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:15:12.518233', '2026-01-27 13:15:12.518233', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Toner giúp làm sạch sâu và loại bỏ bụi bẩn còn sót lại.", "detailed": "Sử dụng toner giúp làm sạch sâu, loại bỏ tạp chất và bã nhờn mà sữa rửa mặt không thể làm sạch hoàn toàn.", "comprehensive": "Toner không chỉ làm sạch bề mặt da mà còn thẩm thấu sâu vào lỗ chân lông, loại bỏ bụi bẩn và dầu thừa, từ đó giảm thiểu tình trạng mụn và mang lại làn da sáng khỏe."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('81d63089-c9c5-4e3c-966d-0163c67b932e', '858bb936-8ac9-49b7-9166-74b02c840ff4', 'concept', 'Lựa Chọn Toner Phù Hợp', 'Mỗi loại da có nhu cầu khác nhau. Da dầu nên chọn toner kiểm soát dầu, trong khi da khô cần toner cấp ẩm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các thành phần trong toner như chiết xuất trà xanh, hoa hồng, và vitamin.", "videoScript": "Trong phần này, chúng ta sẽ bàn về cách chọn toner phù hợp với từng loại da. Hãy cùng tìm hiểu nhé!", "imageDescription": "Một bức ảnh cho thấy các thành phần tự nhiên được sử dụng trong toner và lợi ích của chúng.", "videoDescription": "Video chia sẻ về các loại toner và cách chọn sản phẩm phù hợp với nhu cầu của da."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:15:12.521835', '2026-01-27 13:15:12.521835', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn toner phù hợp với loại da của bạn để có kết quả tốt nhất.", "detailed": "Mỗi loại da có nhu cầu khác nhau. Da dầu nên chọn toner kiểm soát dầu, trong khi da khô cần toner cấp ẩm.", "comprehensive": "Việc lựa chọn toner phù hợp rất quan trọng. Da nhạy cảm có thể cần toner dịu nhẹ, trong khi da mụn cần sản phẩm có thành phần kháng viêm. Hiểu rõ loại da của bạn sẽ giúp bạn chọn được sản phẩm tốt nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9f805c15-f31d-450c-9574-d5166c063eea', '858bb936-8ac9-49b7-9166-74b02c840ff4', 'concept', 'Lợi Ích Dài Hạn Của Toning', 'Sử dụng toner thường xuyên giúp cải thiện tình trạng da, làm giảm nếp nhăn và giữ ẩm cho da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh trước và sau khi sử dụng toner trong một thời gian dài.", "videoScript": "Cuối cùng, hãy cùng khám phá những lợi ích dài hạn của việc sử dụng toner trong quy trình chăm sóc da hàng ngày.", "imageDescription": "Một bức ảnh thể hiện sự khác biệt của làn da trước và sau khi sử dụng toner trong một khoảng thời gian.", "videoDescription": "Video trình bày những lợi ích lâu dài mà toner mang lại cho làn da khi sử dụng thường xuyên."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:15:12.5255', '2026-01-27 13:15:12.5255', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Toning giúp duy trì làn da khỏe mạnh và trẻ trung.", "detailed": "Sử dụng toner thường xuyên giúp cải thiện tình trạng da, làm giảm nếp nhăn và giữ ẩm cho da.", "comprehensive": "Lợi ích của việc sử dụng toner không chỉ dừng lại ở việc làm sạch. Nó giúp cải thiện kết cấu da, giảm thiểu lỗ chân lông và mang lại làn da đều màu và tươi sáng. Sử dụng đều đặn toner sẽ tạo ra một làn da khỏe mạnh và trẻ trung."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('581af0b1-aa72-4f0d-ad8d-3296c3bec115', 'd0d968fd-ca4d-4835-9585-5c9d6a12a1a9', 'concept', 'Cách chèn bảng trong Word', 'Chèn bảng trong Word rất đơn giản và hiệu quả cho việc trình bày thông tin. Bạn có thể thêm, xóa và chỉnh sửa các ô trong bảng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng được chèn vào tài liệu Word với nhiều ô và dữ liệu.", "videoScript": "Chào các bạn! Hôm nay chúng ta sẽ cùng tìm hiểu cách chèn bảng vào Word. Đầu tiên, mở tài liệu, chọn ''Insert'', sau đó nhấp vào ''Table'' và chọn kích thước bảng bạn muốn.", "imageDescription": "Hình ảnh một bảng dữ liệu trong tài liệu Word với các hàng và cột được điền thông tin.", "videoDescription": "Video hướng dẫn cách tạo và chỉnh sửa bảng trong Microsoft Word."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:30:33.483899', '2026-01-27 12:30:33.483899', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bảng giúp bạn tổ chức dữ liệu một cách rõ ràng.", "detailed": "Chèn bảng trong Word rất đơn giản và hiệu quả cho việc trình bày thông tin. Bạn có thể thêm, xóa và chỉnh sửa các ô trong bảng.", "comprehensive": "Các bảng là công cụ hữu ích trong Word giúp người dùng trình bày thông tin một cách có tổ chức và dễ đọc. Bạn có thể tạo bảng nhanh chóng bằng cách sử dụng tab ''Insert'', sau đó chọn ''Table''."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('36f6769a-d0b4-406d-86d0-33eadd8a552c', 'd0d968fd-ca4d-4835-9585-5c9d6a12a1a9', 'concept', 'Tùy chỉnh hình ảnh sau khi chèn', 'Sau khi chèn hình ảnh, bạn có thể tùy chỉnh nó bằng cách thay đổi kích thước, vị trí và hiệu ứng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một hình ảnh trong Word được chỉnh sửa với các công cụ định dạng.", "videoScript": "Xin chào! Hôm nay, chúng ta sẽ học cách tùy chỉnh hình ảnh sau khi đã chèn vào Word. Bạn có thể điều chỉnh kích thước bằng cách kéo các góc hình ảnh và sử dụng các tùy chọn trong tab ''Format''.", "imageDescription": "Hình ảnh mô tả giao diện Word với một hình ảnh được chọn và thanh công cụ định dạng đang mở.", "videoDescription": "Video hướng dẫn cách chỉnh sửa và tùy chỉnh hình ảnh trong Microsoft Word."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:30:33.489445', '2026-01-27 12:30:33.489445', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể thay đổi kích thước và vị trí hình ảnh.", "detailed": "Sau khi chèn hình ảnh, bạn có thể tùy chỉnh nó bằng cách thay đổi kích thước, vị trí và hiệu ứng.", "comprehensive": "Khi đã chèn hình ảnh vào tài liệu Word, bạn có thể dễ dàng tùy chỉnh nó. Sử dụng các công cụ trong tab ''Format'' để điều chỉnh kích thước, áp dụng hiệu ứng hình ảnh và thay đổi bố cục để hình ảnh phù hợp hơn với văn bản xung quanh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8613b8bf-c516-440a-ad5c-b16ad441d3d1', 'a82a703d-7936-4b23-8541-3b435cd2900e', 'concept', 'Cleansing Basics: The First Step to Healthy Skin', 'Làm sạch da không chỉ đơn thuần là rửa mặt. Nó là quá trình loại bỏ bụi bẩn, dầu thừa và lớp trang điểm, giúp da thông thoáng và sẵn sàng hấp thụ các sản phẩm dưỡng da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "A clean bathroom sink with skincare products arranged neatly.", "videoScript": "Today, we will discuss the importance of cleansing your skin. Cleansing is not just about washing your face; it''s about preparing your skin for subsequent skincare steps.", "imageDescription": "An aesthetically pleasing bathroom setup showcasing various cleansing products.", "videoDescription": "A video explaining the basics of skin cleansing and demonstrating how to properly cleanse your face."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:15:44.38691', '2026-01-27 13:15:44.38691', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Làm sạch da là bước đầu tiên trong quy trình chăm sóc da. Nó giúp loại bỏ bụi bẩn và dầu thừa.", "detailed": "Làm sạch da không chỉ đơn thuần là rửa mặt. Nó là quá trình loại bỏ bụi bẩn, dầu thừa và lớp trang điểm, giúp da thông thoáng và sẵn sàng hấp thụ các sản phẩm dưỡng da.", "comprehensive": "Làm sạch da là nền tảng của mọi quy trình chăm sóc da. Bước này không chỉ giúp loại bỏ bụi bẩn và dầu thừa mà còn góp phần ngăn ngừa mụn và cải thiện tình trạng da. Sử dụng sữa rửa mặt phù hợp với loại da và thực hiện đều đặn hai lần mỗi ngày sẽ giúp duy trì làn da khỏe mạnh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5675d544-bf1e-49c6-94f2-f3ef19d74300', 'd0d968fd-ca4d-4835-9585-5c9d6a12a1a9', 'concept', 'Lưu và chia sẻ tài liệu Word với hình ảnh và bảng', 'Sau khi hoàn tất việc chèn hình ảnh và bảng, việc lưu và chia sẻ tài liệu là rất quan trọng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Màn hình máy tính với giao diện Word và tùy chọn lưu tài liệu.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ học cách lưu và chia sẻ tài liệu Word của bạn sau khi đã chèn hình ảnh và bảng. Để lưu tài liệu, hãy vào tab ''File'' và chọn ''Save As''.", "imageDescription": "Hình ảnh hiển thị hộp thoại lưu tài liệu trong Microsoft Word.", "videoDescription": "Video hướng dẫn cách lưu và chia sẻ tài liệu Word có chứa hình ảnh và bảng."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:30:33.493436', '2026-01-27 12:30:33.493436', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đừng quên lưu tài liệu sau khi chèn hình ảnh và bảng.", "detailed": "Sau khi hoàn tất việc chèn hình ảnh và bảng, việc lưu và chia sẻ tài liệu là rất quan trọng.", "comprehensive": "Khi đã hoàn tất việc chèn hình ảnh và bảng vào tài liệu Word của mình, hãy chắc chắn lưu lại công việc của bạn. Bạn có thể lưu tài liệu dưới định dạng .docx hoặc .pdf để dễ dàng chia sẻ với người khác."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('68c96ab0-99fa-4e3b-bd7c-5fb919a5ea60', '980807c3-7f72-4f43-ad22-d0670b74a227', 'concept', 'Phối Hợp Tay Trái Với Tay Phải', 'Phối hợp giữa tay trái và tay phải là rất quan trọng để tạo nên một bản nhạc hoàn chỉnh. Bắt đầu với các giai điệu đơn giản.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nghệ sĩ piano đang chơi tay trái và tay phải đồng thời.", "videoScript": "Hôm nay, chúng ta sẽ học cách phối hợp tay trái và tay phải. Hãy chuẩn bị để thực hành nhé!", "imageDescription": "Một nghệ sĩ piano đang phối hợp tay trái và tay phải, thể hiện sự hòa quyện trong âm nhạc.", "videoDescription": "Video hướng dẫn cách phối hợp tay trái và tay phải trong piano để tạo ra âm thanh harmonize."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:46.243959', '2026-01-27 12:52:46.243959', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách phối hợp giữa tay trái và tay phải để tạo ra âm thanh hòa quyện.", "detailed": "Phối hợp giữa tay trái và tay phải là rất quan trọng để tạo nên một bản nhạc hoàn chỉnh. Bắt đầu với các giai điệu đơn giản.", "comprehensive": "Khi phối hợp tay trái với tay phải, bạn cần chú ý đến nhịp điệu và âm sắc. Thực hành các đoạn nhạc đơn giản với tay trái chơi hợp âm và tay phải chơi giai điệu sẽ giúp bạn phát triển cảm nhận âm nhạc tốt hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c2c1f6e3-aa56-4f5a-9347-cd2d0ccee15f', 'd0d968fd-ca4d-4835-9585-5c9d6a12a1a9', 'concept', 'Sắp xếp và định dạng bảng trong Word', 'Sắp xếp và định dạng bảng giúp tài liệu trông chuyên nghiệp hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng được định dạng trong Word với các kiểu dáng và màu sắc khác nhau.", "videoScript": "Xin chào! Hôm nay chúng ta sẽ cùng tìm hiểu cách định dạng và sắp xếp bảng trong Word. Bạn có thể chọn một kiểu dáng từ tab ''Table Design'' để làm cho bảng của mình nổi bật.", "imageDescription": "Hình ảnh bảng trong Word với các ô có màu sắc khác nhau và kiểu dáng chuyên nghiệp.", "videoDescription": "Video hướng dẫn cách sắp xếp và định dạng bảng trong Microsoft Word."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:30:33.497377', '2026-01-27 12:30:33.497377', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể thay đổi kiểu dáng và màu sắc của bảng.", "detailed": "Sắp xếp và định dạng bảng giúp tài liệu trông chuyên nghiệp hơn.", "comprehensive": "Sau khi chèn bảng vào tài liệu Word, bạn có thể sắp xếp và định dạng nó để làm nổi bật thông tin. Sử dụng các công cụ trong tab ''Table Design'' để chọn kiểu dáng và màu sắc, cũng như để sắp xếp dữ liệu trong bảng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c2b3043e-4cfe-44b1-91c6-c925dd15c30f', 'd4c52247-5e59-474e-81d2-16996c679847', 'concept', 'Các Loại Hợp Âm Nâng Cao', 'Các loại hợp âm nâng cao bao gồm hợp âm 7th, 9th, 11th và các hợp âm biến thể khác, mỗi loại mang đến một âm sắc khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bảng mô tả các loại hợp âm nâng cao và cách chơi chúng trên piano.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu về các loại hợp âm nâng cao và cách sử dụng chúng trong âm nhạc.", "imageDescription": "Một bảng mô tả với các hình ảnh minh họa cho từng loại hợp âm nâng cao.", "videoDescription": "Video giải thích các loại hợp âm nâng cao và cách chơi chúng trên piano."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:49:48.764374', '2026-01-27 12:49:48.764374', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại hợp âm nâng cao như hợp âm 7, 9, và 11.", "detailed": "Các loại hợp âm nâng cao bao gồm hợp âm 7th, 9th, 11th và các hợp âm biến thể khác, mỗi loại mang đến một âm sắc khác nhau.", "comprehensive": "Hợp âm nâng cao được chia thành nhiều loại như hợp âm 7th (major, minor, dominant), 9th (major, minor), và 11th. Mỗi loại hợp âm có cách sử dụng và vai trò riêng trong việc tạo ra cảm xúc âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4ed507a5-126f-446f-b92c-70e6586faaef', 'a82a703d-7936-4b23-8541-3b435cd2900e', 'concept', 'Choosing the Right Cleanser for Your Skin Type', 'Da dầu cần sữa rửa mặt kiềm dầu trong khi da khô nên chọn sữa rửa mặt dịu nhẹ. Hiểu rõ loại da của bạn sẽ giúp bạn lựa chọn sản phẩm phù hợp.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "A variety of cleansers lined up, labeled for different skin types.", "videoScript": "In this segment, we will explore how to choose the right cleanser for your skin type. We''ll discuss the characteristics of different skin types and the best cleansers to use.", "imageDescription": "Different types of facial cleansers for oily, dry, and combination skin.", "videoDescription": "A video tutorial on selecting the right cleanser based on skin type, with product recommendations."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:15:44.396842', '2026-01-27 13:15:44.396842', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mỗi loại da cần một loại sữa rửa mặt khác nhau. Chọn sữa rửa mặt phù hợp là rất quan trọng.", "detailed": "Da dầu cần sữa rửa mặt kiềm dầu trong khi da khô nên chọn sữa rửa mặt dịu nhẹ. Hiểu rõ loại da của bạn sẽ giúp bạn lựa chọn sản phẩm phù hợp.", "comprehensive": "Việc lựa chọn sữa rửa mặt phù hợp với loại da là rất quan trọng để đảm bảo hiệu quả làm sạch mà không làm hỏng hàng rào bảo vệ tự nhiên của da. Da nhờn có thể sử dụng gel hoặc foam cleanser, trong khi da khô nên chọn cream cleanser hoặc dầu tẩy trang để giữ ẩm cho da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a8494533-e14e-4299-98b4-20800d0d591a', 'a82a703d-7936-4b23-8541-3b435cd2900e', 'concept', 'The Importance of Double Cleansing', 'Double cleansing là quá trình sử dụng hai loại sản phẩm làm sạch: dầu tẩy trang trước, sau đó là sữa rửa mặt. Phương pháp này giúp loại bỏ hoàn toàn bụi bẩn và dầu thừa.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "A step-by-step illustration of the double cleansing process.", "videoScript": "Let''s dive into the double cleansing method. First, we use an oil cleanser to break down makeup and impurities, followed by a water-based cleanser to ensure a deep clean.", "imageDescription": "Visual guide showing the double cleansing method with oil and foam cleanser.", "videoDescription": "A demonstration of the double cleansing technique, showing how to properly cleanse the skin in two steps."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:15:44.400631', '2026-01-27 13:15:44.400631', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Double cleansing giúp làm sạch sâu hơn cho da, loại bỏ lớp trang điểm và bụi bẩn.", "detailed": "Double cleansing là quá trình sử dụng hai loại sản phẩm làm sạch: dầu tẩy trang trước, sau đó là sữa rửa mặt. Phương pháp này giúp loại bỏ hoàn toàn bụi bẩn và dầu thừa.", "comprehensive": "Double cleansing đã trở thành một phần quan trọng trong quy trình chăm sóc da của nhiều người. Bằng cách sử dụng dầu tẩy trang để loại bỏ lớp trang điểm và bụi bẩn trên bề mặt da, sau đó là một sữa rửa mặt để làm sạch sâu bên trong lỗ chân lông, bạn sẽ có một làn da sạch sẽ và sáng khỏe hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('166ff325-57c7-4968-b7ea-8ef416244f23', '3df79272-1464-47c7-b18f-8d0490f9c173', 'concept', 'Giới thiệu về Hàm trong Excel', 'Hàm trong Excel là một công cụ mạnh mẽ cho phép người dùng thực hiện các phép toán và tính toán phức tạp chỉ với một cú nhấp chuột.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh mô tả giao diện Excel với các ô tính và thanh công thức.", "videoScript": "Chào mừng bạn đến với bài học về hàm trong Excel. Trong bài học này, chúng ta sẽ khám phá cách sử dụng các hàm cơ bản để làm việc hiệu quả hơn với dữ liệu.", "imageDescription": "Giao diện Excel với các ô đã được nhập hàm và kết quả được hiển thị.", "videoDescription": "Video hướng dẫn về khái niệm và ứng dụng của hàm trong Excel."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:00.968661', '2026-01-27 12:31:00.968661', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hàm là công cụ giúp thực hiện phép toán trong Excel.", "detailed": "Hàm trong Excel là một công cụ mạnh mẽ cho phép người dùng thực hiện các phép toán và tính toán phức tạp chỉ với một cú nhấp chuột.", "comprehensive": "Hàm trong Excel là một phần không thể thiếu trong việc xử lý dữ liệu. Chúng cho phép người dùng thực hiện các phép toán toán học, thống kê, điều kiện và nhiều hơn nữa, giúp tiết kiệm thời gian và nâng cao hiệu quả công việc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2b727f30-8c8a-4c39-a96a-d52055a29cae', '3df79272-1464-47c7-b18f-8d0490f9c173', 'concept', 'Sử Dụng Hàm SUM', 'Hàm SUM là một trong những hàm cơ bản nhất trong Excel, cho phép người dùng tính tổng của một dãy số nhanh chóng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng tính Excel với hàm SUM được áp dụng.", "videoScript": "Trong phần này, chúng ta sẽ tìm hiểu cách sử dụng hàm SUM để tính tổng các giá trị trong một cột. Hãy làm theo các bước sau để áp dụng hàm này.", "imageDescription": "Bảng tính Excel với cột số liệu và ô hiển thị kết quả của hàm SUM.", "videoDescription": "Video hướng dẫn cách sử dụng hàm SUM trong Excel."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:00.977394', '2026-01-27 12:31:00.977394', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hàm SUM giúp tính tổng các số trong Excel.", "detailed": "Hàm SUM là một trong những hàm cơ bản nhất trong Excel, cho phép người dùng tính tổng của một dãy số nhanh chóng.", "comprehensive": "Hàm SUM được sử dụng để tính tổng các giá trị trong một khoảng ô. Nó có thể được áp dụng cho nhiều loại dữ liệu và rất hữu ích trong việc tổng hợp số liệu trong bảng tính."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('59176653-6029-401d-9c47-5f3ed654d768', 'a82a703d-7936-4b23-8541-3b435cd2900e', 'concept', 'Common Cleansing Mistakes to Avoid', 'Một số sai lầm thường gặp khi làm sạch da bao gồm rửa mặt quá mạnh, không rửa sạch sản phẩm và không làm sạch đủ lâu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "A checklist of cleansing dos and don''ts.", "videoScript": "In this section, we''ll highlight common cleansing mistakes and how to avoid them for healthier skin.", "imageDescription": "A visual guide listing common cleansing mistakes and how to avoid them.", "videoDescription": "A video discussing common mistakes made during the cleansing process and tips for effective cleansing."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:15:44.40396', '2026-01-27 13:15:44.40396', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tránh những sai lầm khi làm sạch da để có làn da khỏe mạnh.", "detailed": "Một số sai lầm thường gặp khi làm sạch da bao gồm rửa mặt quá mạnh, không rửa sạch sản phẩm và không làm sạch đủ lâu.", "comprehensive": "Nhiều người thường mắc phải những sai lầm khi làm sạch da như rửa mặt quá mạnh, sử dụng sản phẩm không phù hợp hoặc không rửa sạch mặt sau khi sử dụng sữa rửa mặt. Để có làn da khỏe mạnh, hãy chú ý đến cách bạn làm sạch và đảm bảo thực hiện đúng phương pháp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('964a0207-8442-4ee5-9752-8dd20a010d03', '3df79272-1464-47c7-b18f-8d0490f9c173', 'concept', 'Hàm AVERAGE để Tính Trung Bình', 'Hàm AVERAGE là một công cụ hữu ích trong Excel để tính toán giá trị trung bình của một nhóm số liệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng tính Excel với hàm AVERAGE đang được sử dụng.", "videoScript": "Bây giờ, chúng ta sẽ xem cách sử dụng hàm AVERAGE để tính giá trị trung bình. Hãy chọn các ô bạn muốn tính và nhập hàm vào ô kết quả.", "imageDescription": "Giao diện Excel hiển thị cách sử dụng hàm AVERAGE để tính giá trị trung bình.", "videoDescription": "Video hướng dẫn cách sử dụng hàm AVERAGE trong Excel để tính giá trị trung bình."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:00.980828', '2026-01-27 12:31:00.980828', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hàm AVERAGE giúp tính giá trị trung bình của một dãy số.", "detailed": "Hàm AVERAGE là một công cụ hữu ích trong Excel để tính toán giá trị trung bình của một nhóm số liệu.", "comprehensive": "Hàm AVERAGE được sử dụng để xác định giá trị trung bình của các ô chứa số trong một khoảng nhất định. Hàm này rất hữu ích trong các báo cáo tài chính và phân tích dữ liệu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e1b350f9-9e1f-4b1b-b170-8db8f45bc274', 'a82a703d-7936-4b23-8541-3b435cd2900e', 'concept', 'How to Properly Exfoliate After Cleansing', 'Sau khi làm sạch, tẩy tế bào chết giúp loại bỏ lớp da chết, làm thông thoáng lỗ chân lông và tăng cường hiệu quả của các sản phẩm dưỡng da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "An image of exfoliation products with usage instructions.", "videoScript": "Today, we will learn how to properly exfoliate after cleansing. Exfoliation helps remove dead skin cells and enhances your skin''s texture.", "imageDescription": "Different types of exfoliators including scrubs and chemical exfoliants.", "videoDescription": "A tutorial on exfoliating techniques and the importance of exfoliation in skincare."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:15:44.409823', '2026-01-27 13:15:44.409823', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tẩy tế bào chết phù hợp giúp da mịn màng hơn sau khi làm sạch.", "detailed": "Sau khi làm sạch, tẩy tế bào chết giúp loại bỏ lớp da chết, làm thông thoáng lỗ chân lông và tăng cường hiệu quả của các sản phẩm dưỡng da.", "comprehensive": "Tẩy tế bào chết là bước quan trọng sau khi làm sạch da. Nó giúp loại bỏ lớp da chết, cải thiện kết cấu da và làm cho các sản phẩm dưỡng da thẩm thấu tốt hơn. Tùy vào loại da, bạn có thể chọn tẩy tế bào chết hóa học hoặc vật lý, nhưng hãy nhớ không nên lạm dụng để tránh làm tổn thương da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5e5da842-be95-4202-9495-de190a548fd9', '980807c3-7f72-4f43-ad22-d0670b74a227', 'concept', 'Luyện Tập Hợp Âm Bằng Tay Trái', 'Chơi hợp âm bằng tay trái giúp tạo ra một nền tảng vững chắc cho giai điệu của tay phải. Bắt đầu với những hợp âm đơn giản.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bàn tay trái đang chơi hợp âm trên piano.", "videoScript": "Trong video này, chúng ta sẽ luyện tập chơi hợp âm bằng tay trái. Hãy cùng nhau thực hành nhé!", "imageDescription": "Bàn tay trái đang đặt trên các phím của đàn piano để chơi hợp âm C.", "videoDescription": "Video hướng dẫn cách chơi hợp âm bằng tay trái trên piano."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:46.247838', '2026-01-27 12:52:46.247838', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách chơi hợp âm bằng tay trái để tạo nền cho giai điệu.", "detailed": "Chơi hợp âm bằng tay trái giúp tạo ra một nền tảng vững chắc cho giai điệu của tay phải. Bắt đầu với những hợp âm đơn giản.", "comprehensive": "Kỹ thuật chơi hợp âm bằng tay trái không chỉ giúp làm phong phú âm thanh mà còn giúp bạn hiểu rõ hơn về cấu trúc âm nhạc. Thực hành các hợp âm như C, G, Am, và F để tạo ra nền tảng cho các bản nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4fa0017b-e839-449d-983e-dba6466e3911', '3df79272-1464-47c7-b18f-8d0490f9c173', 'concept', 'Hàm COUNT và COUNTIF', 'Hàm COUNT cho phép đếm số lượng ô có chứa dữ liệu, trong khi COUNTIF cho phép bạn đếm số ô thỏa mãn một điều kiện cụ thể.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng tính Excel với các hàm COUNT và COUNTIF.", "videoScript": "Trong phần này, chúng ta sẽ tìm hiểu cách sử dụng hàm COUNT và COUNTIF. Bạn sẽ thấy sự khác biệt giữa hai hàm này và cách chúng có thể hỗ trợ bạn trong việc phân tích dữ liệu.", "imageDescription": "Bảng tính Excel hiển thị cách sử dụng hàm COUNT và COUNTIF cùng với kết quả.", "videoDescription": "Video hướng dẫn cách sử dụng hai hàm COUNT và COUNTIF trong Excel."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:00.984623', '2026-01-27 12:31:00.984623', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hàm COUNT đếm số ô chứa dữ liệu, COUNTIF đếm theo điều kiện.", "detailed": "Hàm COUNT cho phép đếm số lượng ô có chứa dữ liệu, trong khi COUNTIF cho phép bạn đếm số ô thỏa mãn một điều kiện cụ thể.", "comprehensive": "Hàm COUNT là một công cụ quan trọng để đếm số ô có chứa giá trị trong một bảng tính. Hàm COUNTIF còn mạnh mẽ hơn khi cho phép bạn chỉ định một điều kiện cụ thể để đếm, giúp phân tích dữ liệu hiệu quả hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c5d54099-c760-4ee8-9159-c6c26302d7f6', 'd4c52247-5e59-474e-81d2-16996c679847', 'concept', 'Kỹ Thuật Chơi Hợp Âm Nâng Cao', 'Kỹ thuật chơi hợp âm nâng cao bao gồm việc sử dụng ngón tay linh hoạt và cảm nhận nhịp điệu để tạo ra âm thanh tốt nhất.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Người chơi piano đang thực hành kỹ thuật chơi hợp âm nâng cao.", "videoScript": "Hãy cùng nhau luyện tập các kỹ thuật chơi hợp âm nâng cao, từ việc sử dụng ngón tay đến cách cảm nhận nhịp điệu.", "imageDescription": "Hình ảnh một người chơi piano với các ngón tay trên các phím đàn, thể hiện kỹ thuật chơi hợp âm nâng cao.", "videoDescription": "Video hướng dẫn các kỹ thuật chơi hợp âm nâng cao trên piano."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:49:48.768116', '2026-01-27 12:49:48.768116', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chơi hợp âm nâng cao cần kỹ thuật và sự nhạy bén.", "detailed": "Kỹ thuật chơi hợp âm nâng cao bao gồm việc sử dụng ngón tay linh hoạt và cảm nhận nhịp điệu để tạo ra âm thanh tốt nhất.", "comprehensive": "Để chơi hợp âm nâng cao, người chơi cần phát triển kỹ năng ngón tay linh hoạt, khả năng thay đổi vị trí ngón tay nhanh chóng và cảm nhận tốt về nhịp điệu và cao độ. Việc luyện tập thường xuyên sẽ giúp cải thiện khả năng chơi hợp âm phức tạp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('fc199aad-2f7e-4383-a409-b5ce67e1bc6f', 'd825d720-549c-4003-9315-0c5443034518', 'concept', 'Nhãn Sản Phẩm Chăm Sóc Da: Tổng Quan', 'Nhãn sản phẩm chăm sóc da không chỉ cho biết thành phần chính mà còn hướng dẫn cách sử dụng và những điều cần lưu ý.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhãn sản phẩm chăm sóc da với các thành phần được chỉ rõ.", "videoScript": "Hôm nay, chúng ta sẽ khám phá cách đọc nhãn sản phẩm chăm sóc da. Bạn sẽ học cách nhận biết các thành phần và công dụng của sản phẩm.", "imageDescription": "Nhãn sản phẩm chăm sóc da với các thành phần tự nhiên và công thức cụ thể.", "videoDescription": "Video hướng dẫn cách đọc nhãn sản phẩm chăm sóc da và hiểu các thông tin quan trọng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:16:17.357681', '2026-01-27 13:16:17.357681', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhãn sản phẩm chăm sóc da cung cấp thông tin quan trọng về thành phần và công dụng.", "detailed": "Nhãn sản phẩm chăm sóc da không chỉ cho biết thành phần chính mà còn hướng dẫn cách sử dụng và những điều cần lưu ý.", "comprehensive": "Nhãn sản phẩm chăm sóc da bao gồm các thông tin như thành phần, cách sử dụng, và các chỉ dẫn về an toàn. Việc hiểu rõ nhãn sẽ giúp bạn chọn sản phẩm phù hợp với loại da và nhu cầu cá nhân."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f7c567f9-2dcd-489b-9905-a2612df376ee', 'd825d720-549c-4003-9315-0c5443034518', 'concept', 'Thành Phần Chính Trong Sản Phẩm Chăm Sóc Da', 'Các thành phần như hyaluronic acid, retinol, và vitamin C thường được tìm thấy trong sản phẩm chăm sóc da và có những lợi ích riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các thành phần chăm sóc da phổ biến.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về các thành phần chính trong sản phẩm chăm sóc da và cách chúng hoạt động.", "imageDescription": "Sự kết hợp của các thành phần chăm sóc da với mô tả về công dụng của từng thành phần.", "videoDescription": "Video giải thích các thành phần phổ biến trong sản phẩm chăm sóc da và lợi ích của chúng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:16:17.366255', '2026-01-27 13:16:17.366255', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biết được các thành phần chính giúp chọn sản phẩm tốt hơn.", "detailed": "Các thành phần như hyaluronic acid, retinol, và vitamin C thường được tìm thấy trong sản phẩm chăm sóc da và có những lợi ích riêng.", "comprehensive": "Hiểu rõ các thành phần chính trong sản phẩm chăm sóc da như hyaluronic acid, retinol, và vitamin C sẽ giúp bạn lựa chọn sản phẩm phù hợp với nhu cầu da của mình. Mỗi thành phần có công dụng riêng và có thể ảnh hưởng đến sức khỏe làn da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('85963357-a802-4c57-b5bc-1abc1c673e9b', 'd825d720-549c-4003-9315-0c5443034518', 'concept', 'Cách Đọc Nhãn Sản Phẩm Chăm Sóc Da', 'Cách đọc nhãn sản phẩm chăm sóc da bao gồm việc chú ý đến thứ tự thành phần và các thông tin bổ sung.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhãn sản phẩm với các phần được chú thích.", "videoScript": "Trong video này, chúng ta sẽ hướng dẫn bạn cách đọc nhãn sản phẩm chăm sóc da một cách hiệu quả.", "imageDescription": "Nhãn sản phẩm chăm sóc da với các phần như thành phần, hướng dẫn sử dụng được chú thích rõ ràng.", "videoDescription": "Video chỉ dẫn chi tiết về cách đọc nhãn sản phẩm chăm sóc da và hiểu rõ thông tin trên đó."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:16:17.369808', '2026-01-27 13:16:17.369808', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách đọc nhãn để hiểu rõ hơn về sản phẩm.", "detailed": "Cách đọc nhãn sản phẩm chăm sóc da bao gồm việc chú ý đến thứ tự thành phần và các thông tin bổ sung.", "comprehensive": "Đọc nhãn sản phẩm chăm sóc da không chỉ là việc xem thành phần mà còn là hiểu rõ cách sử dụng, lưu ý về các cảnh báo và cách bảo quản. Việc này giúp bạn tối ưu hóa hiệu quả của sản phẩm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('257c05e9-316b-4815-ac44-863903ad4f7a', '980807c3-7f72-4f43-ad22-d0670b74a227', 'concept', 'Áp Dụng Kỹ Thuật Tay Trái Trong Bản Nhạc', 'Hãy thử áp dụng những gì bạn đã học về tay trái vào một bản nhạc cụ thể. Chọn một bài hát yêu thích và thực hành.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nghệ sĩ piano đang biểu diễn một bản nhạc với tay trái và tay phải.", "videoScript": "Hãy cùng nhau áp dụng kỹ thuật tay trái vào bản nhạc yêu thích của bạn. Chúng ta sẽ thực hành từng phần một.", "imageDescription": "Một nghệ sĩ piano biểu diễn một bản nhạc với sự phối hợp hoàn hảo giữa tay trái và tay phải.", "videoDescription": "Video hướng dẫn cách áp dụng kỹ thuật tay trái vào các bản nhạc thực tế."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:46.251426', '2026-01-27 12:52:46.251426', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Áp dụng kỹ thuật tay trái vào các bản nhạc thực tế.", "detailed": "Hãy thử áp dụng những gì bạn đã học về tay trái vào một bản nhạc cụ thể. Chọn một bài hát yêu thích và thực hành.", "comprehensive": "Khi đã nắm vững kỹ thuật tay trái, hãy áp dụng vào các bản nhạc thực tế. Chọn các bài hát có sự kết hợp giữa tay trái và tay phải để thực hành, và hãy chú ý đến cách mà tay trái hỗ trợ giai điệu chính."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('65e8305c-626a-4677-924d-87b94b5071c0', 'd4c52247-5e59-474e-81d2-16996c679847', 'concept', 'Ứng Dụng Hợp Âm Nâng Cao Trong Sáng Tác', 'Khi sáng tác, việc sử dụng hợp âm nâng cao có thể giúp tạo ra những âm thanh độc đáo và mới mẻ cho tác phẩm của bạn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nhạc sĩ đang sáng tác âm nhạc trên piano với các hợp âm nâng cao.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu cách ứng dụng hợp âm nâng cao trong sáng tác âm nhạc.", "imageDescription": "Hình ảnh một nhạc sĩ đang sáng tác với các ghi chú và hợp âm nâng cao trên bản nhạc.", "videoDescription": "Video hướng dẫn cách sử dụng hợp âm nâng cao trong sáng tác âm nhạc."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:49:48.772621', '2026-01-27 12:49:48.772621', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm nâng cao có thể làm phong phú thêm bài hát của bạn.", "detailed": "Khi sáng tác, việc sử dụng hợp âm nâng cao có thể giúp tạo ra những âm thanh độc đáo và mới mẻ cho tác phẩm của bạn.", "comprehensive": "Hợp âm nâng cao không chỉ giúp làm phong phú thêm âm nhạc mà còn mở ra nhiều khả năng sáng tạo trong việc phát triển giai điệu và hòa âm. Bằng cách kết hợp các hợp âm phức tạp, bạn có thể tạo ra những tác phẩm âm nhạc độc đáo và ấn tượng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('27f927e0-38c2-4511-8c01-f2adf174fa81', 'd825d720-549c-4003-9315-0c5443034518', 'concept', 'Các Chỉ Dẫn An Toàn Khi Sử Dụng Sản Phẩm Chăm Sóc Da', 'Trước khi sử dụng sản phẩm mới, hãy kiểm tra thành phần và thực hiện thử nghiệm trên một vùng da nhỏ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người thử nghiệm sản phẩm chăm sóc da trên vùng da nhỏ.", "videoScript": "Hãy cùng tìm hiểu về các chỉ dẫn an toàn khi sử dụng sản phẩm chăm sóc da trong video này.", "imageDescription": "Hình ảnh mô tả quá trình thử nghiệm sản phẩm chăm sóc da để đảm bảo an toàn cho làn da.", "videoDescription": "Video hướng dẫn các biện pháp an toàn khi sử dụng sản phẩm chăm sóc da."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:16:17.37342', '2026-01-27 13:16:17.37342', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lưu ý an toàn khi sử dụng sản phẩm chăm sóc da.", "detailed": "Trước khi sử dụng sản phẩm mới, hãy kiểm tra thành phần và thực hiện thử nghiệm trên một vùng da nhỏ.", "comprehensive": "Khi sử dụng sản phẩm chăm sóc da, việc đọc nhãn và hiểu rõ các chỉ dẫn an toàn là rất quan trọng. Bạn nên kiểm tra thành phần, thực hiện thử nghiệm trên một vùng da nhỏ và chú ý đến phản ứng của da để tránh kích ứng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0c6248de-c616-46ed-ba54-285ec6b8dd04', 'd825d720-549c-4003-9315-0c5443034518', 'concept', 'Những Lưu Ý Khi Chọn Sản Phẩm Chăm Sóc Da', 'Lưu ý đến loại da của bạn (da dầu, da khô, da nhạy cảm) và chọn sản phẩm tương ứng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các loại da khác nhau và sản phẩm tương ứng.", "videoScript": "Trong video này, chúng ta sẽ thảo luận về cách chọn sản phẩm chăm sóc da phù hợp với từng loại da.", "imageDescription": "Hình ảnh mô tả các loại da như da dầu, da khô, và da nhạy cảm với sản phẩm chăm sóc da phù hợp.", "videoDescription": "Video giải thích cách chọn sản phẩm chăm sóc da dựa trên loại da và tình trạng da."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:16:17.377352', '2026-01-27 13:16:17.377352', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn sản phẩm phù hợp với loại da và tình trạng da.", "detailed": "Lưu ý đến loại da của bạn (da dầu, da khô, da nhạy cảm) và chọn sản phẩm tương ứng.", "comprehensive": "Khi chọn sản phẩm chăm sóc da, hãy xem xét loại da của bạn, các vấn đề cụ thể như mụn, lão hóa hay nhạy cảm. Đọc nhãn và tìm kiếm sản phẩm phù hợp với nhu cầu của làn da bạn để đạt hiệu quả tốt nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0eb6b36d-8c49-4980-9861-2dbd59ace139', 'd4c52247-5e59-474e-81d2-16996c679847', 'concept', 'Phân Tích Các Bản Nhạc Sử Dụng Hợp Âm Nâng Cao', 'Phân tích các bản nhạc nổi tiếng sử dụng hợp âm nâng cao sẽ giúp bạn hiểu rõ hơn về cách mà các nhạc sĩ áp dụng chúng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng phân tích các bản nhạc với hợp âm nâng cao.", "videoScript": "Hãy cùng nhau phân tích một số bản nhạc nổi tiếng để hiểu rõ hơn về cách sử dụng hợp âm nâng cao.", "imageDescription": "Bảng phân tích với các ví dụ về bản nhạc và hợp âm nâng cao được sử dụng.", "videoDescription": "Video phân tích các bản nhạc sử dụng hợp âm nâng cao."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:49:48.77732', '2026-01-27 12:49:48.77732', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phân tích bản nhạc giúp hiểu cách sử dụng hợp âm nâng cao.", "detailed": "Phân tích các bản nhạc nổi tiếng sử dụng hợp âm nâng cao sẽ giúp bạn hiểu rõ hơn về cách mà các nhạc sĩ áp dụng chúng.", "comprehensive": "Bằng cách phân tích các bản nhạc nổi tiếng, chúng ta có thể khám phá cách mà hợp âm nâng cao được sử dụng để tạo ra những cảm xúc và sắc thái khác nhau trong âm nhạc. Việc này không chỉ giúp bạn học hỏi mà còn phát triển khả năng sáng tác của bản thân."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3e155a9b-05ea-48d1-869a-6226d5de5870', '85b80ea8-5a66-481b-b97f-7d945fe98d80', 'concept', 'Giới Thiệu Về Sản Phẩm Chăm Sóc Da', 'Sản phẩm chăm sóc da bao gồm sữa rửa mặt, toner, serum, kem dưỡng ẩm và kem chống nắng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bộ sản phẩm chăm sóc da cơ bản.", "videoScript": "Chào mừng bạn đến với thế giới chăm sóc da! Hôm nay, chúng ta sẽ tìm hiểu về các sản phẩm chăm sóc da cơ bản mà ai cũng nên có.", "imageDescription": "Bộ sản phẩm chăm sóc da gồm sữa rửa mặt, toner, serum và kem dưỡng ẩm.", "videoDescription": "Video giới thiệu về các sản phẩm chăm sóc da cơ bản và cách sử dụng chúng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:16:45.534554', '2026-01-27 13:16:45.534554', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khám phá các sản phẩm chăm sóc da cơ bản.", "detailed": "Sản phẩm chăm sóc da bao gồm sữa rửa mặt, toner, serum, kem dưỡng ẩm và kem chống nắng.", "comprehensive": "Chăm sóc da là một quy trình bao gồm nhiều bước với các sản phẩm khác nhau từ sữa rửa mặt giúp làm sạch, toner cân bằng độ pH, serum cung cấp dưỡng chất, đến kem dưỡng ẩm và kem chống nắng để bảo vệ da khỏi tác hại của môi trường."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f63fbb4b-e5a6-46a9-8158-b591b078503d', '3df79272-1464-47c7-b18f-8d0490f9c173', 'concept', 'Hàm IF và Ứng Dụng của Nó', 'Hàm IF là một trong những hàm điều kiện phổ biến nhất trong Excel, cho phép bạn kiểm tra một điều kiện và trả về các giá trị khác nhau dựa trên kết quả của điều kiện đó.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng tính Excel với hàm IF đang được áp dụng.", "videoScript": "Bây giờ, chúng ta sẽ tìm hiểu cách sử dụng hàm IF để kiểm tra điều kiện trong bảng tính. Hãy làm theo các bước để áp dụng hàm này một cách chính xác.", "imageDescription": "Giao diện Excel hiển thị cách sử dụng hàm IF để kiểm tra điều kiện.", "videoDescription": "Video hướng dẫn cách sử dụng hàm IF trong Excel."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:00.990569', '2026-01-27 12:31:00.990569', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hàm IF dùng để kiểm tra điều kiện và trả về kết quả khác nhau.", "detailed": "Hàm IF là một trong những hàm điều kiện phổ biến nhất trong Excel, cho phép bạn kiểm tra một điều kiện và trả về các giá trị khác nhau dựa trên kết quả của điều kiện đó.", "comprehensive": "Hàm IF rất linh hoạt và mạnh mẽ, cho phép người dùng lập trình logic cho bảng tính của họ. Bạn có thể sử dụng hàm IF để kiểm tra giá trị và quyết định hành động tiếp theo dựa trên kết quả đó, giúp nâng cao khả năng xử lý dữ liệu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1dbffa53-236e-458b-a047-63469931003b', '85b80ea8-5a66-481b-b97f-7d945fe98d80', 'concept', 'Các Loại Serum và Tác Dụng Của Chúng', 'Serum có thể chứa vitamin C, hyaluronic acid, và retinol, mỗi loại có công dụng riêng biệt cho làn da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại serum chăm sóc da.", "videoScript": "Hãy cùng tìm hiểu về các loại serum và công dụng của chúng đối với làn da bạn.", "imageDescription": "Nhiều loại serum khác nhau với các thành phần nổi bật.", "videoDescription": "Video khám phá các loại serum phổ biến và cách chúng giúp cải thiện tình trạng da."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:16:45.541845', '2026-01-27 13:16:45.541845', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Serum là sản phẩm đa năng giúp cải thiện nhiều vấn đề da.", "detailed": "Serum có thể chứa vitamin C, hyaluronic acid, và retinol, mỗi loại có công dụng riêng biệt cho làn da.", "comprehensive": "Serum là sản phẩm dưỡng da đặc biệt, thường có kết cấu nhẹ, chứa nồng độ cao các hoạt chất như vitamin C giúp làm sáng da, hyaluronic acid cung cấp độ ẩm, và retinol giúp trẻ hóa làn da, giảm nếp nhăn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9a15be81-c996-43e2-b313-64e2cd48c5ea', '85b80ea8-5a66-481b-b97f-7d945fe98d80', 'concept', 'Kem Dưỡng Ẩm: Tại Sao Nó Quan Trọng?', 'Kem dưỡng ẩm giúp bảo vệ da khỏi mất nước và duy trì sự mềm mại.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một hũ kem dưỡng ẩm với các thành phần tự nhiên.", "videoScript": "Kem dưỡng ẩm là bước không thể thiếu trong bất kỳ quy trình chăm sóc da nào. Hãy cùng tìm hiểu lý do tại sao.", "imageDescription": "Hũ kem dưỡng ẩm với thành phần từ thiên nhiên giúp giữ ẩm cho da.", "videoDescription": "Video giải thích tầm quan trọng của kem dưỡng ẩm trong chăm sóc da và cách chọn sản phẩm phù hợp."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:16:45.546033', '2026-01-27 13:16:45.546033', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kem dưỡng ẩm giúp giữ nước cho da.", "detailed": "Kem dưỡng ẩm giúp bảo vệ da khỏi mất nước và duy trì sự mềm mại.", "comprehensive": "Kem dưỡng ẩm đóng vai trò quan trọng trong quy trình chăm sóc da, giúp giữ nước và tạo hàng rào bảo vệ da, ngăn ngừa tình trạng da khô, bong tróc. Các thành phần như glycerin và ceramide giúp tăng cường độ ẩm cho da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a4d40b2f-9d95-46c7-9092-bd0edd90a91a', '9cde4cdc-e2bf-49e0-aafc-0570def1ca25', 'concept', 'Hiểu về Diễn cảm trong Piano', 'Diễn cảm trong piano là việc sử dụng kỹ thuật và cảm xúc để truyền tải ý nghĩa của bản nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ piano đang biểu diễn với cảm xúc mãnh liệt.", "videoScript": "Chào mừng đến với bài học hôm nay về diễn cảm trong piano. Hãy cùng khám phá cách mà âm nhạc có thể truyền tải cảm xúc.", "imageDescription": "Hình ảnh một nghệ sĩ piano với ánh sáng mềm mại, thể hiện cảm xúc trong khi chơi.", "videoDescription": "Video này sẽ giới thiệu về khái niệm diễn cảm trong piano và cách áp dụng nó trong biểu diễn."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:06.900408', '2026-01-27 12:53:06.900408', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Diễn cảm là cách thể hiện cảm xúc qua âm nhạc.", "detailed": "Diễn cảm trong piano là việc sử dụng kỹ thuật và cảm xúc để truyền tải ý nghĩa của bản nhạc.", "comprehensive": "Diễn cảm trong biểu diễn piano không chỉ là việc chơi đúng nốt nhạc mà còn là khả năng truyền tải cảm xúc và tâm trạng của tác phẩm. Nó bao gồm cách sử dụng động lực, tempo và kỹ thuật biểu diễn để tạo ra một trải nghiệm âm nhạc sâu sắc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('dbe81d34-2e00-4f55-b2e1-f7348789d1a2', 'dd1d6bb4-9031-43ac-9c44-956e8bae7e61', 'concept', 'Giới Thiệu Về Hợp Âm', 'Hợp âm là sự kết hợp của ba hoặc nhiều nốt nhạc, tạo ra âm thanh hòa quyện. Có nhiều loại hợp âm như hợp âm trưởng và hợp âm thứ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bàn phím piano với các nốt nhạc được đánh dấu hợp âm.", "videoScript": "Chào các bạn! Hôm nay chúng ta sẽ khám phá hợp âm cơ bản. Hợp âm là sự kết hợp của nhiều nốt nhạc, và chúng ta sẽ bắt đầu với hợp âm trưởng.", "imageDescription": "Hình ảnh bàn phím piano với các nốt nhạc C, E, G được đánh dấu cho hợp âm C trưởng.", "videoDescription": "Video hướng dẫn giới thiệu về hợp âm cơ bản trên piano, tập trung vào hợp âm trưởng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:50:16.079204', '2026-01-27 12:50:16.079204', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm là sự kết hợp của ba nốt nhạc trở lên.", "detailed": "Hợp âm là sự kết hợp của ba hoặc nhiều nốt nhạc, tạo ra âm thanh hòa quyện. Có nhiều loại hợp âm như hợp âm trưởng và hợp âm thứ.", "comprehensive": "Hợp âm là sự kết hợp đồng thời của ba nốt nhạc trở lên, thường được sử dụng trong âm nhạc để tạo ra chiều sâu và sự phong phú. Hợp âm có thể được phân loại thành nhiều loại khác nhau, trong đó hợp âm trưởng và hợp âm thứ là hai loại cơ bản nhất. Chúng thường được sử dụng trong sáng tác và biểu diễn nhạc cổ điển, pop, rock, và nhiều thể loại khác."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('659a9050-82cd-42ea-8855-8455e81a89f7', 'dd1d6bb4-9031-43ac-9c44-956e8bae7e61', 'concept', 'Hợp Âm Trưởng', 'Hợp âm trưởng bao gồm nốt gốc, nốt thứ ba lớn và nốt thứ năm. Ví dụ, hợp âm C trưởng gồm nốt C, E, G.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ hợp âm trưởng trên bàn phím piano.", "videoScript": "Bây giờ chúng ta sẽ học cách chơi hợp âm trưởng. Hãy nhìn vào bàn phím và đặt các ngón tay lên các nốt C, E, G.", "imageDescription": "Hình ảnh thể hiện vị trí các ngón tay trên bàn phím piano cho hợp âm C trưởng.", "videoDescription": "Video hướng dẫn cách chơi hợp âm trưởng trên piano, với hướng dẫn cụ thể về vị trí ngón tay."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:50:16.087071', '2026-01-27 12:50:16.087071', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm trưởng tạo ra âm thanh vui vẻ.", "detailed": "Hợp âm trưởng bao gồm nốt gốc, nốt thứ ba lớn và nốt thứ năm. Ví dụ, hợp âm C trưởng gồm nốt C, E, G.", "comprehensive": "Hợp âm trưởng là loại hợp âm tạo ra âm thanh sáng sủa và vui vẻ. Chúng được cấu tạo từ ba nốt nhạc: nốt gốc, nốt thứ ba lớn và nốt thứ năm. Ví dụ, hợp âm C trưởng bao gồm các nốt C, E và G. Hợp âm trưởng thường được sử dụng trong nhiều thể loại nhạc và là nền tảng cho nhiều bài hát."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d98cf3a4-92f9-4b8c-95e5-61c2c8ff15c0', '85b80ea8-5a66-481b-b97f-7d945fe98d80', 'concept', 'Cách Chọn Kem Chống Nắng Phù Hợp', 'Cần chọn kem chống nắng với chỉ số SPF phù hợp cho từng loại da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại kem chống nắng với chỉ số SPF khác nhau.", "videoScript": "Kem chống nắng là hàng rào bảo vệ cho làn da của bạn. Hôm nay, chúng ta sẽ cùng nhau tìm hiểu cách chọn kem chống nắng phù hợp.", "imageDescription": "Nhiều loại kem chống nắng với các chỉ số SPF khác nhau và thành phần tự nhiên.", "videoDescription": "Video hướng dẫn cách chọn kem chống nắng phù hợp với từng loại da và nhu cầu bảo vệ."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:16:45.549141', '2026-01-27 13:16:45.549141', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kem chống nắng bảo vệ da khỏi tia UV.", "detailed": "Cần chọn kem chống nắng với chỉ số SPF phù hợp cho từng loại da.", "comprehensive": "Kem chống nắng là sản phẩm thiết yếu giúp bảo vệ da khỏi tác hại của tia UV. Khi chọn kem chống nắng, bạn nên chú ý đến chỉ số SPF, phổ bảo vệ UVA/UVB, và thành phần an toàn cho da. Hãy tìm sản phẩm phù hợp với loại da của bạn để đạt hiệu quả tốt nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d924af3f-88cb-4637-bc64-879975ca9ac9', '85b80ea8-5a66-481b-b97f-7d945fe98d80', 'concept', 'Tầm Quan Trọng Của Việc Tẩy Tế Bào Chết', 'Việc tẩy tế bào chết giúp loại bỏ da chết, làm sáng da và ngăn ngừa mụn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh sản phẩm tẩy tế bào chết với các thành phần tự nhiên.", "videoScript": "Tẩy tế bào chết là bước không thể thiếu để có làn da khỏe mạnh. Hãy cùng tìm hiểu tầm quan trọng của nó.", "imageDescription": "Sản phẩm tẩy tế bào chết chứa các hạt tự nhiên giúp làm sạch da.", "videoDescription": "Video giải thích lợi ích của việc tẩy tế bào chết và cách thực hiện đúng cách."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:16:45.552636', '2026-01-27 13:16:45.552636', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tẩy tế bào chết giúp làm sạch da.", "detailed": "Việc tẩy tế bào chết giúp loại bỏ da chết, làm sáng da và ngăn ngừa mụn.", "comprehensive": "Tẩy tế bào chết là bước quan trọng trong quy trình chăm sóc da, giúp loại bỏ lớp tế bào chết, làm thông thoáng lỗ chân lông và cải thiện độ sáng của da. Có hai loại tẩy tế bào chết: vật lý và hóa học, mỗi loại có ưu và nhược điểm riêng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1ef37e4c-ad1a-4402-9e1f-f95dd103a638', 'dd1d6bb4-9031-43ac-9c44-956e8bae7e61', 'concept', 'Hợp Âm Thứ', 'Hợp âm thứ bao gồm nốt gốc, nốt thứ ba nhỏ và nốt thứ năm. Ví dụ, hợp âm A thứ gồm nốt A, C, E.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ hợp âm thứ trên bàn phím piano.", "videoScript": "Tiếp theo, chúng ta sẽ học hợp âm thứ. Hãy đặt ngón tay của bạn lên các nốt A, C, E để chơi hợp âm A thứ.", "imageDescription": "Hình ảnh thể hiện vị trí các ngón tay trên bàn phím piano cho hợp âm A thứ.", "videoDescription": "Video hướng dẫn cách chơi hợp âm thứ trên piano, chú trọng vào vị trí ngón tay và âm thanh."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:50:16.091', '2026-01-27 12:50:16.091', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm thứ tạo ra âm thanh buồn hơn.", "detailed": "Hợp âm thứ bao gồm nốt gốc, nốt thứ ba nhỏ và nốt thứ năm. Ví dụ, hợp âm A thứ gồm nốt A, C, E.", "comprehensive": "Hợp âm thứ là loại hợp âm tạo ra âm thanh buồn bã và sâu lắng. Chúng thường được cấu tạo từ ba nốt nhạc: nốt gốc, nốt thứ ba nhỏ và nốt thứ năm. Ví dụ, hợp âm A thứ bao gồm các nốt A, C và E. Hợp âm thứ thường được sử dụng trong các bài hát có chủ đề buồn hoặc sâu lắng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0b132ff6-11b5-46e0-9d03-6d5a9d6bf5b7', 'f3a30319-5825-460a-86fa-045981f35738', 'concept', 'Giới thiệu về Biểu Đồ trong Excel', 'Biểu đồ trong Excel là công cụ mạnh mẽ để trực quan hóa dữ liệu, giúp người dùng dễ dàng nhận diện các xu hướng và mối quan hệ trong dữ liệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một biểu đồ cột trong Excel.", "videoScript": "Xin chào! Hôm nay, chúng ta sẽ tìm hiểu về cách tạo biểu đồ trong Excel. Đầu tiên, hãy mở một bảng tính và nhập dữ liệu của bạn.", "imageDescription": "Mô tả biểu đồ cột với dữ liệu doanh thu hàng tháng.", "videoDescription": "Video hướng dẫn cách tạo biểu đồ trong Excel từ dữ liệu bảng tính."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:29.15747', '2026-01-27 12:31:29.15747', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ giúp bạn trực quan hóa dữ liệu một cách hiệu quả.", "detailed": "Biểu đồ trong Excel là công cụ mạnh mẽ để trực quan hóa dữ liệu, giúp người dùng dễ dàng nhận diện các xu hướng và mối quan hệ trong dữ liệu.", "comprehensive": "Biểu đồ trong Excel là một tính năng cho phép người dùng tạo ra các hình ảnh trực quan từ dữ liệu bảng tính. Chúng giúp tổ chức dữ liệu một cách hiệu quả hơn, từ đó hỗ trợ người dùng trong việc phân tích và đưa ra quyết định."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e0fa2b3c-0aa9-4e42-9b1b-7d8f6ecec635', 'dcdc7756-1c2d-4077-8f20-19cd69d7a272', 'concept', 'Giới Thiệu Về Thành Phần Hoạt Chất Trong Skincare', 'Thành phần hoạt chất là những hợp chất có tác dụng cụ thể trong các sản phẩm skincare. Chúng có thể giúp làm sạch da, cung cấp độ ẩm, hoặc làm sáng da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một sản phẩm skincare với các thành phần được liệt kê.", "videoScript": "Chào mừng các bạn đến với bài học về thành phần hoạt chất trong skincare. Hôm nay, chúng ta sẽ tìm hiểu về các thành phần quan trọng giúp cải thiện sức khỏe làn da.", "imageDescription": "Một sản phẩm serum với thành phần vitamin C, retinol, và axit hyaluronic, đặt trên bàn trang điểm.", "videoDescription": "Video giới thiệu về các thành phần hoạt chất trong sản phẩm skincare, giải thích vai trò và lợi ích của từng thành phần."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:17:20.913284', '2026-01-27 13:17:20.913284', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thành phần hoạt chất là những chất quan trọng trong sản phẩm skincare giúp cải thiện tình trạng da.", "detailed": "Thành phần hoạt chất là những hợp chất có tác dụng cụ thể trong các sản phẩm skincare. Chúng có thể giúp làm sạch da, cung cấp độ ẩm, hoặc làm sáng da.", "comprehensive": "Thành phần hoạt chất là những nguyên liệu chính có trong sản phẩm chăm sóc da, có khả năng tác động mạnh mẽ đến cấu trúc và chức năng của da. Chúng bao gồm các loại như axit hyaluronic, retinol, vitamin C, và nhiều loại khác, mỗi loại có công dụng riêng biệt giúp cải thiện sức khỏe và vẻ đẹp của làn da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5739d7f4-cb79-4988-b3bd-1660fb6cbb92', 'dd1d6bb4-9031-43ac-9c44-956e8bae7e61', 'concept', 'Chơi Hợp Âm Trên Piano', 'Thực hành chơi các hợp âm trưởng và thứ trên piano là rất quan trọng để phát triển kỹ năng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Người chơi piano đang thực hành hợp âm.", "videoScript": "Bây giờ hãy thực hành chơi hợp âm trưởng và thứ. Đặt ngón tay của bạn đúng vị trí và cảm nhận âm thanh.", "imageDescription": "Hình ảnh một người chơi piano đang thực hành các hợp âm trưởng và thứ.", "videoDescription": "Video hướng dẫn thực hành chơi hợp âm trên piano, với mẹo và kỹ thuật để chuyển tiếp mượt mà."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:50:16.095583', '2026-01-27 12:50:16.095583', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hãy thực hành chơi hợp âm trên piano.", "detailed": "Thực hành chơi các hợp âm trưởng và thứ trên piano là rất quan trọng để phát triển kỹ năng.", "comprehensive": "Việc thực hành chơi hợp âm trên piano là rất quan trọng để phát triển kỹ năng chơi nhạc. Bạn nên bắt đầu bằng cách chơi các hợp âm trưởng và thứ, sau đó kết hợp chúng trong một đoạn nhạc đơn giản. Hãy chú ý đến sự chuyển tiếp giữa các hợp âm để âm nhạc trở nên mượt mà hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c8e5c0b1-263e-4a7b-a231-72ee68a36f96', 'f3a30319-5825-460a-86fa-045981f35738', 'concept', 'Các loại Biểu Đồ trong Excel', 'Excel cung cấp nhiều loại biểu đồ khác nhau như biểu đồ cột, biểu đồ đường, biểu đồ tròn, biểu đồ khu vực và nhiều hơn nữa, mỗi loại có cách sử dụng và ứng dụng riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại biểu đồ trong Excel.", "videoScript": "Trong bài học này, chúng ta sẽ khám phá các loại biểu đồ có sẵn trong Excel và khi nào nên sử dụng chúng.", "imageDescription": "Mô tả các loại biểu đồ như cột, đường, tròn.", "videoDescription": "Video giới thiệu về các loại biểu đồ trong Excel và ứng dụng của từng loại."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:29.166555', '2026-01-27 12:31:29.166555', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại biểu đồ như biểu đồ cột, biểu đồ đường, biểu đồ tròn.", "detailed": "Excel cung cấp nhiều loại biểu đồ khác nhau như biểu đồ cột, biểu đồ đường, biểu đồ tròn, biểu đồ khu vực và nhiều hơn nữa, mỗi loại có cách sử dụng và ứng dụng riêng.", "comprehensive": "Trong Excel, bạn có thể tạo ra nhiều loại biểu đồ khác nhau như biểu đồ cột để so sánh dữ liệu, biểu đồ đường để thể hiện xu hướng theo thời gian, biểu đồ tròn để thể hiện tỷ lệ phần trăm, và nhiều loại biểu đồ khác. Mỗi loại biểu đồ sẽ phù hợp với loại dữ liệu và mục đích trình bày khác nhau."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('94337906-e1ea-4866-a993-8c2e4829dda7', 'f3a30319-5825-460a-86fa-045981f35738', 'concept', 'Cách Tạo Biểu Đồ Cột', 'Để tạo biểu đồ cột, bạn cần chọn dữ liệu mà bạn muốn vẽ biểu đồ, sau đó vào tab ''Insert'' và chọn ''Column Chart''.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh quá trình tạo biểu đồ cột trong Excel.", "videoScript": "Bây giờ, hãy cùng nhau xem cách tạo biểu đồ cột trong Excel. Đầu tiên, chọn dữ liệu, sau đó đến tab ''Insert''...", "imageDescription": "Mô tả từng bước tạo biểu đồ cột.", "videoDescription": "Video hướng dẫn chi tiết từng bước tạo biểu đồ cột trong Excel."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:29.170265', '2026-01-27 12:31:29.170265', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn dữ liệu và nhấn nút tạo biểu đồ cột.", "detailed": "Để tạo biểu đồ cột, bạn cần chọn dữ liệu mà bạn muốn vẽ biểu đồ, sau đó vào tab ''Insert'' và chọn ''Column Chart''.", "comprehensive": "Để tạo biểu đồ cột trong Excel, đầu tiên bạn hãy chọn một vùng dữ liệu mà bạn muốn biểu diễn. Sau đó, vào tab ''Insert'' trên thanh công cụ và chọn ''Column Chart''. Excel sẽ tự động tạo ra biểu đồ cột dựa trên dữ liệu đã chọn. Bạn có thể tùy chỉnh biểu đồ theo ý muốn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f6e07897-ecbe-488a-ae3d-352f03db64d1', 'dcdc7756-1c2d-4077-8f20-19cd69d7a272', 'concept', 'Axit Hyaluronic - Cứu Tinh Của Làn Da Khô', 'Axit hyaluronic là một chất tự nhiên có trong cơ thể, có khả năng giữ nước gấp 1000 lần trọng lượng của nó. Nó giúp da luôn ẩm mượt và đầy sức sống.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một sản phẩm chứa axit hyaluronic.", "videoScript": "Hiện tại, axit hyaluronic đang là thành phần được yêu thích trong skincare. Hãy cùng tìm hiểu cách nó giúp cải thiện tình trạng da khô nhé!", "imageDescription": "Một lọ serum axit hyaluronic trên nền cẩm thạch trắng.", "videoDescription": "Video giải thích về axit hyaluronic, cách nó hoạt động và lợi ích cho làn da khô."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:17:20.928288', '2026-01-27 13:17:20.928288', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Axit hyaluronic giúp cấp ẩm cho da, làm da luôn căng mướt.", "detailed": "Axit hyaluronic là một chất tự nhiên có trong cơ thể, có khả năng giữ nước gấp 1000 lần trọng lượng của nó. Nó giúp da luôn ẩm mượt và đầy sức sống.", "comprehensive": "Axit hyaluronic là một polysaccharide tự nhiên, có khả năng hút ẩm và giữ nước rất tốt, giúp cải thiện độ ẩm cho da. Nó hoạt động bằng cách tạo thành một lớp bảo vệ trên bề mặt da, ngăn không cho nước thoát ra ngoài, đồng thời cung cấp độ ẩm cần thiết, giúp da trở nên mềm mại và căng tràn sức sống."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('38f75b21-8815-4089-ae10-d422dc0e131b', 'dcdc7756-1c2d-4077-8f20-19cd69d7a272', 'concept', 'Retinol - Vị Cứu Tinh Cho Làn Da Lão Hóa', 'Retinol là một dạng của vitamin A, có khả năng thúc đẩy quá trình tái tạo tế bào da, giúp giảm nếp nhăn và làm đều màu da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một sản phẩm chứa retinol.", "videoScript": "Chào các bạn! Hôm nay chúng ta sẽ nói về retinol, một thành phần quan trọng trong việc chống lão hóa da. Hãy cùng khám phá cách nó hoạt động nhé!", "imageDescription": "Một lọ kem chứa retinol với ánh sáng mềm mại chiếu lên.", "videoDescription": "Video cung cấp thông tin về retinol, công dụng và cách sử dụng an toàn để đạt được hiệu quả tốt nhất."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:17:20.936151', '2026-01-27 13:17:20.936151', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Retinol giúp giảm nếp nhăn và làm sáng da.", "detailed": "Retinol là một dạng của vitamin A, có khả năng thúc đẩy quá trình tái tạo tế bào da, giúp giảm nếp nhăn và làm đều màu da.", "comprehensive": "Retinol là một dẫn xuất của vitamin A, nổi tiếng trong việc chống lão hóa và cải thiện bề mặt da. Nó giúp thúc đẩy quá trình tái tạo tế bào da, làm giảm sự xuất hiện của nếp nhăn, tăng cường độ đàn hồi và làm sáng màu da. Tuy nhiên, cần sử dụng đúng cách để tránh kích ứng cho da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4f6e2987-4f60-4c7d-8fb1-77e6afdbcdcd', 'f3a30319-5825-460a-86fa-045981f35738', 'concept', 'Tùy Chỉnh Biểu Đồ', 'Excel cho phép người dùng tùy chỉnh biểu đồ bằng cách thay đổi màu sắc, kiểu dáng, tiêu đề và nhãn trục để phù hợp với nhu cầu trình bày.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các tùy chỉnh biểu đồ trong Excel.", "videoScript": "Tiếp theo, chúng ta sẽ học cách tùy chỉnh biểu đồ của mình để nó trông chuyên nghiệp hơn. Hãy nhấp chuột phải vào biểu đồ và chọn ''Format''.", "imageDescription": "Mô tả các tùy chỉnh như màu sắc, tiêu đề, nhãn.", "videoDescription": "Video hướng dẫn các bước tùy chỉnh biểu đồ trong Excel để làm cho nó trở nên chuyên nghiệp hơn."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:29.174248', '2026-01-27 12:31:29.174248', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể thay đổi màu sắc và kiểu dáng của biểu đồ.", "detailed": "Excel cho phép người dùng tùy chỉnh biểu đồ bằng cách thay đổi màu sắc, kiểu dáng, tiêu đề và nhãn trục để phù hợp với nhu cầu trình bày.", "comprehensive": "Tùy chỉnh biểu đồ trong Excel là một bước quan trọng để làm cho biểu đồ của bạn nổi bật và dễ hiểu hơn. Bạn có thể thay đổi màu sắc, kiểu dáng của các thanh hoặc đường biểu diễn, thêm tiêu đề cho biểu đồ, cũng như điều chỉnh các nhãn trục để người xem có thể hiểu rõ hơn về dữ liệu mà bạn đang trình bày."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e0fb9c1f-8ac5-4162-9793-aadf5d8a31da', 'dd1d6bb4-9031-43ac-9c44-956e8bae7e61', 'concept', 'Ứng Dụng Hợp Âm Trong Nhạc', 'Hợp âm là nền tảng cho nhiều bài hát, giúp tạo ra giai điệu và nhịp điệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Các nhạc sĩ đang biểu diễn một bài hát với hợp âm.", "videoScript": "Trong phần này, chúng ta sẽ khám phá cách hợp âm được sử dụng trong các bài hát. Hãy lắng nghe cách chúng tạo ra cảm xúc trong âm nhạc.", "imageDescription": "Hình ảnh các nhạc sĩ chơi nhạc cùng nhau, sử dụng hợp âm để tạo ra giai điệu.", "videoDescription": "Video khám phá ứng dụng của hợp âm trong âm nhạc, với ví dụ từ các bài hát nổi tiếng."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:50:16.099909', '2026-01-27 12:50:16.099909', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm được sử dụng trong nhiều bài hát.", "detailed": "Hợp âm là nền tảng cho nhiều bài hát, giúp tạo ra giai điệu và nhịp điệu.", "comprehensive": "Hợp âm là nền tảng cho cấu trúc của nhiều bài hát trong âm nhạc. Chúng tạo ra nền tảng cho giai điệu và nhịp điệu, giúp người nghe cảm nhận được cảm xúc của bài hát. Việc hiểu và áp dụng hợp âm sẽ giúp bạn sáng tác và chơi nhạc một cách hiệu quả hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a942564f-310f-43b3-a63c-7b621d7e1e19', 'dcdc7756-1c2d-4077-8f20-19cd69d7a272', 'concept', 'Vitamin C - Chất Chống Oxy Hóa Mạnh Mẽ', 'Vitamin C là một chất chống oxy hóa mạnh, giúp làm sáng da, giảm sắc tố và bảo vệ da khỏi sự tổn thương từ môi trường.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một sản phẩm chứa vitamin C.", "videoScript": "Vitamin C là một thành phần không thể thiếu trong skincare. Hãy cùng tìm hiểu về lợi ích tuyệt vời của nó cho làn da nhé!", "imageDescription": "Một sản phẩm serum vitamin C với các lát cam xung quanh.", "videoDescription": "Video giải thích về vitamin C, cách nó tác động đến làn da và tại sao bạn nên đưa nó vào quy trình chăm sóc sắc đẹp của mình."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:17:20.939722', '2026-01-27 13:17:20.939722', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Vitamin C giúp làm sáng da và bảo vệ da khỏi tác hại của môi trường.", "detailed": "Vitamin C là một chất chống oxy hóa mạnh, giúp làm sáng da, giảm sắc tố và bảo vệ da khỏi sự tổn thương từ môi trường.", "comprehensive": "Vitamin C, hay còn gọi là ascorbic acid, là một chất chống oxy hóa quan trọng giúp bảo vệ da khỏi tác hại của gốc tự do và ô nhiễm. Nó không chỉ làm sáng da mà còn giúp làm mờ vết thâm, cải thiện độ đàn hồi và kích thích sản xuất collagen, mang lại làn da khỏe mạnh và rạng rỡ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a52a6615-7f71-4669-a999-31b5d9b53198', 'dcdc7756-1c2d-4077-8f20-19cd69d7a272', 'concept', 'Niacinamide - Giải Pháp Cho Da Nhạy Cảm', 'Niacinamide là một dạng vitamin B3, có tác dụng làm dịu da, giảm mụn và cải thiện độ đàn hồi.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một sản phẩm chứa niacinamide.", "videoScript": "Chào các bạn! Hôm nay chúng ta sẽ khám phá niacinamide, một thành phần tuyệt vời cho da nhạy cảm và dễ bị mụn. Hãy xem cách nó hoạt động nhé!", "imageDescription": "Một lọ kem dưỡng chứa niacinamide với nền hoa cúc.", "videoDescription": "Video giới thiệu về niacinamide, tác dụng và lợi ích của nó trong skincare, đặc biệt cho da nhạy cảm."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:17:20.943272', '2026-01-27 13:17:20.943272', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Niacinamide giúp làm dịu da và cải thiện tình trạng mụn.", "detailed": "Niacinamide là một dạng vitamin B3, có tác dụng làm dịu da, giảm mụn và cải thiện độ đàn hồi.", "comprehensive": "Niacinamide, hay còn gọi là vitamin B3, là một thành phần đa năng trong skincare. Nó giúp làm dịu da, giảm viêm, cải thiện tình trạng mụn và tăng cường hàng rào bảo vệ da. Niacinamide còn có khả năng làm sáng da, làm đều màu da và tăng cường độ đàn hồi, giúp làn da trở nên khỏe mạnh và rạng rỡ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5be41f49-847e-4189-a9ef-a14909968895', '9cde4cdc-e2bf-49e0-aafc-0570def1ca25', 'concept', 'Kỹ thuật diễn cảm cơ bản', 'Kỹ thuật diễn cảm cơ bản gồm cách điều chỉnh âm lượng và tốc độ khi chơi để thể hiện cảm xúc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bàn tay nghệ sĩ piano trên phím đàn, thể hiện sự nhấn nhá.", "videoScript": "Hôm nay, chúng ta sẽ học về các kỹ thuật diễn cảm cơ bản trong việc chơi piano. Hãy bắt đầu nào!", "imageDescription": "Hình ảnh một bàn tay đang nhấn phím đàn piano với sự tập trung cao độ.", "videoDescription": "Video hướng dẫn các kỹ thuật diễn cảm cơ bản cho người chơi piano, từ cách sử dụng bàn tay đến chân đạp."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:06.908881', '2026-01-27 12:53:06.908881', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng chân và tay để tạo cảm xúc.", "detailed": "Kỹ thuật diễn cảm cơ bản gồm cách điều chỉnh âm lượng và tốc độ khi chơi để thể hiện cảm xúc.", "comprehensive": "Trong bài học này, chúng ta sẽ tìm hiểu các kỹ thuật diễn cảm cơ bản như cách sử dụng pedal, điều chỉnh âm lượng và nhịp điệu để tạo ra sự khác biệt trong cách thể hiện bản nhạc. Việc làm chủ các kỹ thuật này sẽ giúp bạn truyền tải cảm xúc một cách hiệu quả hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('745729ee-1a9f-40c1-8193-a09cd27bd39c', 'f3a30319-5825-460a-86fa-045981f35738', 'concept', 'Lưu và Chia Sẻ Biểu Đồ', 'Sau khi tạo xong biểu đồ, bạn có thể lưu bảng tính hoặc sao chép biểu đồ để chia sẻ với người khác.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh lưu và chia sẻ biểu đồ trong Excel.", "videoScript": "Cuối cùng, hãy xem cách lưu và chia sẻ biểu đồ. Bạn chỉ cần nhấp vào ''File'', sau đó chọn ''Save As''...", "imageDescription": "Mô tả cách lưu và chia sẻ biểu đồ.", "videoDescription": "Video hướng dẫn cách lưu và chia sẻ biểu đồ trong Excel."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:29.178214', '2026-01-27 12:31:29.178214', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể lưu và chia sẻ biểu đồ dễ dàng.", "detailed": "Sau khi tạo xong biểu đồ, bạn có thể lưu bảng tính hoặc sao chép biểu đồ để chia sẻ với người khác.", "comprehensive": "Sau khi hoàn thành việc tạo biểu đồ trong Excel, bạn có thể lưu bảng tính để giữ lại biểu đồ. Ngoài ra, Excel cho phép bạn sao chép biểu đồ và dán vào các tài liệu khác như Word hoặc PowerPoint, giúp việc chia sẻ thông tin trở nên dễ dàng hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5c6f8d3a-af13-41ee-b763-946a12fb4d73', 'bf12adab-1b57-43cb-90ae-e9c2b1dabd4d', 'concept', 'Giới thiệu về Lọc Dữ liệu trong Excel', 'Lọc dữ liệu trong Excel cho phép bạn hiển thị một phần dữ liệu theo các tiêu chí cụ thể, giúp dễ dàng phân tích.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một giao diện Excel với các bộ lọc được áp dụng, thể hiện danh sách dữ liệu được lọc.", "videoScript": "Chào mừng bạn đến với bài học về lọc dữ liệu trong Excel. Trong video này, chúng ta sẽ tìm hiểu cách sử dụng các bộ lọc để chỉ hiển thị thông tin cần thiết.", "imageDescription": "Hình ảnh cho thấy một bảng dữ liệu trong Excel với các bộ lọc được bật.", "videoDescription": "Video hướng dẫn từng bước cách sử dụng chức năng lọc trong Excel."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:57.730125', '2026-01-27 12:31:57.730125', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lọc dữ liệu giúp bạn xem chỉ những thông tin cần thiết.", "detailed": "Lọc dữ liệu trong Excel cho phép bạn hiển thị một phần dữ liệu theo các tiêu chí cụ thể, giúp dễ dàng phân tích.", "comprehensive": "Lọc dữ liệu trong Excel là một chức năng mạnh mẽ cho phép người dùng nhanh chóng tìm kiếm và phân tích thông tin bằng cách ẩn các hàng không phù hợp với tiêu chí đã đặt ra. Bài học này sẽ hướng dẫn bạn cách sử dụng công cụ lọc cơ bản và nâng cao để tối ưu hóa quy trình làm việc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6193f023-df2a-4353-953c-63ecdef5d54f', 'bf12adab-1b57-43cb-90ae-e9c2b1dabd4d', 'concept', 'Sắp xếp Dữ liệu trong Excel', 'Sắp xếp dữ liệu trong Excel cho phép bạn sắp xếp các hàng theo thứ tự tăng dần hoặc giảm dần dựa trên một hay nhiều cột.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng dữ liệu trong Excel với các cột được sắp xếp theo thứ tự tăng dần.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu cách sắp xếp dữ liệu trong Excel, bao gồm sắp xếp theo thứ tự chữ cái và số.", "imageDescription": "Hình ảnh mô tả các hàng dữ liệu trong Excel được sắp xếp theo thứ tự từ A đến Z.", "videoDescription": "Video hướng dẫn cách sắp xếp dữ liệu trong Excel với các ví dụ thực tế."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:57.738302', '2026-01-27 12:31:57.738302', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sắp xếp dữ liệu giúp bạn tổ chức thông tin một cách dễ dàng.", "detailed": "Sắp xếp dữ liệu trong Excel cho phép bạn sắp xếp các hàng theo thứ tự tăng dần hoặc giảm dần dựa trên một hay nhiều cột.", "comprehensive": "Sắp xếp dữ liệu trong Excel là quá trình tổ chức dữ liệu theo một thứ tự cụ thể, có thể là từ A đến Z, từ Z đến A, hoặc theo số lượng. Trong bài học này, bạn sẽ học cách sắp xếp dữ liệu theo nhiều tiêu chí và cách sử dụng các tùy chọn sắp xếp nâng cao."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e6c53b82-bec1-4f4d-bd11-a1ebf353a0a5', 'ba24d13e-ca49-4e63-ac35-d7726870c8f1', 'concept', 'Giới thiệu về Quản lý danh mục đầu tư', 'Quản lý danh mục đầu tư bao gồm việc lựa chọn, giám sát và điều chỉnh các tài sản như cổ phiếu, trái phiếu và quỹ đầu tư để đạt được mục tiêu tài chính cá nhân.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang xem biểu đồ chứng khoán trên máy tính.", "videoScript": "Chào mừng đến với bài học về quản lý danh mục đầu tư. Hôm nay, chúng ta sẽ khám phá những khái niệm cơ bản về cách tối ưu hóa danh mục đầu tư của bạn.", "imageDescription": "Một cá nhân đang phân tích các biểu đồ tài chính trên màn hình máy tính, thể hiện sự tập trung vào quản lý đầu tư.", "videoDescription": "Video giới thiệu các khái niệm cơ bản về quản lý danh mục đầu tư, bao gồm lợi ích và các yếu tố cần xem xét."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:01:44.411474', '2026-01-27 13:01:44.411474', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý danh mục đầu tư là quá trình theo dõi và điều chỉnh các khoản đầu tư của bạn để tối ưu hóa lợi nhuận.", "detailed": "Quản lý danh mục đầu tư bao gồm việc lựa chọn, giám sát và điều chỉnh các tài sản như cổ phiếu, trái phiếu và quỹ đầu tư để đạt được mục tiêu tài chính cá nhân.", "comprehensive": "Quản lý danh mục đầu tư là một chiến lược tài chính nhằm tối đa hóa lợi nhuận với mức rủi ro chấp nhận được. Điều này bao gồm việc phân bổ tài sản, theo dõi hiệu suất đầu tư và điều chỉnh danh mục đầu tư dựa trên các yếu tố như thị trường, thời gian đầu tư và mục tiêu cá nhân."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3ac7c56e-299c-4b3b-826b-0dc0288cd3d1', '9a370239-b7fc-4ac9-8bbc-3def509c7ead', 'concept', 'Hiểu Về Mụn', 'Mụn hình thành khi lỗ chân lông bị tắc nghẽn bởi dầu và tế bào chết, dẫn đến viêm và sưng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa về các loại mụn trên da.", "videoScript": "Trong video này, chúng ta sẽ khám phá nguyên nhân gây ra mụn và cách điều trị hiệu quả.", "imageDescription": "Một bức tranh mô tả các loại mụn khác nhau trên khuôn mặt.", "videoDescription": "Một video hướng dẫn về cách nhận diện và điều trị các loại mụn."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:17:46.342583', '2026-01-27 13:17:46.342583', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mụn là vấn đề da phổ biến nhất, thường xuất hiện ở tuổi dậy thì.", "detailed": "Mụn hình thành khi lỗ chân lông bị tắc nghẽn bởi dầu và tế bào chết, dẫn đến viêm và sưng.", "comprehensive": "Mụn có nhiều loại như mụn trứng cá, mụn đầu đen, và mụn bọc. Nguyên nhân gây mụn có thể bao gồm hormone, di truyền, và chế độ ăn uống. Việc điều trị mụn cần phải có sự kiên nhẫn và chăm sóc đúng cách."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('39913594-b399-4fa4-adf2-7d963be0d8fe', '9a370239-b7fc-4ac9-8bbc-3def509c7ead', 'concept', 'Chăm Sóc Da Khô', 'Da khô thường do thiếu nước hoặc dầu tự nhiên. Điều này có thể dẫn đến ngứa và bong tróc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của sản phẩm dưỡng ẩm cho da khô.", "videoScript": "Hãy cùng tôi tìm hiểu cách chăm sóc da khô đúng cách và những sản phẩm nên sử dụng.", "imageDescription": "Một bức ảnh về các loại kem dưỡng ẩm và serum cho da khô.", "videoDescription": "Video hướng dẫn các bước chăm sóc da khô và sản phẩm nên dùng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:17:46.350929', '2026-01-27 13:17:46.350929', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Da khô có thể gây cảm giác khó chịu và nứt nẻ.", "detailed": "Da khô thường do thiếu nước hoặc dầu tự nhiên. Điều này có thể dẫn đến ngứa và bong tróc.", "comprehensive": "Chăm sóc da khô bao gồm việc sử dụng kem dưỡng ẩm, tránh xà phòng mạnh, và uống đủ nước. Các thành phần như glycerin và hyaluronic acid là rất hữu ích cho việc giữ ẩm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d37e6921-0e3f-40ee-a4dd-f2167a47d465', 'bf12adab-1b57-43cb-90ae-e9c2b1dabd4d', 'concept', 'Kết hợp Lọc và Sắp xếp Dữ liệu', 'Kết hợp lọc và sắp xếp dữ liệu giúp bạn tìm kiếm thông tin cụ thể và tổ chức nó theo cách dễ hiểu hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bảng dữ liệu trong Excel với cả lọc và sắp xếp được áp dụng.", "videoScript": "Chào mừng đến với bài học về cách kết hợp lọc và sắp xếp dữ liệu trong Excel. Hãy cùng xem những ví dụ thực tiễn.", "imageDescription": "Hình ảnh cho thấy một bảng dữ liệu trong Excel với bộ lọc và sắp xếp được áp dụng cùng lúc.", "videoDescription": "Video này hướng dẫn cách kết hợp hai tính năng mạnh mẽ của Excel để quản lý dữ liệu hiệu quả."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:57.742132', '2026-01-27 12:31:57.742132', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể lọc và sắp xếp dữ liệu cùng một lúc để có cái nhìn rõ hơn.", "detailed": "Kết hợp lọc và sắp xếp dữ liệu giúp bạn tìm kiếm thông tin cụ thể và tổ chức nó theo cách dễ hiểu hơn.", "comprehensive": "Trong bài học này, bạn sẽ học cách kết hợp chức năng lọc và sắp xếp trong Excel, cho phép bạn không chỉ tìm thấy thông tin cần thiết mà còn tổ chức chúng một cách hiệu quả nhất. Chúng ta sẽ khám phá các tình huống thực tế để áp dụng những kỹ năng này."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f8012fa0-aca3-437b-9d64-dba07248c717', 'bf12adab-1b57-43cb-90ae-e9c2b1dabd4d', 'concept', 'Lọc Dữ liệu Nâng cao với Các Tiêu chí Phức tạp', 'Lọc dữ liệu nâng cao cho phép bạn sử dụng các tiêu chí phức tạp hơn để tìm kiếm thông tin cụ thể.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu mẫu lọc dữ liệu nâng cao trong Excel với nhiều tiêu chí.", "videoScript": "Trong video này, chúng ta sẽ khám phá cách sử dụng lọc nâng cao trong Excel với các tiêu chí phức tạp để tìm kiếm dữ liệu.", "imageDescription": "Hình ảnh cho thấy giao diện lọc nâng cao trong Excel với các tiêu chí được thiết lập.", "videoDescription": "Video hướng dẫn cách sử dụng chức năng lọc nâng cao trong Excel."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:57.746038', '2026-01-27 12:31:57.746038', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể sử dụng nhiều tiêu chí để lọc dữ liệu chính xác hơn.", "detailed": "Lọc dữ liệu nâng cao cho phép bạn sử dụng các tiêu chí phức tạp hơn để tìm kiếm thông tin cụ thể.", "comprehensive": "Bài học này sẽ giúp bạn làm quen với chức năng lọc nâng cao trong Excel, cho phép bạn áp dụng nhiều tiêu chí phức tạp và thậm chí là sử dụng công thức để lọc dữ liệu. Điều này rất hữu ích khi làm việc với các tập dữ liệu lớn và đa dạng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5d6dfb14-3296-4c3a-a2ad-af8b2d0a5e03', '9a370239-b7fc-4ac9-8bbc-3def509c7ead', 'concept', 'Nguyên Nhân Gây Ra Da Nhờn', 'Khi tuyến bã nhờn sản xuất quá nhiều dầu, da sẽ trở nên bóng nhờn và dễ bị mụn hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh về sản phẩm kiểm soát dầu cho da nhờn.", "videoScript": "Trong video này, chúng ta sẽ khám phá nguyên nhân gây ra da nhờn và cách kiểm soát tình trạng này.", "imageDescription": "Một bức tranh mô tả các loại sản phẩm chăm sóc da dành cho da nhờn.", "videoDescription": "Video hướng dẫn cách chăm sóc da nhờn hiệu quả."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:17:46.354697', '2026-01-27 13:17:46.354697', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Da nhờn thường do tuyến bã nhờn hoạt động quá mức.", "detailed": "Khi tuyến bã nhờn sản xuất quá nhiều dầu, da sẽ trở nên bóng nhờn và dễ bị mụn hơn.", "comprehensive": "Da nhờn có thể do nhiều yếu tố như hormone, di truyền, và khí hậu. Việc chăm sóc da nhờn bao gồm sử dụng sản phẩm kiểm soát dầu và làm sạch sâu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3e68e776-3fea-4e54-8d30-349387290770', '9a370239-b7fc-4ac9-8bbc-3def509c7ead', 'concept', 'Chống Lão Hóa Da', 'Nếp nhăn và sự mất đàn hồi là dấu hiệu của lão hóa da. Chúng có thể do ánh nắng, chế độ ăn uống và lối sống.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh về sản phẩm chống lão hóa da.", "videoScript": "Hãy cùng tìm hiểu về các cách chống lão hóa da và sản phẩm hiệu quả.", "imageDescription": "Một bức ảnh về các loại sản phẩm chống lão hóa và các thành phần tự nhiên có lợi.", "videoDescription": "Video hướng dẫn cách chăm sóc da để ngăn ngừa lão hóa."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:17:46.358085', '2026-01-27 13:17:46.358085', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lão hóa da là quá trình tự nhiên, nhưng có thể được giảm thiểu.", "detailed": "Nếp nhăn và sự mất đàn hồi là dấu hiệu của lão hóa da. Chúng có thể do ánh nắng, chế độ ăn uống và lối sống.", "comprehensive": "Để chống lão hóa, việc sử dụng kem chống nắng, giữ ẩm và các sản phẩm chứa retinol là rất quan trọng. Chế độ ăn uống giàu antioxidants cũng giúp bảo vệ da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('74613b7c-c5a7-48a9-a63f-1fbead5290ef', '9a370239-b7fc-4ac9-8bbc-3def509c7ead', 'concept', 'Những Vấn Đề Da Liễu Khác', 'Eczema là tình trạng viêm da, trong khi vẩy nến là tình trạng da tự miễn dịch.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa về eczema và vẩy nến.", "videoScript": "Trong video này, chúng ta sẽ nói về các vấn đề da liễu khác nhau và cách điều trị chúng.", "imageDescription": "Một bức tranh thể hiện các triệu chứng của eczema và vẩy nến trên da.", "videoDescription": "Video cung cấp thông tin về eczema, vẩy nến và cách chăm sóc da cho những người bị ảnh hưởng."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:17:46.362845', '2026-01-27 13:17:46.362845', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều vấn đề da liễu khác như eczema, vẩy nến.", "detailed": "Eczema là tình trạng viêm da, trong khi vẩy nến là tình trạng da tự miễn dịch.", "comprehensive": "Các vấn đề như eczema, vẩy nến cần được chẩn đoán và điều trị bởi bác sĩ chuyên khoa. Việc chăm sóc da đúng cách và sử dụng các loại thuốc cần thiết là rất quan trọng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cc8eed65-d27d-4e1b-8120-5704e62f525e', 'bf12adab-1b57-43cb-90ae-e9c2b1dabd4d', 'concept', 'Thực Hành: Lọc và Sắp xếp Dữ liệu', 'Thực hành là cách tốt nhất để hiểu rõ cách lọc và sắp xếp dữ liệu trong Excel.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang thực hành lọc và sắp xếp dữ liệu trong Excel.", "videoScript": "Chào mừng bạn đến với bài thực hành! Hãy cùng nhau áp dụng các kỹ năng lọc và sắp xếp dữ liệu trong Excel qua các bài tập thực tế.", "imageDescription": "Hình ảnh mô tả một người dùng đang làm việc trên bảng dữ liệu trong Excel thực hành lọc và sắp xếp.", "videoDescription": "Video này sẽ hướng dẫn bạn thực hành lọc và sắp xếp dữ liệu trong Excel."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:31:57.750529', '2026-01-27 12:31:57.750529', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hãy thực hành lọc và sắp xếp dữ liệu để nắm vững kỹ năng.", "detailed": "Thực hành là cách tốt nhất để hiểu rõ cách lọc và sắp xếp dữ liệu trong Excel.", "comprehensive": "Trong bài học thực hành này, bạn sẽ có cơ hội áp dụng những gì đã học về lọc và sắp xếp dữ liệu trong Excel. Chúng tôi sẽ cung cấp cho bạn một tập dữ liệu mẫu và hướng dẫn bạn từng bước thực hiện các thao tác lọc và sắp xếp để giải quyết các bài tập thực tế."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('194d7b69-7049-4400-9539-e94f0882b84c', '22db2630-70bb-4ef0-9e8f-51f654775cd4', 'concept', 'Giới thiệu về Piano trong âm nhạc hiện đại', 'Piano đã trở thành một phần không thể thiếu trong âm nhạc hiện đại, được sử dụng trong nhiều thể loại khác nhau, từ pop, jazz đến cổ điển.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cây piano hiện đại trong một phòng thu âm nhạc.", "videoScript": "Xin chào các bạn! Hôm nay, chúng ta sẽ khám phá vai trò của piano trong âm nhạc hiện đại.", "imageDescription": "Một cây piano hiện đại với thiết kế tinh tế, được đặt trong một phòng thu âm với ánh sáng ấm áp.", "videoDescription": "Video giới thiệu về sự phát triển và ảnh hưởng của piano trong âm nhạc hiện đại, kèm theo các ví dụ minh họa."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:50:42.172097', '2026-01-27 12:50:42.172097', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Piano là một nhạc cụ quan trọng trong âm nhạc hiện đại.", "detailed": "Piano đã trở thành một phần không thể thiếu trong âm nhạc hiện đại, được sử dụng trong nhiều thể loại khác nhau, từ pop, jazz đến cổ điển.", "comprehensive": "Piano không chỉ là một nhạc cụ độc lập mà còn là một phần thiết yếu trong nhiều ban nhạc hiện đại. Với khả năng tạo ra âm thanh phong phú và đa dạng, piano đã góp mặt trong nhiều bản hit và các buổi biểu diễn lớn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d245389a-92d8-4a83-b7ef-6d7059808126', '9a86344a-83ea-473f-82fb-0ce95a20d903', 'concept', 'Giới thiệu về việc chèn đối tượng trong PowerPoint', 'Chèn đối tượng vào PowerPoint giúp bạn tạo ra những bài thuyết trình sinh động hơn. Bạn có thể chèn hình ảnh, biểu đồ, video và nhiều loại đối tượng khác.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một slide PowerPoint với nhiều loại đối tượng khác nhau.", "videoScript": "Chào mừng bạn đến với bài học về chèn đối tượng trong PowerPoint. Hôm nay, chúng ta sẽ khám phá cách thêm hình ảnh, video, và âm thanh vào slide của bạn.", "imageDescription": "Một slide PowerPoint với hình ảnh, biểu đồ, và video được chèn vào.", "videoDescription": "Video hướng dẫn từng bước cách chèn đối tượng vào PowerPoint."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:32:29.018373', '2026-01-27 12:32:29.018373', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách chèn đối tượng vào slide PowerPoint.", "detailed": "Chèn đối tượng vào PowerPoint giúp bạn tạo ra những bài thuyết trình sinh động hơn. Bạn có thể chèn hình ảnh, biểu đồ, video và nhiều loại đối tượng khác.", "comprehensive": "Việc chèn đối tượng trong PowerPoint là một kỹ năng quan trọng giúp tăng tính hấp dẫn cho các bài thuyết trình. Trong bài học này, bạn sẽ tìm hiểu cách chèn hình ảnh, video, âm thanh, biểu đồ và các đối tượng khác, cũng như cách thay đổi kích thước và vị trí của chúng trên slide."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cffa8b05-f190-4483-bc28-a3e53b64aa84', '9a86344a-83ea-473f-82fb-0ce95a20d903', 'concept', 'Cách chèn hình ảnh vào PowerPoint', 'Để chèn hình ảnh vào PowerPoint, bạn có thể sử dụng menu ''Chèn'' và chọn ''Hình ảnh''. Bạn cũng có thể kéo thả hình ảnh từ thư mục vào slide.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa cách chèn hình ảnh trong PowerPoint.", "videoScript": "Trong video này, tôi sẽ chỉ cho bạn cách chèn hình ảnh vào PowerPoint. Bắt đầu bằng việc mở menu ''Chèn'' và chọn ''Hình ảnh''.", "imageDescription": "Giao diện PowerPoint với menu ''Chèn'' và tùy chọn ''Hình ảnh''.", "videoDescription": "Hướng dẫn video về cách chèn hình ảnh vào slide PowerPoint."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:32:29.027863', '2026-01-27 12:32:29.027863', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hướng dẫn từng bước để chèn hình ảnh.", "detailed": "Để chèn hình ảnh vào PowerPoint, bạn có thể sử dụng menu ''Chèn'' và chọn ''Hình ảnh''. Bạn cũng có thể kéo thả hình ảnh từ thư mục vào slide.", "comprehensive": "Chèn hình ảnh là một trong những cách đơn giản nhất để làm cho bài thuyết trình của bạn trở nên sinh động. Bạn có thể chèn hình ảnh từ máy tính, từ internet, hoặc sử dụng các hình ảnh có sẵn trong PowerPoint. Sau khi chèn, bạn có thể điều chỉnh kích thước, vị trí và hiệu ứng cho hình ảnh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('eba9ff5a-5118-415c-a542-f115ca8fcb64', '9a86344a-83ea-473f-82fb-0ce95a20d903', 'concept', 'Chèn video vào PowerPoint', 'Để chèn video vào PowerPoint, bạn vào menu ''Chèn'', chọn ''Video'' và sau đó chọn video từ máy tính hoặc từ nguồn trực tuyến.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa cách chèn video trong PowerPoint.", "videoScript": "Hôm nay, chúng ta sẽ học cách chèn video vào PowerPoint. Mở menu ''Chèn'', chọn ''Video'', và theo dõi các bước tiếp theo.", "imageDescription": "Giao diện PowerPoint với menu ''Chèn'' và tùy chọn ''Video''.", "videoDescription": "Video hướng dẫn về cách chèn video vào bài thuyết trình PowerPoint."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:32:29.032425', '2026-01-27 12:32:29.032425', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hướng dẫn chèn video vào slide.", "detailed": "Để chèn video vào PowerPoint, bạn vào menu ''Chèn'', chọn ''Video'' và sau đó chọn video từ máy tính hoặc từ nguồn trực tuyến.", "comprehensive": "Video có thể giúp trình bày nội dung một cách sống động hơn. Bạn có thể chèn video từ nhiều nguồn khác nhau, bao gồm video trong máy tính hoặc từ YouTube. Sau khi chèn video, bạn có thể điều chỉnh kích thước, vị trí và cài đặt tự động phát cho video."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1ac8c7d7-92be-47ee-b47e-88383b085e3b', '9a86344a-83ea-473f-82fb-0ce95a20d903', 'concept', 'Sử dụng biểu đồ trong PowerPoint', 'Biểu đồ giúp trình bày dữ liệu một cách trực quan hơn. Bạn có thể chèn biểu đồ bằng cách vào menu ''Chèn'' và chọn ''Biểu đồ''.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một slide PowerPoint với biểu đồ.", "videoScript": "Chào mừng đến với bài học về biểu đồ trong PowerPoint. Hôm nay, tôi sẽ hướng dẫn bạn cách chèn và chỉnh sửa biểu đồ để trình bày dữ liệu hiệu quả.", "imageDescription": "Slide PowerPoint với biểu đồ thể hiện dữ liệu.", "videoDescription": "Video hướng dẫn cách chèn và chỉnh sửa biểu đồ trong PowerPoint."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:32:29.03574', '2026-01-27 12:32:29.03574', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chèn và chỉnh sửa biểu đồ trong PowerPoint.", "detailed": "Biểu đồ giúp trình bày dữ liệu một cách trực quan hơn. Bạn có thể chèn biểu đồ bằng cách vào menu ''Chèn'' và chọn ''Biểu đồ''.", "comprehensive": "Biểu đồ là công cụ mạnh mẽ để trình bày thông tin. Trong bài học này, bạn sẽ tìm hiểu cách chèn biểu đồ vào PowerPoint, chọn loại biểu đồ phù hợp, và chỉnh sửa dữ liệu để thể hiện thông tin một cách rõ ràng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('eedd4a49-c1e3-4954-aa0c-3d7e70520e09', '9a86344a-83ea-473f-82fb-0ce95a20d903', 'concept', 'Chèn âm thanh vào PowerPoint', 'Âm thanh có thể làm cho bài thuyết trình của bạn thú vị hơn. Bạn có thể chèn âm thanh từ máy tính hoặc từ các nguồn trực tuyến.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa cách chèn âm thanh trong PowerPoint.", "videoScript": "Trong video này, tôi sẽ chỉ cho bạn cách chèn âm thanh vào PowerPoint. Mở menu ''Chèn'', chọn ''Âm thanh'', và làm theo các bước hướng dẫn.", "imageDescription": "Giao diện PowerPoint với menu ''Chèn'' và tùy chọn ''Âm thanh''.", "videoDescription": "Video hướng dẫn về cách chèn âm thanh vào bài thuyết trình PowerPoint."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:32:29.038905', '2026-01-27 12:32:29.038905', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cách chèn âm thanh vào slide PowerPoint.", "detailed": "Âm thanh có thể làm cho bài thuyết trình của bạn thú vị hơn. Bạn có thể chèn âm thanh từ máy tính hoặc từ các nguồn trực tuyến.", "comprehensive": "Chèn âm thanh vào PowerPoint là một cách tuyệt vời để thu hút sự chú ý của khán giả. Trong bài học này, bạn sẽ tìm hiểu cách chèn âm thanh, cài đặt tự động phát, và điều chỉnh âm lượng để đảm bảo rằng âm thanh phù hợp với nội dung của slide."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5e470d2a-5f85-4f34-8563-4bdee75772a8', '9cde4cdc-e2bf-49e0-aafc-0570def1ca25', 'concept', 'Sử dụng cảm xúc để biểu diễn', 'Sử dụng cảm xúc trong biểu diễn giúp người nghe cảm nhận sâu sắc hơn về tác phẩm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Nghệ sĩ piano biểu diễn với ánh mắt sâu lắng.", "videoScript": "Chào mừng các bạn đến với bài học về cách sử dụng cảm xúc trong biểu diễn. Cùng tôi khám phá những cách mà chúng ta có thể thể hiện cảm xúc qua âm nhạc.", "imageDescription": "Hình ảnh một nghệ sĩ piano đang chơi với ánh mắt đầy cảm xúc, truyền tải tâm trạng của bản nhạc.", "videoDescription": "Video này sẽ hướng dẫn bạn cách sử dụng cảm xúc cá nhân để làm phong phú thêm phần biểu diễn piano."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:06.913158', '2026-01-27 12:53:06.913158', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cảm xúc là chìa khóa để biểu diễn tốt.", "detailed": "Sử dụng cảm xúc trong biểu diễn giúp người nghe cảm nhận sâu sắc hơn về tác phẩm.", "comprehensive": "Trong bài học này, chúng ta sẽ khám phá cách sử dụng cảm xúc cá nhân trong biểu diễn piano. Điều này bao gồm việc kết nối với bản nhạc và thể hiện cảm xúc của riêng bạn, từ niềm vui đến nỗi buồn, để tạo ra một trải nghiệm âm nhạc phong phú cho khán giả."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4f37b9c2-2c03-49a1-9052-1abbd978ca5a', '5f36b291-05f3-4155-b8b3-0910221c4da6', 'concept', 'Cách Chọn Bảo Hiểm Phù Hợp', 'Để chọn bảo hiểm phù hợp, bạn nên xác định nhu cầu bảo vệ của mình, tìm hiểu về các loại hình bảo hiểm và so sánh giữa các công ty bảo hiểm khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang so sánh các sản phẩm bảo hiểm trên máy tính.", "videoScript": "Trong video này, chúng ta sẽ hướng dẫn bạn cách lựa chọn bảo hiểm phù hợp với nhu cầu và khả năng tài chính của bạn.", "imageDescription": "Một bức ảnh cho thấy một người đang ngồi trước máy tính, xem xét và so sánh các lựa chọn bảo hiểm.", "videoDescription": "Video hướng dẫn người xem cách chọn bảo hiểm, từ việc đánh giá nhu cầu đến việc so sánh các sản phẩm bảo hiểm khác nhau."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:06:25.868763', '2026-01-27 13:06:25.868763', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khi chọn bảo hiểm, hãy xem xét nhu cầu và khả năng tài chính của bạn.", "detailed": "Để chọn bảo hiểm phù hợp, bạn nên xác định nhu cầu bảo vệ của mình, tìm hiểu về các loại hình bảo hiểm và so sánh giữa các công ty bảo hiểm khác nhau.", "comprehensive": "Chọn bảo hiểm phù hợp là một quá trình quan trọng. Bạn cần phân tích nhu cầu bảo vệ của bản thân và gia đình, đánh giá khả năng tài chính và nghiên cứu các sản phẩm bảo hiểm trên thị trường. Hãy so sánh các điều khoản, mức phí và dịch vụ khách hàng của các công ty bảo hiểm để đưa ra quyết định tốt nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b96d70e6-a9ee-4e2f-8a13-85b2d93702c0', '5f36b291-05f3-4155-b8b3-0910221c4da6', 'concept', 'Những Sai Lầm Thường Gặp Khi Mua Bảo Hiểm', 'Một số sai lầm phổ biến khi mua bảo hiểm bao gồm không hiểu rõ các điều khoản, chỉ chọn bảo hiểm dựa trên giá thấp nhất, và không xem xét nhu cầu thực tế.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu diễn các sai lầm thường gặp khi mua bảo hiểm.", "videoScript": "Hãy cùng tìm hiểu những sai lầm thường gặp khi mua bảo hiểm và cách để tránh chúng.", "imageDescription": "Một bức ảnh với các biểu tượng minh họa các sai lầm như không đọc hợp đồng, chỉ chú trọng giá cả.", "videoDescription": "Video này sẽ chỉ ra những sai lầm phổ biến mà người tiêu dùng thường mắc phải khi mua bảo hiểm và hướng dẫn cách tránh những lỗi đó."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:06:25.872233', '2026-01-27 13:06:25.872233', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhiều người mắc phải sai lầm khi không đọc kỹ hợp đồng bảo hiểm.", "detailed": "Một số sai lầm phổ biến khi mua bảo hiểm bao gồm không hiểu rõ các điều khoản, chỉ chọn bảo hiểm dựa trên giá thấp nhất, và không xem xét nhu cầu thực tế.", "comprehensive": "Khi mua bảo hiểm, nhiều người mắc phải sai lầm như không đọc kỹ hợp đồng và không hiểu rõ các điều khoản. Một sai lầm khác là chỉ chọn bảo hiểm dựa trên giá cả mà không xem xét đến chất lượng dịch vụ và mức độ bảo vệ. Đôi khi, người tiêu dùng cũng không đánh giá đúng nhu cầu của mình, dẫn đến việc mua sản phẩm không phù hợp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5c244951-5013-4b13-99f2-7c93384b53c5', 'c56ce0af-a1d9-45f1-9691-49646b97741b', 'concept', 'Giới Thiệu Kỹ Năng Giao Tiếp', 'Kỹ năng giao tiếp bao gồm việc lắng nghe, nói và viết một cách rõ ràng và hiệu quả. Điều này rất quan trọng trong môi trường văn phòng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhóm nhân viên đang thảo luận trong văn phòng.", "videoScript": "Xin chào! Hôm nay chúng ta sẽ tìm hiểu về kỹ năng giao tiếp trong môi trường văn phòng. Đầu tiên, hãy cùng nhau khám phá tầm quan trọng của việc giao tiếp hiệu quả.", "imageDescription": "Một bức ảnh cho thấy một nhóm nhân viên ngồi quanh bàn làm việc, trao đổi ý tưởng một cách tích cực.", "videoDescription": "Video này sẽ giải thích về các khía cạnh khác nhau của kỹ năng giao tiếp và cách áp dụng chúng trong môi trường làm việc."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:32:58.692215', '2026-01-27 12:32:58.692215', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kỹ năng giao tiếp là khả năng truyền đạt thông tin một cách hiệu quả.", "detailed": "Kỹ năng giao tiếp bao gồm việc lắng nghe, nói và viết một cách rõ ràng và hiệu quả. Điều này rất quan trọng trong môi trường văn phòng.", "comprehensive": "Kỹ năng giao tiếp hiệu quả không chỉ là việc truyền đạt thông tin mà còn là khả năng hiểu và tương tác với người khác. Nó bao gồm việc lắng nghe tích cực, sử dụng ngôn ngữ cơ thể, và quản lý xung đột."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('91611cfb-babf-437a-92d2-131d047963db', '22db2630-70bb-4ef0-9e8f-51f654775cd4', 'concept', 'Các thể loại nhạc sử dụng Piano', 'Nhiều thể loại nhạc hiện đại như pop, rock và jazz đều có sự góp mặt của piano, tạo nên âm thanh đặc trưng và phong phú.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một buổi biểu diễn nhạc jazz với sự góp mặt của một nghệ sĩ chơi piano.", "videoScript": "Hãy cùng xem cách mà piano được sử dụng trong các thể loại âm nhạc khác nhau.", "imageDescription": "Một nghệ sĩ đang biểu diễn piano trên sân khấu jazz, với ánh đèn chiếu sáng và khán giả thưởng thức.", "videoDescription": "Video trình bày sự đa dạng của piano trong các thể loại âm nhạc, với các đoạn trích từ những bản nhạc nổi tiếng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:50:42.180202', '2026-01-27 12:50:42.180202', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Piano được sử dụng trong nhiều thể loại nhạc như pop, rock, jazz.", "detailed": "Nhiều thể loại nhạc hiện đại như pop, rock và jazz đều có sự góp mặt của piano, tạo nên âm thanh đặc trưng và phong phú.", "comprehensive": "Piano là nhạc cụ linh hoạt, có mặt trong nhiều thể loại âm nhạc hiện đại. Trong pop, nó thường được sử dụng như một nhạc cụ chính để tạo giai điệu. Trong rock, piano có thể thêm chiều sâu cho bản nhạc, trong khi ở jazz, nó là một phần quan trọng của sự improvisation."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('95d41bd1-0d6a-4a50-be78-8755ea9f5322', 'c56ce0af-a1d9-45f1-9691-49646b97741b', 'concept', 'Lắng Nghe Tích Cực', 'Lắng nghe tích cực không chỉ đơn thuần là nghe mà còn là khả năng phản hồi và thể hiện sự quan tâm đến ý kiến của người khác.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang lắng nghe trong cuộc họp.", "videoScript": "Chào mừng đến với bài học về lắng nghe tích cực. Hãy cùng tìm hiểu cách thức mà lắng nghe có thể cải thiện kỹ năng giao tiếp của bạn.", "imageDescription": "Một bức ảnh cho thấy một nhân viên đang chăm chú lắng nghe đồng nghiệp trong một cuộc họp.", "videoDescription": "Video này sẽ đưa ra các kỹ thuật lắng nghe tích cực và cách áp dụng chúng trong môi trường văn phòng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:32:58.701837', '2026-01-27 12:32:58.701837', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lắng nghe tích cực giúp hiểu rõ hơn về ý kiến của người khác.", "detailed": "Lắng nghe tích cực không chỉ đơn thuần là nghe mà còn là khả năng phản hồi và thể hiện sự quan tâm đến ý kiến của người khác.", "comprehensive": "Lắng nghe tích cực là một phần quan trọng trong giao tiếp hiệu quả. Nó bao gồm việc chú ý đến ngôn ngữ cơ thể, thể hiện sự đồng cảm, và đặt câu hỏi để làm rõ thông tin. Kỹ năng này giúp xây dựng mối quan hệ tốt hơn trong môi trường làm việc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('578e3042-28b7-4de4-979b-88ed46e3c6c3', '1dd2e8d8-a244-4d70-b88c-017eac380714', 'concept', 'Giới thiệu về Mục Tiêu Tài Chính', 'Mục tiêu tài chính là những kế hoạch mà bạn đặt ra nhằm đạt được sự ổn định và tự do tài chính. Điều này bao gồm việc tiết kiệm, đầu tư và chi tiêu hợp lý.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa về một người đang viết ra mục tiêu tài chính của mình.", "videoScript": "Chào mừng đến với bài học về mục tiêu tài chính! Trong video này, chúng ta sẽ khám phá tại sao việc xác định mục tiêu tài chính là rất quan trọng và cách để bắt đầu.", "imageDescription": "Một người ngồi trên bàn với giấy và bút, ghi chép các mục tiêu tài chính với biểu đồ và ghi chú xung quanh.", "videoDescription": "Video giới thiệu về các khái niệm cơ bản của mục tiêu tài chính và tầm quan trọng của chúng trong cuộc sống."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:07:08.253948', '2026-01-27 13:07:08.253948', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mục tiêu tài chính là những gì bạn muốn đạt được với tiền bạc của mình.", "detailed": "Mục tiêu tài chính là những kế hoạch mà bạn đặt ra nhằm đạt được sự ổn định và tự do tài chính. Điều này bao gồm việc tiết kiệm, đầu tư và chi tiêu hợp lý.", "comprehensive": "Mục tiêu tài chính đóng vai trò quan trọng trong việc quản lý tài chính cá nhân. Chúng giúp bạn xác định rõ ràng những gì bạn muốn đạt được, từ việc mua nhà, tiết kiệm cho hưu trí, đến việc du lịch. Để đạt được những mục tiêu này, bạn cần lập kế hoạch chi tiết và theo dõi tiến trình của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a21c80ed-2a10-46bc-b915-322589e82884', '1dd2e8d8-a244-4d70-b88c-017eac380714', 'concept', 'Phân loại Mục Tiêu Tài Chính', 'Mục tiêu tài chính có thể được chia thành hai loại chính: mục tiêu ngắn hạn (dưới 1 năm) và mục tiêu dài hạn (trên 1 năm). Mục tiêu ngắn hạn có thể là tiết kiệm cho một kỳ nghỉ, trong khi mục tiêu dài hạn có thể là tiết kiệm cho hưu trí.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ phân loại mục tiêu tài chính thành ngắn hạn và dài hạn.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về hai loại mục tiêu tài chính: ngắn hạn và dài hạn. Hãy cùng xem các ví dụ cụ thể cho từng loại.", "imageDescription": "Một biểu đồ hình tròn minh họa sự phân loại mục tiêu tài chính với các ví dụ cụ thể cho từng loại.", "videoDescription": "Video trình bày cách phân loại mục tiêu tài chính và ví dụ cho từng loại, giúp người xem dễ dàng hình dung."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:07:08.261518', '2026-01-27 13:07:08.261518', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có hai loại mục tiêu tài chính: ngắn hạn và dài hạn.", "detailed": "Mục tiêu tài chính có thể được chia thành hai loại chính: mục tiêu ngắn hạn (dưới 1 năm) và mục tiêu dài hạn (trên 1 năm). Mục tiêu ngắn hạn có thể là tiết kiệm cho một kỳ nghỉ, trong khi mục tiêu dài hạn có thể là tiết kiệm cho hưu trí.", "comprehensive": "Phân loại mục tiêu tài chính giúp bạn dễ dàng quản lý và theo dõi tiến trình của mình. Mục tiêu ngắn hạn thường liên quan đến các khoản chi tiêu cụ thể như mua sắm, trong khi mục tiêu dài hạn có thể bao gồm việc đầu tư vào giáo dục, mua nhà hoặc tiết kiệm cho hưu trí. Việc xác định rõ ràng từng loại mục tiêu sẽ giúp bạn thiết lập các ưu tiên và kế hoạch hành động hiệu quả."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e0f163cd-8a19-404e-93d8-d8bad5d7f799', 'c56ce0af-a1d9-45f1-9691-49646b97741b', 'concept', 'Ngôn Ngữ Cơ Thể Trong Giao Tiếp', 'Ngôn ngữ cơ thể bao gồm cử chỉ, ánh mắt và tư thế. Điều này có thể ảnh hưởng đến cách mà thông điệp được tiếp nhận.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang sử dụng ngôn ngữ cơ thể để giao tiếp.", "videoScript": "Hôm nay, chúng ta sẽ khám phá ngôn ngữ cơ thể và cách nó ảnh hưởng đến giao tiếp. Hãy cùng xem một số ví dụ cụ thể.", "imageDescription": "Một bức ảnh cho thấy một người sử dụng cử chỉ tay trong khi nói chuyện, thể hiện sự tự tin.", "videoDescription": "Video này sẽ phân tích vai trò của ngôn ngữ cơ thể trong giao tiếp và cách sử dụng nó một cách hiệu quả."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:32:58.706992', '2026-01-27 12:32:58.706992', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Ngôn ngữ cơ thể có thể truyền đạt nhiều thông điệp hơn lời nói.", "detailed": "Ngôn ngữ cơ thể bao gồm cử chỉ, ánh mắt và tư thế. Điều này có thể ảnh hưởng đến cách mà thông điệp được tiếp nhận.", "comprehensive": "Ngôn ngữ cơ thể đóng vai trò quan trọng trong giao tiếp. Nó có thể bổ sung cho lời nói hoặc thậm chí mâu thuẫn với chúng. Hiểu rõ về ngôn ngữ cơ thể của bản thân và người khác là yếu tố quan trọng trong việc tạo dựng mối quan hệ và truyền đạt thông điệp một cách hiệu quả."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('02af3711-728e-47db-a103-bd9e42dc16f7', 'c56ce0af-a1d9-45f1-9691-49646b97741b', 'concept', 'Quản Lý Xung Đột', 'Khi xung đột xảy ra, việc quản lý nó một cách hiệu quả có thể giúp cải thiện mối quan hệ và tạo ra giải pháp tốt hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhóm đang giải quyết xung đột trong cuộc họp.", "videoScript": "Hãy cùng tìm hiểu cách quản lý xung đột trong môi trường văn phòng. Chúng ta sẽ xem xét các bước cần thiết để giải quyết vấn đề một cách hiệu quả.", "imageDescription": "Một bức ảnh cho thấy một nhóm nhân viên đang thảo luận để giải quyết một xung đột, với biểu cảm nghiêm túc.", "videoDescription": "Video này sẽ trình bày các phương pháp quản lý xung đột và cách áp dụng chúng trong cuộc sống hàng ngày tại văn phòng."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:32:58.711116', '2026-01-27 12:32:58.711116', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý xung đột là kỹ năng quan trọng để duy trì môi trường làm việc hòa thuận.", "detailed": "Khi xung đột xảy ra, việc quản lý nó một cách hiệu quả có thể giúp cải thiện mối quan hệ và tạo ra giải pháp tốt hơn.", "comprehensive": "Quản lý xung đột bao gồm việc nhận diện nguồn gốc của xung đột, lắng nghe các bên liên quan, và tìm kiếm giải pháp hợp lý. Kỹ năng này không chỉ giúp giải quyết vấn đề mà còn có thể củng cố mối quan hệ trong nhóm làm việc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d6933a3e-9457-4e10-88d4-50c1213397e7', '1dd2e8d8-a244-4d70-b88c-017eac380714', 'concept', 'Lập Kế Hoạch Đạt Được Mục Tiêu Tài Chính', 'Lập kế hoạch đạt được mục tiêu tài chính bao gồm việc xác định số tiền cần tiết kiệm hàng tháng, thời gian cần thiết và các bước cụ thể để theo dõi tiến trình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng kế hoạch tài chính với các mục tiêu và thời gian đạt được.", "videoScript": "Trong video này, chúng ta sẽ đi qua các bước cụ thể để lập kế hoạch đạt được mục tiêu tài chính của bạn. Hãy cùng bắt đầu nhé!", "imageDescription": "Hình ảnh một bảng kế hoạch tài chính với các mục tiêu được ghi chú rõ ràng, cùng với các mốc thời gian và số tiền cần tiết kiệm.", "videoDescription": "Video hướng dẫn người xem cách lập kế hoạch cụ thể để đạt được mục tiêu tài chính cá nhân."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:07:08.265194', '2026-01-27 13:07:08.265194', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Để đạt được mục tiêu tài chính, bạn cần lập kế hoạch cụ thể.", "detailed": "Lập kế hoạch đạt được mục tiêu tài chính bao gồm việc xác định số tiền cần tiết kiệm hàng tháng, thời gian cần thiết và các bước cụ thể để theo dõi tiến trình.", "comprehensive": "Quá trình lập kế hoạch đạt được mục tiêu tài chính đòi hỏi sự nghiêm túc và kỷ luật. Đầu tiên, bạn cần xác định rõ ràng các mục tiêu của mình. Tiếp theo, hãy tính toán số tiền bạn cần tiết kiệm hàng tháng và thiết lập một ngân sách phù hợp. Cuối cùng, theo dõi tiến trình của bạn và điều chỉnh kế hoạch nếu cần thiết để đảm bảo bạn đang đi đúng hướng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3972ce85-4bbf-4490-952c-4b81fc6ae4c9', '9cde4cdc-e2bf-49e0-aafc-0570def1ca25', 'concept', 'Tạo ra sự tương phản trong diễn cảm', 'Tạo sự tương phản trong diễn cảm có thể làm nổi bật những phần quan trọng của bản nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Nghệ sĩ piano chuyển đổi giữa các âm lượng khác nhau.", "videoScript": "Hôm nay, chúng ta sẽ cùng nhau khám phá cách tạo ra sự tương phản trong diễn cảm. Hãy cùng nhau thực hành nào!", "imageDescription": "Hình ảnh một nghệ sĩ piano đang chơi với sự thay đổi âm lượng rõ rệt, thể hiện sự tương phản.", "videoDescription": "Video này sẽ hướng dẫn bạn cách tạo ra sự tương phản trong diễn cảm khi biểu diễn piano, giúp âm nhạc trở nên sinh động hơn."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:06.916771', '2026-01-27 12:53:06.916771', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sự tương phản giúp cho âm nhạc sinh động.", "detailed": "Tạo sự tương phản trong diễn cảm có thể làm nổi bật những phần quan trọng của bản nhạc.", "comprehensive": "Bài học này sẽ tập trung vào việc tạo ra sự tương phản trong diễn cảm khi chơi piano. Chúng ta sẽ học cách thay đổi âm lượng, tốc độ và cách nhấn phím để tạo ra những khoảnh khắc nổi bật và thu hút trong bản nhạc. Sự tương phản không chỉ làm cho âm nhạc trở nên sinh động mà còn giúp khán giả dễ dàng cảm nhận được những biến đổi trong cảm xúc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('887bdd55-c6cb-4685-957b-2d5ea9727a8d', '1dd2e8d8-a244-4d70-b88c-017eac380714', 'concept', 'Theo dõi và Điều chỉnh Mục Tiêu Tài Chính', 'Việc theo dõi mục tiêu tài chính giúp bạn nhận biết những gì đang hoạt động và những gì không. Nếu bạn thấy mình không đạt được tiến trình như mong muốn, hãy điều chỉnh kế hoạch của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang xem xét bảng theo dõi tài chính của mình.", "videoScript": "Trong video này, chúng ta sẽ bàn về cách theo dõi và điều chỉnh mục tiêu tài chính của bạn để luôn đi đúng hướng.", "imageDescription": "Một người ngồi trước máy tính, phân tích bảng theo dõi tiến trình tài chính với nhiều biểu đồ và số liệu.", "videoDescription": "Video hướng dẫn người xem các phương pháp theo dõi và điều chỉnh mục tiêu tài chính hiệu quả."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:07:08.269923', '2026-01-27 13:07:08.269923', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi tiến trình của bạn và điều chỉnh khi cần thiết.", "detailed": "Việc theo dõi mục tiêu tài chính giúp bạn nhận biết những gì đang hoạt động và những gì không. Nếu bạn thấy mình không đạt được tiến trình như mong muốn, hãy điều chỉnh kế hoạch của mình.", "comprehensive": "Theo dõi và điều chỉnh mục tiêu tài chính là một phần quan trọng trong việc quản lý tài chính cá nhân. Bạn nên thường xuyên xem xét tiến trình của mình, so sánh nó với kế hoạch ban đầu và xác định các yếu tố có thể ảnh hưởng đến khả năng đạt được mục tiêu. Nếu cần, hãy điều chỉnh kế hoạch của bạn để phù hợp hơn với thực tế tài chính hiện tại của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f6721581-8f5c-47e5-8e70-a2d1d1858e69', '1dd2e8d8-a244-4d70-b88c-017eac380714', 'concept', 'Cách Giữ Động Lực Để Đạt Được Mục Tiêu Tài Chính', 'Để giữ động lực, bạn có thể đặt ra các phần thưởng nhỏ khi đạt được từng bước tiến trong mục tiêu tài chính của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang ăn mừng khi đạt được mục tiêu tài chính nhỏ.", "videoScript": "Trong video này, chúng ta sẽ khám phá những cách khác nhau để giữ động lực trong quá trình đạt được mục tiêu tài chính của bạn.", "imageDescription": "Một người đứng bên bảng mục tiêu tài chính với nụ cười, ăn mừng khi đã đạt được một mốc quan trọng.", "videoDescription": "Video chia sẻ các chiến lược để duy trì động lực trong việc thực hiện mục tiêu tài chính cá nhân."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:07:08.273622', '2026-01-27 13:07:08.273622', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giữ động lực là rất quan trọng để đạt được mục tiêu tài chính.", "detailed": "Để giữ động lực, bạn có thể đặt ra các phần thưởng nhỏ khi đạt được từng bước tiến trong mục tiêu tài chính của mình.", "comprehensive": "Giữ động lực trong quá trình đạt được mục tiêu tài chính có thể là một thách thức. Để duy trì động lực, bạn nên thiết lập các phần thưởng cho bản thân khi đạt được các mốc quan trọng. Hãy nhớ rằng mỗi bước tiến, dù nhỏ, đều đáng được công nhận. Ngoài ra, hãy chia sẻ tiến trình của bạn với người khác để nhận được sự hỗ trợ và khích lệ từ họ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5b613c6c-bc7b-4439-9451-760dd8506cb6', 'c56ce0af-a1d9-45f1-9691-49646b97741b', 'concept', 'Viết Email Chuyên Nghiệp', 'Một email chuyên nghiệp cần có cấu trúc rõ ràng và ngôn ngữ lịch sự. Điều này giúp nâng cao hình ảnh cá nhân và công ty.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang viết email trên máy tính.", "videoScript": "Chào mừng bạn đến với bài học về viết email chuyên nghiệp. Hôm nay, chúng ta sẽ tìm hiểu cách viết một email hiệu quả và chuyên nghiệp.", "imageDescription": "Một bức ảnh cho thấy một nhân viên đang ngồi trước máy tính, chăm chú viết email.", "videoDescription": "Video này sẽ hướng dẫn bạn cách viết email một cách chuyên nghiệp và hiệu quả trong môi trường văn phòng."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:32:58.715309', '2026-01-27 12:32:58.715309', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Viết email chuyên nghiệp là kỹ năng cần thiết trong công việc.", "detailed": "Một email chuyên nghiệp cần có cấu trúc rõ ràng và ngôn ngữ lịch sự. Điều này giúp nâng cao hình ảnh cá nhân và công ty.", "comprehensive": "Viết email chuyên nghiệp không chỉ đơn thuần là việc truyền đạt thông tin mà còn là cách thể hiện sự tôn trọng và chuyên nghiệp. Một email tốt nên có tiêu đề rõ ràng, lời chào lịch sự, nội dung mạch lạc và lời kết phù hợp. Hãy cùng xem xét các mẹo để cải thiện kỹ năng viết email của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c8897c7b-0563-45b6-979d-84b1c70fd8aa', 'd2b414fb-5274-4213-a74a-33a08abcf0ef', 'concept', 'Giới thiệu về Rủi ro Đầu tư', 'Rủi ro đầu tư là khả năng mà các khoản đầu tư có thể mất giá trị hoặc không mang lại lợi nhuận như mong muốn. Hiểu rõ về các loại rủi ro là rất quan trọng để có thể đưa ra quyết định đầu tư thông minh.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một biểu đồ thể hiện các loại rủi ro đầu tư.", "videoScript": "Trong video này, chúng ta sẽ khám phá khái niệm rủi ro đầu tư và tại sao nó lại quan trọng đối với các nhà đầu tư.", "imageDescription": "Hình ảnh minh họa các loại rủi ro đầu tư với biểu đồ và các phân khúc khác nhau.", "videoDescription": "Video giải thích về rủi ro đầu tư, các loại rủi ro và cách nhận diện chúng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:07:43.150175', '2026-01-27 13:07:43.150175', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Rủi ro đầu tư là khả năng mất mát tài sản trong quá trình đầu tư.", "detailed": "Rủi ro đầu tư là khả năng mà các khoản đầu tư có thể mất giá trị hoặc không mang lại lợi nhuận như mong muốn. Hiểu rõ về các loại rủi ro là rất quan trọng để có thể đưa ra quyết định đầu tư thông minh.", "comprehensive": "Rủi ro đầu tư không chỉ là việc mất tiền mà còn bao gồm nhiều yếu tố như biến động thị trường, lãi suất, tình hình kinh tế và chính trị. Có nhiều loại rủi ro khác nhau như rủi ro thị trường, rủi ro tín dụng, và rủi ro thanh khoản. Việc nhận diện và quản lý những rủi ro này là chìa khóa để bảo vệ tài sản của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3a6be57b-3229-48af-9ee1-0c11509dd383', 'd2b414fb-5274-4213-a74a-33a08abcf0ef', 'concept', 'Các loại Rủi ro Đầu tư', 'Các loại rủi ro đầu tư bao gồm rủi ro thị trường, rủi ro tín dụng, rủi ro thanh khoản, và rủi ro lạm phát. Mỗi loại rủi ro đều ảnh hưởng đến quyết định đầu tư của bạn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các loại rủi ro đầu tư với biểu tượng.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu chi tiết về từng loại rủi ro đầu tư và cách mà chúng ảnh hưởng đến quyết định đầu tư.", "imageDescription": "Biểu tượng đại diện cho từng loại rủi ro đầu tư.", "videoDescription": "Video khám phá các loại rủi ro khác nhau và ví dụ về cách mỗi loại có thể tác động đến nhà đầu tư."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:07:43.157943', '2026-01-27 13:07:43.157943', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại rủi ro đầu tư như rủi ro thị trường, rủi ro tín dụng.", "detailed": "Các loại rủi ro đầu tư bao gồm rủi ro thị trường, rủi ro tín dụng, rủi ro thanh khoản, và rủi ro lạm phát. Mỗi loại rủi ro đều ảnh hưởng đến quyết định đầu tư của bạn.", "comprehensive": "Rủi ro thị trường liên quan đến biến động giá cả, rủi ro tín dụng liên quan đến khả năng thanh toán của người vay, rủi ro thanh khoản là khả năng chuyển đổi tài sản thành tiền mặt nhanh chóng và rủi ro lạm phát ảnh hưởng đến sức mua của tiền tệ. Hiểu rõ các loại rủi ro này giúp nhà đầu tư có chiến lược phù hợp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6f633fdd-ca35-4cc0-a014-000fb0c7f13d', 'd2b414fb-5274-4213-a74a-33a08abcf0ef', 'concept', 'Cách Đánh giá Rủi ro trong Đầu tư', 'Đánh giá rủi ro trong đầu tư có thể thực hiện thông qua phân tích tài chính, xem xét các chỉ số như tỷ lệ P/E, và nghiên cứu xu hướng thị trường.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng so sánh phân tích rủi ro đầu tư.", "videoScript": "Trong video này, chúng ta sẽ xem xét cách đánh giá rủi ro trong các khoản đầu tư và các công cụ hữu ích để thực hiện điều này.", "imageDescription": "Hình ảnh bảng so sánh các yếu tố rủi ro và lợi nhuận dự kiến.", "videoDescription": "Video hướng dẫn cách đánh giá rủi ro đầu tư bằng các phương pháp phân tích khác nhau."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:07:43.16135', '2026-01-27 13:07:43.16135', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể đánh giá rủi ro thông qua phân tích tài chính và nghiên cứu thị trường.", "detailed": "Đánh giá rủi ro trong đầu tư có thể thực hiện thông qua phân tích tài chính, xem xét các chỉ số như tỷ lệ P/E, và nghiên cứu xu hướng thị trường.", "comprehensive": "Quá trình đánh giá rủi ro bao gồm việc phân tích các yếu tố kinh tế vĩ mô, xu hướng ngành, và điều kiện tài chính của công ty. Sử dụng các công cụ như phân tích SWOT hoặc mô hình định giá có thể giúp bạn nhận diện và đánh giá được mức độ rủi ro của các khoản đầu tư tiềm năng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2479c69b-4e88-4786-9b6b-1cae45cc4614', 'ddc59470-7309-47b3-89d3-e28953442868', 'concept', 'Giới thiệu về Quản lý Thời gian', 'Quản lý thời gian là quá trình lập kế hoạch và kiểm soát thời gian mà bạn dành cho các hoạt động khác nhau, nhằm tối ưu hóa hiệu suất công việc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang làm việc tại bàn làm việc với lịch trình và đồng hồ.", "videoScript": "Mở đầu với hình ảnh văn phòng bận rộn, giới thiệu tầm quan trọng của quản lý thời gian. Sau đó, đưa ra các mẹo cụ thể để cải thiện quản lý thời gian.", "imageDescription": "Hình ảnh một văn phòng với một nhân viên chăm chỉ làm việc bên bàn làm việc, xung quanh có các tài liệu và lịch trình.", "videoDescription": "Video giới thiệu về quản lý thời gian trong môi trường văn phòng, bao gồm các mẹo và công cụ hỗ trợ."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:33:24.532617', '2026-01-27 12:33:24.532617', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý thời gian giúp tăng hiệu quả công việc.", "detailed": "Quản lý thời gian là quá trình lập kế hoạch và kiểm soát thời gian mà bạn dành cho các hoạt động khác nhau, nhằm tối ưu hóa hiệu suất công việc.", "comprehensive": "Quản lý thời gian là một kỹ năng quan trọng trong môi trường văn phòng, giúp nhân viên phân bổ thời gian hợp lý cho các nhiệm vụ, tránh sự lãng phí thời gian và nâng cao hiệu quả công việc. Kỹ năng này bao gồm lập kế hoạch, ưu tiên nhiệm vụ và sử dụng các công cụ hỗ trợ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6db0d92d-18e2-4d96-9690-5a9ef1b9c9d9', 'd2b414fb-5274-4213-a74a-33a08abcf0ef', 'concept', 'Chiến lược Quản lý Rủi ro', 'Một trong những chiến lược quản lý rủi ro phổ biến là đa dạng hóa danh mục đầu tư, giúp giảm thiểu rủi ro bằng cách không đặt tất cả trứng vào một giỏ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ thể hiện chiến lược đa dạng hóa đầu tư.", "videoScript": "Video này sẽ giới thiệu các chiến lược quản lý rủi ro và cách áp dụng chúng vào thực tế.", "imageDescription": "Hình ảnh mô tả một danh mục đầu tư đa dạng với nhiều loại tài sản khác nhau.", "videoDescription": "Video hướng dẫn về các chiến lược quản lý rủi ro trong đầu tư và lợi ích của chúng."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:07:43.164712', '2026-01-27 13:07:43.164712', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý rủi ro có thể bao gồm đa dạng hóa danh mục đầu tư.", "detailed": "Một trong những chiến lược quản lý rủi ro phổ biến là đa dạng hóa danh mục đầu tư, giúp giảm thiểu rủi ro bằng cách không đặt tất cả trứng vào một giỏ.", "comprehensive": "Quản lý rủi ro bao gồm nhiều chiến lược như đa dạng hóa, sử dụng hợp đồng tương lai để phòng ngừa rủi ro, và thiết lập các mức cắt lỗ. Việc áp dụng các chiến lược này giúp nhà đầu tư bảo vệ tài sản và tối ưu hóa lợi nhuận trong dài hạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c3289a5b-e39f-482c-bdf1-00126a01b17d', 'ddc59470-7309-47b3-89d3-e28953442868', 'concept', 'Kỹ thuật Pomodoro', 'Kỹ thuật Pomodoro là phương pháp quản lý thời gian giúp tăng cường sự tập trung và năng suất bằng cách chia nhỏ thời gian làm việc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một đồng hồ bấm giờ với các khoảng thời gian Pomodoro.", "videoScript": "Giới thiệu về kỹ thuật Pomodoro, giải thích cách hoạt động và lợi ích của nó. Đưa ra ví dụ thực tế về cách áp dụng trong công việc.", "imageDescription": "Hình ảnh đồng hồ bấm giờ với các mốc thời gian Pomodoro được đánh dấu rõ ràng.", "videoDescription": "Video hướng dẫn sử dụng kỹ thuật Pomodoro để quản lý thời gian hiệu quả, bao gồm cách thiết lập và theo dõi."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:33:24.541699', '2026-01-27 12:33:24.541699', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Pomodoro giúp làm việc hiệu quả hơn.", "detailed": "Kỹ thuật Pomodoro là phương pháp quản lý thời gian giúp tăng cường sự tập trung và năng suất bằng cách chia nhỏ thời gian làm việc.", "comprehensive": "Kỹ thuật Pomodoro được phát triển bởi Francesco Cirillo, sử dụng khoảng thời gian 25 phút làm việc liên tục, sau đó nghỉ 5 phút. Sau 4 Pomodoros, bạn có thể nghỉ dài hơn. Phương pháp này giúp cải thiện sự tập trung và giảm thiểu mệt mỏi."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('fb20e220-db89-4651-bbf4-645e3a42b382', 'ddc59470-7309-47b3-89d3-e28953442868', 'concept', 'Lập danh sách công việc', 'Lập danh sách công việc là một phương pháp đơn giản nhưng hiệu quả để theo dõi các nhiệm vụ cần hoàn thành.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một danh sách công việc với các mục đã hoàn thành.", "videoScript": "Nêu rõ lý do tại sao lập danh sách công việc quan trọng, cách tạo danh sách hiệu quả và cách theo dõi tiến độ.", "imageDescription": "Hình ảnh một danh sách công việc trên màn hình máy tính với các mục được đánh dấu là đã hoàn thành.", "videoDescription": "Video giới thiệu cách lập danh sách công việc và các mẹo để duy trì động lực hoàn thành."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:33:24.546294', '2026-01-27 12:33:24.546294', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Danh sách giúp bạn nhớ công việc cần làm.", "detailed": "Lập danh sách công việc là một phương pháp đơn giản nhưng hiệu quả để theo dõi các nhiệm vụ cần hoàn thành.", "comprehensive": "Danh sách công việc giúp bạn tổ chức các nhiệm vụ theo mức độ ưu tiên, giúp bạn kiểm soát công việc tốt hơn. Các công cụ như ứng dụng quản lý công việc có thể hỗ trợ bạn trong việc này."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('12b1aa0c-3e48-49e4-b35e-1d02b7c31063', 'd2b414fb-5274-4213-a74a-33a08abcf0ef', 'concept', 'Tầm Quan trọng của Quản lý Rủi ro', 'Quản lý rủi ro không chỉ giúp bảo vệ tài sản mà còn giúp nâng cao khả năng sinh lời của các khoản đầu tư.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh thể hiện sự ổn định tài chính qua quản lý rủi ro.", "videoScript": "Trong video này, chúng ta sẽ thảo luận về tầm quan trọng của quản lý rủi ro trong đầu tư và những lợi ích dài hạn mà nó mang lại.", "imageDescription": "Hình ảnh minh họa cho sự ổn định tài chính và lợi nhuận từ việc quản lý rủi ro hiệu quả.", "videoDescription": "Video giải thích tầm quan trọng của quản lý rủi ro trong đầu tư và cách nó ảnh hưởng đến sự thành công tài chính."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:07:43.169566', '2026-01-27 13:07:43.169566', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý rủi ro là cần thiết để bảo vệ tài sản đầu tư.", "detailed": "Quản lý rủi ro không chỉ giúp bảo vệ tài sản mà còn giúp nâng cao khả năng sinh lời của các khoản đầu tư.", "comprehensive": "Tầm quan trọng của quản lý rủi ro nằm ở chỗ nó giúp nhà đầu tư duy trì sự ổn định tài chính và đạt được các mục tiêu đầu tư. Bằng cách nhận diện và quản lý rủi ro, nhà đầu tư có thể tạo ra một danh mục đầu tư bền vững và giảm thiểu khả năng mất mát lớn trong các giai đoạn biến động của thị trường."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7495e067-9a36-4056-97c7-ceb25c04209c', 'ddc59470-7309-47b3-89d3-e28953442868', 'concept', 'Sắp xếp ưu tiên công việc', 'Sắp xếp ưu tiên công việc là kỹ năng cần thiết trong quản lý thời gian, giúp bạn quyết định nhiệm vụ nào cần thực hiện trước.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng Eisenhower Box với các công việc được phân loại.", "videoScript": "Giới thiệu về sắp xếp ưu tiên công việc, giải thích Eisenhower Box và hướng dẫn cách sử dụng nó trong thực tế.", "imageDescription": "Hình ảnh bảng Eisenhower Box với các ô được đánh dấu rõ ràng cho các loại công việc khác nhau.", "videoDescription": "Video hướng dẫn cách sắp xếp ưu tiên công việc để đạt hiệu quả cao trong công việc."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:33:24.550096', '2026-01-27 12:33:24.550096', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Ưu tiên giúp bạn hoàn thành việc quan trọng trước.", "detailed": "Sắp xếp ưu tiên công việc là kỹ năng cần thiết trong quản lý thời gian, giúp bạn quyết định nhiệm vụ nào cần thực hiện trước.", "comprehensive": "Việc ưu tiên công việc dựa trên mức độ khẩn cấp và quan trọng giúp bạn tập trung vào những nhiệm vụ có giá trị cao nhất. Kỹ thuật Eisenhower Box là một công cụ hữu ích trong việc phân loại và quản lý các công việc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7db8a017-f227-4666-8db0-01d0adecfcfb', 'ddc59470-7309-47b3-89d3-e28953442868', 'concept', 'Sử dụng công cụ hỗ trợ quản lý thời gian', 'Có nhiều công cụ hỗ trợ quản lý thời gian như ứng dụng lịch, phần mềm quản lý công việc và các công cụ lên kế hoạch.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Giao diện của một ứng dụng quản lý thời gian trên điện thoại.", "videoScript": "Giới thiệu các công cụ quản lý thời gian phổ biến, cách sử dụng và lợi ích của từng công cụ.", "imageDescription": "Hình ảnh một ứng dụng quản lý thời gian trên điện thoại di động với các nhiệm vụ và lịch trình được hiển thị.", "videoDescription": "Video hướng dẫn sử dụng các công cụ hỗ trợ quản lý thời gian trong môi trường văn phòng."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:33:24.553206', '2026-01-27 12:33:24.553206', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Công cụ giúp bạn quản lý thời gian tốt hơn.", "detailed": "Có nhiều công cụ hỗ trợ quản lý thời gian như ứng dụng lịch, phần mềm quản lý công việc và các công cụ lên kế hoạch.", "comprehensive": "Sử dụng công cụ hỗ trợ quản lý thời gian sẽ giúp bạn tổ chức công việc hiệu quả hơn. Một số công cụ như Trello, Asana, hoặc Google Calendar cho phép bạn theo dõi công việc, lên lịch và nhắc nhở bạn về các nhiệm vụ quan trọng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('aeb89246-b793-4c9d-a728-9cc9b0375e9e', '41684847-516b-4b77-be52-8c2bd61fc37a', 'concept', 'Nhập Môn Đầu Tư', 'Đầu tư là việc sử dụng tiền của bạn để mua tài sản với hy vọng rằng giá trị của nó sẽ tăng lên theo thời gian.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người trẻ tuổi đang đọc sách về đầu tư.", "videoScript": "Chào mừng bạn đến với bài học đầu tiên về đầu tư. Hôm nay, chúng ta sẽ khám phá khái niệm đầu tư và tại sao nó quan trọng trong quản lý tài chính cá nhân.", "imageDescription": "Một người đàn ông trẻ tuổi ngồi trên ghế sofa, đọc một cuốn sách về đầu tư với một tách cà phê bên cạnh.", "videoDescription": "Video giới thiệu về đầu tư, giải thích tại sao nó là một phần quan trọng trong tài chính cá nhân."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:08:22.174785', '2026-01-27 13:08:22.174785', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đầu tư là cách bạn sử dụng tiền để sinh lời.", "detailed": "Đầu tư là việc sử dụng tiền của bạn để mua tài sản với hy vọng rằng giá trị của nó sẽ tăng lên theo thời gian.", "comprehensive": "Đầu tư là một phương pháp tài chính cá nhân quan trọng, nơi bạn sử dụng tiền của mình để mua các tài sản như cổ phiếu, trái phiếu, hoặc bất động sản, nhằm tạo ra lợi nhuận trong tương lai. Việc nắm vững các khái niệm cơ bản về đầu tư sẽ giúp bạn đưa ra quyết định thông minh hơn trong việc quản lý tài sản của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9e7b71b9-e30b-4ad8-a64d-3737779fec3a', '9eb5f626-2967-4667-85fc-c96962ff50fd', 'concept', 'Giới Thiệu Về Biểu Đồ', 'Biểu đồ giúp minh họa mối quan hệ giữa các dữ liệu khác nhau, từ đó giúp người dùng phân tích và đưa ra quyết định.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại biểu đồ như biểu đồ cột, biểu đồ đường, biểu đồ tròn.", "videoScript": "Xin chào các bạn! Hôm nay, chúng ta sẽ cùng tìm hiểu về biểu đồ và vai trò của nó trong việc trình bày dữ liệu. Chúng ta sẽ khám phá các loại biểu đồ khác nhau và cách sử dụng chúng.", "imageDescription": "Hình ảnh minh họa nhiều loại biểu đồ khác nhau, cho thấy cách chúng thể hiện dữ liệu.", "videoDescription": "Video giới thiệu về biểu đồ, bao gồm các loại biểu đồ phổ biến và cách chúng giúp người dùng hiểu thông tin."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:33:54.450461', '2026-01-27 12:33:54.450461', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ là một công cụ trực quan hóa dữ liệu giúp dễ dàng hiểu thông tin.", "detailed": "Biểu đồ giúp minh họa mối quan hệ giữa các dữ liệu khác nhau, từ đó giúp người dùng phân tích và đưa ra quyết định.", "comprehensive": "Biểu đồ là công cụ hữu ích trong việc trình bày số liệu một cách trực quan, bao gồm nhiều loại như biểu đồ cột, biểu đồ đường, và biểu đồ tròn. Mỗi loại biểu đồ có cách sử dụng và ứng dụng riêng, phù hợp với từng loại dữ liệu và mục đích phân tích."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1cca5028-152b-410b-8935-fbfc5217361d', '22db2630-70bb-4ef0-9e8f-51f654775cd4', 'concept', 'Kỹ thuật chơi Piano trong âm nhạc hiện đại', 'Các kỹ thuật như arpeggio, legato và staccato giúp tạo ra âm thanh đặc sắc khi chơi piano trong âm nhạc hiện đại.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nghệ sĩ piano đang tập luyện với các ghi chú âm nhạc.", "videoScript": "Hôm nay, chúng ta sẽ tìm hiểu về các kỹ thuật chơi piano cơ bản trong âm nhạc hiện đại.", "imageDescription": "Một nghệ sĩ đang ngồi trước piano, chăm chú luyện tập với các ghi chú âm nhạc xung quanh.", "videoDescription": "Video hướng dẫn các kỹ thuật chơi piano, với các ví dụ thực tế và giải thích rõ ràng."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:50:42.183918', '2026-01-27 12:50:42.183918', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều kỹ thuật để chơi piano như arpeggio, legato.", "detailed": "Các kỹ thuật như arpeggio, legato và staccato giúp tạo ra âm thanh đặc sắc khi chơi piano trong âm nhạc hiện đại.", "comprehensive": "Khi chơi piano trong âm nhạc hiện đại, các kỹ thuật như arpeggio, legato, và staccato không chỉ tạo ra âm thanh phong phú mà còn thể hiện cảm xúc và cá tính của người chơi. Việc kết hợp các kỹ thuật này là điều cần thiết để tạo ra những giai điệu cuốn hút."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2c340e88-63eb-4dfe-b92e-084023c6af4f', '22db2630-70bb-4ef0-9e8f-51f654775cd4', 'concept', 'Piano và sự sáng tạo trong sáng tác nhạc', 'Với khả năng tạo ra nhiều âm sắc khác nhau, piano thường được sử dụng để sáng tác và tạo ra những giai điệu mới.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhạc sĩ đang sáng tác trên piano với một cuốn sổ tay.", "videoScript": "Trong video này, chúng ta sẽ khám phá cách mà piano hỗ trợ quá trình sáng tác nhạc.", "imageDescription": "Một nhạc sĩ ngồi trước piano, viết nốt nhạc vào cuốn sổ tay, với ánh sáng dịu nhẹ chiếu sáng không gian.", "videoDescription": "Video giải thích cách mà piano có thể được sử dụng trong quá trình sáng tác, kèm theo các ví dụ về các bản nhạc nổi tiếng."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:50:42.189014', '2026-01-27 12:50:42.189014', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Piano giúp nghệ sĩ sáng tác nhạc dễ dàng hơn.", "detailed": "Với khả năng tạo ra nhiều âm sắc khác nhau, piano thường được sử dụng để sáng tác và tạo ra những giai điệu mới.", "comprehensive": "Piano không chỉ là nhạc cụ để biểu diễn mà còn là công cụ hỗ trợ sáng tác hiệu quả. Nhiều nhạc sĩ đã sử dụng piano để phát triển ý tưởng âm nhạc, từ những giai điệu đơn giản đến những bản nhạc phức tạp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ddf621da-2271-43fd-b367-46a6802e5909', '9eb5f626-2967-4667-85fc-c96962ff50fd', 'concept', 'Các Loại Biểu Đồ Thông Dụng', 'Biểu đồ cột thường dùng để so sánh các giá trị, biểu đồ đường để thể hiện xu hướng theo thời gian, và biểu đồ tròn để thể hiện tỷ lệ phần trăm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ cột, biểu đồ đường và biểu đồ tròn với các dữ liệu giả định.", "videoScript": "Chào mừng các bạn đến với bài học hôm nay! Chúng ta sẽ khám phá các loại biểu đồ thông dụng, bao gồm biểu đồ cột, đường và tròn. Cùng xem cách mỗi loại biểu đồ có thể được sử dụng trong thực tế.", "imageDescription": "Hình ảnh mô tả ba loại biểu đồ thông dụng với dữ liệu mẫu cho từng loại.", "videoDescription": "Video hướng dẫn về các loại biểu đồ thông dụng trong tin học văn phòng và cách sử dụng chúng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:33:54.460543', '2026-01-27 12:33:54.460543', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại biểu đồ như biểu đồ cột, biểu đồ đường, và biểu đồ tròn.", "detailed": "Biểu đồ cột thường dùng để so sánh các giá trị, biểu đồ đường để thể hiện xu hướng theo thời gian, và biểu đồ tròn để thể hiện tỷ lệ phần trăm.", "comprehensive": "Trong tin học văn phòng, các loại biểu đồ như biểu đồ cột, biểu đồ đường, và biểu đồ tròn được sử dụng phổ biến. Biểu đồ cột giúp bạn so sánh các giá trị khác nhau nhanh chóng, biểu đồ đường cho thấy xu hướng dữ liệu theo thời gian, và biểu đồ tròn thể hiện tỷ lệ giữa các phần trong một tổng thể."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('63ffe6c8-d27a-457b-9c69-96861e8ffb5a', '41684847-516b-4b77-be52-8c2bd61fc37a', 'concept', 'Các Loại Hình Đầu Tư', 'Các loại hình đầu tư bao gồm cổ phiếu, trái phiếu, quỹ đầu tư, bất động sản, và nhiều hình thức khác, mỗi loại đều có những ưu nhược điểm riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các biểu tượng của các loại hình đầu tư khác nhau.", "videoScript": "Hãy cùng nhau khám phá các loại hình đầu tư khác nhau. Mỗi loại hình đều có những đặc điểm riêng mà bạn cần hiểu để đưa ra quyết định đúng đắn.", "imageDescription": "Một bảng điều khiển với các biểu tượng đại diện cho cổ phiếu, trái phiếu, và bất động sản.", "videoDescription": "Video giải thích các loại hình đầu tư như cổ phiếu, trái phiếu, và bất động sản, cùng với ưu nhược điểm của chúng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:08:22.181837', '2026-01-27 13:08:22.181837', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại hình đầu tư như cổ phiếu, trái phiếu, và bất động sản.", "detailed": "Các loại hình đầu tư bao gồm cổ phiếu, trái phiếu, quỹ đầu tư, bất động sản, và nhiều hình thức khác, mỗi loại đều có những ưu nhược điểm riêng.", "comprehensive": "Trong thế giới đầu tư, có nhiều loại hình khác nhau mà bạn có thể lựa chọn. Cổ phiếu cho phép bạn sở hữu một phần của công ty, trong khi trái phiếu là khoản vay mà bạn cho một tổ chức hoặc chính phủ. Bất động sản có thể mang lại lợi nhuận từ cho thuê hoặc tăng giá trị theo thời gian. Mỗi loại hình đầu tư đều có rủi ro và lợi nhuận tiềm năng khác nhau, và việc hiểu rõ chúng sẽ giúp bạn đưa ra quyết định đầu tư thông minh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('eedc0f18-8f51-46c3-9c77-93c9d8d1f160', '9eb5f626-2967-4667-85fc-c96962ff50fd', 'concept', 'Cách Tạo Biểu Đồ Trong Excel', 'Để tạo biểu đồ, bạn chỉ cần chọn dữ liệu bạn muốn biểu diễn, sau đó vào tab ''Insert'' và chọn loại biểu đồ mà bạn muốn sử dụng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giao diện Excel với các bước tạo biểu đồ.", "videoScript": "Hôm nay, chúng ta sẽ học cách tạo biểu đồ trong Excel. Bắt đầu bằng việc chọn dữ liệu, sau đó vào tab ''Insert'' và chọn loại biểu đồ mà bạn muốn. Hãy cùng thực hành nhé!", "imageDescription": "Hình ảnh hướng dẫn từng bước để tạo biểu đồ trong Excel từ việc chọn dữ liệu đến việc hoàn thiện biểu đồ.", "videoDescription": "Video hướng dẫn chi tiết từng bước để tạo biểu đồ trong Excel, bao gồm các mẹo và thủ thuật."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:33:54.46606', '2026-01-27 12:33:54.46606', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể tạo biểu đồ trong Excel bằng cách chọn dữ liệu và nhấn vào nút ''Insert''.", "detailed": "Để tạo biểu đồ, bạn chỉ cần chọn dữ liệu bạn muốn biểu diễn, sau đó vào tab ''Insert'' và chọn loại biểu đồ mà bạn muốn sử dụng.", "comprehensive": "Trong Excel, việc tạo biểu đồ rất đơn giản. Bạn chỉ cần chọn vùng dữ liệu cần thiết, sau đó vào tab ''Insert'', chọn loại biểu đồ phù hợp. Bạn có thể tùy chỉnh biểu đồ bằng cách thêm tiêu đề, nhãn trục và các yếu tố khác để biểu diễn dữ liệu một cách rõ ràng hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('592f6831-0663-4cb6-baae-a0ce63bc67eb', '41684847-516b-4b77-be52-8c2bd61fc37a', 'concept', 'Rủi Ro Trong Đầu Tư', 'Mỗi loại hình đầu tư đều có rủi ro riêng, từ rủi ro thị trường đến rủi ro tín dụng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người ngồi trên bàn làm việc, lo lắng về đầu tư.", "videoScript": "Trong bài học này, chúng ta sẽ thảo luận về rủi ro trong đầu tư. Tại sao bạn cần phải hiểu rủi ro và cách để quản lý nó?", "imageDescription": "Một người phụ nữ đang nhìn vào màn hình máy tính với biểu đồ chứng khoán, vẻ mặt căng thẳng.", "videoDescription": "Video giải thích về các loại rủi ro trong đầu tư và cách quản lý chúng."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:08:22.185295', '2026-01-27 13:08:22.185295', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đầu tư luôn đi kèm với rủi ro.", "detailed": "Mỗi loại hình đầu tư đều có rủi ro riêng, từ rủi ro thị trường đến rủi ro tín dụng.", "comprehensive": "Rủi ro là một phần không thể thiếu trong đầu tư. Nó có thể đến từ nhiều nguồn khác nhau, bao gồm biến động thị trường, thay đổi lãi suất, và tình hình kinh tế toàn cầu. Việc hiểu rõ các loại rủi ro có thể giúp bạn đặt ra các chiến lược đầu tư hợp lý và bảo vệ tài sản của mình khỏi những biến động không lường trước."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7d6e06c4-ce97-496f-935f-895dc809d131', '9eb5f626-2967-4667-85fc-c96962ff50fd', 'concept', 'Tùy Chỉnh Biểu Đồ', 'Excel cho phép bạn tùy chỉnh biểu đồ bằng cách thay đổi màu sắc, loại đường và thêm các yếu tố như chú thích và tiêu đề.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ trước và sau khi tùy chỉnh.", "videoScript": "Trong bài học này, chúng ta sẽ cùng tìm hiểu cách tùy chỉnh biểu đồ trong Excel. Hãy cùng xem cách thay đổi màu sắc, tiêu đề và các yếu tố khác để biểu đồ của bạn trở nên nổi bật hơn.", "imageDescription": "Hình ảnh so sánh biểu đồ trước và sau khi tùy chỉnh để thấy sự khác biệt.", "videoDescription": "Video hướng dẫn các bước tùy chỉnh biểu đồ trong Excel để tăng tính trực quan và hiệu quả trong việc trình bày dữ liệu."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:33:54.469771', '2026-01-27 12:33:54.469771', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể tùy chỉnh biểu đồ bằng cách thay đổi màu sắc và tiêu đề.", "detailed": "Excel cho phép bạn tùy chỉnh biểu đồ bằng cách thay đổi màu sắc, loại đường và thêm các yếu tố như chú thích và tiêu đề.", "comprehensive": "Tùy chỉnh biểu đồ là một phần quan trọng để truyền tải thông điệp rõ ràng hơn. Trong Excel, bạn có thể thay đổi màu sắc, kiểu biểu đồ, thêm tiêu đề, nhãn cho các trục và chú thích để biểu đồ trở nên trực quan hơn. Việc này giúp người xem dễ dàng hiểu và phân tích dữ liệu hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a836e46f-0ef5-4611-ad4b-63dce64bfe7a', '41684847-516b-4b77-be52-8c2bd61fc37a', 'concept', 'Chiến Lược Đầu Tư Hiệu Quả', 'Các chiến lược đầu tư có thể bao gồm đầu tư dài hạn, đầu tư theo giá trị, và đầu tư theo xu hướng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ chứng khoán với các mũi tên chỉ hướng tăng trưởng.", "videoScript": "Hôm nay, chúng ta sẽ khám phá các chiến lược đầu tư hiệu quả. Bạn sẽ học cách xác định chiến lược phù hợp với mình.", "imageDescription": "Một biểu đồ chứng khoán với các mũi tên chỉ ra xu hướng tăng trưởng, cùng với các ghi chú chiến lược đầu tư.", "videoDescription": "Video hướng dẫn về các chiến lược đầu tư khác nhau và cách áp dụng chúng vào thực tế."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:08:22.188985', '2026-01-27 13:08:22.188985', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều chiến lược đầu tư khác nhau.", "detailed": "Các chiến lược đầu tư có thể bao gồm đầu tư dài hạn, đầu tư theo giá trị, và đầu tư theo xu hướng.", "comprehensive": "Chiến lược đầu tư là những kế hoạch mà bạn lập ra để đạt được mục tiêu tài chính của mình. Các chiến lược này có thể bao gồm đầu tư dài hạn, nơi bạn giữ tài sản trong nhiều năm, hoặc đầu tư ngắn hạn, nơi bạn mua và bán thường xuyên để kiếm lời. Đầu tư theo giá trị tập trung vào việc tìm kiếm các tài sản bị định giá thấp, trong khi đầu tư theo xu hướng tìm kiếm các tài sản có xu hướng tăng trưởng. Hiểu rõ các chiến lược này sẽ giúp bạn tối ưu hóa lợi nhuận."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('25f1a29d-336a-46d1-a61f-8b76e089d330', '9eb5f626-2967-4667-85fc-c96962ff50fd', 'concept', 'Phân Tích Dữ Liệu Qua Biểu Đồ', 'Sử dụng biểu đồ giúp người dùng dễ dàng nhận ra xu hướng và mẫu trong dữ liệu mà không cần phải xem xét từng số liệu một.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ với phân tích dữ liệu đi kèm.", "videoScript": "Chào các bạn! Trong bài học này, chúng ta sẽ khám phá cách phân tích dữ liệu thông qua biểu đồ. Hãy cùng tìm hiểu cách mà biểu đồ giúp chúng ta nhận ra xu hướng và mối quan hệ trong dữ liệu.", "imageDescription": "Hình ảnh thể hiện một biểu đồ cùng với các phân tích dữ liệu để dễ dàng hiểu hơn.", "videoDescription": "Video giải thích cách sử dụng biểu đồ để phân tích dữ liệu hiệu quả, bao gồm các ví dụ thực tế."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:33:54.475572', '2026-01-27 12:33:54.475572', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ giúp bạn phân tích dữ liệu một cách trực quan.", "detailed": "Sử dụng biểu đồ giúp người dùng dễ dàng nhận ra xu hướng và mẫu trong dữ liệu mà không cần phải xem xét từng số liệu một.", "comprehensive": "Biểu đồ không chỉ là công cụ trình bày dữ liệu mà còn là một phương tiện mạnh mẽ để phân tích. Bằng cách sử dụng các loại biểu đồ khác nhau, người dùng có thể dễ dàng nhận diện các xu hướng, mẫu và mối quan hệ giữa các biến số trong dữ liệu. Việc này giúp đưa ra các quyết định dựa trên dữ liệu một cách chính xác hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d2b36434-4fd9-4cda-b6f1-b650955e2a09', '41684847-516b-4b77-be52-8c2bd61fc37a', 'concept', 'Lập Kế Hoạch Tài Chính Cá Nhân', 'Một kế hoạch tài chính cá nhân bao gồm việc thiết lập mục tiêu, ngân sách và các chiến lược đầu tư.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang viết kế hoạch tài chính trên giấy.", "videoScript": "Trong bài học cuối cùng này, chúng ta sẽ tìm hiểu cách lập kế hoạch tài chính cá nhân. Bắt đầu với các mục tiêu của bạn!", "imageDescription": "Một người đàn ông ngồi tại bàn làm việc, viết ra kế hoạch tài chính với nhiều ghi chú xung quanh.", "videoDescription": "Video hướng dẫn cách lập kế hoạch tài chính cá nhân và tầm quan trọng của nó trong đầu tư."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:08:22.191938', '2026-01-27 13:08:22.191938', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kế hoạch tài chính giúp bạn quản lý tiền bạc tốt hơn.", "detailed": "Một kế hoạch tài chính cá nhân bao gồm việc thiết lập mục tiêu, ngân sách và các chiến lược đầu tư.", "comprehensive": "Lập kế hoạch tài chính cá nhân là một bước quan trọng trong việc quản lý tài sản của bạn. Nó bao gồm việc xác định mục tiêu tài chính, lập ngân sách chi tiêu hàng tháng, và xác định các chiến lược đầu tư phù hợp. Kế hoạch này không chỉ giúp bạn tiết kiệm và đầu tư hiệu quả mà còn giúp bạn chuẩn bị cho những tình huống khẩn cấp và đạt được sự ổn định tài chính lâu dài."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('217e650a-c7c5-4128-95b3-dd37d724ddd9', 'ac610c24-e58c-4189-bda5-66876bcbdf7e', 'concept', 'Khái Niệm Về Quỹ Khẩn Cấp', 'Quỹ khẩn cấp giúp bạn đối phó với các tình huống không lường trước như mất việc làm, tai nạn hoặc chi phí y tế. Nó nên đủ để trang trải từ 3 đến 6 tháng chi phí sinh hoạt.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một chiếc hũ đựng tiền với nhãn ''Quỹ Khẩn Cấp'' bên cạnh các biểu tượng của sự an toàn như ô tô, nhà ở.", "videoScript": "Chào mừng đến với bài học về quỹ khẩn cấp. Hôm nay, chúng ta sẽ tìm hiểu về khái niệm quỹ khẩn cấp và lý do tại sao nó lại quan trọng với tài chính cá nhân của bạn.", "imageDescription": "Hình ảnh một chiếc hũ đựng tiền với nhãn ''Quỹ Khẩn Cấp'', đại diện cho việc tiết kiệm cho tương lai.", "videoDescription": "Video giải thích khái niệm quỹ khẩn cấp và tầm quan trọng của nó trong cuộc sống tài chính hàng ngày."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:01.535084', '2026-01-27 13:09:01.535084', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quỹ khẩn cấp là số tiền bạn dành riêng để sử dụng trong trường hợp khẩn cấp.", "detailed": "Quỹ khẩn cấp giúp bạn đối phó với các tình huống không lường trước như mất việc làm, tai nạn hoặc chi phí y tế. Nó nên đủ để trang trải từ 3 đến 6 tháng chi phí sinh hoạt.", "comprehensive": "Quỹ khẩn cấp là một phần quan trọng trong quản lý tài chính cá nhân. Nó được định nghĩa là số tiền được dành riêng để sử dụng trong các tình huống khẩn cấp, giúp bạn duy trì sự ổn định tài chính trong những lúc khó khăn. Lý tưởng nhất, quỹ này nên đủ để trang trải 3 đến 6 tháng chi phí sinh hoạt của bạn, đảm bảo bạn có thể đối phó với những sự kiện bất ngờ mà không cần phải vay mượn hay sử dụng thẻ tín dụng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('09ab9011-5bed-4a8c-88b7-5f1ab3230754', 'ac610c24-e58c-4189-bda5-66876bcbdf7e', 'concept', 'Tại Sao Cần Có Quỹ Khẩn Cấp?', 'Khi bạn có quỹ khẩn cấp, bạn sẽ cảm thấy an tâm hơn trong cuộc sống và có thể đối phó với các tình huống bất ngờ mà không lo lắng về tài chính.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang cầm một chiếc ô trong cơn mưa, biểu tượng cho sự bảo vệ tài chính.", "videoScript": "Trong bài học này, chúng ta sẽ khám phá lý do tại sao một quỹ khẩn cấp lại cần thiết cho mọi người và những lợi ích mà nó mang lại.", "imageDescription": "Hình ảnh một người sử dụng ô để che mưa, tượng trưng cho sự bảo vệ mà quỹ khẩn cấp mang lại.", "videoDescription": "Video trình bày các lý do tại sao có quỹ khẩn cấp là cần thiết và những lợi ích mà nó mang lại cho cuộc sống tài chính của bạn."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:01.543483', '2026-01-27 13:09:01.543483', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quỹ khẩn cấp giúp bạn không phải vay mượn khi gặp khó khăn.", "detailed": "Khi bạn có quỹ khẩn cấp, bạn sẽ cảm thấy an tâm hơn trong cuộc sống và có thể đối phó với các tình huống bất ngờ mà không lo lắng về tài chính.", "comprehensive": "Quỹ khẩn cấp đóng vai trò quan trọng trong việc bảo vệ bạn khỏi những khó khăn tài chính không lường trước. Nó cung cấp sự an toàn, giúp bạn không phải phụ thuộc vào các khoản vay hoặc thẻ tín dụng khi gặp phải những tình huống khẩn cấp như mất việc, bệnh tật hoặc các sự cố bất ngờ khác. Có một quỹ khẩn cấp cũng giúp bạn duy trì tâm lý thoải mái hơn trong cuộc sống hàng ngày."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8aa6dd4c-ec33-40eb-af66-56e148acde5f', 'ac610c24-e58c-4189-bda5-66876bcbdf7e', 'concept', 'Cách Xác Định Số Tiền Cần Có Trong Quỹ Khẩn Cấp', 'Để xác định số tiền cần có trong quỹ khẩn cấp, hãy tính toán tổng chi phí sinh hoạt hàng tháng và nhân với số tháng bạn muốn có quỹ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng tính với các mục chi phí và số tiền tổng cộng ở phía dưới.", "videoScript": "Hãy cùng nhau tìm hiểu cách xác định số tiền cần có trong quỹ khẩn cấp. Tôi sẽ chỉ cho bạn cách tính toán đơn giản để bạn có thể biết được mình cần tiết kiệm bao nhiêu.", "imageDescription": "Hình ảnh của một bảng tính tài chính thể hiện các mục chi phí và tổng chi phí hàng tháng.", "videoDescription": "Video hướng dẫn người xem cách xác định số tiền tối thiểu cần có trong quỹ khẩn cấp thông qua việc tính toán chi phí sinh hoạt."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:01.546926', '2026-01-27 13:09:01.546926', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn nên có từ 3 đến 6 tháng chi phí sinh hoạt trong quỹ của mình.", "detailed": "Để xác định số tiền cần có trong quỹ khẩn cấp, hãy tính toán tổng chi phí sinh hoạt hàng tháng và nhân với số tháng bạn muốn có quỹ.", "comprehensive": "Xác định số tiền cần có trong quỹ khẩn cấp là một bước quan trọng trong việc xây dựng tài chính cá nhân. Một quy tắc phổ biến là có từ 3 đến 6 tháng chi phí sinh hoạt trong quỹ của bạn. Để tính toán cụ thể, hãy xác định tổng chi phí sinh hoạt hàng tháng của bạn bao gồm tiền thuê nhà, thực phẩm, hóa đơn và các chi phí khác. Sau đó, nhân số này với số tháng mà bạn muốn đảm bảo, để có được số tiền tối thiểu bạn cần có trong quỹ khẩn cấp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0e4d1a9e-afa9-4fd8-b59d-e518c294f117', '8919c949-8d38-4436-92d6-137f600ea20c', 'concept', 'Giới thiệu về Giấy', 'Giấy có nhiều loại khác nhau, mỗi loại phù hợp với mục đích sử dụng khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của các loại giấy khác nhau trên bàn làm việc.", "videoScript": "Chào mừng bạn đến với bài học về giấy. Hôm nay, chúng ta sẽ khám phá các loại giấy khác nhau và cách chúng được sử dụng trong văn phòng.", "imageDescription": "Một bàn làm việc với nhiều loại giấy như giấy in, giấy photocopy và giấy ghi chú.", "videoDescription": "Video giới thiệu về các loại giấy và ứng dụng của chúng trong công việc văn phòng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:34:21.766557', '2026-01-27 12:34:21.766557', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giấy là vật liệu cơ bản trong văn phòng.", "detailed": "Giấy có nhiều loại khác nhau, mỗi loại phù hợp với mục đích sử dụng khác nhau.", "comprehensive": "Giấy được sản xuất từ bột giấy và có nhiều dạng như giấy in, giấy photocopy, và giấy ghi chú. Mỗi loại giấy có độ dày, độ bền và khả năng hấp thụ mực khác nhau, ảnh hưởng đến chất lượng in ấn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e8768514-3df5-47fa-85fb-0c7dd713411b', '8919c949-8d38-4436-92d6-137f600ea20c', 'concept', 'Kích thước Giấy Tiêu chuẩn', 'Kích thước A4 là 210 x 297 mm, A5 là 148 x 210 mm. Mỗi kích thước phù hợp với các mục đích khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ so sánh các kích thước giấy tiêu chuẩn.", "videoScript": "Trong phần này, chúng ta sẽ tìm hiểu về các kích thước giấy tiêu chuẩn và cách chúng ảnh hưởng đến công việc hàng ngày.", "imageDescription": "Một biểu đồ thể hiện kích thước của giấy A4, A5 và Letter để so sánh.", "videoDescription": "Video giải thích các kích thước giấy tiêu chuẩn và ứng dụng của chúng trong văn phòng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:34:21.775219', '2026-01-27 12:34:21.775219', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều kích thước giấy tiêu chuẩn như A4, A5.", "detailed": "Kích thước A4 là 210 x 297 mm, A5 là 148 x 210 mm. Mỗi kích thước phù hợp với các mục đích khác nhau.", "comprehensive": "Kích thước giấy tiêu chuẩn như A4, A5, và Letter có sự khác biệt rõ rệt. A4 thường được sử dụng cho văn bản và tài liệu, trong khi A5 thường được sử dụng cho sổ tay và ghi chú. Kích thước giấy ảnh hưởng đến cách trình bày và thiết kế tài liệu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7983c47e-c84c-41b0-a2fc-0ff2ca32a5fc', '8919c949-8d38-4436-92d6-137f600ea20c', 'concept', 'Lựa chọn Giấy phù hợp cho In ấn', 'Giấy có độ dày và độ bóng khác nhau, ảnh hưởng đến kết quả in.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại giấy in ấn khác nhau.", "videoScript": "Hãy cùng khám phá cách lựa chọn giấy phù hợp cho in ấn để đảm bảo chất lượng tài liệu tối ưu.", "imageDescription": "Một bộ sưu tập các loại giấy in ấn với thông tin về độ dày và độ bóng.", "videoDescription": "Video hướng dẫn cách chọn giấy in ấn phù hợp với mục đích sử dụng."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:34:21.779212', '2026-01-27 12:34:21.779212', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn giấy đúng giúp in ấn chất lượng hơn.", "detailed": "Giấy có độ dày và độ bóng khác nhau, ảnh hưởng đến kết quả in.", "comprehensive": "Khi lựa chọn giấy cho in ấn, bạn cần xem xét độ dày, độ bóng và khả năng hấp thụ mực. Giấy dày hơn thường thích hợp cho hình ảnh và tài liệu quan trọng, trong khi giấy mỏng hơn phù hợp cho các tài liệu nội bộ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e466e830-bccb-4bba-9d5a-7ee502e52963', '8919c949-8d38-4436-92d6-137f600ea20c', 'concept', 'Giấy và Màu sắc', 'Màu sắc giấy có thể tạo nên sự khác biệt trong thiết kế.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giấy nhiều màu sắc trên bàn làm việc.", "videoScript": "Hãy cùng tìm hiểu về vai trò của màu sắc giấy trong thiết kế tài liệu và cách nó tác động đến người đọc.", "imageDescription": "Một bộ sưu tập giấy với nhiều màu sắc khác nhau, từ giấy trắng đến giấy màu sáng.", "videoDescription": "Video phân tích tác động của màu sắc giấy trong văn phòng và thiết kế."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:34:21.783554', '2026-01-27 12:34:21.783554', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giấy có nhiều màu sắc khác nhau.", "detailed": "Màu sắc giấy có thể tạo nên sự khác biệt trong thiết kế.", "comprehensive": "Màu sắc giấy có vai trò quan trọng trong việc truyền tải thông điệp. Giấy trắng thường được sử dụng cho tài liệu chính thức, trong khi giấy màu có thể được sử dụng để tạo điểm nhấn hoặc thu hút sự chú ý."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b5b162bb-e288-4862-8fd2-3c09e1945bd5', 'ac610c24-e58c-4189-bda5-66876bcbdf7e', 'concept', 'Các Bước Để Xây Dựng Quỹ Khẩn Cấp', 'Bắt đầu bằng cách xác định số tiền bạn cần, sau đó tạo một kế hoạch tiết kiệm hàng tháng để đạt được mục tiêu đó.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một biểu đồ thể hiện quá trình tiết kiệm từng tháng cho quỹ khẩn cấp.", "videoScript": "Trong bài học này, chúng ta sẽ đi qua từng bước để xây dựng quỹ khẩn cấp của bạn từ việc xác định số tiền cần thiết cho đến lập kế hoạch tiết kiệm hàng tháng.", "imageDescription": "Hình ảnh một biểu đồ thể hiện tiến trình tiết kiệm cho quỹ khẩn cấp theo từng tháng.", "videoDescription": "Video hướng dẫn chi tiết các bước cần thiết để xây dựng quỹ khẩn cấp, từ việc xác định mục tiêu đến thực hiện kế hoạch tiết kiệm."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:01.549952', '2026-01-27 13:09:01.549952', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn cần lập kế hoạch và bắt đầu tiết kiệm hàng tháng.", "detailed": "Bắt đầu bằng cách xác định số tiền bạn cần, sau đó tạo một kế hoạch tiết kiệm hàng tháng để đạt được mục tiêu đó.", "comprehensive": "Để xây dựng quỹ khẩn cấp hiệu quả, bạn cần thực hiện các bước cụ thể. Đầu tiên, xác định số tiền cần thiết cho quỹ khẩn cấp. Tiếp theo, lập một kế hoạch tiết kiệm hàng tháng với một khoản tiền cụ thể mà bạn có thể dành ra. Hãy xem xét việc cắt giảm các chi phí không cần thiết để tăng tốc độ tiết kiệm. Cuối cùng, hãy theo dõi tiến trình của bạn và điều chỉnh kế hoạch nếu cần thiết để đảm bảo bạn đạt được mục tiêu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('82a3f069-1007-490c-bb06-a89c80b45a4e', 'ba24d13e-ca49-4e63-ac35-d7726870c8f1', 'concept', 'Tại sao cần quản lý danh mục đầu tư?', 'Việc quản lý danh mục đầu tư là cần thiết để đảm bảo rằng bạn đang đi đúng hướng với các mục tiêu tài chính của mình và có thể điều chỉnh kịp thời khi thị trường thay đổi.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhóm người thảo luận về các chiến lược đầu tư.", "videoScript": "Trong bài học này, chúng ta sẽ thảo luận về lý do tại sao việc quản lý danh mục đầu tư là cần thiết cho tương lai tài chính của bạn.", "imageDescription": "Một nhóm các chuyên gia tài chính đang thảo luận và phân tích các chiến lược đầu tư trong một cuộc họp.", "videoDescription": "Video giải thích tầm quan trọng của quản lý danh mục đầu tư và cách nó có thể ảnh hưởng đến sự thành công tài chính cá nhân."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:01:44.419292', '2026-01-27 13:01:44.419292', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý danh mục đầu tư giúp bạn tối đa hóa lợi nhuận và giảm rủi ro.", "detailed": "Việc quản lý danh mục đầu tư là cần thiết để đảm bảo rằng bạn đang đi đúng hướng với các mục tiêu tài chính của mình và có thể điều chỉnh kịp thời khi thị trường thay đổi.", "comprehensive": "Quản lý danh mục đầu tư là rất quan trọng vì nó giúp bạn bảo vệ tài sản của mình, tối ưu hóa lợi nhuận và duy trì một chiến lược đầu tư bền vững. Nó cũng giúp bạn hiểu rõ hơn về các rủi ro và cơ hội trong thị trường tài chính."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e32e9ffb-7352-445f-9e8c-693e2614774a', 'ac610c24-e58c-4189-bda5-66876bcbdf7e', 'concept', 'Những Lỗi Cần Tránh Khi Xây Dựng Quỹ Khẩn Cấp', 'Một trong những lỗi phổ biến là sử dụng quỹ khẩn cấp cho các chi phí không khẩn cấp như mua sắm hoặc du lịch.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang cầm một bảng hiệu ''Không Sử Dụng Quỹ Khẩn Cấp'' bên cạnh các vật dụng không cần thiết.", "videoScript": "Trong bài học cuối cùng này, chúng ta sẽ thảo luận về những lỗi cần tránh khi xây dựng quỹ khẩn cấp, đảm bảo rằng bạn có một quỹ vững chắc cho những tình huống khẩn cấp.", "imageDescription": "Hình ảnh một người giữ bảng hiệu ''Không Sử Dụng Quỹ Khẩn Cấp'' bên cạnh các vật phẩm như quần áo mới và đồ điện tử.", "videoDescription": "Video chỉ ra những lỗi phổ biến mà mọi người thường gặp phải khi xây dựng quỹ khẩn cấp và cách để tránh chúng."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:01.553304', '2026-01-27 13:09:01.553304', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đừng sử dụng quỹ khẩn cấp cho các chi phí không khẩn cấp.", "detailed": "Một trong những lỗi phổ biến là sử dụng quỹ khẩn cấp cho các chi phí không khẩn cấp như mua sắm hoặc du lịch.", "comprehensive": "Khi xây dựng quỹ khẩn cấp, có một số lỗi mà bạn cần tránh để đảm bảo quỹ của bạn luôn sẵn sàng khi cần thiết. Một trong những lỗi phổ biến nhất là sử dụng quỹ khẩn cấp cho các chi phí không khẩn cấp như mua sắm, du lịch hoặc các khoản chi tiêu xa hoa khác. Hãy nhớ rằng quỹ này chỉ nên được sử dụng trong các tình huống thực sự khẩn cấp. Ngoài ra, không nên trì hoãn việc bắt đầu tiết kiệm quỹ khẩn cấp vì lý do này hay lý do khác. Hãy bắt đầu ngay hôm nay, ngay cả khi bạn chỉ có thể tiết kiệm một khoản nhỏ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ff2941e9-22e0-404c-a4a0-a8af131868b7', 'a3d61119-14aa-4364-9789-43153104671d', 'concept', 'Những Nguyên Tắc Cơ Bản Của Quản Lý Tài Chính Cá Nhân', 'Quản lý tài chính cá nhân là việc lập kế hoạch cho thu nhập và chi tiêu của bạn để đạt được mục tiêu tài chính.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ quản lý tài chính cá nhân", "videoScript": "Chào mừng đến với bài học về quản lý tài chính cá nhân. Hôm nay, chúng ta sẽ khám phá các nguyên tắc cơ bản giúp bạn kiểm soát tài chính của mình.", "imageDescription": "Một biểu đồ thể hiện cách phân bổ ngân sách cho các khoản chi tiêu khác nhau.", "videoDescription": "Video này hướng dẫn người xem về các nguyên tắc cơ bản trong quản lý tài chính cá nhân."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:28.358663', '2026-01-27 13:09:28.358663', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý tài chính cá nhân giúp bạn tiết kiệm và đầu tư hiệu quả.", "detailed": "Quản lý tài chính cá nhân là việc lập kế hoạch cho thu nhập và chi tiêu của bạn để đạt được mục tiêu tài chính.", "comprehensive": "Quản lý tài chính cá nhân bao gồm việc thiết lập ngân sách, theo dõi chi tiêu, tiết kiệm cho tương lai và đầu tư nhằm gia tăng tài sản. Điều này không chỉ giúp bạn kiểm soát tài chính mà còn chuẩn bị cho các tình huống khẩn cấp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('df70c5fe-7261-4749-b657-0439ab32e235', 'a3d61119-14aa-4364-9789-43153104671d', 'concept', 'Cách Lập Ngân Sách Hiệu Quả', 'Một ngân sách hiệu quả sẽ giúp bạn theo dõi thu nhập và chi tiêu hàng tháng, từ đó điều chỉnh chi tiêu cho phù hợp.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Mẫu ngân sách hàng tháng", "videoScript": "Trong video này, chúng ta sẽ cùng nhau tìm hiểu cách lập ngân sách hàng tháng, từ việc xác định thu nhập đến việc phân bổ chi tiêu.", "imageDescription": "Mẫu ngân sách hàng tháng với các danh mục thu nhập và chi tiêu.", "videoDescription": "Video hướng dẫn cách lập ngân sách hiệu quả để quản lý tài chính cá nhân."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:28.366048', '2026-01-27 13:09:28.366048', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lập ngân sách giúp bạn kiểm soát chi tiêu.", "detailed": "Một ngân sách hiệu quả sẽ giúp bạn theo dõi thu nhập và chi tiêu hàng tháng, từ đó điều chỉnh chi tiêu cho phù hợp.", "comprehensive": "Để lập ngân sách hiệu quả, bạn cần xác định nguồn thu nhập hàng tháng, phân loại các khoản chi tiêu như cần thiết và không cần thiết, và tạo ra một kế hoạch để tiết kiệm cho các mục tiêu tài chính trong tương lai."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('685b90ae-3445-416d-af39-354462ac31c2', '8919c949-8d38-4436-92d6-137f600ea20c', 'concept', 'Giấy và Bảo vệ Môi trường', 'Việc sử dụng giấy tái chế giúp giảm thiểu lượng rác thải.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giấy tái chế và quy trình sản xuất.", "videoScript": "Chúng ta sẽ cùng tìm hiểu về giấy tái chế và vai trò của nó trong việc bảo vệ môi trường.", "imageDescription": "Quy trình sản xuất giấy tái chế từ giấy cũ và các sản phẩm cuối cùng.", "videoDescription": "Video giải thích lợi ích của giấy tái chế và cách sử dụng giấy một cách bền vững."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:34:21.787211', '2026-01-27 12:34:21.787211', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giấy tái chế tốt cho môi trường.", "detailed": "Việc sử dụng giấy tái chế giúp giảm thiểu lượng rác thải.", "comprehensive": "Sử dụng giấy tái chế không chỉ giúp giảm lượng rác thải mà còn tiết kiệm tài nguyên. Nhiều công ty hiện nay đã chuyển sang sử dụng giấy tái chế để thể hiện trách nhiệm xã hội và bảo vệ môi trường."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('23e6772a-8d0f-46fb-82de-083d82384328', '22db2630-70bb-4ef0-9e8f-51f654775cd4', 'concept', 'Tương lai của Piano trong âm nhạc hiện đại', 'Với sự tiến bộ của công nghệ, piano có thể sẽ có nhiều hình thức mới và ứng dụng trong âm nhạc hiện đại.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cây piano điện tử hiện đại với các tính năng kỹ thuật số.", "videoScript": "Hãy cùng nhìn vào tương lai của piano và cách nó sẽ tiếp tục phát triển trong âm nhạc hiện đại.", "imageDescription": "Một cây piano điện tử với màn hình cảm ứng và các nhạc cụ kỹ thuật số, thể hiện sự kết hợp giữa truyền thống và hiện đại.", "videoDescription": "Video thảo luận về sự tiến bộ của piano trong tương lai, với các công nghệ mới và cách chúng ảnh hưởng đến âm nhạc."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:50:42.193887', '2026-01-27 12:50:42.193887', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Piano sẽ tiếp tục phát triển trong âm nhạc tương lai.", "detailed": "Với sự tiến bộ của công nghệ, piano có thể sẽ có nhiều hình thức mới và ứng dụng trong âm nhạc hiện đại.", "comprehensive": "Tương lai của piano trong âm nhạc hiện đại có thể sẽ bị ảnh hưởng bởi công nghệ, với sự phát triển của các nhạc cụ kỹ thuật số và phần mềm âm nhạc. Tuy nhiên, piano vẫn giữ được sự hấp dẫn và vai trò quan trọng trong ngành công nghiệp âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('94529eeb-4d8b-4ca7-9c2c-6c0c1e9f0deb', '9cde4cdc-e2bf-49e0-aafc-0570def1ca25', 'concept', 'Kỹ thuật biểu diễn nâng cao', 'Kỹ thuật biểu diễn nâng cao bao gồm các phương pháp như phrasing và dynamics.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Nghệ sĩ piano với phong cách biểu diễn chuyên nghiệp.", "videoScript": "Chào mừng các bạn đến với bài học cuối cùng về kỹ thuật biểu diễn nâng cao. Hãy cùng tôi khám phá những bí quyết để trở thành một nghệ sĩ piano xuất sắc.", "imageDescription": "Hình ảnh một nghệ sĩ piano biểu diễn trên sân khấu lớn với kỹ thuật điêu luyện.", "videoDescription": "Video hướng dẫn các kỹ thuật biểu diễn nâng cao cho người chơi piano, giúp họ trở thành nghệ sĩ chuyên nghiệp."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:06.921292', '2026-01-27 12:53:06.921292', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kỹ thuật nâng cao giúp biểu diễn chuyên nghiệp hơn.", "detailed": "Kỹ thuật biểu diễn nâng cao bao gồm các phương pháp như phrasing và dynamics.", "comprehensive": "Trong bài học cuối cùng này, chúng ta sẽ tìm hiểu về các kỹ thuật biểu diễn nâng cao như phrasing, dynamics và cách sử dụng các hiệu ứng âm thanh để nâng cao diễn cảm trong biểu diễn piano. Những kỹ thuật này sẽ giúp bạn trở thành một nghệ sĩ piano chuyên nghiệp, có khả năng truyền tải cảm xúc mạnh mẽ và sâu sắc qua từng nốt nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d21f4cab-5c33-43d9-b452-3c33c9ffbdbe', '300a6b5a-5f22-482b-a3e8-6d67014f9ab6', 'concept', 'Giới thiệu về Excel và theo dõi tiến độ', 'Trong bài học này, chúng ta sẽ tìm hiểu cách sử dụng Excel để theo dõi tiến độ công việc, từ việc nhập dữ liệu đến việc sử dụng công thức để tính toán.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng Excel với các cột như ''Công việc'', ''Tiến độ'', ''Ngày bắt đầu'', ''Ngày kết thúc''.", "videoScript": "Chào mừng đến với bài học về Excel. Hôm nay, chúng ta sẽ học cách theo dõi tiến độ công việc bằng cách sử dụng các tính năng cơ bản của Excel.", "imageDescription": "Hình ảnh minh họa bảng Excel với các cột theo dõi tiến độ công việc.", "videoDescription": "Video hướng dẫn cách sử dụng Excel để theo dõi tiến độ công việc với các ví dụ thực tiễn."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:34:51.86464', '2026-01-27 12:34:51.86464', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Excel là một công cụ mạnh mẽ để theo dõi tiến độ công việc.", "detailed": "Trong bài học này, chúng ta sẽ tìm hiểu cách sử dụng Excel để theo dõi tiến độ công việc, từ việc nhập dữ liệu đến việc sử dụng công thức để tính toán.", "comprehensive": "Excel không chỉ giúp bạn nhập và lưu trữ dữ liệu mà còn cho phép bạn theo dõi tiến độ dự án một cách hiệu quả. Chúng ta sẽ khám phá cách tổ chức bảng tính, sử dụng công thức tự động và tạo biểu đồ để trực quan hóa tiến độ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d7f91690-2f83-4645-88d5-aaba31bdd0f8', 'a3d61119-14aa-4364-9789-43153104671d', 'concept', 'Tiết Kiệm và Đầu Tư: Hai Bên Của Một Đồng Xu', 'Tiết kiệm là việc dành dụm tiền cho các mục tiêu ngắn hạn, trong khi đầu tư là cách gia tăng tài sản cho tương lai.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Sự khác nhau giữa tiết kiệm và đầu tư", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ nói về sự khác nhau giữa tiết kiệm và đầu tư. Tại sao cả hai đều quan trọng trong quản lý tài chính cá nhân?", "imageDescription": "Hình ảnh so sánh giữa tiết kiệm và đầu tư với các biểu đồ trình bày lợi nhuận.", "videoDescription": "Video này giải thích vai trò của tiết kiệm và đầu tư trong việc quản lý tài chính cá nhân."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:28.369453', '2026-01-27 13:09:28.369453', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tiết kiệm để có tiền, đầu tư để sinh lời.", "detailed": "Tiết kiệm là việc dành dụm tiền cho các mục tiêu ngắn hạn, trong khi đầu tư là cách gia tăng tài sản cho tương lai.", "comprehensive": "Tiết kiệm và đầu tư là hai khía cạnh quan trọng trong quản lý tài chính. Tiết kiệm giúp bạn tạo ra quỹ dự phòng, trong khi đầu tư là cách để gia tăng tài sản thông qua các kênh như cổ phiếu, trái phiếu hoặc bất động sản."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('508e07cd-0305-49fa-ae83-b39e62ee71fc', 'a3d61119-14aa-4364-9789-43153104671d', 'concept', 'Quản Lý Nợ: Làm Thế Nào Để Không Bị Nợ Nần?', 'Quản lý nợ tốt bao gồm việc theo dõi các khoản nợ, trả nợ đúng hạn và không vay mượn quá nhiều.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ quản lý nợ", "videoScript": "Trong video hôm nay, chúng ta sẽ thảo luận về cách quản lý nợ hiệu quả để bạn có thể tránh được tình trạng nợ nần.", "imageDescription": "Biểu đồ thể hiện kế hoạch trả nợ theo thời gian.", "videoDescription": "Video hướng dẫn cách quản lý nợ cá nhân để duy trì tài chính ổn định."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:28.372865', '2026-01-27 13:09:28.372865', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý nợ giúp bạn tránh rơi vào tình trạng nợ nần.", "detailed": "Quản lý nợ tốt bao gồm việc theo dõi các khoản nợ, trả nợ đúng hạn và không vay mượn quá nhiều.", "comprehensive": "Để quản lý nợ hiệu quả, bạn cần lập danh sách tất cả các khoản nợ, xác định lãi suất và thời gian trả nợ, từ đó xây dựng một kế hoạch trả nợ hợp lý. Hãy cố gắng tránh các khoản nợ tiêu dùng không cần thiết."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('89fa7305-aacc-4ed8-a507-63791fc06b98', '300a6b5a-5f22-482b-a3e8-6d67014f9ab6', 'concept', 'Tạo bảng theo dõi tiến độ', 'Trong bài học này, chúng ta sẽ tạo một bảng theo dõi tiến độ với các tiêu chí như công việc, người phụ trách, và tiến độ hoàn thành.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bảng theo dõi tiến độ với các ô chứa thông tin công việc.", "videoScript": "Bây giờ, hãy cùng nhau tạo bảng theo dõi tiến độ. Tôi sẽ hướng dẫn bạn từng bước để thiết lập bảng này.", "imageDescription": "Bảng Excel hiển thị thông tin về tiến độ công việc và các cột dữ liệu.", "videoDescription": "Video hướng dẫn tạo bảng theo dõi tiến độ trong Excel với các bước chi tiết."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:34:51.872509', '2026-01-27 12:34:51.872509', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hãy tạo một bảng để theo dõi tiến độ dự án.", "detailed": "Trong bài học này, chúng ta sẽ tạo một bảng theo dõi tiến độ với các tiêu chí như công việc, người phụ trách, và tiến độ hoàn thành.", "comprehensive": "Việc tạo bảng theo dõi tiến độ là bước đầu tiên để quản lý dự án hiệu quả. Chúng ta sẽ tạo bảng, nhập dữ liệu, và sử dụng các công thức Excel để tự động tính toán tiến độ hoàn thành."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d53c86eb-4d3e-432c-8491-fa4477182e8d', 'a8ed5036-7e88-45df-bce9-a9dd16dceb27', 'concept', 'Giới Thiệu Về Hòa Âm', 'Hòa âm không chỉ đơn thuần là việc chơi nhiều nốt cùng lúc, mà còn là cách sắp xếp và phối hợp các âm thanh để tạo ra một tác phẩm âm nhạc hài hòa.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bàn phím piano với các nốt nhạc được đánh dấu.", "videoScript": "Trong video này, chúng ta sẽ khám phá khái niệm hòa âm và cách nó được sử dụng trong âm nhạc.", "imageDescription": "Hình ảnh một bàn phím piano với các nốt nhạc và hợp âm được đánh dấu rõ ràng.", "videoDescription": "Video giải thích khái niệm hòa âm, với ví dụ minh họa từ các tác phẩm âm nhạc nổi tiếng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:27.293441', '2026-01-27 12:53:27.293441', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hòa âm là sự kết hợp của nhiều nốt nhạc để tạo ra âm thanh phong phú.", "detailed": "Hòa âm không chỉ đơn thuần là việc chơi nhiều nốt cùng lúc, mà còn là cách sắp xếp và phối hợp các âm thanh để tạo ra một tác phẩm âm nhạc hài hòa.", "comprehensive": "Hòa âm là một khái niệm quan trọng trong âm nhạc, thể hiện sự kết hợp giữa các nốt nhạc theo quy tắc nhất định nhằm tạo ra những âm thanh đẹp đẽ, có chiều sâu và cảm xúc. Hòa âm giúp cho người nghe cảm nhận được sự phong phú và đa dạng của âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('92f14e3a-2c31-4d3a-aa64-636cc9da503c', 'a3d61119-14aa-4364-9789-43153104671d', 'concept', 'Xây Dựng Quỹ Khẩn Cấp: Tại Sao Là Quan Trọng?', 'Quỹ khẩn cấp là số tiền bạn dành ra để sử dụng trong các tình huống khẩn cấp, giúp bạn không phải vay mượn khi gặp khó khăn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Quỹ khẩn cấp và tầm quan trọng của nó", "videoScript": "Chào mừng các bạn đến với bài học hôm nay về quỹ khẩn cấp. Tại sao quỹ này lại quan trọng và làm thế nào để xây dựng nó?", "imageDescription": "Hình ảnh mô tả quỹ khẩn cấp và các lợi ích của việc có quỹ này.", "videoDescription": "Video giải thích về quỹ khẩn cấp và tầm quan trọng của nó trong quản lý tài chính cá nhân."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:28.377771', '2026-01-27 13:09:28.377771', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quỹ khẩn cấp giúp bạn sẵn sàng cho các tình huống bất ngờ.", "detailed": "Quỹ khẩn cấp là số tiền bạn dành ra để sử dụng trong các tình huống khẩn cấp, giúp bạn không phải vay mượn khi gặp khó khăn.", "comprehensive": "Xây dựng quỹ khẩn cấp là một phần quan trọng trong quản lý tài chính cá nhân. Một quỹ khẩn cấp nên đủ để chi trả cho 3-6 tháng chi phí sống, giúp bạn yên tâm hơn khi đối mặt với những rủi ro tài chính như mất việc làm hoặc chi phí y tế phát sinh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ad8d3a74-25de-4c15-8bf3-8987dcfaa4cb', '300a6b5a-5f22-482b-a3e8-6d67014f9ab6', 'concept', 'Sử dụng công thức để tính toán tiến độ', 'Chúng ta sẽ học cách sử dụng công thức SUM và AVERAGE để tính tổng số công việc và tỷ lệ hoàn thành.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các công thức Excel trong ô tính.", "videoScript": "Bây giờ, chúng ta sẽ tìm hiểu về cách sử dụng công thức trong Excel để tính toán tiến độ công việc. Hãy xem cách tôi thực hiện điều này.", "imageDescription": "Hình ảnh minh họa các công thức Excel được sử dụng để tính toán tiến độ.", "videoDescription": "Video giải thích cách sử dụng các công thức trong Excel để tính toán tiến độ công việc."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:34:51.87695', '2026-01-27 12:34:51.87695', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng công thức đơn giản để tính toán tiến độ.", "detailed": "Chúng ta sẽ học cách sử dụng công thức SUM và AVERAGE để tính tổng số công việc và tỷ lệ hoàn thành.", "comprehensive": "Công thức là một phần quan trọng trong Excel. Trong bài học này, bạn sẽ học cách sử dụng các công thức như COUNTIF và SUMIF để theo dõi tiến độ và phân tích dữ liệu hiệu quả."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('19285830-42c1-41cc-b0e6-2ec0b64d691e', '300a6b5a-5f22-482b-a3e8-6d67014f9ab6', 'concept', 'Tạo biểu đồ để trực quan hóa tiến độ', 'Biểu đồ giúp bạn thấy rõ hơn về tiến độ dự án. Chúng ta sẽ tạo biểu đồ cột và biểu đồ đường để theo dõi tiến độ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một biểu đồ cột thể hiện tiến độ công việc.", "videoScript": "Chúng ta sẽ tạo một biểu đồ để trực quan hóa tiến độ công việc. Hãy theo dõi từng bước trong video này.", "imageDescription": "Hình ảnh biểu đồ trong Excel thể hiện tiến độ hoàn thành công việc theo thời gian.", "videoDescription": "Video hướng dẫn cách tạo và tùy chỉnh biểu đồ trong Excel để theo dõi tiến độ công việc."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:34:51.881824', '2026-01-27 12:34:51.881824', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng biểu đồ để trực quan hóa dữ liệu tiến độ.", "detailed": "Biểu đồ giúp bạn thấy rõ hơn về tiến độ dự án. Chúng ta sẽ tạo biểu đồ cột và biểu đồ đường để theo dõi tiến độ.", "comprehensive": "Biểu đồ là công cụ mạnh mẽ để trực quan hóa dữ liệu. Trong bài học này, bạn sẽ học cách tạo và tùy chỉnh biểu đồ trong Excel để thấy được tiến độ công việc một cách rõ ràng nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f2380f4a-c389-4a40-a028-30dfbafb709d', '4a9ac50e-e506-44b8-b4dc-58dfcae36034', 'concept', 'Nguyên tắc cơ bản của việc theo dõi ngân sách', 'Theo dõi ngân sách là quá trình ghi chép các khoản thu và chi tiêu hàng tháng để bạn có cái nhìn rõ ràng về tình hình tài chính của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một chiếc sổ ghi chép và bút viết bên cạnh máy tính xách tay.", "videoScript": "Chào mừng bạn đến với bài học về theo dõi ngân sách. Hôm nay, chúng ta sẽ khám phá những nguyên tắc cơ bản để quản lý tài chính cá nhân.", "imageDescription": "Một chiếc sổ tay với các bảng phân tích ngân sách, bên cạnh là một chiếc bút và máy tính xách tay.", "videoDescription": "Video hướng dẫn về cách theo dõi ngân sách và lý do tại sao nó quan trọng trong quản lý tài chính cá nhân."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:59.504325', '2026-01-27 13:09:59.504325', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi ngân sách giúp bạn quản lý tài chính hiệu quả.", "detailed": "Theo dõi ngân sách là quá trình ghi chép các khoản thu và chi tiêu hàng tháng để bạn có cái nhìn rõ ràng về tình hình tài chính của mình.", "comprehensive": "Việc theo dõi ngân sách không chỉ giúp bạn xác định các khoản chi tiêu không cần thiết mà còn giúp bạn lập kế hoạch cho các mục tiêu tài chính trong tương lai. Bằng cách ghi chép các khoản thu nhập và chi tiêu hàng tháng, bạn có thể điều chỉnh thói quen chi tiêu của mình để đạt được sự ổn định tài chính."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('38d92e09-d0c4-416b-b389-3dc294d9a713', '300a6b5a-5f22-482b-a3e8-6d67014f9ab6', 'concept', 'Chia sẻ và bảo mật bảng Excel', 'Chia sẻ bảng Excel với đồng nghiệp và bảo mật thông tin là điều cần thiết. Bài học này sẽ hướng dẫn bạn cách thực hiện điều đó.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu tượng chia sẻ và bảo mật trong Excel.", "videoScript": "Cuối cùng, chúng ta sẽ học cách chia sẻ bảng Excel và bảo mật dữ liệu của bạn. Hãy theo dõi nhé!", "imageDescription": "Hình ảnh minh họa cách chia sẻ bảng Excel và cài đặt bảo mật.", "videoDescription": "Video hướng dẫn cách chia sẻ bảng Excel và các phương pháp bảo mật dữ liệu."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:34:51.886724', '2026-01-27 12:34:51.886724', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách chia sẻ bảng Excel và bảo mật dữ liệu.", "detailed": "Chia sẻ bảng Excel với đồng nghiệp và bảo mật thông tin là điều cần thiết. Bài học này sẽ hướng dẫn bạn cách thực hiện điều đó.", "comprehensive": "Trong môi trường làm việc hiện đại, việc chia sẻ và bảo mật dữ liệu là rất quan trọng. Chúng ta sẽ tìm hiểu các phương pháp chia sẻ bảng Excel và các biện pháp bảo mật để bảo vệ thông tin của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3c9249bd-f4e9-43af-8ff9-bcb83380814e', 'a8ed5036-7e88-45df-bce9-a9dd16dceb27', 'concept', 'Các Loại Hợp Âm Cơ Bản', 'Hợp âm trưởng tạo ra âm thanh vui tươi, trong khi hợp âm thứ thường mang lại cảm giác buồn hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Sơ đồ các loại hợp âm trên bàn phím piano.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về các loại hợp âm cơ bản và cách chúng ảnh hưởng đến cảm xúc âm nhạc.", "imageDescription": "Sơ đồ cho thấy cách chơi các hợp âm trưởng và thứ trên bàn phím piano.", "videoDescription": "Video mô tả các loại hợp âm cơ bản, với ví dụ âm nhạc minh họa từ các bài hát nổi tiếng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:27.301755', '2026-01-27 12:53:27.301755', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại hợp âm như hợp âm trưởng và hợp âm thứ.", "detailed": "Hợp âm trưởng tạo ra âm thanh vui tươi, trong khi hợp âm thứ thường mang lại cảm giác buồn hơn.", "comprehensive": "Hợp âm là một trong những yếu tố cốt lõi của hòa âm. Các loại hợp âm cơ bản bao gồm hợp âm trưởng, hợp âm thứ, hợp âm giảm và hợp âm tăng. Mỗi loại hợp âm mang lại một cảm xúc khác nhau và được sử dụng để tạo ra các bầu không khí khác nhau trong âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('495d1ec6-4d60-4e6c-9852-252876b82a9e', 'a8ed5036-7e88-45df-bce9-a9dd16dceb27', 'concept', 'Cấu Trúc Hòa Âm Trong Một Bản Nhạc', 'Cấu trúc hòa âm có thể thay đổi tùy theo phong cách âm nhạc và cảm xúc mà tác giả muốn truyền tải.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Sơ đồ cấu trúc hòa âm của một bản nhạc phổ biến.", "videoScript": "Trong video này, chúng ta sẽ phân tích cấu trúc hòa âm trong một bản nhạc cụ thể.", "imageDescription": "Sơ đồ mô tả cấu trúc hòa âm của một bài hát nổi tiếng, với các phần được đánh dấu rõ ràng.", "videoDescription": "Video hướng dẫn phân tích cấu trúc hòa âm, giúp người xem hiểu rõ hơn về cách thức hoạt động của hòa âm trong một bản nhạc."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:27.305154', '2026-01-27 12:53:27.305154', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mỗi bản nhạc có cấu trúc hòa âm riêng.", "detailed": "Cấu trúc hòa âm có thể thay đổi tùy theo phong cách âm nhạc và cảm xúc mà tác giả muốn truyền tải.", "comprehensive": "Cấu trúc hòa âm trong một bản nhạc bao gồm các phần như điệp khúc, cầu và các đoạn chuyển tiếp. Mỗi phần có vai trò riêng trong việc xây dựng câu chuyện và cảm xúc cho bản nhạc. Nhận thức được cấu trúc này giúp người chơi piano nâng cao khả năng cảm thụ và biểu diễn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8d7c9461-ac6d-4c2e-bad3-ae470383986d', '4a9ac50e-e506-44b8-b4dc-58dfcae36034', 'concept', 'Cách lập ngân sách hiệu quả', 'Một ngân sách hiệu quả bao gồm các khoản thu nhập, chi tiêu cố định và chi tiêu linh hoạt, giúp bạn cân bằng tài chính.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ ngân sách với các mục thu nhập và chi tiêu rõ ràng.", "videoScript": "Trong video này, chúng ta sẽ cùng nhau tìm hiểu cách lập ngân sách hiệu quả để tối ưu hóa tài chính cá nhân.", "imageDescription": "Một biểu đồ ngân sách minh họa các khoản thu nhập và chi tiêu, với các màu khác nhau để phân biệt.", "videoDescription": "Video mô tả quy trình lập ngân sách và các mẹo để duy trì ngân sách hiệu quả."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:59.512307', '2026-01-27 13:09:59.512307', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lập ngân sách giúp bạn kiểm soát chi tiêu.", "detailed": "Một ngân sách hiệu quả bao gồm các khoản thu nhập, chi tiêu cố định và chi tiêu linh hoạt, giúp bạn cân bằng tài chính.", "comprehensive": "Để lập ngân sách hiệu quả, bạn cần xác định nguồn thu nhập hàng tháng, danh sách các khoản chi tiêu cố định như tiền thuê nhà và hóa đơn, cũng như các khoản chi tiêu linh hoạt như ăn uống và giải trí. Việc phân chia rõ ràng các khoản mục này sẽ giúp bạn dễ dàng theo dõi và điều chỉnh khi cần thiết."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('02cd9f05-cdd8-4f78-a179-568b29d53ede', '4a9ac50e-e506-44b8-b4dc-58dfcae36034', 'concept', 'Công cụ theo dõi ngân sách hữu ích', 'Có nhiều ứng dụng và công cụ trực tuyến giúp bạn theo dõi ngân sách, từ Excel cho đến các ứng dụng chuyên dụng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Màn hình điện thoại với ứng dụng theo dõi ngân sách mở.", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ cùng khám phá một số công cụ và ứng dụng hữu ích để theo dõi ngân sách cá nhân.", "imageDescription": "Một màn hình điện thoại hiển thị giao diện của ứng dụng theo dõi ngân sách với các số liệu trực quan.", "videoDescription": "Video giới thiệu các ứng dụng và công cụ giúp theo dõi ngân sách hiệu quả."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:59.515761', '2026-01-27 13:09:59.515761', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng ứng dụng giúp theo dõi ngân sách dễ dàng hơn.", "detailed": "Có nhiều ứng dụng và công cụ trực tuyến giúp bạn theo dõi ngân sách, từ Excel cho đến các ứng dụng chuyên dụng.", "comprehensive": "Các công cụ theo dõi ngân sách như ứng dụng Mint, YNAB (You Need A Budget) hay thậm chí một bảng tính Excel đơn giản có thể hỗ trợ bạn theo dõi thu nhập, chi tiêu và tiết kiệm. Những công cụ này thường cung cấp các tính năng như thông báo chi tiêu, phân tích tài chính và lập kế hoạch ngân sách."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('678a8aba-3005-4c87-a898-34a4e76da922', '4a9ac50e-e506-44b8-b4dc-58dfcae36034', 'concept', 'Cách điều chỉnh ngân sách khi cần thiết', 'Khi có thay đổi về thu nhập hoặc chi tiêu, bạn cần điều chỉnh ngân sách để phù hợp với tình hình tài chính mới.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang xem xét ngân sách và điều chỉnh các khoản chi tiêu.", "videoScript": "Trong bài học này, chúng ta sẽ thảo luận về cách điều chỉnh ngân sách khi có sự thay đổi trong thu nhập hoặc chi tiêu.", "imageDescription": "Hình ảnh một người đang ngồi bên bàn làm việc, xem xét các bảng tính ngân sách và ghi chú lại các điều chỉnh.", "videoDescription": "Video hướng dẫn về cách điều chỉnh ngân sách cá nhân để phù hợp với tình hình tài chính."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:59.521456', '2026-01-27 13:09:59.521456', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Điều chỉnh ngân sách giúp bạn linh hoạt hơn.", "detailed": "Khi có thay đổi về thu nhập hoặc chi tiêu, bạn cần điều chỉnh ngân sách để phù hợp với tình hình tài chính mới.", "comprehensive": "Việc điều chỉnh ngân sách là một phần quan trọng trong quản lý tài chính cá nhân. Nếu bạn nhận thấy rằng một khoản chi tiêu nào đó vượt quá ngân sách, hãy xem xét lại các khoản khác và điều chỉnh cho phù hợp. Đồng thời, cũng cần xem xét các nguồn thu nhập mới có thể giúp bạn bù đắp cho những khoản chi tiêu vượt mức."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b7f30339-2d6a-4997-bd8a-1412650450bf', '6dc1e2dc-af82-4233-8f65-255cd6fdf4dd', 'concept', 'Giới thiệu về Biểu Đồ Cột', 'Biểu đồ cột giúp người dùng dễ dàng so sánh các giá trị khác nhau thông qua các cột thẳng đứng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một biểu đồ cột đơn giản với các cột màu sắc khác nhau.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về biểu đồ cột và cách sử dụng chúng để trực quan hóa dữ liệu.", "imageDescription": "Biểu đồ cột với 4 cột, mỗi cột có màu sắc khác nhau và có ghi chú giá trị phía trên.", "videoDescription": "Video hướng dẫn giới thiệu và giải thích về biểu đồ cột, cách nó hoạt động và ứng dụng của nó trong công việc."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:35:23.308196', '2026-01-27 12:35:23.308196', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ cột là công cụ trực quan để thể hiện dữ liệu.", "detailed": "Biểu đồ cột giúp người dùng dễ dàng so sánh các giá trị khác nhau thông qua các cột thẳng đứng.", "comprehensive": "Biểu đồ cột là một trong những loại biểu đồ phổ biến trong tin học văn phòng, nó cho phép người dùng thể hiện và so sánh các giá trị một cách trực quan. Các cột thường được sắp xếp theo thứ tự và có thể biểu diễn nhiều loại dữ liệu khác nhau."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8108bac4-8331-4adf-8d95-601163c51da0', 'a8ed5036-7e88-45df-bce9-a9dd16dceb27', 'concept', 'Sự Kết Hợp Các Hợp Âm', 'Việc kết hợp các hợp âm một cách hợp lý giúp tạo ra chiều sâu cho bản nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Mô phỏng một trình tự hợp âm trên piano.", "videoScript": "Trong video này, chúng ta sẽ khám phá cách kết hợp các hợp âm để tạo ra âm thanh phong phú.", "imageDescription": "Hình ảnh mô tả cách kết hợp các hợp âm trong một trình tự trên bàn phím piano.", "videoDescription": "Video trình bày các kỹ thuật kết hợp hợp âm, với ví dụ minh họa từ các tác phẩm âm nhạc."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:27.3095', '2026-01-27 12:53:27.3095', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kết hợp hợp âm tạo ra âm thanh phong phú hơn.", "detailed": "Việc kết hợp các hợp âm một cách hợp lý giúp tạo ra chiều sâu cho bản nhạc.", "comprehensive": "Sự kết hợp các hợp âm là một nghệ thuật trong hòa âm. Bằng cách sử dụng các hợp âm khác nhau trong một trình tự nhất định, người chơi có thể tạo ra những chuyển động âm thanh thú vị và cuốn hút. Điều này không chỉ làm tăng độ phong phú của âm nhạc mà còn dẫn dắt người nghe qua các cung bậc cảm xúc khác nhau."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7ead0d4d-9261-4401-9ab0-a035b1a03c18', '6dc1e2dc-af82-4233-8f65-255cd6fdf4dd', 'concept', 'Cách Tạo Biểu Đồ Cột trong Excel', 'Để tạo biểu đồ cột trong Excel, đầu tiên chọn dữ liệu bạn muốn biểu diễn, sau đó vào tab ''Insert'' và chọn biểu đồ cột từ danh sách.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giao diện Excel với một biểu đồ cột đang được tạo ra.", "videoScript": "Chào mừng các bạn đến với video hướng dẫn tạo biểu đồ cột trong Excel. Hãy cùng bắt đầu nào!", "imageDescription": "Giao diện Excel với bảng dữ liệu và biểu đồ cột hiển thị bên cạnh.", "videoDescription": "Video hướng dẫn chi tiết từng bước tạo biểu đồ cột trong Excel với ví dụ thực tế."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:35:23.315529', '2026-01-27 12:35:23.315529', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể tạo biểu đồ cột trong Excel bằng cách chọn dữ liệu và nhấp vào ''Insert''.", "detailed": "Để tạo biểu đồ cột trong Excel, đầu tiên chọn dữ liệu bạn muốn biểu diễn, sau đó vào tab ''Insert'' và chọn biểu đồ cột từ danh sách.", "comprehensive": "Cách tạo biểu đồ cột trong Excel rất đơn giản. Bạn chỉ cần chọn một dải ô chứa dữ liệu, sau đó vào tab ''Insert'' trên thanh công cụ. Từ đó, bạn chọn biểu tượng biểu đồ cột và chọn kiểu biểu đồ mà bạn muốn. Cuối cùng, điều chỉnh biểu đồ theo nhu cầu của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('68c82df8-252b-4b41-aa06-f65c0080b24b', '4a9ac50e-e506-44b8-b4dc-58dfcae36034', 'concept', 'Lời khuyên để tiết kiệm tiền', 'Cách tiết kiệm tiền hiệu quả bao gồm việc giảm thiểu chi tiêu và tối ưu hóa các khoản đầu tư.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một piggy bank và một danh sách các khoản tiết kiệm.", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ cùng nhau tìm hiểu một số lời khuyên hữu ích để tiết kiệm tiền một cách hiệu quả.", "imageDescription": "Một con lợn đất nằm cạnh một danh sách chi tiêu và tiết kiệm, thể hiện quyết tâm tiết kiệm.", "videoDescription": "Video chia sẻ các mẹo và chiến lược tiết kiệm tiền trong cuộc sống hàng ngày."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:09:59.525524', '2026-01-27 13:09:59.525524', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tiết kiệm là chìa khóa cho tương lai tài chính vững chắc.", "detailed": "Cách tiết kiệm tiền hiệu quả bao gồm việc giảm thiểu chi tiêu và tối ưu hóa các khoản đầu tư.", "comprehensive": "Để tiết kiệm tiền hiệu quả, hãy bắt đầu từ việc xác định các khoản chi tiêu không cần thiết và tìm cách giảm thiểu chúng. Bạn cũng có thể áp dụng các mẹo tiết kiệm như tự nấu ăn thay vì ăn ngoài, mua sắm thông minh và đầu tư vào các sản phẩm có lợi suất cao. Lập kế hoạch cho các mục tiêu tiết kiệm cũng rất quan trọng để giữ động lực."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('542e7509-52dc-46f2-a81a-ae7ed9a46ffe', '6dc1e2dc-af82-4233-8f65-255cd6fdf4dd', 'concept', 'Tùy Chỉnh Biểu Đồ Cột', 'Excel cho phép bạn tùy chỉnh màu sắc, kiểu dáng và các yếu tố khác của biểu đồ cột để phù hợp với nhu cầu của bạn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ cột với các tùy chỉnh màu sắc và tiêu đề.", "videoScript": "Trong video này, chúng ta sẽ cùng nhau tìm hiểu cách tùy chỉnh biểu đồ cột trong Excel để làm cho chúng trở nên hấp dẫn hơn.", "imageDescription": "Biểu đồ cột với các cột có màu sắc khác nhau và một tiêu đề nổi bật.", "videoDescription": "Video hướng dẫn cách thay đổi màu sắc, phong cách và các yếu tố khác của biểu đồ cột trong Excel."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:35:23.318742', '2026-01-27 12:35:23.318742', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể thay đổi màu sắc và kiểu cột dễ dàng.", "detailed": "Excel cho phép bạn tùy chỉnh màu sắc, kiểu dáng và các yếu tố khác của biểu đồ cột để phù hợp với nhu cầu của bạn.", "comprehensive": "Tùy chỉnh biểu đồ cột là bước quan trọng để làm nổi bật thông tin mà bạn muốn truyền đạt. Bạn có thể thay đổi màu sắc của từng cột, thêm tiêu đề, điều chỉnh trục và thêm ghi chú để làm cho biểu đồ trở nên rõ ràng và dễ hiểu hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6a4ff992-29b5-4743-bb40-0fc6c4bdc5aa', 'bb96f278-5f6f-4ff3-9b93-3d3fa98353d7', 'concept', 'Hiểu Về Căng Thẳng và Ảnh Hưởng Đến Da', 'Căng thẳng kích thích sản xuất hormone cortisol, dẫn đến tình trạng da xỉn màu và nổi mụn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bức tranh thể hiện sự căng thẳng và làn da xỉn màu.", "videoScript": "Video này sẽ giải thích mối liên hệ giữa căng thẳng và sức khỏe làn da, cùng với một số mẹo để giảm căng thẳng.", "imageDescription": "Một người phụ nữ với làn da xỉn màu đang cầm đầu, biểu hiện sự căng thẳng.", "videoDescription": "Một video giáo dục giải thích tác động của căng thẳng đến sức khỏe làn da và hướng dẫn các phương pháp giảm căng thẳng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:10:26.528366', '2026-01-27 13:10:26.528366', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Căng thẳng có thể làm da bạn xỉn màu và nổi mụn.", "detailed": "Căng thẳng kích thích sản xuất hormone cortisol, dẫn đến tình trạng da xỉn màu và nổi mụn.", "comprehensive": "Căng thẳng là một phản ứng tự nhiên của cơ thể, nhưng nếu kéo dài, nó có thể gây ra sự mất cân bằng hormone. Hormone cortisol gia tăng có thể dẫn đến tình trạng viêm da, mụn trứng cá, và làm giảm độ đàn hồi của da. Hiểu rõ về mối liên hệ giữa căng thẳng và sức khỏe làn da giúp bạn có những biện pháp phòng ngừa hiệu quả."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6c2d8ec2-10de-4dc7-b6db-4530acdec697', 'bb96f278-5f6f-4ff3-9b93-3d3fa98353d7', 'concept', 'Kỹ Thuật Thư Giãn Để Giảm Căng Thẳng', 'Các kỹ thuật thư giãn như thiền và yoga có thể giúp giảm mức độ căng thẳng, từ đó cải thiện tình trạng da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bức tranh minh họa một người đang thiền trong khung cảnh yên bình.", "videoScript": "Trong video này, chúng ta sẽ hướng dẫn một vài bài tập thiền và yoga đơn giản để giảm căng thẳng.", "imageDescription": "Một người ngồi thiền trong công viên, xung quanh có cây cỏ và hoa tươi.", "videoDescription": "Video hướng dẫn kỹ thuật thiền và yoga giúp giảm căng thẳng và cải thiện sức khỏe làn da."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:10:26.536862', '2026-01-27 13:10:26.536862', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thư giãn giúp giảm căng thẳng và cải thiện sức khỏe làn da.", "detailed": "Các kỹ thuật thư giãn như thiền và yoga có thể giúp giảm mức độ căng thẳng, từ đó cải thiện tình trạng da.", "comprehensive": "Thư giãn là một yếu tố quan trọng trong việc quản lý căng thẳng. Các phương pháp như thiền, yoga, và hít thở sâu không chỉ giúp bạn thư giãn mà còn làm giảm mức cortisol trong cơ thể. Nghiên cứu cho thấy việc thực hành những kỹ thuật này thường xuyên có thể cải thiện tình trạng da, mang lại sự tươi sáng và khỏe mạnh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('85d6c697-631b-405e-a43a-4f3b361ca590', 'bb96f278-5f6f-4ff3-9b93-3d3fa98353d7', 'concept', 'Dinh Dưỡng và Ảnh Hưởng Đến Căng Thẳng', 'Thực phẩm giàu omega-3 và vitamin có thể giúp giảm căng thẳng và cải thiện sức khỏe làn da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bữa ăn lành mạnh với nhiều loại rau củ và cá.", "videoScript": "Video này sẽ trình bày một số thực phẩm tốt cho sức khỏe tâm thần và làn da.", "imageDescription": "Một đĩa ăn đẹp mắt với rau xanh, cá hồi và hạt óc chó.", "videoDescription": "Hướng dẫn dinh dưỡng giúp giảm căng thẳng và cải thiện sức khỏe làn da."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:10:26.540693', '2026-01-27 13:10:26.540693', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chế độ ăn uống lành mạnh giúp giảm căng thẳng và tốt cho da.", "detailed": "Thực phẩm giàu omega-3 và vitamin có thể giúp giảm căng thẳng và cải thiện sức khỏe làn da.", "comprehensive": "Chế độ dinh dưỡng có ảnh hưởng lớn đến cả sức khỏe tinh thần và làn da. Các thực phẩm giàu omega-3 như cá hồi, quả óc chó và các loại hạt có khả năng giảm viêm, trong khi vitamin C và E giúp bảo vệ da khỏi tác động tiêu cực của môi trường. Một chế độ ăn uống cân bằng không chỉ giúp giảm căng thẳng mà còn cải thiện tình trạng da, mang lại sự rạng rỡ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f854389e-d0dc-4ce6-b6d9-515a449950ac', '6dc1e2dc-af82-4233-8f65-255cd6fdf4dd', 'concept', 'Phân Tích Dữ Liệu qua Biểu Đồ Cột', 'Sử dụng biểu đồ cột giúp bạn nhanh chóng nhận diện xu hướng và mối quan hệ giữa các dữ liệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ cột thể hiện sự thay đổi dữ liệu theo thời gian.", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ khám phá cách biểu đồ cột giúp phân tích dữ liệu hiệu quả.", "imageDescription": "Một biểu đồ cột cho thấy sự thay đổi doanh thu theo từng tháng trong năm.", "videoDescription": "Video hướng dẫn cách sử dụng biểu đồ cột để phân tích dữ liệu và đưa ra quyết định hợp lý."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:35:23.321991', '2026-01-27 12:35:23.321991', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ cột giúp bạn phân tích dữ liệu một cách trực quan.", "detailed": "Sử dụng biểu đồ cột giúp bạn nhanh chóng nhận diện xu hướng và mối quan hệ giữa các dữ liệu.", "comprehensive": "Biểu đồ cột không chỉ là công cụ hiển thị dữ liệu mà còn hỗ trợ bạn trong việc phân tích dữ liệu. Bằng cách quan sát các cột, bạn có thể nhanh chóng phát hiện các xu hướng, mẫu và sự khác biệt trong dữ liệu, từ đó đưa ra quyết định chính xác hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5429204c-1e51-495b-a006-8a2eb7c3676d', 'bb96f278-5f6f-4ff3-9b93-3d3fa98353d7', 'concept', 'Chăm Sóc Da Để Giảm Căng Thẳng', 'Sử dụng sản phẩm chăm sóc da tự nhiên có thể mang lại cảm giác thư thái và giảm căng thẳng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang áp dụng mặt nạ dưỡng da trong không gian thư giãn.", "videoScript": "Video này sẽ hướng dẫn bạn cách tự làm mặt nạ dưỡng da tại nhà từ nguyên liệu tự nhiên.", "imageDescription": "Một người phụ nữ đang thư giãn với mặt nạ dưỡng da và nến thơm xung quanh.", "videoDescription": "Hướng dẫn làm mặt nạ dưỡng da tự nhiên và cách chăm sóc da để giảm căng thẳng."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:10:26.544614', '2026-01-27 13:10:26.544614', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chăm sóc da có thể giúp bạn cảm thấy thư giãn.", "detailed": "Sử dụng sản phẩm chăm sóc da tự nhiên có thể mang lại cảm giác thư thái và giảm căng thẳng.", "comprehensive": "Việc chăm sóc da không chỉ là về vẻ bề ngoài mà còn là một cách để thư giãn và chăm sóc bản thân. Sử dụng các sản phẩm chăm sóc da tự nhiên như dầu dừa, tinh dầu oải hương và mặt nạ từ thiên nhiên không chỉ giúp cải thiện tình trạng da mà còn tạo ra cảm giác thư thái, giúp bạn giảm căng thẳng trong cuộc sống hàng ngày."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('bc4ef96a-943e-4edb-ad46-6cbbe98db282', 'bb96f278-5f6f-4ff3-9b93-3d3fa98353d7', 'concept', 'Thói Quen Ngủ Tốt Để Cải Thiện Da', 'Giấc ngủ đủ và chất lượng cao là điều cần thiết cho sức khỏe làn da và tinh thần.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang ngủ thoải mái trong một không gian yên tĩnh.", "videoScript": "Video này sẽ cung cấp các mẹo để có giấc ngủ ngon và cải thiện sức khỏe da.", "imageDescription": "Một người nằm ngủ trong một phòng tối và yên tĩnh, với ánh sáng nhẹ nhàng.", "videoDescription": "Hướng dẫn thói quen ngủ tốt để giảm căng thẳng và cải thiện sức khỏe làn da."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:10:26.548376', '2026-01-27 13:10:26.548376', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Ngủ đủ giấc giúp da khỏe mạnh và giảm căng thẳng.", "detailed": "Giấc ngủ đủ và chất lượng cao là điều cần thiết cho sức khỏe làn da và tinh thần.", "comprehensive": "Ngủ đủ giấc là một phần không thể thiếu trong việc duy trì sức khỏe làn da. Trong khi ngủ, cơ thể phục hồi và tái tạo tế bào da. Thiếu ngủ không chỉ làm tăng mức độ căng thẳng mà còn dẫn đến tình trạng da xỉn màu, quầng thâm dưới mắt và lão hóa sớm. Thiết lập thói quen ngủ tốt như đi ngủ và thức dậy vào cùng một giờ mỗi ngày có thể làm cải thiện đáng kể sức khỏe của làn da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('28c7ad70-8cb5-45fd-881c-744528272804', '6dc1e2dc-af82-4233-8f65-255cd6fdf4dd', 'concept', 'Lỗi Thường Gặp khi Tạo Biểu Đồ Cột', 'Một số lỗi phổ biến khi tạo biểu đồ cột bao gồm chọn sai loại dữ liệu hoặc không điều chỉnh đúng trục.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ cột bị sai lệch do lỗi dữ liệu.", "videoScript": "Trong video này, chúng ta sẽ cùng nhau tìm hiểu những lỗi thường gặp khi tạo biểu đồ cột và cách khắc phục chúng.", "imageDescription": "Một biểu đồ cột với các giá trị không chính xác và gây nhầm lẫn.", "videoDescription": "Video hướng dẫn nhận diện và sửa chữa các lỗi khi tạo biểu đồ cột trong Excel."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:35:23.325245', '2026-01-27 12:35:23.325245', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có thể bạn sẽ gặp một số lỗi khi tạo biểu đồ cột.", "detailed": "Một số lỗi phổ biến khi tạo biểu đồ cột bao gồm chọn sai loại dữ liệu hoặc không điều chỉnh đúng trục.", "comprehensive": "Khi tạo biểu đồ cột, người dùng có thể gặp một số lỗi phổ biến như: không chọn đúng dải ô dữ liệu, không điều chỉnh đúng trục hoành hoặc trục tung, hoặc không sử dụng loại biểu đồ phù hợp với dữ liệu. Những lỗi này có thể làm ảnh hưởng đến độ chính xác và tính rõ ràng của biểu đồ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3edd5f16-e5a7-415b-9cca-4f55dd07d733', '55ffda79-3855-4d3d-a344-0a48fcab1d04', 'concept', 'Tầm Quan Trọng Của Giấc Ngủ', 'Trong khi bạn ngủ, cơ thể sản xuất collagen và tế bào mới, giúp da khỏe mạnh hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người phụ nữ đang ngủ ngon trong một căn phòng tối.", "videoScript": "Mở đầu với hình ảnh một người phụ nữ đang chuẩn bị đi ngủ. Giọng nói lồng ghép giải thích về tầm quan trọng của giấc ngủ đối với sức khỏe da, sau đó chuyển sang các cảnh quay về quá trình phục hồi da trong khi ngủ.", "imageDescription": "Hình ảnh một người phụ nữ nằm trên giường với ánh sáng nhẹ nhàng, thể hiện sự thư giãn và an yên.", "videoDescription": "Video mô tả quá trình phục hồi của da trong giấc ngủ, với các thông điệp về collagen và sự tái tạo tế bào."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:11:03.080894', '2026-01-27 13:11:03.080894', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giấc ngủ giúp da phục hồi và tái tạo.", "detailed": "Trong khi bạn ngủ, cơ thể sản xuất collagen và tế bào mới, giúp da khỏe mạnh hơn.", "comprehensive": "Giấc ngủ đóng vai trò quan trọng trong việc duy trì sức khỏe da. Khi ngủ đủ giấc, cơ thể có thời gian để phục hồi, sản xuất collagen và các tế bào mới, giúp da trở nên tươi sáng và đàn hồi hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2e817022-3a33-4ea1-b53f-1078ba302687', '55ffda79-3855-4d3d-a344-0a48fcab1d04', 'concept', 'Giấc Ngủ Ảnh Hưởng Đến Làn Da Như Thế Nào?', 'Thiếu ngủ làm tăng hormone stress, dẫn đến tình trạng viêm và mụn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người phụ nữ có dấu hiệu mệt mỏi và làn da xỉn màu.", "videoScript": "Bắt đầu với hình ảnh một người phụ nữ cảm thấy mệt mỏi. Lời lồng ghép giải thích về tác động của giấc ngủ đến làn da, với các biểu đồ hoặc hình ảnh minh họa cho mụn và nếp nhăn.", "imageDescription": "Hình ảnh một người phụ nữ với làn da xỉn màu và quầng thâm dưới mắt, thể hiện tác động tiêu cực của giấc ngủ không đủ.", "videoDescription": "Video giải thích cách giấc ngủ ảnh hưởng đến sức khỏe da, với hình ảnh rõ ràng về dấu hiệu của làn da thiếu ngủ."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:11:03.090934', '2026-01-27 13:11:03.090934', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Ngủ không đủ giấc có thể gây ra mụn và nếp nhăn.", "detailed": "Thiếu ngủ làm tăng hormone stress, dẫn đến tình trạng viêm và mụn.", "comprehensive": "Khi bạn thiếu ngủ, cơ thể sản xuất nhiều hormone cortisol, điều này có thể dẫn đến tình trạng viêm và mụn. Đồng thời, da không có đủ thời gian để phục hồi, khiến nếp nhăn xuất hiện sớm hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('85d1eab2-2b1e-4f16-920c-c87531162e71', '55ffda79-3855-4d3d-a344-0a48fcab1d04', 'concept', 'Lợi Ích Của Giấc Ngủ Đối Với Da', 'Giấc ngủ giúp cải thiện tuần hoàn máu, cung cấp oxy và dinh dưỡng cho da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người phụ nữ thức dậy với làn da rạng rỡ.", "videoScript": "Mở đầu với cảnh một người phụ nữ thức dậy sau một giấc ngủ ngon. Lời lồng ghép mô tả về lợi ích của giấc ngủ đối với da và các mẹo để có giấc ngủ ngon.", "imageDescription": "Hình ảnh một người phụ nữ tươi cười, với làn da rạng rỡ và khỏe mạnh, thể hiện lợi ích của giấc ngủ.", "videoDescription": "Video trình bày các lợi ích của giấc ngủ đối với sức khỏe da, cùng với các mẹo để cải thiện giấc ngủ."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:11:03.095765', '2026-01-27 13:11:03.095765', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Ngủ đủ giấc giúp da tái tạo và khỏe mạnh.", "detailed": "Giấc ngủ giúp cải thiện tuần hoàn máu, cung cấp oxy và dinh dưỡng cho da.", "comprehensive": "Khi bạn ngủ đủ giấc, cơ thể có cơ hội tái tạo và phục hồi. Giấc ngủ giúp cải thiện tuần hoàn máu, cung cấp oxy và dinh dưỡng cần thiết cho da, giúp làn da trở nên khỏe mạnh và đầy sức sống."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6e9bcb1f-043b-419d-859d-003668121835', 'ba24d13e-ca49-4e63-ac35-d7726870c8f1', 'concept', 'Các phương pháp quản lý danh mục đầu tư', 'Các phương pháp quản lý danh mục đầu tư bao gồm phân bổ tài sản dựa trên rủi ro, đầu tư vào các quỹ chỉ số, và chiến lược đầu tư chủ động nhằm tìm kiếm lợi nhuận cao hơn thị trường.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại tài sản tài chính như cổ phiếu, trái phiếu và vàng.", "videoScript": "Bài học hôm nay sẽ giới thiệu về các phương pháp khác nhau để quản lý danh mục đầu tư, giúp bạn lựa chọn phương pháp phù hợp nhất với mình.", "imageDescription": "Một bảng phân loại các loại tài sản tài chính, bao gồm cổ phiếu, trái phiếu và hàng hóa.", "videoDescription": "Video trình bày các phương pháp quản lý danh mục đầu tư khác nhau và cách áp dụng chúng để tối ưu hóa lợi nhuận."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:01:44.423182', '2026-01-27 13:01:44.423182', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều phương pháp như phân bổ tài sản, đầu tư chỉ số và đầu tư chủ động.", "detailed": "Các phương pháp quản lý danh mục đầu tư bao gồm phân bổ tài sản dựa trên rủi ro, đầu tư vào các quỹ chỉ số, và chiến lược đầu tư chủ động nhằm tìm kiếm lợi nhuận cao hơn thị trường.", "comprehensive": "Phân bổ tài sản là một phương pháp chính trong quản lý danh mục đầu tư, bao gồm việc phân chia tiền của bạn vào các loại tài sản khác nhau như cổ phiếu, trái phiếu và bất động sản để giảm rủi ro. Ngoài ra, đầu tư chỉ số là một cách để theo dõi hiệu suất của thị trường mà không cần phải chọn từng cổ phiếu, trong khi đầu tư chủ động đòi hỏi sự nghiên cứu và phân tích sâu hơn để tìm kiếm cơ hội tốt hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b87271ea-9904-4aac-b3b9-15c5e7bdfd1b', 'ba24d13e-ca49-4e63-ac35-d7726870c8f1', 'concept', 'Cách phân bổ tài sản hiệu quả', 'Để phân bổ tài sản hiệu quả, bạn cần xem xét độ tuổi, mục tiêu tài chính và mức độ chấp nhận rủi ro của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh mô tả biểu đồ phân bổ tài sản.", "videoScript": "Trong bài học này, chúng ta sẽ khám phá cách phân bổ tài sản để bạn có thể quản lý danh mục đầu tư của mình một cách hiệu quả.", "imageDescription": "Một biểu đồ thể hiện cách phân bổ tài sản giữa cổ phiếu, trái phiếu và tiền mặt.", "videoDescription": "Video hướng dẫn cách phân bổ tài sản trong danh mục đầu tư cá nhân để đạt được mục tiêu tài chính."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:01:44.426595', '2026-01-27 13:01:44.426595', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phân bổ tài sản là việc chia tiền của bạn giữa các loại đầu tư khác nhau.", "detailed": "Để phân bổ tài sản hiệu quả, bạn cần xem xét độ tuổi, mục tiêu tài chính và mức độ chấp nhận rủi ro của mình.", "comprehensive": "Phân bổ tài sản hiệu quả yêu cầu bạn phải cân nhắc kỹ lưỡng về các yếu tố như mục tiêu tài chính, thời gian đầu tư và khả năng chấp nhận rủi ro. Một số nguyên tắc chung bao gồm việc đầu tư vào cổ phiếu cho tăng trưởng, trái phiếu cho độ an toàn và tiền mặt cho tính thanh khoản."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2feaf71e-aab1-4c8d-a474-04f34c683cd1', 'd772e784-c579-4914-a5e1-3777fff5963b', 'concept', 'Giới thiệu về Biểu Đồ Đường', 'Biểu đồ đường thường được sử dụng để thể hiện sự thay đổi của dữ liệu theo thời gian, giúp người xem dễ dàng nhận ra các xu hướng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một biểu đồ đường thể hiện sự thay đổi nhiệt độ theo tháng.", "videoScript": "Chào mừng bạn đến với bài học về biểu đồ đường. Hôm nay, chúng ta sẽ khám phá cách tạo và sử dụng biểu đồ đường để trình bày dữ liệu một cách hiệu quả.", "imageDescription": "Hình ảnh một biểu đồ đường với các điểm dữ liệu nối với nhau bằng một đường mượt mà, thể hiện sự thay đổi nhiệt độ theo tháng.", "videoDescription": "Video hướng dẫn cách tạo biểu đồ đường từ dữ liệu thực tế, bao gồm các bước từ việc thu thập dữ liệu đến việc tạo và tùy chỉnh biểu đồ."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:35:54.084283', '2026-01-27 12:35:54.084283', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ đường là một công cụ trực quan giúp hiển thị dữ liệu theo thời gian.", "detailed": "Biểu đồ đường thường được sử dụng để thể hiện sự thay đổi của dữ liệu theo thời gian, giúp người xem dễ dàng nhận ra các xu hướng.", "comprehensive": "Biểu đồ đường là một trong những loại biểu đồ phổ biến trong phân tích dữ liệu. Nó thể hiện mối quan hệ giữa hai biến số, với một biến số được đặt trên trục hoành (trục x) và biến còn lại trên trục tung (trục y). Điều này cho phép người dùng theo dõi sự thay đổi của dữ liệu theo thời gian và nhận diện các xu hướng, điểm cao nhất và thấp nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c3b7b6c3-f9f6-436d-b7b2-5935890b690a', 'a8ed5036-7e88-45df-bce9-a9dd16dceb27', 'concept', 'Phân Tích Hòa Âm Trong Các Tác Phẩm Nổi Tiếng', 'Phân tích hòa âm trong các tác phẩm nổi tiếng giúp chúng ta hiểu rõ hơn về cách các nhạc sĩ sáng tạo.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bảng phân tích hòa âm trong một tác phẩm cổ điển.", "videoScript": "Trong video này, chúng ta sẽ phân tích hòa âm trong một tác phẩm nổi tiếng và khám phá cách mà nhạc sĩ đã tạo ra cảm xúc.", "imageDescription": "Bảng phân tích hòa âm của một tác phẩm cổ điển nổi tiếng, với các hợp âm và cấu trúc được đánh dấu.", "videoDescription": "Video hướng dẫn phân tích hòa âm trong một tác phẩm cổ điển, giúp người xem hiểu rõ hơn về kỹ thuật sáng tác của nhạc sĩ."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:27.313145', '2026-01-27 12:53:27.313145', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chúng ta có thể học hỏi từ các tác phẩm nổi tiếng.", "detailed": "Phân tích hòa âm trong các tác phẩm nổi tiếng giúp chúng ta hiểu rõ hơn về cách các nhạc sĩ sáng tạo.", "comprehensive": "Việc phân tích hòa âm trong các tác phẩm nổi tiếng không chỉ giúp nâng cao kỹ năng chơi piano mà còn mở rộng kiến thức về lý thuyết âm nhạc. Những tác phẩm như ''Clair de Lune'' của Debussy hay ''Nocturne'' của Chopin đều có những kỹ thuật hòa âm độc đáo mà chúng ta có thể học hỏi để cải thiện khả năng cảm thụ âm nhạc của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6be71a7e-5a07-4609-8e7e-76064489ef5e', 'ba24d13e-ca49-4e63-ac35-d7726870c8f1', 'concept', 'Theo dõi và điều chỉnh danh mục đầu tư', 'Việc theo dõi danh mục đầu tư định kỳ giúp bạn đánh giá hiệu suất và đưa ra quyết định điều chỉnh kịp thời.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang kiểm tra hiệu suất danh mục đầu tư trên điện thoại.", "videoScript": "Hôm nay, chúng ta sẽ tìm hiểu cách theo dõi và điều chỉnh danh mục đầu tư của bạn để đảm bảo bạn đạt được mục tiêu tài chính.", "imageDescription": "Một cá nhân đang sử dụng ứng dụng trên điện thoại để theo dõi hiệu suất danh mục đầu tư của mình.", "videoDescription": "Video trình bày các bước cần thiết để theo dõi và điều chỉnh danh mục đầu tư hiệu quả."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:01:44.430497', '2026-01-27 13:01:44.430497', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi danh mục đầu tư giúp bạn biết khi nào cần thay đổi.", "detailed": "Việc theo dõi danh mục đầu tư định kỳ giúp bạn đánh giá hiệu suất và đưa ra quyết định điều chỉnh kịp thời.", "comprehensive": "Theo dõi và điều chỉnh danh mục đầu tư là một phần không thể thiếu trong quản lý đầu tư. Điều này bao gồm việc thường xuyên xem xét hiệu suất của các khoản đầu tư, so sánh với các chỉ số thị trường và thực hiện các điều chỉnh cần thiết để phù hợp với mục tiêu tài chính và điều kiện thị trường hiện tại."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b940d0bb-0800-4cf4-b224-defbfa824747', '55ffda79-3855-4d3d-a344-0a48fcab1d04', 'concept', 'Mẹo Để Có Giấc Ngủ Tốt Hơn Cho Da', 'Tránh sử dụng điện thoại trước khi ngủ và tạo môi trường yên tĩnh.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một phòng ngủ yên tĩnh và thoải mái.", "videoScript": "Video bắt đầu với hình ảnh một phòng ngủ được thiết kế tốt. Lời lồng ghép cung cấp các mẹo để có giấc ngủ tốt hơn như tạo thói quen và điều chỉnh môi trường ngủ.", "imageDescription": "Hình ảnh một phòng ngủ tối, yên tĩnh, với ánh sáng nhẹ nhàng và không gian sạch sẽ, tạo cảm giác thư giãn.", "videoDescription": "Video hướng dẫn các mẹo để có giấc ngủ tốt hơn, từ việc thiết lập thói quen đến cải thiện môi trường ngủ."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:11:03.099326', '2026-01-27 13:11:03.099326', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hãy tạo thói quen đi ngủ đúng giờ.", "detailed": "Tránh sử dụng điện thoại trước khi ngủ và tạo môi trường yên tĩnh.", "comprehensive": "Để cải thiện giấc ngủ, hãy thiết lập một thói quen đi ngủ đúng giờ, hạn chế sử dụng điện thoại và thiết lập môi trường yên tĩnh, tối và thoải mái. Những điều này sẽ giúp bạn có giấc ngủ sâu hơn và da sẽ được hưởng lợi từ điều đó."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7c0e9d03-617b-408a-91db-6accbb3fcfe4', '55ffda79-3855-4d3d-a344-0a48fcab1d04', 'concept', 'Giấc Ngủ Và Sản Phẩm Chăm Sóc Da', 'Các sản phẩm như serum và kem dưỡng ẩm hoạt động hiệu quả nhất vào ban đêm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người phụ nữ đang thoa kem dưỡng da trước khi ngủ.", "videoScript": "Video bắt đầu với cảnh một người phụ nữ thoa kem dưỡng da. Lời lồng ghép giải thích về tầm quan trọng của việc sử dụng sản phẩm chăm sóc da trước khi ngủ và cách chúng hỗ trợ phục hồi da.", "imageDescription": "Hình ảnh một người phụ nữ chăm sóc da trước khi đi ngủ, với các sản phẩm dưỡng da xung quanh.", "videoDescription": "Video trình bày cách giấc ngủ kết hợp với sản phẩm chăm sóc da giúp cải thiện sức khỏe da."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:11:03.103838', '2026-01-27 13:11:03.103838', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng sản phẩm dưỡng da trước khi ngủ.", "detailed": "Các sản phẩm như serum và kem dưỡng ẩm hoạt động hiệu quả nhất vào ban đêm.", "comprehensive": "Khi bạn ngủ, làn da hấp thụ tốt hơn các sản phẩm dưỡng da. Sử dụng serum và kem dưỡng ẩm trước khi ngủ giúp cung cấp độ ẩm và dưỡng chất cần thiết, hỗ trợ quá trình phục hồi da hiệu quả nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('88af0124-ff5f-4acb-9caf-92cab52778da', '051808b5-36fa-4d63-b08b-7c036c3903ca', 'concept', 'Giới Thiệu Về Nhịp Điệu', 'Nhịp điệu là cách mà âm nhạc được sắp xếp theo thời gian. Nó bao gồm các yếu tố như tempo, meter và các loại nhịp khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bản nhạc với các ghi chú và ký hiệu nhịp điệu.", "videoScript": "Chào mừng đến với bài học về nhịp điệu. Hôm nay, chúng ta sẽ khám phá cách nhịp điệu ảnh hưởng đến âm nhạc của chúng ta.", "imageDescription": "Hình ảnh cho thấy một trang nhạc piano với các ký hiệu nhịp điệu rõ ràng.", "videoDescription": "Video giải thích về nhịp điệu trong âm nhạc với các ví dụ trên piano."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:51.963649', '2026-01-27 12:53:51.963649', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhịp điệu là yếu tố chính trong âm nhạc, tạo ra sự chuyển động và cảm xúc.", "detailed": "Nhịp điệu là cách mà âm nhạc được sắp xếp theo thời gian. Nó bao gồm các yếu tố như tempo, meter và các loại nhịp khác nhau.", "comprehensive": "Nhịp điệu không chỉ là sự lặp lại của các âm thanh mà còn là cách mà chúng ta cảm nhận và tương tác với âm nhạc. Nó giúp xác định cấu trúc của một bản nhạc và ảnh hưởng đến cách người nghe cảm nhận nó."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('88b99781-cc9f-4504-a28f-917178938e8e', '051808b5-36fa-4d63-b08b-7c036c3903ca', 'concept', 'Các Loại Nhịp Điệu Cơ Bản', 'Nhịp 4/4 là loại nhịp phổ biến nhất, thường được sử dụng trong pop và rock. Nhịp 3/4 tạo cảm giác nhẹ nhàng, thường thấy trong vals. Nhịp 6/8 mang đến cảm giác nhanh và nhịp nhàng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ thể hiện các loại nhịp điệu khác nhau.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu về các loại nhịp điệu cơ bản. Hãy cùng xem xét cách mà mỗi loại nhịp ảnh hưởng đến âm nhạc.", "imageDescription": "Hình ảnh với biểu đồ minh họa các loại nhịp điệu cơ bản và ví dụ âm nhạc đi kèm.", "videoDescription": "Video giới thiệu về các loại nhịp điệu trong âm nhạc, với ví dụ cụ thể từ piano."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:51.971262', '2026-01-27 12:53:51.971262', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại nhịp điệu như nhịp 4/4, nhịp 3/4, và nhịp 6/8.", "detailed": "Nhịp 4/4 là loại nhịp phổ biến nhất, thường được sử dụng trong pop và rock. Nhịp 3/4 tạo cảm giác nhẹ nhàng, thường thấy trong vals. Nhịp 6/8 mang đến cảm giác nhanh và nhịp nhàng.", "comprehensive": "Mỗi loại nhịp điệu có tác động khác nhau đến cảm xúc của bản nhạc. Nhịp 4/4 thường tạo ra sự ổn định, trong khi nhịp 3/4 mang lại sự lãng mạn. Nhịp 6/8 có thể tạo ra một cảm giác chuyển động nhanh chóng và năng động."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('26a5f3bb-df8f-4f3b-8c5c-5a61ea9d0a07', '051808b5-36fa-4d63-b08b-7c036c3903ca', 'concept', 'Cách Đếm Nhịp Điệu', 'Để đếm nhịp, bạn cần xác định nhịp điệu của bản nhạc và đếm từng nhịp theo cách thức đã được quy định.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang đếm nhịp khi chơi piano.", "videoScript": "Hãy theo dõi cách mà tôi đếm nhịp trong khi chơi piano. Điều này rất quan trọng để giữ cho âm nhạc của bạn luôn nhịp nhàng.", "imageDescription": "Hình ảnh cho thấy một nghệ sĩ piano đang đếm nhịp khi chơi một bản nhạc.", "videoDescription": "Video hướng dẫn cách đếm nhịp điệu trong khi chơi piano, với các ví dụ thực tế."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:51.974938', '2026-01-27 12:53:51.974938', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đếm nhịp là quá trình xác định số lượng âm trong một khoảng thời gian nhất định.", "detailed": "Để đếm nhịp, bạn cần xác định nhịp điệu của bản nhạc và đếm từng nhịp theo cách thức đã được quy định.", "comprehensive": "Việc đếm nhịp không chỉ giúp bạn chơi đúng mà còn giúp bạn cảm nhận được sự chuyển động của âm nhạc. Hãy luyện tập đếm nhịp cùng với bản nhạc để phát triển kỹ năng này."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('541f6da5-0efe-4fdd-81ff-9f11c97b97bd', '051808b5-36fa-4d63-b08b-7c036c3903ca', 'concept', 'Sự Kết Hợp Giữa Nhịp Điệu và Giai Điệu', 'Giai điệu là chuỗi âm thanh trong khi nhịp điệu là cách mà những âm thanh đó được sắp xếp theo thời gian. Sự kết hợp của chúng tạo ra bản nhạc hoàn chỉnh.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bản nhạc với giai điệu và nhịp điệu được phân biệt rõ ràng.", "videoScript": "Chúng ta sẽ cùng tìm hiểu cách mà nhịp điệu ảnh hưởng đến giai điệu trong âm nhạc. Hãy cùng tôi thử nghiệm một vài ví dụ.", "imageDescription": "Hình ảnh một bản nhạc cho thấy rõ sự kết hợp giữa giai điệu và nhịp điệu.", "videoDescription": "Video giải thích sự tương tác giữa nhịp điệu và giai điệu trong âm nhạc, với các ví dụ từ piano."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:51.978422', '2026-01-27 12:53:51.978422', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhịp điệu và giai điệu là hai yếu tố tạo nên âm nhạc.", "detailed": "Giai điệu là chuỗi âm thanh trong khi nhịp điệu là cách mà những âm thanh đó được sắp xếp theo thời gian. Sự kết hợp của chúng tạo ra bản nhạc hoàn chỉnh.", "comprehensive": "Cách mà nhịp điệu và giai điệu tương tác với nhau là rất quan trọng trong việc tạo ra cảm xúc trong âm nhạc. Hãy thử nghiệm với việc thay đổi nhịp điệu trong khi giữ giai điệu để cảm nhận sự khác biệt."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('50335ce3-d5a6-401e-b16f-549a4679b58a', 'd36bd957-a3e0-4f13-8847-41e19dfcd6bf', 'concept', 'Tại Sao Nước Quan Trọng Trong Chăm Sóc Da?', 'Nước giúp giữ ẩm cho da, ngăn ngừa tình trạng khô da và lão hóa sớm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người phụ nữ đang uống nước với làn da sáng khỏe.", "videoScript": "Chào mừng đến với video hôm nay! Chúng ta sẽ tìm hiểu về vai trò của nước trong chăm sóc da. Bạn có biết rằng cơ thể con người gồm khoảng 60% nước? Đủ nước giúp da bạn luôn căng mọng và khỏe mạnh.", "imageDescription": "Một người phụ nữ có làn da mịn màng, đang cầm một cốc nước trong tay.", "videoDescription": "Video giải thích vai trò của nước trong việc duy trì độ ẩm và sức khỏe của làn da."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:11:35.269726', '2026-01-27 13:11:35.269726', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nước giúp duy trì độ ẩm cho da.", "detailed": "Nước giúp giữ ẩm cho da, ngăn ngừa tình trạng khô da và lão hóa sớm.", "comprehensive": "Nước không chỉ giúp duy trì độ ẩm cho da mà còn hỗ trợ quá trình tái tạo tế bào da, giúp da khỏe mạnh và sáng bóng. Việc cung cấp đủ nước cho cơ thể cũng giúp da giảm thiểu tình trạng mụn và các vấn đề khác."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4d0250cb-22f2-4f82-8db4-bebbe103e864', 'd772e784-c579-4914-a5e1-3777fff5963b', 'concept', 'Cách tạo Biểu Đồ Đường trong Excel', 'Để tạo biểu đồ đường trong Excel, bạn cần mở ứng dụng, chọn vùng dữ liệu cần vẽ biểu đồ và sau đó vào tab ''Chèn'' để chọn loại biểu đồ đường.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Giao diện Excel với một biểu đồ đường được tạo ra.", "videoScript": "Trong video này, chúng ta sẽ học cách tạo biểu đồ đường trong Excel. Hãy làm theo từng bước để xem kết quả cuối cùng.", "imageDescription": "Hình ảnh giao diện Excel với một biểu đồ đường hiển thị, cùng với vùng dữ liệu đã chọn.", "videoDescription": "Video hướng dẫn từng bước cách tạo biểu đồ đường trong Excel, từ việc nhập dữ liệu đến việc tùy chỉnh biểu đồ."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:35:54.092434', '2026-01-27 12:35:54.092434', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mở Excel, chọn dữ liệu và chọn loại biểu đồ đường.", "detailed": "Để tạo biểu đồ đường trong Excel, bạn cần mở ứng dụng, chọn vùng dữ liệu cần vẽ biểu đồ và sau đó vào tab ''Chèn'' để chọn loại biểu đồ đường.", "comprehensive": "Đầu tiên, bạn mở Excel và nhập dữ liệu của mình vào bảng tính. Sau đó, hãy chọn toàn bộ dữ liệu bao gồm cả tiêu đề. Tiếp theo, vào tab ''Chèn'' trên thanh công cụ, tìm và chọn ''Biểu đồ đường'' từ danh sách các loại biểu đồ. Excel sẽ tự động tạo một biểu đồ đường dựa trên dữ liệu bạn đã chọn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2c88b2ea-2860-439b-a452-dd91fff78ff6', '051808b5-36fa-4d63-b08b-7c036c3903ca', 'concept', 'Ứng Dụng Nhịp Điệu Trong Sáng Tác', 'Khi sáng tác, bạn có thể chọn loại nhịp điệu phù hợp để truyền tải cảm xúc mà bạn muốn. Nhịp điệu có thể thay đổi theo từng phần của bản nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nhạc sĩ đang sáng tác nhạc trên piano.", "videoScript": "Hãy theo dõi quá trình sáng tác của tôi. Tôi sẽ cho bạn thấy cách mà nhịp điệu có thể được áp dụng để tạo ra một bản nhạc mới.", "imageDescription": "Hình ảnh một nhạc sĩ đang làm việc trên bản nhạc của mình, với các ghi chú và nhịp điệu khác nhau.", "videoDescription": "Video hướng dẫn cách áp dụng nhịp điệu trong quá trình sáng tác nhạc trên piano."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:53:51.981772', '2026-01-27 12:53:51.981772', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhịp điệu có thể được dùng để sáng tác nhạc.", "detailed": "Khi sáng tác, bạn có thể chọn loại nhịp điệu phù hợp để truyền tải cảm xúc mà bạn muốn. Nhịp điệu có thể thay đổi theo từng phần của bản nhạc.", "comprehensive": "Việc áp dụng nhịp điệu trong sáng tác giúp tạo ra đặc trưng riêng cho tác phẩm. Hãy thử nghiệm với các loại nhịp điệu khác nhau và quan sát cách mà chúng ảnh hưởng đến cảm xúc của bản nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('52ff1594-c6ab-40b2-95b2-4025a68049df', '6fdf2752-f255-4c42-beb1-7425d8e2187c', 'concept', 'Giới thiệu về Chiến lược Đầu tư Dài hạn', 'Chiến lược đầu tư dài hạn tập trung vào việc mua và giữ tài sản trong nhiều năm, nhằm mục đích tối ưu hóa lợi nhuận từ giá trị tài sản gia tăng theo thời gian.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một biểu đồ thể hiện sự tăng trưởng của đầu tư theo thời gian.", "videoScript": "Chúng ta sẽ tìm hiểu về chiến lược đầu tư dài hạn và tại sao nó lại quan trọng. Đầu tư dài hạn có thể giúp bạn đạt được mục tiêu tài chính trong tương lai.", "imageDescription": "Biểu đồ đường thể hiện sự gia tăng giá trị đầu tư theo năm.", "videoDescription": "Video giải thích lý do tại sao đầu tư dài hạn có thể mang lại lợi ích lớn cho cá nhân trong việc xây dựng tài sản."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:02:19.185561', '2026-01-27 13:02:19.185561', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đầu tư dài hạn là việc giữ tài sản trong một khoảng thời gian dài để gia tăng giá trị.", "detailed": "Chiến lược đầu tư dài hạn tập trung vào việc mua và giữ tài sản trong nhiều năm, nhằm mục đích tối ưu hóa lợi nhuận từ giá trị tài sản gia tăng theo thời gian.", "comprehensive": "Chiến lược đầu tư dài hạn không chỉ đơn thuần là việc nắm giữ tài sản mà còn bao gồm việc nghiên cứu thị trường, phân tích xu hướng, và quản lý danh mục đầu tư để giảm thiểu rủi ro và tối đa hóa lợi nhuận. Các nhà đầu tư dài hạn thường tìm kiếm các cổ phiếu có nền tảng vững chắc hoặc quỹ đầu tư có tiềm năng tăng trưởng ổn định."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c53eb840-dcd8-44c4-a06e-d6f81d81e5a6', '6fdf2752-f255-4c42-beb1-7425d8e2187c', 'concept', 'Lợi ích của Đầu tư Dài hạn', 'Lợi ích của đầu tư dài hạn bao gồm khả năng tích lũy tài sản, giảm thiểu biến động và chi phí giao dịch, cũng như tận dụng lãi kép.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu tượng lãi kép với hình ảnh cây phát triển.", "videoScript": "Trong video này, chúng ta sẽ khám phá các lợi ích của việc đầu tư dài hạn và cách mà nó giúp bạn xây dựng tài sản qua thời gian.", "imageDescription": "Hình ảnh cây lớn biểu thị sự phát triển và thành công từ đầu tư dài hạn.", "videoDescription": "Video thảo luận về các lợi ích của đầu tư dài hạn và cách để hiện thực hóa mục tiêu tài chính."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:02:19.193625', '2026-01-27 13:02:19.193625', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đầu tư dài hạn giúp tích lũy tài sản và giảm thiểu rủi ro.", "detailed": "Lợi ích của đầu tư dài hạn bao gồm khả năng tích lũy tài sản, giảm thiểu biến động và chi phí giao dịch, cũng như tận dụng lãi kép.", "comprehensive": "Đầu tư dài hạn mang lại nhiều lợi ích như tăng trưởng tài sản ổn định, giảm thiểu rủi ro do biến động ngắn hạn của thị trường, và khả năng tối ưu hóa lợi nhuận thông qua lãi kép. Những nhà đầu tư kiên nhẫn thường thấy lợi nhuận cao hơn so với những người đầu tư ngắn hạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('fc0274a2-dde2-424c-bc4c-86b35775dfaa', 'd772e784-c579-4914-a5e1-3777fff5963b', 'concept', 'Tùy chỉnh Biểu Đồ Đường', 'Excel cho phép bạn tùy chỉnh biểu đồ đường bằng cách thay đổi màu sắc, kiểu đường, và thêm tiêu đề cho biểu đồ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ đường với các tùy chỉnh màu sắc và kiểu dáng.", "videoScript": "Hãy cùng tìm hiểu cách tùy chỉnh biểu đồ đường trong Excel để nó trở nên đẹp mắt và dễ hiểu hơn.", "imageDescription": "Hình ảnh một biểu đồ đường được tùy chỉnh với màu sắc khác nhau và có tiêu đề, nổi bật các điểm dữ liệu.", "videoDescription": "Video hướng dẫn cách tùy chỉnh biểu đồ đường trong Excel, bao gồm thay đổi màu sắc, kiểu đường và thêm tiêu đề."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:35:54.095841', '2026-01-27 12:35:54.095841', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể thay đổi màu sắc và kiểu đường của biểu đồ.", "detailed": "Excel cho phép bạn tùy chỉnh biểu đồ đường bằng cách thay đổi màu sắc, kiểu đường, và thêm tiêu đề cho biểu đồ.", "comprehensive": "Sau khi tạo biểu đồ đường, bạn có thể tùy chỉnh nó để làm nổi bật thông tin quan trọng. Bạn có thể thay đổi màu sắc của đường, thêm các điểm dữ liệu, điều chỉnh trục x và y, cũng như thêm tiêu đề và chú thích để biểu đồ trở nên trực quan hơn. Các tùy chọn này có thể được truy cập thông qua menu ''Thiết kế'' và ''Định dạng'' trong Excel."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b60956ef-a176-4f19-b499-0f739b426c55', 'd772e784-c579-4914-a5e1-3777fff5963b', 'concept', 'Sử dụng Dữ liệu Thực tế để Tạo Biểu Đồ Đường', 'Khi sử dụng dữ liệu thực tế, bạn có thể minh họa cho các xu hướng và biến đổi trong lĩnh vực bạn quan tâm, như doanh thu, nhiệt độ, hoặc bất kỳ thông số nào khác.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một biểu đồ đường thể hiện dữ liệu doanh thu theo tháng.", "videoScript": "Trong video này, chúng ta sẽ sử dụng dữ liệu thực tế để tạo một biểu đồ đường, giúp bạn hiểu rõ hơn về cách trình bày thông tin.", "imageDescription": "Hình ảnh biểu đồ đường hiển thị doanh thu hàng tháng, với các điểm dữ liệu được nối với nhau.", "videoDescription": "Video hướng dẫn cách sử dụng dữ liệu thực tế để tạo biểu đồ đường, bao gồm cách thu thập dữ liệu và nhập vào Excel."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:35:54.09936', '2026-01-27 12:35:54.09936', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng dữ liệu thực tế giúp biểu đồ trở nên ý nghĩa hơn.", "detailed": "Khi sử dụng dữ liệu thực tế, bạn có thể minh họa cho các xu hướng và biến đổi trong lĩnh vực bạn quan tâm, như doanh thu, nhiệt độ, hoặc bất kỳ thông số nào khác.", "comprehensive": "Việc sử dụng dữ liệu thực tế để tạo biểu đồ đường là rất quan trọng vì nó giúp bạn có cái nhìn rõ nét về xu hướng và sự thay đổi theo thời gian. Ví dụ, bạn có thể sử dụng dữ liệu doanh thu hàng tháng để xem xét sự tăng trưởng của doanh nghiệp, hoặc dữ liệu nhiệt độ hàng tuần để phân tích khí hậu. Trong bài học này, chúng ta sẽ tìm hiểu cách thu thập dữ liệu thực tế và nhập vào Excel để tạo biểu đồ đường."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f5567a5e-5d47-466b-9db7-67d6a4880c36', '41ad0fcd-a7b8-40b3-825f-9188560a8977', 'concept', 'Hiểu Về Căng Thẳng Khi Biểu Diễn', 'Căng thẳng có thể xuất hiện trước và trong khi biểu diễn piano. Điều quan trọng là nhận biết và quản lý cảm xúc này để có một buổi biểu diễn thành công.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ piano biểu diễn trên sân khấu với ánh đèn chiếu sáng.", "videoScript": "Mở đầu bằng hình ảnh một nghệ sĩ chuẩn bị trước buổi biểu diễn. Sau đó, chuyển sang các kỹ thuật giúp giảm căng thẳng như hít thở sâu.", "imageDescription": "Hình ảnh một nghệ sĩ piano biểu diễn trước khán giả, thể hiện sự tập trung và căng thẳng.", "videoDescription": "Video hướng dẫn nghệ sĩ piano cách quản lý căng thẳng trước và trong khi biểu diễn."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:54:20.809244', '2026-01-27 12:54:20.809244', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Căng thẳng là điều bình thường khi biểu diễn. Hãy chấp nhận nó.", "detailed": "Căng thẳng có thể xuất hiện trước và trong khi biểu diễn piano. Điều quan trọng là nhận biết và quản lý cảm xúc này để có một buổi biểu diễn thành công.", "comprehensive": "Căng thẳng khi biểu diễn là một phản ứng tự nhiên của cơ thể. Nó có thể giúp bạn tập trung hơn, nhưng nếu quá mức, nó có thể cản trở khả năng thể hiện. Hãy học cách nhận diện và kiểm soát căng thẳng thông qua các kỹ thuật thư giãn, như hít thở sâu hoặc thiền."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9d39d2cd-671a-4c8e-ab37-0cb191447db5', '41ad0fcd-a7b8-40b3-825f-9188560a8977', 'concept', 'Kỹ Thuật Thư Giãn Trước Buổi Biểu Diễn', 'Trước khi biểu diễn, hãy dành thời gian để thư giãn. Các kỹ thuật như hít thở sâu hoặc yoga có thể giúp bạn giảm lo âu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ đang thiền trước khi biểu diễn.", "videoScript": "Giới thiệu về tầm quan trọng của việc thư giãn trước buổi biểu diễn. Hướng dẫn các bài tập hít thở đơn giản.", "imageDescription": "Hình ảnh một nghệ sĩ ngồi thiền trong phòng chờ, tạo không khí yên tĩnh và thư giãn.", "videoDescription": "Video hướng dẫn cách thư giãn trước khi biểu diễn piano với các bài tập hít thở."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:54:20.818749', '2026-01-27 12:54:20.818749', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thư giãn là cần thiết để biểu diễn tốt.", "detailed": "Trước khi biểu diễn, hãy dành thời gian để thư giãn. Các kỹ thuật như hít thở sâu hoặc yoga có thể giúp bạn giảm lo âu.", "comprehensive": "Kỹ thuật thư giãn như thiền, yoga, hoặc hít thở sâu có thể giúp bạn làm dịu tâm trí và cơ thể trước buổi biểu diễn. Hãy thử các phương pháp này ít nhất 15 phút trước khi bước lên sân khấu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b57e0c78-ced1-4622-9fa3-56ac246f1767', 'd772e784-c579-4914-a5e1-3777fff5963b', 'concept', 'Phân tích và Đọc Biểu Đồ Đường', 'Khi đọc biểu đồ đường, bạn cần chú ý đến các điểm cao nhất, thấp nhất và xu hướng chung của dữ liệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một biểu đồ đường với các điểm cao và thấp được đánh dấu.", "videoScript": "Hãy cùng tìm hiểu cách phân tích và đọc biểu đồ đường để tối ưu hóa việc sử dụng dữ liệu.", "imageDescription": "Hình ảnh biểu đồ đường với các điểm cao nhất và thấp nhất được đánh dấu rõ ràng.", "videoDescription": "Video hướng dẫn cách đọc và phân tích biểu đồ đường, giúp người xem hiểu rõ hơn về các xu hướng và điểm nổi bật trong dữ liệu."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:35:54.106509', '2026-01-27 12:35:54.106509', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ đường giúp bạn phân tích dữ liệu một cách trực quan.", "detailed": "Khi đọc biểu đồ đường, bạn cần chú ý đến các điểm cao nhất, thấp nhất và xu hướng chung của dữ liệu.", "comprehensive": "Phân tích biểu đồ đường là một kỹ năng quan trọng trong việc hiểu và diễn giải dữ liệu. Khi bạn nhìn vào biểu đồ, hãy chú ý đến các điểm cực trị, xem xét các xu hướng tăng hoặc giảm, và so sánh các khoảng thời gian khác nhau. Việc đọc biểu đồ đường không chỉ giúp bạn hiểu rõ hơn về dữ liệu mà còn hỗ trợ trong việc ra quyết định dựa trên thông tin đó."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8117e653-219e-4961-9385-eff770bb80fd', '41ad0fcd-a7b8-40b3-825f-9188560a8977', 'concept', 'Tạo Khả Năng Tập Trung Khi Biểu Diễn', 'Tập trung là yếu tố quan trọng trong biểu diễn piano. Hãy tìm cách để giữ tâm trí của bạn ở hiện tại và tránh khỏi những suy nghĩ tiêu cực.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ piano đang chơi nhạc với sự tập trung cao độ.", "videoScript": "Mở đầu với cảnh nghệ sĩ biểu diễn, sau đó chia sẻ các mẹo để giữ tập trung trong suốt buổi biểu diễn.", "imageDescription": "Hình ảnh nghệ sĩ piano với nét mặt tập trung, hoàn toàn đắm chìm trong âm nhạc.", "videoDescription": "Video chia sẻ các kỹ thuật giúp nghệ sĩ piano giữ tập trung khi biểu diễn."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:54:20.822946', '2026-01-27 12:54:20.822946', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tập trung giúp bạn biểu diễn tốt hơn.", "detailed": "Tập trung là yếu tố quan trọng trong biểu diễn piano. Hãy tìm cách để giữ tâm trí của bạn ở hiện tại và tránh khỏi những suy nghĩ tiêu cực.", "comprehensive": "Khi biểu diễn, việc giữ tập trung là rất quan trọng. Bạn có thể làm điều này bằng cách thực hành thường xuyên, tạo ra một không gian biểu diễn thoải mái, và sử dụng các kỹ thuật như hình dung để chuẩn bị cho buổi biểu diễn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('88beac49-5384-4076-8a73-5c55c061e44b', '41ad0fcd-a7b8-40b3-825f-9188560a8977', 'concept', 'Phát Triển Sự Tự Tin Trên Sân Khấu', 'Tự tin là yếu tố quyết định đến sự thành công của một buổi biểu diễn. Hãy luyện tập thường xuyên và chuẩn bị kỹ lưỡng để tăng cường sự tự tin.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ piano đứng trên sân khấu với tư thế tự tin.", "videoScript": "Chia sẻ các câu chuyện thành công của nghệ sĩ piano về sự tự tin. Hướng dẫn cách chuẩn bị để xây dựng sự tự tin.", "imageDescription": "Hình ảnh nghệ sĩ đứng tự tin trên sân khấu, tạo dáng trước khi biểu diễn.", "videoDescription": "Video hướng dẫn cách phát triển sự tự tin khi biểu diễn piano."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:54:20.826286', '2026-01-27 12:54:20.826286', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tự tin là chìa khóa để biểu diễn thành công.", "detailed": "Tự tin là yếu tố quyết định đến sự thành công của một buổi biểu diễn. Hãy luyện tập thường xuyên và chuẩn bị kỹ lưỡng để tăng cường sự tự tin.", "comprehensive": "Sự tự tin trên sân khấu có thể được phát triển thông qua việc luyện tập, chuẩn bị kỹ lưỡng và tạo dựng hình ảnh tích cực về bản thân. Hãy tham gia các buổi biểu diễn nhỏ trước khi lên sân khấu lớn để làm quen với áp lực."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7c44a876-2d3b-4705-b822-df1d562996ac', '41ad0fcd-a7b8-40b3-825f-9188560a8977', 'concept', 'Quản Lý Cảm Xúc Khi Biểu Diễn', 'Cảm xúc có thể ảnh hưởng đến việc biểu diễn. Hãy học cách quản lý chúng để truyền tải tốt hơn âm nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ piano thể hiện cảm xúc qua âm nhạc.", "videoScript": "Giới thiệu về tầm quan trọng của cảm xúc trong âm nhạc. Hướng dẫn cách sử dụng cảm xúc để cải thiện buổi biểu diễn.", "imageDescription": "Hình ảnh một nghệ sĩ piano đang chơi với cảm xúc mãnh liệt, thể hiện sự kết nối với khán giả.", "videoDescription": "Video hướng dẫn cách quản lý và thể hiện cảm xúc khi biểu diễn piano."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:54:20.831543', '2026-01-27 12:54:20.831543', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý cảm xúc là quan trọng khi biểu diễn.", "detailed": "Cảm xúc có thể ảnh hưởng đến việc biểu diễn. Hãy học cách quản lý chúng để truyền tải tốt hơn âm nhạc.", "comprehensive": "Khi biểu diễn, việc quản lý cảm xúc là rất quan trọng. Hãy tìm cách để nhận diện, chấp nhận và điều khiển cảm xúc của bạn. Sử dụng âm nhạc để diễn đạt cảm xúc và kết nối với khán giả."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('90389239-1add-4620-80b1-72867fa50918', 'd36bd957-a3e0-4f13-8847-41e19dfcd6bf', 'concept', 'Cách Cung Cấp Nước Đủ Cho Da', 'Bạn nên uống ít nhất 2 lít nước mỗi ngày để giữ cho da luôn ẩm mịn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một đĩa trái cây tươi mát và một chai nước.", "videoScript": "Trong video này, chúng ta sẽ cùng khám phá các cách để cung cấp nước cho làn da của bạn. Từ việc uống nước cho đến việc bổ sung thực phẩm giàu nước, mọi thứ đều quan trọng!", "imageDescription": "Đĩa trái cây tươi ngon cùng với một chai nước trong suốt.", "videoDescription": "Video hướng dẫn cách cung cấp đủ nước cho da qua chế độ ăn uống và thói quen hàng ngày."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:11:35.278559', '2026-01-27 13:11:35.278559', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Uống đủ nước mỗi ngày.", "detailed": "Bạn nên uống ít nhất 2 lít nước mỗi ngày để giữ cho da luôn ẩm mịn.", "comprehensive": "Để cung cấp đủ nước cho da, bạn cần uống nước thường xuyên, ăn thực phẩm giàu nước như trái cây và rau củ, và sử dụng các sản phẩm chăm sóc da chứa nước. Hãy nhớ rằng, việc cung cấp đủ nước không chỉ từ nước uống mà còn từ các nguồn thực phẩm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d427421a-fddc-493a-af42-9376d95ea7c0', '320c50fc-7675-4eb0-b835-3ae88e246f60', 'concept', 'Giới thiệu về cấu trúc bài hát', 'Một bài hát thường có cấu trúc cơ bản như sau: phần mở đầu (Intro), phần chính (Verse), điệp khúc (Chorus), và phần kết thúc (Outro). Mỗi phần đóng vai trò quan trọng trong việc tạo nên sự hấp dẫn cho bài hát.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa cấu trúc bài hát với các phần rõ ràng.", "videoScript": "Chào mừng các bạn đến với bài học hôm nay! Hôm nay, chúng ta sẽ khám phá cấu trúc của một bài hát và tìm hiểu các phần của nó.", "imageDescription": "Một biểu đồ mô tả cấu trúc bài hát với các phần như Intro, Verse, Chorus, Bridge, và Outro.", "videoDescription": "Video này sẽ giúp bạn hiểu rõ về cấu trúc bài hát, với các ví dụ cụ thể và phân tích từng phần."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:54:48.622999', '2026-01-27 12:54:48.622999', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cấu trúc bài hát thường gồm có phần mở đầu, điệp khúc, và phần kết thúc.", "detailed": "Một bài hát thường có cấu trúc cơ bản như sau: phần mở đầu (Intro), phần chính (Verse), điệp khúc (Chorus), và phần kết thúc (Outro). Mỗi phần đóng vai trò quan trọng trong việc tạo nên sự hấp dẫn cho bài hát.", "comprehensive": "Cấu trúc bài hát là một yếu tố quan trọng trong sáng tác âm nhạc. Nó thường bao gồm phần mở đầu (Intro), các đoạn chính (Verse), điệp khúc (Chorus), cầu (Bridge) và phần kết thúc (Outro). Hiểu rõ từng phần giúp nhạc sĩ tổ chức ý tưởng và cảm xúc trong bài hát một cách hiệu quả."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5e232607-35d6-4389-98af-d8cad6f8aff3', '320c50fc-7675-4eb0-b835-3ae88e246f60', 'concept', 'Vai trò của điệp khúc trong bài hát', 'Điệp khúc là một trong những phần quan trọng nhất của bài hát. Nó thường chứa thông điệp chính và là phần mà người nghe dễ dàng ghi nhớ nhất.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa một điệp khúc với lời bài hát nổi bật.", "videoScript": "Trong bài học này, chúng ta sẽ tập trung vào vai trò của điệp khúc trong một bài hát. Hãy cùng xem những ví dụ nhé!", "imageDescription": "Một hình ảnh thể hiện lời bài hát trong điệp khúc với phông chữ lớn và nổi bật.", "videoDescription": "Video này giải thích vai trò quan trọng của điệp khúc trong bài hát và cách nó ảnh hưởng đến cảm xúc của người nghe."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:54:48.630637', '2026-01-27 12:54:48.630637', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Điệp khúc là phần dễ nhớ và thường lặp lại nhiều lần trong bài hát.", "detailed": "Điệp khúc là một trong những phần quan trọng nhất của bài hát. Nó thường chứa thông điệp chính và là phần mà người nghe dễ dàng ghi nhớ nhất.", "comprehensive": "Điệp khúc không chỉ là phần dễ nhớ mà còn là phần thể hiện cảm xúc mạnh mẽ nhất trong bài hát. Nó thường được lặp lại nhiều lần, tạo ra sự kết nối và ấn tượng sâu sắc với người nghe."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f9ffd1ed-c265-4b23-ac4f-b02808d36dfd', '259b3fb6-9ae3-4a9d-b383-2669ed79d8e9', 'concept', 'Giới thiệu về Lề trong tài liệu', 'Lề giúp định hình cách trình bày tài liệu, tạo sự thoải mái cho người đọc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các loại lề trong tài liệu văn phòng.", "videoScript": "Chào mừng bạn đến với bài học về lề. Hôm nay, chúng ta sẽ tìm hiểu về tầm quan trọng của việc cài đặt lề trong tài liệu. Hãy cùng xem cách điều chỉnh lề trong phần mềm văn phòng.", "imageDescription": "Hình ảnh so sánh các loại lề (trái, phải, trên, dưới) trong một tài liệu.", "videoDescription": "Video hướng dẫn cách cài đặt lề trong các phần mềm văn phòng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:36:30.263363', '2026-01-27 12:36:30.263363', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lề là khoảng cách giữa nội dung và mép giấy.", "detailed": "Lề giúp định hình cách trình bày tài liệu, tạo sự thoải mái cho người đọc.", "comprehensive": "Lề trong tài liệu không chỉ là khoảng cách giữa nội dung và mép giấy mà còn ảnh hưởng đến cách thức in ấn và bố trí văn bản. Một lề hợp lý giúp tài liệu dễ đọc hơn và tạo cảm giác chuyên nghiệp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d906317e-6de8-4d2f-8d91-cb2800afb693', '259b3fb6-9ae3-4a9d-b383-2669ed79d8e9', 'concept', 'Cách cài đặt Lề trong Word', 'Để cài đặt lề trong Word, truy cập tab Layout, chọn Margins và điều chỉnh lề theo ý muốn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh hướng dẫn cài đặt lề trong Word.", "videoScript": "Trong video này, chúng ta sẽ xem cách cài đặt lề trong Word. Bắt đầu bằng cách vào tab Layout, tìm kiếm mục Margins và chọn lề phù hợp với nhu cầu của bạn.", "imageDescription": "Hình ảnh chụp màn hình của tab Layout trong Word với các tùy chọn lề.", "videoDescription": "Video hướng dẫn từng bước cài đặt lề trong Microsoft Word."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:36:30.271011', '2026-01-27 12:36:30.271011', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể dễ dàng cài đặt lề trong Word qua tab Layout.", "detailed": "Để cài đặt lề trong Word, truy cập tab Layout, chọn Margins và điều chỉnh lề theo ý muốn.", "comprehensive": "Cài đặt lề trong Word rất đơn giản. Đầu tiên, bạn vào tab Layout, sau đó chọn Margins. Tại đây, bạn có thể chọn lề mặc định hoặc tự điều chỉnh lề với các kích thước tùy chỉnh. Lưu ý rằng lề sẽ ảnh hưởng đến cách tài liệu được in và trình bày."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d145f7be-65bd-4d85-9b40-fd08d570af2a', '320c50fc-7675-4eb0-b835-3ae88e246f60', 'concept', 'Sáng tạo trong việc xây dựng phần cầu', 'Phần cầu là một phần rất quan trọng giúp tạo sự chuyển tiếp giữa các phần của bài hát. Nó thường mang lại một cảm xúc khác biệt và tạo sự bất ngờ cho người nghe.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa phần cầu trong một bài hát với các nốt nhạc.", "videoScript": "Hôm nay, chúng ta sẽ tìm hiểu về phần cầu trong bài hát, một phần giúp tạo sự đa dạng và bất ngờ.", "imageDescription": "Một hình ảnh minh họa phần cầu với các nốt nhạc khác biệt so với phần điệp khúc.", "videoDescription": "Video này sẽ khám phá cách xây dựng phần cầu trong bài hát và tầm quan trọng của nó trong việc sáng tạo âm nhạc."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:54:48.634082', '2026-01-27 12:54:48.634082', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phần cầu giúp tạo sự chuyển đổi và mới mẻ cho bài hát.", "detailed": "Phần cầu là một phần rất quan trọng giúp tạo sự chuyển tiếp giữa các phần của bài hát. Nó thường mang lại một cảm xúc khác biệt và tạo sự bất ngờ cho người nghe.", "comprehensive": "Phần cầu (Bridge) là một yếu tố sáng tạo trong cấu trúc bài hát, cho phép nhạc sĩ thay đổi giai điệu và cảm xúc. Nó thường xuất hiện giữa các điệp khúc và giúp tạo ra một cảm giác mới mẻ, kết nối các phần khác nhau của bài hát lại với nhau."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e18911e7-f1b5-4d14-a636-597b8003620a', '320c50fc-7675-4eb0-b835-3ae88e246f60', 'concept', 'Phân tích một bài hát nổi tiếng', 'Phân tích cấu trúc của một bài hát nổi tiếng sẽ giúp chúng ta hiểu rõ hơn về cách mà nhạc sĩ tổ chức ý tưởng và cảm xúc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa cấu trúc của bài hát nổi tiếng được phân tích.", "videoScript": "Chào mừng các bạn đến với phần phân tích bài hát nổi tiếng. Hãy cùng tìm hiểu từng phần của bài hát và ý nghĩa của chúng.", "imageDescription": "Một biểu đồ mô tả cấu trúc của bài hát nổi tiếng với các phần được đánh dấu rõ ràng.", "videoDescription": "Video này sẽ hướng dẫn bạn cách phân tích cấu trúc một bài hát nổi tiếng, giúp bạn hiểu sâu hơn về âm nhạc."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:54:48.639377', '2026-01-27 12:54:48.639377', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chúng ta sẽ cùng phân tích cấu trúc của một bài hát nổi tiếng.", "detailed": "Phân tích cấu trúc của một bài hát nổi tiếng sẽ giúp chúng ta hiểu rõ hơn về cách mà nhạc sĩ tổ chức ý tưởng và cảm xúc.", "comprehensive": "Trong bài học này, chúng ta sẽ phân tích một bài hát nổi tiếng, khám phá từng phần của nó và hiểu cách mà cấu trúc bài hát ảnh hưởng đến người nghe và cảm xúc tổng thể."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('37a1bbb0-86ab-45e5-8eab-6069b190b60c', '320c50fc-7675-4eb0-b835-3ae88e246f60', 'concept', 'Thực hành sáng tác bài hát của riêng bạn', 'Trong bài học này, bạn sẽ thực hành sáng tác một bài hát của riêng mình, sử dụng cấu trúc mà chúng ta đã tìm hiểu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa một nhạc sĩ đang sáng tác bài hát.", "videoScript": "Hôm nay, chúng ta sẽ thực hành sáng tác bài hát của riêng bạn. Hãy cùng tôi khám phá các bước để hoàn thành bài hát nhé!", "imageDescription": "Một bức tranh thể hiện một nhạc sĩ đang ngồi bên đàn piano và viết lời bài hát.", "videoDescription": "Video này sẽ hướng dẫn bạn cách sáng tác một bài hát của riêng mình, từ việc lên ý tưởng đến hoàn thiện cấu trúc."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:54:48.642955', '2026-01-27 12:54:48.642955', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hãy thử sáng tác một bài hát với cấu trúc mà bạn đã học.", "detailed": "Trong bài học này, bạn sẽ thực hành sáng tác một bài hát của riêng mình, sử dụng cấu trúc mà chúng ta đã tìm hiểu.", "comprehensive": "Bài học hôm nay sẽ giúp bạn áp dụng những gì đã học để sáng tác một bài hát của riêng mình. Bạn sẽ sử dụng cấu trúc đã học và thử nghiệm với các ý tưởng sáng tạo của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('69acedd1-3a79-4fbf-b5b0-6490709e6062', '259b3fb6-9ae3-4a9d-b383-2669ed79d8e9', 'concept', 'Bố cục trang trong tài liệu', 'Bố cục trang ảnh hưởng đến cách người đọc tiếp nhận thông tin trong tài liệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh ví dụ về các kiểu bố cục trang.", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ khám phá các kiểu bố cục trang khác nhau. Một bố cục hợp lý sẽ giúp tài liệu của bạn trở nên hấp dẫn hơn.", "imageDescription": "Hình ảnh mô tả các kiểu bố cục khác nhau như bố cục một cột, hai cột, và bố cục có hình ảnh.", "videoDescription": "Video giải thích về các kiểu bố cục trang trong tài liệu văn phòng."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:36:30.275453', '2026-01-27 12:36:30.275453', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bố cục trang là cách sắp xếp nội dung trên trang.", "detailed": "Bố cục trang ảnh hưởng đến cách người đọc tiếp nhận thông tin trong tài liệu.", "comprehensive": "Bố cục trang là yếu tố quan trọng trong việc trình bày nội dung. Nó bao gồm việc sắp xếp các phần như tiêu đề, đoạn văn, hình ảnh và bảng biểu một cách hợp lý, giúp người đọc dễ dàng theo dõi và hiểu nội dung chính của tài liệu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('fc1b4150-6d39-404a-ae46-6664dcdb6d2b', '3c97a877-b240-475b-89dc-474d0a2b17b5', 'concept', 'Giới Thiệu Kỹ Thuật Phòng Thủ', 'Kỹ thuật phòng thủ bao gồm nhiều phương pháp như phòng thủ cá nhân và phòng thủ zon. Mỗi phương pháp có những ưu nhược điểm riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cầu thủ bóng rổ đang thực hiện kỹ thuật phòng thủ cá nhân.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về các kỹ thuật phòng thủ cơ bản trong bóng rổ. Đầu tiên là phòng thủ cá nhân, nơi cầu thủ theo sát đối thủ. Tiếp theo là phòng thủ zon, nơi cầu thủ phân chia khu vực để bảo vệ.", "imageDescription": "Một cầu thủ bóng rổ trong tư thế phòng thủ, chân rộng và tay dang ra để ngăn cản đối thủ.", "videoDescription": "Video giới thiệu các kỹ thuật phòng thủ trong bóng rổ, bao gồm phòng thủ cá nhân và phòng thủ zon."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:55:58.400722', '2026-01-27 12:55:58.400722', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kỹ thuật phòng thủ là cách để ngăn cản đối thủ ghi điểm.", "detailed": "Kỹ thuật phòng thủ bao gồm nhiều phương pháp như phòng thủ cá nhân và phòng thủ zon. Mỗi phương pháp có những ưu nhược điểm riêng.", "comprehensive": "Kỹ thuật phòng thủ trong bóng rổ là một phần quan trọng trong chiến thuật đội bóng. Nó không chỉ bao gồm việc ngăn cản đối thủ ghi điểm mà còn giúp đội bóng kiểm soát trận đấu. Phòng thủ cá nhân tập trung vào việc theo sát từng cầu thủ đối phương, trong khi phòng thủ zon chia khu vực trên sân và mỗi cầu thủ sẽ bảo vệ một khu vực nhất định."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a13b460c-abfb-46c3-b400-59c285d2ada0', '259b3fb6-9ae3-4a9d-b383-2669ed79d8e9', 'concept', 'Cách điều chỉnh Bố cục trong Word', 'Để điều chỉnh bố cục trong Word, sử dụng tab Layout và chọn các tùy chọn như Columns, Breaks.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh hướng dẫn điều chỉnh bố cục trong Word.", "videoScript": "Trong video này, chúng ta sẽ học cách điều chỉnh bố cục trong Word. Hãy cùng nhau khám phá các tùy chọn có sẵn trong tab Layout.", "imageDescription": "Hình ảnh chụp màn hình tab Layout với các tùy chọn bố cục.", "videoDescription": "Video hướng dẫn cách điều chỉnh bố cục tài liệu trong Microsoft Word."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:36:30.279785', '2026-01-27 12:36:30.279785', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể điều chỉnh bố cục trong Word qua tab Layout.", "detailed": "Để điều chỉnh bố cục trong Word, sử dụng tab Layout và chọn các tùy chọn như Columns, Breaks.", "comprehensive": "Để điều chỉnh bố cục trong Word, bạn truy cập tab Layout. Tại đây, bạn có thể chọn các tùy chọn như Columns để chia trang thành nhiều cột, hoặc sử dụng Breaks để tạo ngắt trang hoặc ngắt đoạn. Việc điều chỉnh bố cục sẽ giúp tài liệu của bạn dễ đọc và chuyên nghiệp hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c2b208a0-2b9a-44f4-baac-0705dde68997', '259b3fb6-9ae3-4a9d-b383-2669ed79d8e9', 'concept', 'Lưu ý khi cài đặt lề và bố cục', 'Khi cài đặt lề và bố cục, hãy xem xét kích thước giấy và nội dung tài liệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các yếu tố cần lưu ý khi cài đặt lề và bố cục.", "videoScript": "Trong video này, chúng ta sẽ thảo luận về những điều cần lưu ý khi cài đặt lề và bố cục. Những yếu tố này ảnh hưởng lớn đến cách tài liệu của bạn được trình bày.", "imageDescription": "Hình ảnh biểu thị các yếu tố quan trọng như kích thước giấy và kiểu tài liệu.", "videoDescription": "Video chia sẻ các lưu ý quan trọng khi cài đặt lề và bố cục trong văn phòng."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:36:30.28337', '2026-01-27 12:36:30.28337', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cần lưu ý đến kích thước giấy và loại tài liệu.", "detailed": "Khi cài đặt lề và bố cục, hãy xem xét kích thước giấy và nội dung tài liệu.", "comprehensive": "Khi cài đặt lề và bố cục cho tài liệu, bạn cần lưu ý đến kích thước giấy, loại tài liệu (như báo cáo, thư, hoặc tài liệu quảng cáo) và cách thức in ấn. Một lề quá nhỏ có thể gây khó khăn khi in, trong khi lề quá lớn có thể làm giảm không gian cho nội dung chính."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('fc64913f-2f9c-466b-a22f-fb37a9598439', '3c97a877-b240-475b-89dc-474d0a2b17b5', 'concept', 'Phòng Thủ Cá Nhân', 'Khi thực hiện phòng thủ cá nhân, cầu thủ cần giữ khoảng cách hợp lý và thường xuyên thay đổi vị trí để theo sát đối thủ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh cầu thủ phòng thủ đang theo sát đối thủ trong trận đấu.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về phòng thủ cá nhân. Đầu tiên, hãy xem cách cầu thủ giữ khoảng cách và di chuyển theo đối thủ.", "imageDescription": "Cầu thủ A đang áp sát cầu thủ B, tạo áp lực để ngăn cản anh ta ghi điểm.", "videoDescription": "Video hướng dẫn về kỹ thuật phòng thủ cá nhân trong bóng rổ, bao gồm cách di chuyển và giữ khoảng cách hợp lý."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:55:58.409123', '2026-01-27 12:55:58.409123', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phòng thủ cá nhân là khi một cầu thủ theo sát đối thủ của mình.", "detailed": "Khi thực hiện phòng thủ cá nhân, cầu thủ cần giữ khoảng cách hợp lý và thường xuyên thay đổi vị trí để theo sát đối thủ.", "comprehensive": "Phòng thủ cá nhân là kỹ thuật phòng thủ phổ biến nhất trong bóng rổ. Nó yêu cầu cầu thủ phải nhanh nhẹn và có khả năng đọc tình huống tốt. Cầu thủ phòng thủ cần phải giữ khoảng cách đủ gần để ngăn cản đối thủ, nhưng cũng phải đủ xa để tránh bị vượt qua. Kỹ năng này rất quan trọng trong việc kiểm soát không gian và thời gian của trận đấu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8139fbcd-82c4-4c35-8972-f91c507b6597', '3c97a877-b240-475b-89dc-474d0a2b17b5', 'concept', 'Phòng Thủ Zon', 'Phòng thủ zon giúp đội bóng kiểm soát không gian hiệu quả, nhưng cần sự phối hợp cao giữa các cầu thủ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh đội bóng đang thực hiện phòng thủ zon trên sân.", "videoScript": "Trong video này, chúng ta sẽ xem xét cách thực hiện phòng thủ zon. Hãy chú ý đến cách các cầu thủ di chuyển và phối hợp với nhau.", "imageDescription": "Các cầu thủ đứng ở vị trí zon của mình, tạo thành một bức tường chắn đối thủ.", "videoDescription": "Video giải thích về phòng thủ zon trong bóng rổ, với ví dụ minh họa từ một trận đấu."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:55:58.413278', '2026-01-27 12:55:58.413278', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phòng thủ zon là khi mỗi cầu thủ bảo vệ một khu vực trên sân.", "detailed": "Phòng thủ zon giúp đội bóng kiểm soát không gian hiệu quả, nhưng cần sự phối hợp cao giữa các cầu thủ.", "comprehensive": "Phòng thủ zon là một chiến thuật phòng thủ mà trong đó mỗi cầu thủ chịu trách nhiệm bảo vệ một khu vực nhất định trên sân. Kỹ thuật này giúp giảm thiểu khả năng ghi điểm của đối thủ bằng cách tập trung nhiều cầu thủ vào một khu vực. Tuy nhiên, phòng thủ zon yêu cầu sự giao tiếp và phối hợp chặt chẽ giữa các cầu thủ để đảm bảo không có khu vực nào bị bỏ trống."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('eb46342a-ff1e-4891-93db-7a4fc865b1b1', '3c97a877-b240-475b-89dc-474d0a2b17b5', 'concept', 'Kỹ Thuật Chặn Bóng', 'Kỹ thuật chặn bóng cần sự chính xác và thời gian hợp lý để đạt hiệu quả cao.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh cầu thủ đang thực hiện động tác chặn bóng trong trận đấu.", "videoScript": "Trong video này, chúng ta sẽ xem cách thực hiện kỹ thuật chặn bóng. Hãy chú ý đến thời gian và cách cầu thủ chuẩn bị cho cú nhảy.", "imageDescription": "Cầu thủ A đang nhảy lên để chặn cú ném của cầu thủ B.", "videoDescription": "Video hướng dẫn thực hiện kỹ thuật chặn bóng trong bóng rổ, với các ví dụ minh họa từ các trận đấu."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:55:58.417085', '2026-01-27 12:55:58.417085', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chặn bóng là kỹ thuật để ngăn cản cú ném của đối thủ.", "detailed": "Kỹ thuật chặn bóng cần sự chính xác và thời gian hợp lý để đạt hiệu quả cao.", "comprehensive": "Kỹ thuật chặn bóng là một kỹ năng phòng thủ quan trọng trong bóng rổ. Nó yêu cầu cầu thủ phải có thời gian và sự chính xác trong việc nhảy lên để ngăn cản cú ném của đối phương. Để chặn bóng hiệu quả, cầu thủ cần phải đọc được động tác của đối thủ và lựa chọn thời điểm nhảy một cách hợp lý. Chặn bóng không chỉ giúp ngăn cản điểm mà còn tạo cơ hội phản công cho đội bóng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4bb71725-b335-421a-b8b0-5165bbbae996', 'a396e2ac-ac31-48c9-bc1e-cafc2374cb47', 'concept', 'Giới thiệu về Microsoft Project', 'Microsoft Project là công cụ mạnh mẽ trong việc lập kế hoạch và quản lý dự án. Nó cho phép người dùng tạo và theo dõi các nhiệm vụ, phân bổ nguồn lực và đánh giá tiến độ dự án.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giao diện Microsoft Project với các tính năng nổi bật.", "videoScript": "Chào mừng bạn đến với bài học về Microsoft Project. Hôm nay, chúng ta sẽ khám phá cách phần mềm này giúp quản lý dự án hiệu quả.", "imageDescription": "Giao diện chính của Microsoft Project hiển thị danh sách các nhiệm vụ và biểu đồ Gantt.", "videoDescription": "Video giới thiệu Microsoft Project, các tính năng chính và cách sử dụng cơ bản."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:37:01.591338', '2026-01-27 12:37:01.591338', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Microsoft Project là phần mềm quản lý dự án giúp người dùng lập kế hoạch, theo dõi và quản lý tiến độ dự án.", "detailed": "Microsoft Project là công cụ mạnh mẽ trong việc lập kế hoạch và quản lý dự án. Nó cho phép người dùng tạo và theo dõi các nhiệm vụ, phân bổ nguồn lực và đánh giá tiến độ dự án.", "comprehensive": "Microsoft Project là phần mềm quản lý dự án được thiết kế để giúp người dùng lập kế hoạch, thực hiện và theo dõi các dự án một cách hiệu quả. Với các tính năng như tạo lịch trình, phân bổ nguồn lực, và theo dõi tiến độ, Microsoft Project trở thành một công cụ không thể thiếu cho các nhà quản lý dự án trong việc đảm bảo rằng dự án hoàn thành đúng hạn và trong ngân sách."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6bf05506-a862-4f00-b1b7-94f547336f92', 'a396e2ac-ac31-48c9-bc1e-cafc2374cb47', 'concept', 'Lập kế hoạch dự án với Microsoft Project', 'Trong Microsoft Project, bạn có thể lập kế hoạch dự án bằng cách thêm nhiệm vụ, xác định thời gian bắt đầu và kết thúc, và thiết lập mối quan hệ giữa các nhiệm vụ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ Gantt hiển thị tiến độ dự án.", "videoScript": "Trong phần này, chúng ta sẽ cùng nhau lập kế hoạch cho một dự án bằng Microsoft Project. Hãy xem cách chúng ta thêm nhiệm vụ và thiết lập thời gian.", "imageDescription": "Biểu đồ Gantt trong Microsoft Project với các nhiệm vụ và thời gian được biểu thị rõ ràng.", "videoDescription": "Video hướng dẫn lập kế hoạch dự án với Microsoft Project, bao gồm cách tạo và sắp xếp nhiệm vụ."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:37:01.599429', '2026-01-27 12:37:01.599429', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lập kế hoạch dự án bao gồm xác định nhiệm vụ và thời gian hoàn thành.", "detailed": "Trong Microsoft Project, bạn có thể lập kế hoạch dự án bằng cách thêm nhiệm vụ, xác định thời gian bắt đầu và kết thúc, và thiết lập mối quan hệ giữa các nhiệm vụ.", "comprehensive": "Lập kế hoạch dự án trong Microsoft Project yêu cầu bạn phải xác định các nhiệm vụ cần thiết, ước lượng thời gian cho từng nhiệm vụ, và thiết lập các mối quan hệ phụ thuộc giữa chúng. Bạn có thể dùng biểu đồ Gantt để trực quan hóa tiến độ và sự phụ thuộc này, giúp bạn có cái nhìn tổng quát và chi tiết hơn về dự án."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a87890f1-f943-4c03-9ee2-106b6a23b868', '3c97a877-b240-475b-89dc-474d0a2b17b5', 'concept', 'Phòng Thủ Khi Đối Thủ Ném Phạt', 'Khi đối thủ ném phạt, đội phòng thủ cần phải tạo áp lực để làm giảm sự tự tin của người ném.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh cầu thủ đứng ở vị trí phòng thủ khi đối thủ thực hiện cú ném phạt.", "videoScript": "Trong video này, chúng ta sẽ thảo luận về cách phòng thủ khi đối thủ ném phạt. Hãy xem cách đội phòng thủ tạo áp lực tâm lý lên người ném.", "imageDescription": "Cầu thủ phòng thủ đứng ở một bên sân, tạo áp lực tâm lý cho cầu thủ ném phạt.", "videoDescription": "Video trình bày chiến thuật phòng thủ khi đối thủ thực hiện cú ném phạt trong bóng rổ."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:55:58.421704', '2026-01-27 12:55:58.421704', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phòng thủ khi đối thủ ném phạt là cách để giảm nguy cơ ghi điểm từ cú ném phạt.", "detailed": "Khi đối thủ ném phạt, đội phòng thủ cần phải tạo áp lực để làm giảm sự tự tin của người ném.", "comprehensive": "Phòng thủ trong tình huống ném phạt là một phần quan trọng của chiến thuật phòng thủ. Trong tình huống này, không thể chặn cú ném, nhưng đội phòng thủ có thể tạo áp lực tâm lý lên cầu thủ ném phạt bằng cách đứng ở vị trí chiến lược và thể hiện sự tập trung. Điều này không chỉ giúp giảm khả năng ghi điểm từ cú ném phạt mà còn tạo cơ hội cho đội bóng lấy lại bóng sau cú ném không thành công."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('29f615dc-75fe-4f19-96e1-c8f800ba7839', '6fdf2752-f255-4c42-beb1-7425d8e2187c', 'concept', 'Cách Lập Kế Hoạch Đầu Tư Dài Hạn', 'Để lập kế hoạch đầu tư dài hạn, bạn cần xác định mục tiêu tài chính, thời gian đầu tư, và mức độ rủi ro bạn sẵn sàng chấp nhận.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng kế hoạch đầu tư với các mục tiêu tài chính.", "videoScript": "Hãy cùng tìm hiểu cách lập kế hoạch đầu tư dài hạn hiệu quả. Chúng ta sẽ đi qua các bước cần thiết để xác định mục tiêu và xây dựng danh mục đầu tư.", "imageDescription": "Hình ảnh bảng kế hoạch đầu tư với các mục tiêu và thời gian rõ ràng.", "videoDescription": "Video hướng dẫn lập kế hoạch đầu tư dài hạn, bao gồm cách xác định mục tiêu và quản lý rủi ro."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:02:19.197614', '2026-01-27 13:02:19.197614', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lập kế hoạch đầu tư dài hạn cần xác định mục tiêu tài chính và thời gian đầu tư.", "detailed": "Để lập kế hoạch đầu tư dài hạn, bạn cần xác định mục tiêu tài chính, thời gian đầu tư, và mức độ rủi ro bạn sẵn sàng chấp nhận.", "comprehensive": "Kế hoạch đầu tư dài hạn bao gồm việc thiết lập mục tiêu tài chính cụ thể, như mua nhà, nghỉ hưu, hoặc giáo dục con cái. Bạn cũng cần xác định thời gian đầu tư, lựa chọn loại tài sản phù hợp, và xây dựng một danh mục đầu tư đa dạng để giảm thiểu rủi ro."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8774870b-b68c-4f54-97e8-dd6c8d6a4997', '6fdf2752-f255-4c42-beb1-7425d8e2187c', 'concept', 'Quản Lý Danh Mục Đầu Tư Dài Hạn', 'Quản lý danh mục đầu tư dài hạn bao gồm việc theo dõi hiệu suất đầu tư, điều chỉnh phân bổ tài sản và tái đầu tư lợi nhuận.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ thể hiện sự phân bổ tài sản trong danh mục đầu tư.", "videoScript": "Trong video này, chúng ta sẽ thảo luận về cách quản lý danh mục đầu tư dài hạn và các yếu tố cần xem xét để đảm bảo lợi ích tối đa.", "imageDescription": "Biểu đồ tròn thể hiện sự phân bổ tài sản của một danh mục đầu tư dài hạn.", "videoDescription": "Video hướng dẫn quản lý danh mục đầu tư dài hạn, bao gồm theo dõi, điều chỉnh và tối ưu hóa lợi nhuận."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:02:19.201945', '2026-01-27 13:02:19.201945', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý danh mục đầu tư là việc theo dõi và điều chỉnh các khoản đầu tư.", "detailed": "Quản lý danh mục đầu tư dài hạn bao gồm việc theo dõi hiệu suất đầu tư, điều chỉnh phân bổ tài sản và tái đầu tư lợi nhuận.", "comprehensive": "Để quản lý danh mục đầu tư dài hạn một cách hiệu quả, nhà đầu tư cần thường xuyên theo dõi hiệu suất của từng tài sản, đánh giá lại chiến lược phân bổ tài sản, và tái đầu tư lợi nhuận để tối ưu hóa lợi nhuận. Việc này cũng cần phải cân nhắc đến các yếu tố như thay đổi trong tình hình kinh tế và chính sách đầu tư cá nhân."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3bff6c9d-6257-40bc-b052-8a180790115f', 'a396e2ac-ac31-48c9-bc1e-cafc2374cb47', 'concept', 'Theo dõi tiến độ dự án', 'Microsoft Project cung cấp các công cụ để theo dõi tiến độ dự án, bao gồm cập nhật trạng thái nhiệm vụ và theo dõi thời gian thực tế.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh báo cáo tiến độ dự án trong Microsoft Project.", "videoScript": "Bây giờ, chúng ta sẽ tìm hiểu cách theo dõi tiến độ của dự án trong Microsoft Project. Hãy cùng xem cách cập nhật trạng thái nhiệm vụ.", "imageDescription": "Báo cáo tiến độ dự án hiển thị các nhiệm vụ đã hoàn thành và các nhiệm vụ còn lại.", "videoDescription": "Video hướng dẫn theo dõi tiến độ dự án trong Microsoft Project, bao gồm cách cập nhật trạng thái và sử dụng báo cáo."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:37:01.603082', '2026-01-27 12:37:01.603082', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi tiến độ giúp bạn biết dự án đang ở đâu.", "detailed": "Microsoft Project cung cấp các công cụ để theo dõi tiến độ dự án, bao gồm cập nhật trạng thái nhiệm vụ và theo dõi thời gian thực tế.", "comprehensive": "Theo dõi tiến độ dự án trong Microsoft Project là một phần quan trọng trong quản lý dự án. Bạn có thể cập nhật trạng thái của từng nhiệm vụ, so sánh thời gian thực tế với thời gian ước lượng và sử dụng các báo cáo để đánh giá tình hình dự án. Việc này giúp bạn điều chỉnh kế hoạch kịp thời và đảm bảo dự án không bị trễ hạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9983f6ab-a322-4e64-baba-b8999d049e70', '6456d6c3-ddb5-4719-b480-68c6f12638ab', 'concept', 'Giới Thiệu Về Kỹ Thuật Chuyền Bóng', 'Kỹ thuật chuyền bóng giúp chuyển giao bóng giữa các cầu thủ, tạo cơ hội ghi điểm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cầu thủ bóng rổ đang thực hiện kỹ thuật chuyền bóng.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về kỹ thuật chuyền bóng trong bóng rổ, các kiểu chuyền khác nhau và cách sử dụng chúng trong trận đấu.", "imageDescription": "Cầu thủ bóng rổ đứng ở vị trí chuyền bóng, với bóng trong tay và tư thế sẵn sàng.", "videoDescription": "Video hướng dẫn kỹ thuật chuyền bóng, bao gồm các phương pháp và lưu ý khi thực hiện."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:56:27.277528', '2026-01-27 12:56:27.277528', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chuyền bóng là kỹ thuật cơ bản trong bóng rổ.", "detailed": "Kỹ thuật chuyền bóng giúp chuyển giao bóng giữa các cầu thủ, tạo cơ hội ghi điểm.", "comprehensive": "Chuyền bóng là một trong những kỹ thuật cơ bản và quan trọng nhất trong bóng rổ. Nó không chỉ giúp đội bóng kiểm soát bóng mà còn tạo ra cơ hội ghi điểm. Có nhiều kiểu chuyền bóng khác nhau, bao gồm chuyền bóng bằng một tay, hai tay và chuyền bóng nảy."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('79c3302d-d7ac-4c2b-80e4-5fc49974fc0f', '6456d6c3-ddb5-4719-b480-68c6f12638ab', 'concept', 'Kỹ Thuật Chuyền Bóng Bằng Một Tay', 'Kỹ thuật chuyền bóng bằng một tay thường được sử dụng khi cầu thủ cần chuyền bóng nhanh và chính xác.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cầu thủ thực hiện chuyền bóng bằng một tay.", "videoScript": "Video này sẽ hướng dẫn bạn cách thực hiện chuyền bóng bằng một tay, bao gồm các bí quyết và kỹ thuật để nâng cao hiệu suất.", "imageDescription": "Cầu thủ đang ở tư thế chuyền bóng bằng một tay, bóng được ném đi với độ chính xác cao.", "videoDescription": "Hướng dẫn chi tiết về chuyền bóng bằng một tay trong bóng rổ."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:56:27.285544', '2026-01-27 12:56:27.285544', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chuyền bóng bằng một tay giúp kiểm soát tốt hơn.", "detailed": "Kỹ thuật chuyền bóng bằng một tay thường được sử dụng khi cầu thủ cần chuyền bóng nhanh và chính xác.", "comprehensive": "Chuyền bóng bằng một tay là một kỹ thuật quan trọng trong bóng rổ, đặc biệt khi cần chuyền bóng nhanh trong các tình huống khẩn cấp. Để thực hiện kỹ thuật này, cầu thủ cần có sự phối hợp tốt giữa tay và mắt, cùng với việc đứng vững và giữ thăng bằng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cd1f430f-bf16-4531-8b8c-767f14130079', '6456d6c3-ddb5-4719-b480-68c6f12638ab', 'concept', 'Chuyền Bóng Nảy', 'Chuyền bóng nảy là kỹ thuật hiệu quả khi khoảng cách giữa hai cầu thủ khá xa.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh cầu thủ thực hiện chuyền bóng nảy.", "videoScript": "Trong video này, chúng ta sẽ xem cách thực hiện chuyền bóng nảy và những tình huống thích hợp để sử dụng kỹ thuật này.", "imageDescription": "Cầu thủ ném bóng từ vị trí cao, bóng đang nảy xuống đất trước khi đến tay đồng đội.", "videoDescription": "Hướng dẫn và phân tích kỹ thuật chuyền bóng nảy trong bóng rổ."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:56:27.28902', '2026-01-27 12:56:27.28902', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chuyền bóng nảy giúp bóng đến tay đồng đội dễ dàng hơn.", "detailed": "Chuyền bóng nảy là kỹ thuật hiệu quả khi khoảng cách giữa hai cầu thủ khá xa.", "comprehensive": "Kỹ thuật chuyền bóng nảy là một trong những phương pháp chuyền bóng phổ biến trong bóng rổ. Kỹ thuật này giúp bóng nảy lên và dễ dàng đến tay cầu thủ đồng đội, đặc biệt trong các tình huống phòng ngự. Để thực hiện, cầu thủ cần xác định khoảng cách và góc ném chính xác."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3983f72b-8c9b-4211-a40c-fb83c36b4775', 'd36bd957-a3e0-4f13-8847-41e19dfcd6bf', 'concept', 'Nước và Quy Trình Chăm Sóc Da', 'Nước giúp làm sạch và chuẩn bị cho da hấp thụ các sản phẩm khác tốt hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bộ sản phẩm chăm sóc da với nước làm nền.", "videoScript": "Hãy cùng tìm hiểu vai trò của nước trong quy trình chăm sóc da. Nước không chỉ giúp làm sạch mà còn tạo điều kiện cho các sản phẩm khác hoạt động hiệu quả hơn.", "imageDescription": "Bộ sản phẩm chăm sóc da bao gồm sữa rửa mặt, toner và kem dưỡng với nước là bước đầu.", "videoDescription": "Video hướng dẫn quy trình chăm sóc da với sự hiện diện của nước ở mỗi bước."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:11:35.282133', '2026-01-27 13:11:35.282133', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nước là bước đầu tiên trong quy trình chăm sóc da.", "detailed": "Nước giúp làm sạch và chuẩn bị cho da hấp thụ các sản phẩm khác tốt hơn.", "comprehensive": "Trong quy trình chăm sóc da, nước đóng vai trò quan trọng không chỉ trong việc làm sạch mà còn giúp mở lỗ chân lông, chuẩn bị cho các sản phẩm dưỡng da tiếp theo thẩm thấu tốt hơn. Việc sử dụng nước đúng cách sẽ tối ưu hóa hiệu quả của các sản phẩm chăm sóc da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('fe2ba5a8-c6ce-4ed1-a41a-69c910249ef8', 'a396e2ac-ac31-48c9-bc1e-cafc2374cb47', 'concept', 'Quản lý nguồn lực trong Microsoft Project', 'Trong Microsoft Project, bạn có thể thêm nguồn lực như nhân viên và thiết bị, và phân bổ chúng cho các nhiệm vụ để tối ưu hóa hiệu suất.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giao diện quản lý nguồn lực trong Microsoft Project.", "videoScript": "Trong phần này, chúng ta sẽ cùng khám phá cách quản lý nguồn lực trong Microsoft Project. Hãy xem cách thêm và gán nguồn lực cho các nhiệm vụ.", "imageDescription": "Giao diện quản lý nguồn lực hiển thị danh sách nguồn lực và tình trạng sử dụng.", "videoDescription": "Video hướng dẫn quản lý nguồn lực trong Microsoft Project, bao gồm cách thêm và phân bổ nguồn lực cho nhiệm vụ."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:37:01.606885', '2026-01-27 12:37:01.606885', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý nguồn lực giúp phân bổ công việc hiệu quả.", "detailed": "Trong Microsoft Project, bạn có thể thêm nguồn lực như nhân viên và thiết bị, và phân bổ chúng cho các nhiệm vụ để tối ưu hóa hiệu suất.", "comprehensive": "Quản lý nguồn lực là một phần quan trọng trong Microsoft Project, cho phép bạn theo dõi và phân bổ nguồn lực một cách hiệu quả. Bạn có thể thêm nguồn lực với các thông tin như tên, loại, và mức chi phí. Sau đó, bạn có thể gán nguồn lực cho các nhiệm vụ khác nhau, theo dõi tình trạng sử dụng và điều chỉnh khi cần thiết để đảm bảo rằng dự án không gặp phải sự thiếu hụt nguồn lực."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9bd1a011-786f-4a4e-bdd0-8a4924dcc5b6', '6456d6c3-ddb5-4719-b480-68c6f12638ab', 'concept', 'Chuyền Bóng Hai Tay', 'Kỹ thuật chuyền bóng hai tay thường được sử dụng khi cần độ chính xác cao.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh cầu thủ thực hiện chuyền bóng hai tay.", "videoScript": "Video này sẽ hướng dẫn chi tiết cách thực hiện chuyền bóng hai tay và những lợi ích của kỹ thuật này.", "imageDescription": "Cầu thủ đang trong tư thế chuyền bóng hai tay, với bóng giữ chặt và sẵn sàng ném đi.", "videoDescription": "Hướng dẫn chuyền bóng hai tay trong bóng rổ, bao gồm cách thực hiện và vị trí đứng."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:56:27.292887', '2026-01-27 12:56:27.292887', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chuyền bóng hai tay giúp chuyền bóng ổn định hơn.", "detailed": "Kỹ thuật chuyền bóng hai tay thường được sử dụng khi cần độ chính xác cao.", "comprehensive": "Chuyền bóng hai tay là kỹ thuật giúp cầu thủ chuyền bóng với độ ổn định và chính xác cao. Kỹ thuật này thường được sử dụng trong các tình huống cần chuyền bóng cho đồng đội trong không gian hẹp hoặc khi đối thủ đang áp sát. Để thực hiện kỹ thuật này, cầu thủ cần đứng vững và giữ bóng chắc chắn giữa hai tay."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1b10b91d-b231-4136-8de0-c2dcf905b703', '6456d6c3-ddb5-4719-b480-68c6f12638ab', 'concept', 'Lưu Ý Khi Thực Hiện Kỹ Thuật Chuyền Bóng', 'Đảm bảo rằng bạn có sự phối hợp tốt giữa tay và mắt.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh cầu thủ đang thực hiện chuyền bóng với tư thế đúng.", "videoScript": "Trong video này, chúng ta sẽ thảo luận về những lưu ý quan trọng khi thực hiện kỹ thuật chuyền bóng trong bóng rổ.", "imageDescription": "Cầu thủ có tư thế đứng vững, tập trung vào đồng đội, chuẩn bị chuyền bóng.", "videoDescription": "Những lưu ý khi thực hiện các kỹ thuật chuyền bóng trong bóng rổ."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:56:27.296225', '2026-01-27 12:56:27.296225', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cần chú ý đến tư thế và khoảng cách khi chuyền bóng.", "detailed": "Đảm bảo rằng bạn có sự phối hợp tốt giữa tay và mắt.", "comprehensive": "Khi thực hiện kỹ thuật chuyền bóng, cầu thủ cần chú ý đến nhiều yếu tố như tư thế đứng, khoảng cách đến đồng đội, cũng như thời gian và không gian. Sự phối hợp giữa tay và mắt là rất quan trọng để đảm bảo chuyền bóng chính xác. Ngoài ra, cầu thủ cũng cần quan sát tình huống xung quanh để đưa ra quyết định hợp lý."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a8a64b76-c126-4213-abef-b8b0e4254031', 'd36bd957-a3e0-4f13-8847-41e19dfcd6bf', 'concept', 'Lợi Ích Của Nước Đối Với Da Mụn', 'Cung cấp đủ nước giúp giảm tình trạng mụn và làm sạch da từ bên trong.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người trẻ tuổi với làn da sáng và khỏe mạnh.", "videoScript": "Trong video này, chúng ta sẽ khám phá cách nước có thể giúp giảm mụn và cải thiện tình trạng da của bạn. Đừng bỏ lỡ những mẹo hữu ích nhé!", "imageDescription": "Một người trẻ tuổi có làn da sạch mụn, đang cười tươi.", "videoDescription": "Video chia sẻ lợi ích của nước trong việc chăm sóc da mụn."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:11:35.285929', '2026-01-27 13:11:35.285929', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nước giúp giảm mụn hiệu quả.", "detailed": "Cung cấp đủ nước giúp giảm tình trạng mụn và làm sạch da từ bên trong.", "comprehensive": "Nước có vai trò quan trọng trong việc thanh lọc cơ thể, giúp loại bỏ độc tố và giảm tình trạng viêm nhiễm, từ đó giảm thiểu mụn trên da. Uống đủ nước hàng ngày không chỉ cải thiện sức khỏe tổng thể mà còn giúp làn da trở nên khỏe mạnh và sạch mụn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('02db5dfe-1feb-4f40-b204-9d0031ecf3dc', 'a396e2ac-ac31-48c9-bc1e-cafc2374cb47', 'concept', 'Tạo báo cáo trong Microsoft Project', 'Microsoft Project cho phép bạn tạo các báo cáo chi tiết về tiến độ, nguồn lực và ngân sách của dự án.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh báo cáo dự án trong Microsoft Project.", "videoScript": "Cuối cùng, chúng ta sẽ tìm hiểu cách tạo báo cáo trong Microsoft Project. Hãy xem cách tổng hợp thông tin và xuất báo cáo.", "imageDescription": "Mẫu báo cáo dự án hiển thị thông tin về tiến độ và nguồn lực.", "videoDescription": "Video hướng dẫn tạo báo cáo trong Microsoft Project, bao gồm cách tùy chỉnh và xuất báo cáo."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:37:01.61208', '2026-01-27 12:37:01.61208', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Báo cáo giúp bạn trình bày tiến độ và kết quả dự án.", "detailed": "Microsoft Project cho phép bạn tạo các báo cáo chi tiết về tiến độ, nguồn lực và ngân sách của dự án.", "comprehensive": "Tạo báo cáo trong Microsoft Project là một cách hiệu quả để tổng hợp và trình bày thông tin về dự án. Bạn có thể tạo các báo cáo tùy chỉnh về tiến độ, sử dụng nguồn lực, và ngân sách, giúp bạn có cái nhìn rõ ràng về tình hình dự án và đưa ra quyết định hợp lý. Các báo cáo này có thể xuất ra dưới nhiều định dạng khác nhau để chia sẻ với các bên liên quan."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('50e21e8d-ddf2-484f-9d99-b2b8852bc565', '655ee4c3-c649-4744-a97e-8d5d402940a9', 'concept', 'Khái Niệm Về Chiến Thuật Tấn Công', 'Chiến thuật tấn công bao gồm nhiều phương pháp mà đội bóng sử dụng để phá vỡ hàng phòng ngự của đối thủ và tạo ra cơ hội ghi điểm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một đội bóng rổ trong quá trình tấn công với các cầu thủ di chuyển và phối hợp.", "videoScript": "Đoạn video sẽ bắt đầu với việc giải thích các khái niệm cơ bản về chiến thuật tấn công, sau đó chuyển sang phân tích các tình huống thực tế trong trận đấu.", "imageDescription": "Hình ảnh một đội bóng rổ đang tấn công, với cầu thủ cầm bóng và các cầu thủ khác đang chạy vào vị trí.", "videoDescription": "Video phân tích chiến thuật tấn công trong bóng rổ, giới thiệu các kỹ thuật và chiến lược hiệu quả."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:56:50.582442', '2026-01-27 12:56:50.582442', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chiến thuật tấn công là cách mà đội bóng rổ tổ chức để ghi điểm.", "detailed": "Chiến thuật tấn công bao gồm nhiều phương pháp mà đội bóng sử dụng để phá vỡ hàng phòng ngự của đối thủ và tạo ra cơ hội ghi điểm.", "comprehensive": "Chiến thuật tấn công trong bóng rổ không chỉ đơn thuần là ghi điểm mà còn bao gồm việc di chuyển, phối hợp giữa các cầu thủ, và lựa chọn thời điểm tấn công để tối ưu hóa cơ hội ghi điểm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8c6ae22e-b21c-4d0d-b8b9-764f4bf3666d', '655ee4c3-c649-4744-a97e-8d5d402940a9', 'concept', 'Các Loại Hình Tấn Công', 'Các loại hình tấn công trong bóng rổ bao gồm tấn công nhanh, tấn công có tổ chức, và tấn công từ xa.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Sơ đồ các loại hình tấn công trong bóng rổ.", "videoScript": "Đoạn video sẽ giới thiệu về các loại hình tấn công, với các ví dụ minh họa từ các trận đấu nổi tiếng.", "imageDescription": "Hình ảnh sơ đồ minh họa các loại hình tấn công như tấn công nhanh và tấn công có tổ chức.", "videoDescription": "Video giải thích các loại hình tấn công trong bóng rổ với ví dụ thực tế."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:56:50.590412', '2026-01-27 12:56:50.590412', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại hình tấn công như tấn công nhanh, tấn công có tổ chức.", "detailed": "Các loại hình tấn công trong bóng rổ bao gồm tấn công nhanh, tấn công có tổ chức, và tấn công từ xa.", "comprehensive": "Tấn công nhanh thường được sử dụng để tận dụng khoảng trống trong phòng ngự của đối thủ, trong khi tấn công có tổ chức yêu cầu sự phối hợp chặt chẽ giữa các cầu thủ và sử dụng các chiến thuật như pick and roll."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2f2b3c31-ed70-43fc-97e5-289410509e96', 'd36bd957-a3e0-4f13-8847-41e19dfcd6bf', 'concept', 'Nước và Lão Hóa Da', 'Uống đủ nước giữ cho da luôn căng bóng, hạn chế nếp nhăn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một phụ nữ trung niên với làn da trẻ trung.", "videoScript": "Chào mừng bạn đến với video về cách nước có thể giúp ngăn ngừa lão hóa da. Hãy cùng tìm hiểu những bí quyết để duy trì vẻ đẹp trẻ trung nhé!", "imageDescription": "Một người phụ nữ trung niên với làn da mịn màng và khỏe mạnh, đang cười tươi.", "videoDescription": "Video chia sẻ về vai trò của nước trong việc chống lão hóa da."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:11:35.289288', '2026-01-27 13:11:35.289288', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nước giúp ngăn ngừa lão hóa da.", "detailed": "Uống đủ nước giữ cho da luôn căng bóng, hạn chế nếp nhăn.", "comprehensive": "Nước giữ vai trò quan trọng trong việc ngăn ngừa lão hóa da. Khi cơ thể thiếu nước, da sẽ trở nên khô, kém đàn hồi và dễ xuất hiện nếp nhăn. Việc cung cấp đủ nước và chăm sóc da đúng cách sẽ giúp duy trì độ ẩm, làm chậm quá trình lão hóa và giúp da luôn tươi trẻ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0e84a164-35d8-4526-878b-2d47969330ab', '92fa883e-8464-41dd-bbaf-0a6ef4877e28', 'concept', 'Giới thiệu về Báo cáo tiến độ dự án', 'Báo cáo tiến độ dự án là tài liệu tổng hợp các hoạt động đã thực hiện, tiến độ hiện tại và những khó khăn gặp phải trong quá trình thực hiện dự án.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng báo cáo tiến độ dự án với các cột thời gian và trạng thái.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về cách tạo một báo cáo tiến độ dự án hiệu quả và các yếu tố cần chú ý.", "imageDescription": "Bảng báo cáo tiến độ dự án trên màn hình máy tính với các biểu đồ và số liệu thể hiện tiến độ công việc.", "videoDescription": "Video hướng dẫn tạo báo cáo tiến độ dự án, bao gồm các bước và mẹo để đảm bảo thông tin được cung cấp một cách rõ ràng và chính xác."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:37:31.940353', '2026-01-27 12:37:31.940353', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Báo cáo tiến độ dự án giúp theo dõi sự phát triển của dự án.", "detailed": "Báo cáo tiến độ dự án là tài liệu tổng hợp các hoạt động đã thực hiện, tiến độ hiện tại và những khó khăn gặp phải trong quá trình thực hiện dự án.", "comprehensive": "Báo cáo tiến độ dự án không chỉ giúp quản lý dự án theo dõi tiến độ mà còn cung cấp thông tin cho các bên liên quan về tình hình thực hiện dự án, đánh giá rủi ro và đưa ra các biện pháp khắc phục kịp thời."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('afd8a46a-8991-47a1-b15e-932766daa723', '92fa883e-8464-41dd-bbaf-0a6ef4877e28', 'concept', 'Cấu trúc của một báo cáo tiến độ dự án', 'Cấu trúc báo cáo tiến độ thường bao gồm các phần: phần mở đầu, nội dung chi tiết về tiến độ, các vấn đề gặp phải và kết luận.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh cấu trúc báo cáo tiến độ dự án với các phần được đánh dấu rõ ràng.", "videoScript": "Hãy cùng tìm hiểu cấu trúc của một báo cáo tiến độ dự án và cách tổ chức thông tin một cách hợp lý.", "imageDescription": "Mô hình cấu trúc báo cáo tiến độ dự án với các phần như tiêu đề, nội dung và kết luận được phân chia rõ ràng.", "videoDescription": "Video giới thiệu cấu trúc của báo cáo tiến độ dự án và cách trình bày các thông tin một cách hiệu quả."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:37:31.948281', '2026-01-27 12:37:31.948281', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Báo cáo gồm tiêu đề, nội dung và kết luận.", "detailed": "Cấu trúc báo cáo tiến độ thường bao gồm các phần: phần mở đầu, nội dung chi tiết về tiến độ, các vấn đề gặp phải và kết luận.", "comprehensive": "Một báo cáo tiến độ hoàn chỉnh cần có các phần như tiêu đề, mục lục, phần tóm tắt, phân tích tiến độ theo từng giai đoạn, các biện pháp khắc phục và phần kết luận."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('02fe4b31-d0a5-45ee-898f-3c3c13946bd5', '655ee4c3-c649-4744-a97e-8d5d402940a9', 'concept', 'Chiến Thuật Pick and Roll', 'Chiến thuật pick and roll bao gồm một cầu thủ tạo ra một màn chắn để giúp cầu thủ cầm bóng có không gian tấn công.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa cho chiến thuật pick and roll trong bóng rổ.", "videoScript": "Đoạn video sẽ mô tả chi tiết cách thực hiện chiến thuật pick and roll và phân tích lợi ích của nó trong trận đấu.", "imageDescription": "Hình ảnh một cầu thủ thực hiện màn chắn cho cầu thủ cầm bóng trong chiến thuật pick and roll.", "videoDescription": "Video phân tích chiến thuật pick and roll, trình bày cách thực hiện và ví dụ từ các cầu thủ chuyên nghiệp."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:56:50.595876', '2026-01-27 12:56:50.595876', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Pick and roll là một chiến thuật phổ biến trong bóng rổ.", "detailed": "Chiến thuật pick and roll bao gồm một cầu thủ tạo ra một màn chắn để giúp cầu thủ cầm bóng có không gian tấn công.", "comprehensive": "Pick and roll không chỉ đơn thuần là một chiến thuật, mà còn là một phần quan trọng trong lối chơi của nhiều đội bóng, yêu cầu sự hiểu biết và phối hợp tốt giữa các cầu thủ để đạt hiệu quả cao nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b934382f-e556-468d-9d29-a425a8ad1cc4', '655ee4c3-c649-4744-a97e-8d5d402940a9', 'concept', 'Tấn Công Từ Xa', 'Tấn công từ xa thường được thực hiện bởi các cầu thủ có khả năng ném bóng tốt và cần phải có sự phối hợp với đồng đội để tạo ra khoảng trống.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Cầu thủ thực hiện cú ném bóng từ xa.", "videoScript": "Video sẽ trình bày kỹ thuật ném bóng từ xa, các yếu tố cần thiết để thực hiện thành công, và các ví dụ từ các cầu thủ nổi tiếng.", "imageDescription": "Hình ảnh cầu thủ ném bóng từ ngoài vòng 3 điểm trong một trận đấu.", "videoDescription": "Video hướng dẫn kỹ thuật tấn công từ xa trong bóng rổ, bao gồm phân tích chiến thuật và ví dụ thực tế."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:56:50.601616', '2026-01-27 12:56:50.601616', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tấn công từ xa là khi cầu thủ ném bóng từ xa để ghi điểm.", "detailed": "Tấn công từ xa thường được thực hiện bởi các cầu thủ có khả năng ném bóng tốt và cần phải có sự phối hợp với đồng đội để tạo ra khoảng trống.", "comprehensive": "Tấn công từ xa không chỉ bao gồm việc ném bóng từ ngoài vòng 3 điểm mà còn đòi hỏi cầu thủ phải biết lựa chọn thời điểm và vị trí thích hợp để tối ưu hóa khả năng ghi điểm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('75cebadd-b2f8-4da2-bcf1-b26c3a22807f', '655ee4c3-c649-4744-a97e-8d5d402940a9', 'concept', 'Phân Tích Chiến Thuật Tấn Công Của Đội Bóng Chuyên Nghiệp', 'Phân tích sẽ tập trung vào việc đội bóng chuyên nghiệp sử dụng các chiến thuật tấn công khác nhau để ghi điểm và đánh bại đối thủ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ chiến thuật tấn công của một đội bóng chuyên nghiệp.", "videoScript": "Video sẽ phân tích các chiến thuật tấn công của đội bóng chuyên nghiệp, với các ví dụ từ các trận đấu cụ thể.", "imageDescription": "Hình ảnh biểu đồ chiến thuật tấn công của một đội bóng chuyên nghiệp trong trận đấu.", "videoDescription": "Video phân tích chiến thuật tấn công của các đội bóng chuyên nghiệp, trình bày cách họ áp dụng các chiến thuật đó trong thực tế."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:56:50.607433', '2026-01-27 12:56:50.607433', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phân tích cách mà các đội bóng chuyên nghiệp thực hiện chiến thuật tấn công.", "detailed": "Phân tích sẽ tập trung vào việc đội bóng chuyên nghiệp sử dụng các chiến thuật tấn công khác nhau để ghi điểm và đánh bại đối thủ.", "comprehensive": "Phân tích sâu về cách mà các đội bóng chuyên nghiệp như Golden State Warriors hay Los Angeles Lakers thực hiện chiến thuật tấn công, từ việc lựa chọn cầu thủ đến cách sắp xếp vị trí trên sân."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('af4d0707-fc27-4ba8-b015-6ecbc8d6aef4', 'be67c408-3381-498d-9fb4-1939e30a70ba', 'concept', 'Giới Thiệu Chiến Thuật Phòng Thủ', 'Chiến thuật phòng thủ trong bóng rổ bao gồm nhiều phương pháp khác nhau nhằm ngăn cản đối thủ thực hiện những cú ném và tấn công hiệu quả.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Minh họa một đội bóng đang thực hiện chiến thuật phòng thủ.", "videoScript": "Video bắt đầu với cảnh các cầu thủ thực hiện chiến thuật phòng thủ. Giọng nói lồng ghép giải thích các nguyên tắc cơ bản của phòng thủ.", "imageDescription": "Một đội bóng đứng thành hàng ngang, với các cầu thủ tập trung vào đối thủ đang chuẩn bị ném bóng.", "videoDescription": "Video này trình bày các kiểu chiến thuật phòng thủ trong bóng rổ và các kỹ thuật để thực hiện chúng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:57:15.817024', '2026-01-27 12:57:15.817024', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chiến thuật phòng thủ trong bóng rổ là cách mà đội bóng ngăn chặn đối thủ ghi điểm.", "detailed": "Chiến thuật phòng thủ trong bóng rổ bao gồm nhiều phương pháp khác nhau nhằm ngăn cản đối thủ thực hiện những cú ném và tấn công hiệu quả.", "comprehensive": "Chiến thuật phòng thủ trong bóng rổ không chỉ đơn thuần là ngăn chặn đối thủ ghi điểm mà còn liên quan đến việc tổ chức đội hình, phối hợp giữa các cầu thủ và đọc được ý đồ tấn công của đối phương."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8be83018-811b-4737-89d8-8bc319e61415', 'be67c408-3381-498d-9fb4-1939e30a70ba', 'concept', 'Phòng Thủ 1:1', 'Phòng thủ 1:1 yêu cầu cầu thủ phải theo sát đối thủ của mình, dự đoán các động tác và phản ứng nhanh chóng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một cầu thủ phòng thủ đang theo sát đối thủ.", "videoScript": "Video giới thiệu các kỹ thuật phòng thủ 1:1, với các ví dụ thực tế từ các trận đấu.", "imageDescription": "Cảnh một cầu thủ phòng thủ đang trong tư thế sẵn sàng để ngăn chặn đối thủ.", "videoDescription": "Khám phá các chiến thuật và kỹ thuật của phòng thủ 1:1 trong bóng rổ."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:57:15.826327', '2026-01-27 12:57:15.826327', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phòng thủ 1:1 là khi mỗi cầu thủ bảo vệ một đối thủ.", "detailed": "Phòng thủ 1:1 yêu cầu cầu thủ phải theo sát đối thủ của mình, dự đoán các động tác và phản ứng nhanh chóng.", "comprehensive": "Phòng thủ 1:1 là một phương pháp phòng thủ cổ điển trong bóng rổ. Nó yêu cầu mỗi cầu thủ phải có kỹ năng cá nhân tốt, khả năng đọc tình huống và sự quyết đoán trong việc ngăn chặn các pha tấn công."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e6efe13d-cbba-49be-8b1c-a06fc28772ad', '92fa883e-8464-41dd-bbaf-0a6ef4877e28', 'concept', 'Các chỉ số quan trọng trong báo cáo tiến độ', 'Trong báo cáo tiến độ, các chỉ số như thời gian hoàn thành, ngân sách sử dụng và chất lượng thực hiện là rất quan trọng để đánh giá tình hình dự án.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ thể hiện các chỉ số tiến độ dự án.", "videoScript": "Trong video này, chúng ta sẽ khám phá các chỉ số quan trọng trong báo cáo tiến độ dự án và cách chúng ảnh hưởng đến quyết định quản lý.", "imageDescription": "Biểu đồ đường thể hiện tiến độ hoàn thành dự án theo thời gian, với các chỉ số ngân sách và chất lượng đi kèm.", "videoDescription": "Video phân tích các chỉ số quan trọng trong báo cáo tiến độ dự án và cách sử dụng chúng để đưa ra quyết định hợp lý."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:37:31.95214', '2026-01-27 12:37:31.95214', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chỉ số quan trọng như thời gian, ngân sách và chất lượng.", "detailed": "Trong báo cáo tiến độ, các chỉ số như thời gian hoàn thành, ngân sách sử dụng và chất lượng thực hiện là rất quan trọng để đánh giá tình hình dự án.", "comprehensive": "Các chỉ số quan trọng trong báo cáo tiến độ bao gồm tỷ lệ hoàn thành công việc, thời gian dự kiến so với thời gian thực tế, ngân sách đã sử dụng so với ngân sách ban đầu, và đánh giá chất lượng sản phẩm/dịch vụ đã cung cấp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('647b82db-aa81-4f83-b1a2-b89fec49af7a', '92fa883e-8464-41dd-bbaf-0a6ef4877e28', 'concept', 'Cách trình bày báo cáo tiến độ', 'Trình bày báo cáo tiến độ dự án nên sử dụng bảng, biểu đồ và văn bản rõ ràng để người đọc dễ dàng nắm bắt thông tin.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh báo cáo tiến độ dự án được trình bày chuyên nghiệp.", "videoScript": "Hãy cùng tìm hiểu cách trình bày báo cáo tiến độ để thu hút sự chú ý và đảm bảo thông tin được truyền đạt hiệu quả.", "imageDescription": "Mẫu báo cáo tiến độ dự án được trình bày đẹp mắt với các biểu đồ và bảng thông tin rõ ràng.", "videoDescription": "Video hướng dẫn cách trình bày báo cáo tiến độ dự án một cách chuyên nghiệp và dễ hiểu."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:37:31.95596', '2026-01-27 12:37:31.95596', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng bảng và biểu đồ để dễ hiểu.", "detailed": "Trình bày báo cáo tiến độ dự án nên sử dụng bảng, biểu đồ và văn bản rõ ràng để người đọc dễ dàng nắm bắt thông tin.", "comprehensive": "Một báo cáo tiến độ hiệu quả cần sử dụng các công cụ trình bày như bảng, biểu đồ hình tròn, biểu đồ cột và đồ thị để trực quan hóa số liệu, kết hợp với văn bản mô tả chi tiết để giải thích các thông tin quan trọng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ca1688ea-19ba-4edb-aa46-bbb318c302d3', '92fa883e-8464-41dd-bbaf-0a6ef4877e28', 'concept', 'Phân tích và đánh giá báo cáo tiến độ', 'Phân tích báo cáo tiến độ giúp đánh giá tình hình dự án và xác định các vấn đề cần khắc phục.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh phân tích báo cáo tiến độ với các ghi chú và đề xuất.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu cách phân tích và đánh giá một báo cáo tiến độ dự án để đưa ra quyết định quản lý hiệu quả.", "imageDescription": "Mô hình phân tích báo cáo tiến độ dự án với các ghi chú về vấn đề và các biện pháp đề xuất để cải thiện tình hình.", "videoDescription": "Video trình bày quy trình phân tích và đánh giá báo cáo tiến độ dự án, bao gồm các kỹ thuật và công cụ hữu ích."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:37:31.959594', '2026-01-27 12:37:31.959594', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phân tích để có biện pháp điều chỉnh.", "detailed": "Phân tích báo cáo tiến độ giúp đánh giá tình hình dự án và xác định các vấn đề cần khắc phục.", "comprehensive": "Việc phân tích và đánh giá báo cáo tiến độ không chỉ giúp nhận diện các vấn đề tiềm ẩn mà còn giúp đưa ra các biện pháp điều chỉnh kịp thời để đảm bảo dự án đi đúng hướng và đạt được mục tiêu đề ra."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('dbc683c7-dc57-4cb3-975f-aa102cc1975a', 'be67c408-3381-498d-9fb4-1939e30a70ba', 'concept', 'Phòng Thủ Zonal', 'Trong phòng thủ zonal, cầu thủ sẽ di chuyển để bảo vệ khu vực của mình, hỗ trợ lẫn nhau để ngăn chặn đối thủ tấn công.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Sơ đồ chiến thuật phòng thủ zonal.", "videoScript": "Video giải thích cách thức hoạt động của phòng thủ zonal, với các ví dụ từ các trận đấu chuyên nghiệp.", "imageDescription": "Một sơ đồ thể hiện cách các cầu thủ di chuyển để bảo vệ khu vực của mình trong phòng thủ zonal.", "videoDescription": "Tìm hiểu về chiến thuật phòng thủ zonal và cách áp dụng nó trong bóng rổ."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:57:15.830323', '2026-01-27 12:57:15.830323', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phòng thủ zonal là khi cầu thủ bảo vệ một khu vực thay vì một đối thủ cụ thể.", "detailed": "Trong phòng thủ zonal, cầu thủ sẽ di chuyển để bảo vệ khu vực của mình, hỗ trợ lẫn nhau để ngăn chặn đối thủ tấn công.", "comprehensive": "Phòng thủ zonal là một chiến thuật hiệu quả để đối phó với các đội bóng có khả năng tấn công mạnh. Nó yêu cầu sự phối hợp chặt chẽ giữa các cầu thủ và khả năng giao tiếp tốt."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2f15aaff-bad2-405a-9cc7-595068a55f02', 'be67c408-3381-498d-9fb4-1939e30a70ba', 'concept', 'Phòng Thủ Toàn Diện', 'Trong phòng thủ toàn diện, mỗi cầu thủ có trách nhiệm không chỉ ngăn chặn đối thủ mà còn hỗ trợ đồng đội trong các tình huống khẩn cấp.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Đội bóng thực hiện phòng thủ toàn diện.", "videoScript": "Video trình bày các kỹ thuật và chiến thuật trong phòng thủ toàn diện, với sự tham gia của toàn đội.", "imageDescription": "Cảnh các cầu thủ trong đội hình phòng thủ đang di chuyển và phối hợp với nhau để ngăn chặn đối thủ.", "videoDescription": "Khám phá cách thức hoạt động của phòng thủ toàn diện trong bóng rổ và lợi ích của nó."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:57:15.833611', '2026-01-27 12:57:15.833611', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phòng thủ toàn diện yêu cầu tất cả cầu thủ tham gia vào việc ngăn chặn đối thủ.", "detailed": "Trong phòng thủ toàn diện, mỗi cầu thủ có trách nhiệm không chỉ ngăn chặn đối thủ mà còn hỗ trợ đồng đội trong các tình huống khẩn cấp.", "comprehensive": "Phòng thủ toàn diện là một chiến thuật phức tạp nhưng hiệu quả, đòi hỏi sự phối hợp chặt chẽ giữa tất cả các thành viên trong đội, nhằm tạo ra một bức tường phòng thủ khó vượt qua cho đối thủ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d3dc3a84-e694-4987-adbe-f69b389d5b6c', 'be67c408-3381-498d-9fb4-1939e30a70ba', 'concept', 'Phân Tích Thống Kê Phòng Thủ', 'Các thông số như điểm ghi được, số lần mất bóng và tỷ lệ cản phá là những chỉ số quan trọng trong phân tích phòng thủ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ thống kê phòng thủ trong bóng rổ.", "videoScript": "Video phân tích các số liệu thống kê phòng thủ, giải thích cách các thông số ảnh hưởng đến chiến thuật và kết quả trận đấu.", "imageDescription": "Một biểu đồ hiển thị các số liệu thống kê về hiệu quả phòng thủ của một đội bóng trong một mùa giải.", "videoDescription": "Tìm hiểu cách phân tích thống kê có thể cải thiện hiệu quả phòng thủ trong bóng rổ."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:57:15.837336', '2026-01-27 12:57:15.837336', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phân tích thống kê giúp hiểu rõ hiệu quả phòng thủ của đội.", "detailed": "Các thông số như điểm ghi được, số lần mất bóng và tỷ lệ cản phá là những chỉ số quan trọng trong phân tích phòng thủ.", "comprehensive": "Phân tích thống kê phòng thủ không chỉ giúp đội bóng cải thiện chiến thuật mà còn giúp các cầu thủ cá nhân nâng cao kỹ năng và hiệu quả trên sân đấu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('239afce9-378b-467f-9119-32802571259b', '6fdf2752-f255-4c42-beb1-7425d8e2187c', 'concept', 'Tâm Lý Đầu Tư Dài Hạn', 'Tâm lý đầu tư dài hạn yêu cầu sự kiên nhẫn, kỷ luật và khả năng vượt qua cảm xúc trong các giai đoạn thị trường biến động.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang thiền định, thể hiện sự bình tĩnh và kiên nhẫn.", "videoScript": "Trong video này, chúng ta sẽ khám phá tâm lý đầu tư dài hạn và cách để duy trì sự kiên nhẫn trong quá trình đầu tư.", "imageDescription": "Hình ảnh một người ngồi thiền, thể hiện tâm lý bình tĩnh và kiên nhẫn trong đầu tư.", "videoDescription": "Video thảo luận về tầm quan trọng của tâm lý trong đầu tư dài hạn và cách để quản lý cảm xúc."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:02:19.206598', '2026-01-27 13:02:19.206598', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tâm lý đầu tư dài hạn là sự kiên nhẫn và kỷ luật.", "detailed": "Tâm lý đầu tư dài hạn yêu cầu sự kiên nhẫn, kỷ luật và khả năng vượt qua cảm xúc trong các giai đoạn thị trường biến động.", "comprehensive": "Đầu tư dài hạn không chỉ là một chiến lược tài chính mà còn là một hành trình tâm lý. Các nhà đầu tư cần phải phát triển sự kiên nhẫn, kỷ luật và khả năng quản lý cảm xúc của mình để không bị ảnh hưởng bởi các biến động ngắn hạn của thị trường. Việc giữ vững kế hoạch đầu tư và không bị cuốn theo những cơn sốt hay sự sụp đổ tạm thời là rất quan trọng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a7ae1101-c3a5-4c0b-976c-dc7d87a5cf32', 'd423d2c7-b79a-4eef-a960-87fd4b2784a4', 'concept', 'Giới Thiệu về Biểu Đồ Tròn', 'Biểu đồ tròn được sử dụng để minh họa tỷ lệ phần trăm của các phần trong một tổng thể. Mỗi phần của biểu đồ thể hiện một phần của tổng thể và được thể hiện bằng một mảnh ghép của hình tròn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Vẽ một biểu đồ tròn với ba phần: A (50%), B (30%), C (20%).", "videoScript": "Chào mừng các bạn đến với bài học về biểu đồ tròn. Hôm nay, chúng ta sẽ cùng tìm hiểu cách tạo ra một biểu đồ tròn đơn giản và ý nghĩa của nó trong việc phân tích dữ liệu.", "imageDescription": "Hình ảnh biểu đồ tròn với ba phần màu sắc khác nhau thể hiện tỷ lệ phần trăm của A, B và C.", "videoDescription": "Video hướng dẫn cách tạo biểu đồ tròn trong Excel, với ví dụ minh họa và các bước thực hiện cụ thể."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:38:08.617855', '2026-01-27 12:38:08.617855', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ tròn là một công cụ trực quan giúp thể hiện tỷ lệ phần trăm của các thành phần trong một tổng thể.", "detailed": "Biểu đồ tròn được sử dụng để minh họa tỷ lệ phần trăm của các phần trong một tổng thể. Mỗi phần của biểu đồ thể hiện một phần của tổng thể và được thể hiện bằng một mảnh ghép của hình tròn.", "comprehensive": "Biểu đồ tròn là một dạng đồ thị hình tròn được chia thành các phần để thể hiện tỷ lệ phần trăm của các thành phần trong một tập hợp. Việc sử dụng biểu đồ tròn giúp người xem dễ dàng nhận ra sự phân bố của các thành phần và so sánh chúng với nhau."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3b7c0e5b-188e-4c51-bb39-3eb5cc73adc8', '66f15918-baf4-4eb8-a780-da5d7b570c8f', 'concept', 'Khởi đầu với Lập kế hoạch Tập luyện', 'Để bắt đầu, bạn cần xác định mục tiêu tập luyện của mình và lên kế hoạch cho từng buổi tập cụ thể.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cầu thủ bóng rổ đang viết kế hoạch tập luyện trên bảng trắng.", "videoScript": "Mở đầu với một cầu thủ bóng rổ giới thiệu về tầm quan trọng của việc lập kế hoạch tập luyện, sau đó chia sẻ các bước cụ thể để tạo ra một kế hoạch hiệu quả.", "imageDescription": "Một cầu thủ bóng rổ đang cẩn thận lập kế hoạch tập luyện của mình trên bảng trắng, với các mục tiêu và lịch trình rõ ràng.", "videoDescription": "Video hướng dẫn cách lập kế hoạch tập luyện cho cầu thủ bóng rổ, với các ví dụ cụ thể và lời khuyên từ chuyên gia."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:57:44.022271', '2026-01-27 12:57:44.022271', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lập kế hoạch tập luyện là bước đầu tiên để trở thành cầu thủ bóng rổ giỏi.", "detailed": "Để bắt đầu, bạn cần xác định mục tiêu tập luyện của mình và lên kế hoạch cho từng buổi tập cụ thể.", "comprehensive": "Lập kế hoạch tập luyện bao gồm việc phân tích kỹ năng hiện tại, xác định mục tiêu cụ thể, và thiết lập lịch trình tập luyện hàng tuần. Bạn cũng nên xem xét việc tích hợp các bài tập thể lực bổ trợ như chạy, nhảy và rèn luyện sức mạnh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('52e2b6cb-f310-47be-8f60-5ae3237ae12e', '66f15918-baf4-4eb8-a780-da5d7b570c8f', 'concept', 'Xác định Mục tiêu Tập luyện', 'Bạn cần xác định mục tiêu ngắn hạn và dài hạn, như cải thiện sức mạnh, tốc độ, hoặc kỹ năng chơi bóng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng ghi chú với các mục tiêu tập luyện cho bóng rổ.", "videoScript": "Một cầu thủ bóng rổ chia sẻ về cách xác định mục tiêu cá nhân và lý do tại sao nó quan trọng trong quá trình tập luyện.", "imageDescription": "Một bảng ghi chú đầy màu sắc với các mục tiêu tập luyện được ghi rõ ràng, ví dụ như ''Nâng cao sức mạnh cơ bắp'' và ''Cải thiện kỹ năng ném bóng''.", "videoDescription": "Video giải thích cách xác định mục tiêu tập luyện cho cầu thủ bóng rổ, với sự tham gia của các chuyên gia thể thao."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:57:44.030565', '2026-01-27 12:57:44.030565', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mục tiêu rõ ràng giúp bạn tập trung hơn vào việc luyện tập.", "detailed": "Bạn cần xác định mục tiêu ngắn hạn và dài hạn, như cải thiện sức mạnh, tốc độ, hoặc kỹ năng chơi bóng.", "comprehensive": "Mục tiêu tập luyện nên được chia thành các loại: mục tiêu thể lực (như nâng cao sức bền và sức mạnh), mục tiêu kỹ thuật (như cải thiện kỹ năng ném bóng), và mục tiêu chiến thuật (như hiểu rõ hơn về chiến thuật thi đấu)."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('84c26ab2-b8bd-4c88-8120-4663656d1692', '66f15918-baf4-4eb8-a780-da5d7b570c8f', 'concept', 'Lịch trình Tập luyện Hiệu quả', 'Một lịch trình tập luyện nên bao gồm các buổi tập thể lực, kỹ năng và nghỉ ngơi hợp lý.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một lịch trình tập luyện cho một tuần.", "videoScript": "Chia sẻ về cách xây dựng một lịch trình tập luyện hiệu quả, bao gồm ví dụ thực tế và mẹo từ các huấn luyện viên.", "imageDescription": "Một lịch trình tập luyện chi tiết cho một tuần với các mục tiêu cụ thể cho từng ngày và thời gian dành cho từng hoạt động.", "videoDescription": "Video hướng dẫn cách lập lịch trình tập luyện cho bóng rổ, với các chiến lược và mẹo hữu ích từ những người có kinh nghiệm."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:57:44.035093', '2026-01-27 12:57:44.035093', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lên lịch tập luyện giúp bạn tổ chức thời gian tốt hơn.", "detailed": "Một lịch trình tập luyện nên bao gồm các buổi tập thể lực, kỹ năng và nghỉ ngơi hợp lý.", "comprehensive": "Lịch trình tập luyện cần được thiết kế để đảm bảo sự đa dạng trong các bài tập, bao gồm cả các buổi tập nâng cao sức mạnh, kỹ thuật ném và các hoạt động phục hồi. Bạn cũng nên để ý đến thời gian hồi phục giữa các buổi tập."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('600a76ed-2884-457c-a755-e7fdce3cebd2', 'd423d2c7-b79a-4eef-a960-87fd4b2784a4', 'concept', 'Cách Tạo Biểu Đồ Tròn Trong Excel', 'Để tạo biểu đồ tròn trong Excel, bạn cần mở tệp Excel, chọn vùng dữ liệu cần hiển thị, sau đó vào tab Insert, chọn biểu đồ tròn và chọn kiểu biểu đồ bạn muốn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giao diện Excel với dữ liệu và menu tạo biểu đồ tròn.", "videoScript": "Trong video này, tôi sẽ hướng dẫn các bạn cách tạo biểu đồ tròn trong Excel một cách dễ dàng. Hãy làm theo các bước sau.", "imageDescription": "Giao diện Excel với dữ liệu đã được chọn và menu tạo biểu đồ tròn được mở.", "videoDescription": "Video hướng dẫn chi tiết cách tạo biểu đồ tròn trong Excel, bao gồm các bước từ nhập dữ liệu đến hoàn thành biểu đồ."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:38:08.62628', '2026-01-27 12:38:08.62628', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mở Excel, chọn dữ liệu, và chọn biểu đồ tròn từ tab Insert.", "detailed": "Để tạo biểu đồ tròn trong Excel, bạn cần mở tệp Excel, chọn vùng dữ liệu cần hiển thị, sau đó vào tab Insert, chọn biểu đồ tròn và chọn kiểu biểu đồ bạn muốn.", "comprehensive": "Đầu tiên, mở Excel và nhập dữ liệu bạn muốn hiển thị dưới dạng biểu đồ tròn. Sau đó, chọn vùng dữ liệu đó, vào tab Insert trên thanh công cụ, chọn biểu đồ tròn và chọn kiểu biểu đồ bạn muốn sử dụng. Bạn có thể tùy chỉnh biểu đồ theo sở thích."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2a42622e-c159-49df-aaea-f6275c5fa369', '66f15918-baf4-4eb8-a780-da5d7b570c8f', 'concept', 'Tập luyện Thể lực và Kỹ năng', 'Các buổi tập nên bao gồm cả bài tập thể lực như chạy, nhảy và bài tập nâng cao kỹ năng chơi bóng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cầu thủ bóng rổ đang tập luyện kỹ năng ném bóng và chạy.", "videoScript": "Hướng dẫn về cách kết hợp giữa tập luyện thể lực và kỹ năng, với các bài tập mẫu và giải thích từ huấn luyện viên.", "imageDescription": "Một cầu thủ bóng rổ thực hiện các bài tập thể lực kết hợp với ném bóng, thể hiện sự nỗ lực và quyết tâm.", "videoDescription": "Video giới thiệu cách tập luyện thể lực và kỹ năng cho cầu thủ bóng rổ, với các bài tập cụ thể và hướng dẫn chi tiết."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:57:44.041727', '2026-01-27 12:57:44.041727', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Kết hợp tập luyện thể lực và kỹ năng để cải thiện hiệu suất.", "detailed": "Các buổi tập nên bao gồm cả bài tập thể lực như chạy, nhảy và bài tập nâng cao kỹ năng chơi bóng.", "comprehensive": "Tập luyện thể lực và kỹ năng cần được kết hợp một cách hợp lý. Bạn có thể dành một phần buổi tập cho thể lực, như các bài tập cardio và sức mạnh, sau đó chuyển sang các bài tập kỹ năng như ném bóng, dribbling và phòng ngự."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('728e5263-4a8a-4ce3-9c1e-c406c9877c68', '66f15918-baf4-4eb8-a780-da5d7b570c8f', 'concept', 'Đánh giá và Điều chỉnh Kế hoạch Tập luyện', 'Sau một thời gian luyện tập, bạn nên đánh giá tiến độ và điều chỉnh kế hoạch cho phù hợp.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cầu thủ bóng rổ đang xem xét và điều chỉnh kế hoạch tập luyện.", "videoScript": "Cầu thủ chia sẻ về quá trình đánh giá kế hoạch tập luyện của mình, cách họ nhận biết sự tiến bộ và điều chỉnh để đạt kết quả tốt hơn.", "imageDescription": "Một cầu thủ bóng rổ xem xét kế hoạch tập luyện của mình, ghi chú lại các điểm cần cải thiện và điều chỉnh mục tiêu.", "videoDescription": "Video hướng dẫn cách đánh giá và điều chỉnh kế hoạch tập luyện cho bóng rổ, với các mẹo từ chuyên gia."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:57:44.045953', '2026-01-27 12:57:44.045953', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đánh giá kế hoạch giúp bạn cải thiện quá trình tập luyện.", "detailed": "Sau một thời gian luyện tập, bạn nên đánh giá tiến độ và điều chỉnh kế hoạch cho phù hợp.", "comprehensive": "Việc đánh giá và điều chỉnh kế hoạch tập luyện là rất quan trọng. Bạn cần xem xét những gì đã đạt được, những gì cần cải thiện, và điều chỉnh các mục tiêu cũng như lịch trình tập luyện cho phù hợp với sự tiến bộ của bản thân."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('83004310-7e89-48f9-98a0-4532432aaa64', 'd423d2c7-b79a-4eef-a960-87fd4b2784a4', 'concept', 'Tùy Chỉnh Biểu Đồ Tròn', 'Trong Excel, bạn có thể tùy chỉnh biểu đồ tròn bằng cách thay đổi màu sắc, thêm nhãn và điều chỉnh kích thước. Điều này giúp biểu đồ trở nên trực quan hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ tròn với các phần được tô màu khác nhau và có nhãn.", "videoScript": "Bây giờ, chúng ta sẽ cùng nhau tìm hiểu cách tùy chỉnh biểu đồ tròn để nó trở nên hấp dẫn hơn. Hãy xem các bước sau đây.", "imageDescription": "Biểu đồ tròn đã được tùy chỉnh với màu sắc và nhãn rõ ràng cho từng phần.", "videoDescription": "Video hướng dẫn cách tùy chỉnh biểu đồ tròn trong Excel, bao gồm cách thay đổi màu sắc, thêm nhãn và điều chỉnh kích thước."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:38:08.629763', '2026-01-27 12:38:08.629763', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể thay đổi màu sắc và kiểu dáng của biểu đồ tròn.", "detailed": "Trong Excel, bạn có thể tùy chỉnh biểu đồ tròn bằng cách thay đổi màu sắc, thêm nhãn và điều chỉnh kích thước. Điều này giúp biểu đồ trở nên trực quan hơn.", "comprehensive": "Biểu đồ tròn trong Excel cho phép bạn tùy chỉnh nhiều yếu tố như màu sắc, kiểu dáng, kích thước, và các nhãn. Để làm điều này, bạn chỉ cần nhấp chuột phải vào biểu đồ và chọn các tùy chọn cần thiết từ menu ngữ cảnh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ecea9107-f829-4778-8a44-0303fe1b73a5', '9407f18d-e805-4ef8-b981-059b21396348', 'concept', 'Giới thiệu về Đánh giá Rủi ro Đầu tư', 'Rủi ro đầu tư xuất hiện khi có sự không chắc chắn về lợi nhuận trong tương lai. Đánh giá rủi ro giúp nhà đầu tư đưa ra quyết định thông minh hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhà đầu tư đang phân tích dữ liệu tài chính.", "videoScript": "Chào mừng đến với khóa học về đánh giá rủi ro đầu tư. Trong bài học này, chúng ta sẽ khám phá rủi ro là gì và tại sao nó lại quan trọng trong quyết định đầu tư.", "imageDescription": "Một người đứng trước màn hình máy tính với biểu đồ tài chính và ghi chú, thể hiện sự tập trung vào việc phân tích rủi ro.", "videoDescription": "Video giới thiệu về các khái niệm liên quan đến rủi ro đầu tư và tầm quan trọng của việc đánh giá rủi ro."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:02:55.277921', '2026-01-27 13:02:55.277921', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Rủi ro đầu tư là khả năng mất mát tài chính. Hãy tìm hiểu cách đánh giá và quản lý nó.", "detailed": "Rủi ro đầu tư xuất hiện khi có sự không chắc chắn về lợi nhuận trong tương lai. Đánh giá rủi ro giúp nhà đầu tư đưa ra quyết định thông minh hơn.", "comprehensive": "Đánh giá rủi ro đầu tư là một phần quan trọng trong quản lý tài chính cá nhân. Bài học này sẽ giới thiệu các khái niệm cơ bản về rủi ro, các loại rủi ro và cách xác định mức độ rủi ro của các khoản đầu tư khác nhau."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5f9db9ad-1d34-44c5-81c5-3b0805a50011', '9407f18d-e805-4ef8-b981-059b21396348', 'concept', 'Các loại Rủi ro trong Đầu tư', 'Rủi ro thị trường, rủi ro tín dụng và rủi ro thanh khoản là ba loại rủi ro chính mà nhà đầu tư cần phải xem xét. Mỗi loại rủi ro có đặc điểm và cách đánh giá riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ thể hiện các loại rủi ro khác nhau.", "videoScript": "Bây giờ, chúng ta sẽ tìm hiểu về các loại rủi ro trong đầu tư và cách mà mỗi loại có thể ảnh hưởng đến danh mục đầu tư của bạn.", "imageDescription": "Một biểu đồ thể hiện các loại rủi ro trong đầu tư với các mũi tên chỉ ra các yếu tố ảnh hưởng.", "videoDescription": "Video giải thích các loại rủi ro trong đầu tư và tầm quan trọng của việc nhận biết chúng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:02:55.286666', '2026-01-27 13:02:55.286666', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại rủi ro như rủi ro thị trường, rủi ro tín dụng và rủi ro thanh khoản.", "detailed": "Rủi ro thị trường, rủi ro tín dụng và rủi ro thanh khoản là ba loại rủi ro chính mà nhà đầu tư cần phải xem xét. Mỗi loại rủi ro có đặc điểm và cách đánh giá riêng.", "comprehensive": "Trong bài học này, chúng ta sẽ đi sâu vào các loại rủi ro trong đầu tư, bao gồm rủi ro thị trường, rủi ro tín dụng, rủi ro thanh khoản, và những yếu tố ảnh hưởng đến chúng. Chúng ta cũng sẽ thảo luận về cách nhận diện và quản lý từng loại rủi ro."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a37607ad-acc9-436c-8e6b-82d79e40035b', '9407f18d-e805-4ef8-b981-059b21396348', 'concept', 'Phân tích Rủi ro: Các Công cụ và Kỹ thuật', 'Các công cụ như phân tích SWOT và giá trị rủi ro (VAR) giúp nhà đầu tư đánh giá rủi ro hiệu quả. Những công cụ này cung cấp cái nhìn sâu sắc về mức độ rủi ro của các khoản đầu tư.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhà phân tích đang sử dụng phần mềm phân tích dữ liệu.", "videoScript": "Trong phần này, chúng ta sẽ xem xét các công cụ phân tích rủi ro và cách sử dụng chúng để đánh giá rủi ro trong đầu tư.", "imageDescription": "Một nhà phân tích tài chính đang làm việc với phần mềm phân tích dữ liệu trên máy tính, với biểu đồ và số liệu xung quanh.", "videoDescription": "Video hướng dẫn sử dụng các công cụ phân tích rủi ro trong đầu tư và cách áp dụng chúng vào quyết định đầu tư."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:02:55.290205', '2026-01-27 13:02:55.290205', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng các công cụ như phân tích SWOT, VAR để đánh giá rủi ro.", "detailed": "Các công cụ như phân tích SWOT và giá trị rủi ro (VAR) giúp nhà đầu tư đánh giá rủi ro hiệu quả. Những công cụ này cung cấp cái nhìn sâu sắc về mức độ rủi ro của các khoản đầu tư.", "comprehensive": "Trong bài học này, chúng ta sẽ tìm hiểu về các công cụ và kỹ thuật phân tích rủi ro, bao gồm phân tích SWOT, VAR, và các mô hình định lượng khác. Chúng ta cũng sẽ thảo luận về cách áp dụng những công cụ này vào thực tế."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('49563011-2c5e-4ceb-aabd-b55458b27127', 'd423d2c7-b79a-4eef-a960-87fd4b2784a4', 'concept', 'Phân Tích Dữ Liệu Qua Biểu Đồ Tròn', 'Sử dụng biểu đồ tròn, bạn có thể nhanh chóng nhận ra sự phân bổ và tỷ lệ giữa các phần trong dữ liệu, từ đó đưa ra quyết định hợp lý hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ tròn với tỷ lệ phần trăm rõ ràng và ghi chú phân tích.", "videoScript": "Trong phần này, chúng ta sẽ tìm hiểu cách phân tích dữ liệu thông qua biểu đồ tròn. Hãy xem cách mà các phần của biểu đồ có thể cho bạn thông tin quý giá.", "imageDescription": "Biểu đồ tròn với tỷ lệ phần trăm rõ ràng, kèm theo ghi chú phân tích dữ liệu.", "videoDescription": "Video giải thích cách sử dụng biểu đồ tròn để phân tích dữ liệu, với ví dụ cụ thể và hướng dẫn chi tiết."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:38:08.632963', '2026-01-27 12:38:08.632963', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ tròn giúp bạn hiểu rõ hơn về tỷ lệ các phần trong dữ liệu.", "detailed": "Sử dụng biểu đồ tròn, bạn có thể nhanh chóng nhận ra sự phân bổ và tỷ lệ giữa các phần trong dữ liệu, từ đó đưa ra quyết định hợp lý hơn.", "comprehensive": "Biểu đồ tròn không chỉ là một công cụ trực quan, mà còn là một phương pháp hiệu quả để phân tích dữ liệu. Bằng cách nhìn vào tỷ lệ các phần trong biểu đồ, bạn có thể đưa ra những nhận định và quyết định dựa trên sự phân bổ của các thành phần."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6f174601-03b5-4046-b6fd-93db1c47b9e3', 'd423d2c7-b79a-4eef-a960-87fd4b2784a4', 'concept', 'Kết Luận và Ứng Dụng Biểu Đồ Tròn', 'Biểu đồ tròn giúp người dùng trình bày dữ liệu một cách trực quan và dễ hiểu. Nó có thể được ứng dụng trong nhiều lĩnh vực như kinh doanh, giáo dục và nghiên cứu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ tròn được sử dụng trong một báo cáo.", "videoScript": "Cuối cùng, chúng ta sẽ kết luận về vai trò của biểu đồ tròn trong việc trình bày và phân tích dữ liệu. Hãy cùng khám phá những ứng dụng thực tiễn của nó.", "imageDescription": "Một báo cáo có chứa biểu đồ tròn minh họa cho các phần dữ liệu khác nhau.", "videoDescription": "Video tóm tắt và kết luận về ứng dụng của biểu đồ tròn trong các lĩnh vực khác nhau như kinh doanh và giáo dục."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:38:08.637863', '2026-01-27 12:38:08.637863', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ tròn là công cụ hữu ích trong việc trình bày dữ liệu.", "detailed": "Biểu đồ tròn giúp người dùng trình bày dữ liệu một cách trực quan và dễ hiểu. Nó có thể được ứng dụng trong nhiều lĩnh vực như kinh doanh, giáo dục và nghiên cứu.", "comprehensive": "Kết luận, biểu đồ tròn không chỉ là một công cụ trình bày dữ liệu hiệu quả mà còn là một phần quan trọng trong việc phân tích và ra quyết định. Việc sử dụng biểu đồ tròn trong các báo cáo và thuyết trình có thể nâng cao khả năng truyền đạt thông tin của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('be61b83f-79ba-4511-8114-2346d8fdb9b4', '9407f18d-e805-4ef8-b981-059b21396348', 'concept', 'Quản lý Rủi ro Đầu tư', 'Quản lý rủi ro bao gồm việc xác định, phân tích và giảm thiểu rủi ro. Một chiến lược quản lý rủi ro hiệu quả là rất quan trọng để bảo vệ tài sản của bạn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một danh mục đầu tư đa dạng.", "videoScript": "Bây giờ, hãy cùng khám phá cách quản lý rủi ro đầu tư để bảo vệ tài sản của bạn và tối ưu hóa lợi nhuận.", "imageDescription": "Một biểu đồ mô tả danh mục đầu tư đa dạng với nhiều loại tài sản khác nhau như cổ phiếu, trái phiếu và bất động sản.", "videoDescription": "Video giải thích các chiến lược quản lý rủi ro trong đầu tư và cách áp dụng chúng vào thực tế."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:02:55.293388', '2026-01-27 13:02:55.293388', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý rủi ro giúp giảm thiểu tổn thất trong đầu tư.", "detailed": "Quản lý rủi ro bao gồm việc xác định, phân tích và giảm thiểu rủi ro. Một chiến lược quản lý rủi ro hiệu quả là rất quan trọng để bảo vệ tài sản của bạn.", "comprehensive": "Bài học này sẽ tập trung vào các chiến lược quản lý rủi ro đầu tư, bao gồm đa dạng hóa danh mục đầu tư, sử dụng bảo hiểm và các công cụ tài chính khác. Chúng ta sẽ thảo luận về cách thực hiện một kế hoạch quản lý rủi ro hiệu quả."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('56db72f3-869d-4c8d-99fb-78ffc6a020bc', '70fd8ebf-4ad8-4041-a696-1e04d14e3ff5', 'concept', 'Khởi Động và Giãn Cơ', 'Khởi động giúp tăng cường tuần hoàn máu, giảm nguy cơ chấn thương và cải thiện hiệu suất tập luyện.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cầu thủ bóng rổ đang khởi động và giãn cơ trước khi tập.", "videoScript": "Trong video này, chúng tôi sẽ hướng dẫn bạn các bài tập khởi động hiệu quả trước khi tập luyện bóng rổ.", "imageDescription": "Một cầu thủ bóng rổ đứng trên sân, thực hiện các động tác giãn cơ với bóng.", "videoDescription": "Video hướng dẫn khởi động và giãn cơ cho cầu thủ bóng rổ."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:05.134372', '2026-01-27 12:58:05.134372', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khởi động trước khi tập luyện giúp cơ thể chuẩn bị tốt hơn.", "detailed": "Khởi động giúp tăng cường tuần hoàn máu, giảm nguy cơ chấn thương và cải thiện hiệu suất tập luyện.", "comprehensive": "Khởi động bao gồm các bài tập như chạy bộ nhẹ, giãn cơ và các động tác khởi động khớp. Điều này giúp làm nóng cơ bắp, tăng cường độ linh hoạt và tạo điều kiện cho sự phục hồi tốt hơn sau tập luyện."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c0d26217-a2e6-4964-90bd-1d9af82b1bce', '70fd8ebf-4ad8-4041-a696-1e04d14e3ff5', 'concept', 'Chế Độ Dinh Dưỡng Sau Tập', 'Cần bổ sung protein và carbohydrate để phục hồi cơ bắp và nạp lại năng lượng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bữa ăn nhẹ sau tập luyện với các thực phẩm giàu protein và carbohydrate.", "videoScript": "Chúng tôi sẽ chỉ cho bạn những thực phẩm tốt nhất để phục hồi sau khi tập luyện bóng rổ.", "imageDescription": "Một bữa ăn nhẹ bao gồm sữa chua, chuối và bánh mì nguyên cám.", "videoDescription": "Hướng dẫn về chế độ dinh dưỡng sau khi tập luyện cho cầu thủ bóng rổ."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:05.141505', '2026-01-27 12:58:05.141505', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Ăn uống đúng cách sau khi tập luyện rất quan trọng.", "detailed": "Cần bổ sung protein và carbohydrate để phục hồi cơ bắp và nạp lại năng lượng.", "comprehensive": "Sau khi tập luyện, cơ thể cần protein để sửa chữa cơ bắp và carbohydrate để phục hồi năng lượng. Một bữa ăn nhẹ với protein như sữa chua hoặc trứng, kết hợp với carbohydrate như chuối hoặc bánh mì nguyên cám là lựa chọn lý tưởng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ee29ba38-1bad-4d87-9727-ac054240cbd2', '70fd8ebf-4ad8-4041-a696-1e04d14e3ff5', 'concept', 'Giấc Ngủ và Phục Hồi', 'Ngủ đủ giấc giúp cơ thể tái tạo và phục hồi tốt hơn sau tập luyện.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cầu thủ bóng rổ đang ngủ ngon trên giường.", "videoScript": "Trong video này, chúng tôi sẽ thảo luận về tầm quan trọng của giấc ngủ trong phục hồi sau tập luyện.", "imageDescription": "Một cầu thủ bóng rổ nằm trên giường, đang ngủ say.", "videoDescription": "Tìm hiểu về ảnh hưởng của giấc ngủ đến phục hồi cho cầu thủ bóng rổ."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:05.144918', '2026-01-27 12:58:05.144918', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giấc ngủ là yếu tố quan trọng trong phục hồi.", "detailed": "Ngủ đủ giấc giúp cơ thể tái tạo và phục hồi tốt hơn sau tập luyện.", "comprehensive": "Giấc ngủ đóng vai trò quan trọng trong quá trình phục hồi của cơ thể. Nó giúp tái tạo mô cơ và phục hồi năng lượng. Nên cố gắng ngủ từ 7 đến 9 giờ mỗi đêm để đảm bảo cơ thể có đủ thời gian phục hồi."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cdc3eb63-c812-4173-a3f9-aa035c277fc1', '6b8cb42a-c31e-4233-aa9e-958c76f23089', 'concept', 'Khái Niệm Về Cỡ Chữ', 'Cỡ chữ quyết định độ lớn của văn bản và ảnh hưởng đến khả năng đọc hiểu của người dùng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các cỡ chữ khác nhau trong tài liệu văn phòng.", "videoScript": "Xin chào các bạn! Hôm nay, chúng ta sẽ cùng tìm hiểu về cỡ chữ trong tài liệu văn phòng. Cỡ chữ có vai trò rất quan trọng trong việc tạo ra sự hấp dẫn cho văn bản.", "imageDescription": "Một bảng so sánh giữa các cỡ chữ từ nhỏ đến lớn, hiển thị cách mà cỡ chữ ảnh hưởng đến văn bản.", "videoDescription": "Video giới thiệu về cỡ chữ, cách chọn cỡ chữ phù hợp cho từng loại tài liệu."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:38:33.523946', '2026-01-27 12:38:33.523946', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cỡ chữ là kích thước của chữ viết trong tài liệu.", "detailed": "Cỡ chữ quyết định độ lớn của văn bản và ảnh hưởng đến khả năng đọc hiểu của người dùng.", "comprehensive": "Cỡ chữ không chỉ là kích thước mà còn liên quan đến kiểu dáng và khoảng cách giữa các ký tự. Việc chọn cỡ chữ phù hợp giúp tài liệu trở nên dễ đọc và thu hút sự chú ý của người xem."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('21b939b5-a2f5-499f-9b86-0e7af26fe344', '9407f18d-e805-4ef8-b981-059b21396348', 'concept', 'Đánh giá Rủi ro và Quyết định Đầu tư', 'Quyết định đầu tư nên dựa trên một đánh giá rủi ro kỹ lưỡng. Việc hiểu rõ rủi ro sẽ giúp bạn chọn lựa các khoản đầu tư phù hợp với mục tiêu tài chính của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhà đầu tư đang xem xét các lựa chọn đầu tư.", "videoScript": "Cuối cùng, chúng ta sẽ kết hợp tất cả những gì đã học và xem xét cách mà đánh giá rủi ro có thể giúp bạn đưa ra quyết định đầu tư thông minh hơn.", "imageDescription": "Một nhà đầu tư đang ngồi trước bàn làm việc với tài liệu và biểu đồ, thể hiện sự suy nghĩ và phân tích.", "videoDescription": "Video tổng kết các kiến thức về đánh giá rủi ro và cách áp dụng vào quyết định đầu tư."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:02:55.296675', '2026-01-27 13:02:55.296675', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đánh giá rủi ro giúp đưa ra quyết định đầu tư thông minh.", "detailed": "Quyết định đầu tư nên dựa trên một đánh giá rủi ro kỹ lưỡng. Việc hiểu rõ rủi ro sẽ giúp bạn chọn lựa các khoản đầu tư phù hợp với mục tiêu tài chính của mình.", "comprehensive": "Trong bài học cuối cùng này, chúng ta sẽ thảo luận về mối liên hệ giữa đánh giá rủi ro và quyết định đầu tư. Chúng ta sẽ xem xét cách các nhà đầu tư thành công sử dụng đánh giá rủi ro để đưa ra quyết định đầu tư tốt hơn và tránh các cạm bẫy tiềm ẩn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('72c9c875-9196-48dc-9825-6466cca6a65e', 'ba8045a4-e9e2-4488-9dba-8bad6245dd9e', 'concept', 'Tác Động Của Chế Độ Ăn Uống Đến Sức Khỏe Da', 'Các loại thực phẩm như rau xanh, trái cây và omega-3 giúp cải thiện sức khỏe da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bát salad tươi ngon với rau xanh và trái cây.", "videoScript": "Xin chào các bạn! Hôm nay, chúng ta sẽ khám phá cách chế độ ăn uống ảnh hưởng đến sức khỏe da của bạn. Hãy cùng tìm hiểu những thực phẩm tốt nhất cho làn da.", "imageDescription": "Hình ảnh một bát salad đầy màu sắc, chứa nhiều loại rau và trái cây tươi, tượng trưng cho chế độ ăn uống lành mạnh.", "videoDescription": "Video giới thiệu về tác động của chế độ ăn uống đến sức khỏe da, bao gồm các loại thực phẩm nên ăn."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:12:09.008413', '2026-01-27 13:12:09.008413', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chế độ ăn uống có ảnh hưởng lớn đến sức khỏe da.", "detailed": "Các loại thực phẩm như rau xanh, trái cây và omega-3 giúp cải thiện sức khỏe da.", "comprehensive": "Một chế độ ăn uống cân bằng với nhiều vitamin, khoáng chất và axit béo omega-3 có thể giúp duy trì làn da khỏe mạnh, tăng cường độ đàn hồi và ngăn ngừa lão hóa sớm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ec52fc53-43ec-4033-af94-363bcc430dd3', '6b8cb42a-c31e-4233-aa9e-958c76f23089', 'concept', 'Tầm Quan Trọng Của Cỡ Chữ Trong Văn Bản', 'Một cỡ chữ quá nhỏ sẽ khiến người đọc khó khăn, trong khi cỡ chữ quá lớn có thể gây rối mắt.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh so sánh tài liệu với cỡ chữ khác nhau.", "videoScript": "Trong video này, chúng ta sẽ khám phá tại sao cỡ chữ lại quan trọng và cách nó ảnh hưởng đến người đọc.", "imageDescription": "Một tài liệu có cỡ chữ nhỏ và lớn với các phản hồi từ người đọc về tính dễ đọc.", "videoDescription": "Video trình bày về tầm quan trọng của cỡ chữ trong việc tạo ra tài liệu hiệu quả."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:38:33.532124', '2026-01-27 12:38:33.532124', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cỡ chữ ảnh hưởng đến sự chú ý và khả năng đọc của người xem.", "detailed": "Một cỡ chữ quá nhỏ sẽ khiến người đọc khó khăn, trong khi cỡ chữ quá lớn có thể gây rối mắt.", "comprehensive": "Chọn cỡ chữ phù hợp không chỉ giúp văn bản dễ đọc mà còn giúp truyền tải thông điệp một cách hiệu quả. Cỡ chữ cũng có thể thể hiện sự chuyên nghiệp và quy cách của tài liệu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('402ea18f-5cc5-468c-bc15-7e20628d4bde', '6b8cb42a-c31e-4233-aa9e-958c76f23089', 'concept', 'Cách Chọn Cỡ Chữ Phù Hợp', 'Cần xem xét loại tài liệu, đối tượng nhận và ngữ cảnh khi chọn cỡ chữ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh hướng dẫn chọn cỡ chữ theo ngữ cảnh.", "videoScript": "Hãy cùng tìm hiểu cách chọn cỡ chữ phù hợp cho từng loại tài liệu và đối tượng đọc khác nhau.", "imageDescription": "Biểu đồ hướng dẫn cách chọn cỡ chữ dựa trên loại tài liệu và đối tượng độc giả.", "videoDescription": "Video hướng dẫn cách chọn cỡ chữ dựa trên các tiêu chí cụ thể."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:38:33.535426', '2026-01-27 12:38:33.535426', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn cỡ chữ phù hợp với nội dung và đối tượng độc giả.", "detailed": "Cần xem xét loại tài liệu, đối tượng nhận và ngữ cảnh khi chọn cỡ chữ.", "comprehensive": "Khi chọn cỡ chữ, nên cân nhắc các yếu tố như loại hình tài liệu (thuyết trình, báo cáo, thư tín), độ tuổi của người đọc, và môi trường mà họ sẽ tiếp xúc với tài liệu (trực tuyến hoặc in ấn)."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('433b4812-0f58-49d5-bf5d-b2e41d4619ab', 'de47e720-238c-4738-a851-d44c5a8ae204', 'concept', 'Hiểu về các loại tài sản đầu tư', 'Tài sản đầu tư bao gồm cổ phiếu, trái phiếu, bất động sản và quỹ đầu tư. Mỗi loại có ưu điểm và nhược điểm riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các loại tài sản đầu tư như cổ phiếu, trái phiếu và bất động sản.", "videoScript": "Chào mừng bạn đến với bài học về các loại tài sản đầu tư. Hôm nay, chúng ta sẽ khám phá các loại tài sản khác nhau mà bạn có thể đầu tư vào.", "imageDescription": "Một infographic thể hiện sự phân loại các loại tài sản đầu tư.", "videoDescription": "Video giải thích các loại tài sản đầu tư và cách chúng hoạt động."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:03:26.825364', '2026-01-27 13:03:26.825364', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại tài sản đầu tư như cổ phiếu, trái phiếu và bất động sản.", "detailed": "Tài sản đầu tư bao gồm cổ phiếu, trái phiếu, bất động sản và quỹ đầu tư. Mỗi loại có ưu điểm và nhược điểm riêng.", "comprehensive": "Để chọn tài sản đầu tư hiệu quả, bạn cần hiểu rõ các loại tài sản như cổ phiếu (mua cổ phần của công ty), trái phiếu (cho vay tiền cho chính phủ hoặc doanh nghiệp), bất động sản (mua đất hoặc nhà để cho thuê hoặc bán) và quỹ đầu tư (đầu tư vào một nhóm tài sản). Mỗi loại có mức độ rủi ro và lợi nhuận khác nhau, vì vậy việc hiểu rõ từng loại là rất quan trọng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3fe1470f-22e4-41c1-8211-cfd1559ef68f', 'de47e720-238c-4738-a851-d44c5a8ae204', 'concept', 'Đánh giá rủi ro và lợi nhuận', 'Đánh giá rủi ro giúp bạn xác định khả năng mất mát trong đầu tư. Lợi nhuận là phần thưởng cho sự chấp nhận rủi ro.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ thể hiện mối quan hệ giữa rủi ro và lợi nhuận trong đầu tư.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu về cách đánh giá rủi ro và lợi nhuận khi đầu tư.", "imageDescription": "Một biểu đồ minh họa sự tương quan giữa rủi ro và lợi nhuận trong các loại tài sản.", "videoDescription": "Video hướng dẫn về cách phân tích rủi ro và lợi nhuận trong đầu tư."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:03:26.833063', '2026-01-27 13:03:26.833063', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mỗi tài sản đầu tư có mức độ rủi ro và lợi nhuận khác nhau.", "detailed": "Đánh giá rủi ro giúp bạn xác định khả năng mất mát trong đầu tư. Lợi nhuận là phần thưởng cho sự chấp nhận rủi ro.", "comprehensive": "Khi đầu tư, việc đánh giá rủi ro và lợi nhuận là rất quan trọng. Rủi ro có thể đến từ biến động thị trường, tình hình kinh tế hoặc thay đổi trong chính sách. Lợi nhuận thường tỷ lệ thuận với mức độ rủi ro. Bạn cần phân tích tỷ lệ rủi ro/lợi nhuận để đưa ra quyết định đầu tư hợp lý."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('326c4e39-0bd3-42a9-be7e-ecf3b00ae00f', 'de47e720-238c-4738-a851-d44c5a8ae204', 'concept', 'Lập kế hoạch đầu tư', 'Khi lập kế hoạch đầu tư, bạn cần xác định mục tiêu tài chính, thời gian đầu tư và khả năng chấp nhận rủi ro.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một biểu mẫu lập kế hoạch đầu tư.", "videoScript": "Chào các bạn! Hôm nay chúng ta sẽ cùng nhau lập kế hoạch đầu tư cho tương lai tài chính của bạn.", "imageDescription": "Một biểu mẫu lập kế hoạch đầu tư với các mục tiêu và chiến lược.", "videoDescription": "Video hướng dẫn cách lập kế hoạch đầu tư hiệu quả."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:03:26.837268', '2026-01-27 13:03:26.837268', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lập kế hoạch giúp bạn có mục tiêu rõ ràng trong đầu tư.", "detailed": "Khi lập kế hoạch đầu tư, bạn cần xác định mục tiêu tài chính, thời gian đầu tư và khả năng chấp nhận rủi ro.", "comprehensive": "Lập kế hoạch đầu tư là bước quan trọng để đảm bảo bạn đạt được mục tiêu tài chính. Bạn cần xác định các mục tiêu ngắn hạn và dài hạn, thời gian đầu tư (ngắn hạn, trung hạn, dài hạn) và mức độ rủi ro mà bạn có thể chấp nhận. Hãy cân nhắc các yếu tố như thu nhập, chi phí sinh hoạt và các khoản đầu tư hiện tại của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e01f9bd4-5ffa-4cf3-94e4-791c845fbe14', '6b8cb42a-c31e-4233-aa9e-958c76f23089', 'concept', 'Các Lỗi Thường Gặp Khi Chọn Cỡ Chữ', 'Một số lỗi phổ biến gồm chọn cỡ chữ quá nhỏ hoặc quá lớn, không đồng nhất giữa các phần của tài liệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các lỗi chọn cỡ chữ trong tài liệu.", "videoScript": "Trong video này, chúng ta sẽ cùng điểm qua các lỗi thường gặp khi chọn cỡ chữ và cách khắc phục chúng.", "imageDescription": "Một tài liệu với các đoạn văn bản có cỡ chữ không đồng nhất và phản hồi từ người đọc.", "videoDescription": "Video phân tích các lỗi phổ biến khi chọn cỡ chữ và cách cải thiện."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:38:33.539896', '2026-01-27 12:38:33.539896', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhiều người chọn cỡ chữ không phù hợp, dẫn đến khó đọc.", "detailed": "Một số lỗi phổ biến gồm chọn cỡ chữ quá nhỏ hoặc quá lớn, không đồng nhất giữa các phần của tài liệu.", "comprehensive": "Các lỗi khi chọn cỡ chữ có thể gây ảnh hưởng lớn đến trải nghiệm người đọc. Một trong những lỗi thường gặp là không giữ sự đồng nhất trong toàn bộ tài liệu, dẫn đến sự khó chịu cho người đọc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8c082307-b0fb-49b4-b256-d69d25632942', 'de47e720-238c-4738-a851-d44c5a8ae204', 'concept', 'Theo dõi và điều chỉnh danh mục đầu tư', 'Cần thường xuyên theo dõi hiệu suất của các tài sản trong danh mục và điều chỉnh khi cần thiết.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một biểu đồ theo dõi hiệu suất danh mục đầu tư.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu cách theo dõi và điều chỉnh danh mục đầu tư của bạn.", "imageDescription": "Một biểu đồ thể hiện sự thay đổi trong hiệu suất của danh mục đầu tư theo thời gian.", "videoDescription": "Video hướng dẫn cách theo dõi và điều chỉnh danh mục đầu tư hiệu quả."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:03:26.841132', '2026-01-27 13:03:26.841132', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi danh mục đầu tư giúp bạn đưa ra quyết định kịp thời.", "detailed": "Cần thường xuyên theo dõi hiệu suất của các tài sản trong danh mục và điều chỉnh khi cần thiết.", "comprehensive": "Theo dõi và điều chỉnh danh mục đầu tư là rất quan trọng để đảm bảo bạn đạt được mục tiêu tài chính. Bạn nên thường xuyên đánh giá hiệu suất của các tài sản trong danh mục của mình và thực hiện điều chỉnh nếu cần thiết. Điều này có thể bao gồm việc bán tài sản kém hiệu quả hoặc đầu tư thêm vào các tài sản có tiềm năng tăng trưởng cao hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a33b7a32-72c0-446a-857d-1285d58f9a05', 'de47e720-238c-4738-a851-d44c5a8ae204', 'concept', 'Tìm hiểu về các công cụ đầu tư', 'Các công cụ đầu tư bao gồm ứng dụng theo dõi thị trường, phần mềm phân tích và các nền tảng giao dịch.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một ứng dụng đầu tư trên điện thoại di động.", "videoScript": "Chào các bạn! Hôm nay chúng ta sẽ khám phá các công cụ đầu tư hữu ích mà bạn có thể sử dụng.", "imageDescription": "Một giao diện ứng dụng đầu tư hiển thị thông tin thị trường và danh mục đầu tư.", "videoDescription": "Video giới thiệu các công cụ đầu tư hiện đại và cách sử dụng chúng."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:03:26.844945', '2026-01-27 13:03:26.844945', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều công cụ hỗ trợ đầu tư như ứng dụng và phần mềm.", "detailed": "Các công cụ đầu tư bao gồm ứng dụng theo dõi thị trường, phần mềm phân tích và các nền tảng giao dịch.", "comprehensive": "Các công cụ đầu tư là những trợ thủ đắc lực giúp bạn đưa ra quyết định đầu tư thông minh. Bạn có thể sử dụng ứng dụng theo dõi thị trường để cập nhật thông tin, phần mềm phân tích để đánh giá hiệu suất tài sản, và các nền tảng giao dịch để mua bán tài sản một cách dễ dàng. Hiểu rõ cách sử dụng các công cụ này sẽ giúp bạn tối ưu hóa lợi nhuận đầu tư."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a5ad8ff7-dc72-48a5-8a6d-960926b0efb0', 'ba8045a4-e9e2-4488-9dba-8bad6245dd9e', 'concept', 'Thực Phẩm Cần Tránh Để Bảo Vệ Da', 'Đường và chất béo bão hòa có thể gây viêm da và làm da trở nên xỉn màu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một đĩa thức ăn nhanh với đồ chiên và đồ ngọt.", "videoScript": "Chào mừng các bạn đến với video hôm nay! Chúng ta sẽ nói về những thực phẩm nên tránh để bảo vệ sức khỏe da của bạn.", "imageDescription": "Hình ảnh một đĩa thức ăn nhanh đầy dầu mỡ và đồ ngọt, biểu thị cho những thực phẩm cần tránh để bảo vệ sức khỏe da.", "videoDescription": "Video hướng dẫn về các thực phẩm cần tránh để có làn da khỏe mạnh."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:12:09.016185', '2026-01-27 13:12:09.016185', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tránh thực phẩm nhiều đường và chất béo bão hòa.", "detailed": "Đường và chất béo bão hòa có thể gây viêm da và làm da trở nên xỉn màu.", "comprehensive": "Việc tiêu thụ quá nhiều đường và chất béo bão hòa có thể làm tăng sản xuất dầu trên da, dẫn đến mụn trứng cá và viêm nhiễm. Hãy hạn chế các loại thực phẩm này để giữ cho làn da luôn sạch và khỏe mạnh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ef0913c0-369f-4032-a89e-bdf18004e564', '6b8cb42a-c31e-4233-aa9e-958c76f23089', 'concept', 'Xu Hướng Chọn Cỡ Chữ Hiện Nay', 'Nhiều nhà thiết kế đang ưa chuộng cỡ chữ lớn cho tiêu đề và cỡ chữ vừa cho nội dung chính.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các xu hướng cỡ chữ trong thiết kế hiện đại.", "videoScript": "Hãy cùng khám phá những xu hướng mới trong việc chọn cỡ chữ cho tài liệu và thiết kế.", "imageDescription": "Một bộ sưu tập các mẫu thiết kế sử dụng cỡ chữ hiện đại và hấp dẫn.", "videoDescription": "Video trình bày các xu hướng chọn cỡ chữ hiện đại trong thiết kế và văn bản."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:38:33.543932', '2026-01-27 12:38:33.543932', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Xu hướng hiện nay là chọn cỡ chữ dễ đọc và thân thiện với người dùng.", "detailed": "Nhiều nhà thiết kế đang ưa chuộng cỡ chữ lớn cho tiêu đề và cỡ chữ vừa cho nội dung chính.", "comprehensive": "Xu hướng chọn cỡ chữ hiện nay không chỉ tập trung vào kích thước mà còn vào kiểu chữ, khoảng cách giữa các ký tự và độ tương phản với nền. Việc này giúp tăng cường khả năng tiếp cận và trải nghiệm người dùng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2c3b73cb-1d72-4d93-9b1b-857826134bc6', '70fd8ebf-4ad8-4041-a696-1e04d14e3ff5', 'concept', 'Xoa Bóp và Kỹ Thuật Massage', 'Các kỹ thuật massage có thể giúp tăng cường lưu thông máu và giảm căng thẳng cơ bắp.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một chuyên gia massage đang xoa bóp cho một cầu thủ bóng rổ.", "videoScript": "Chúng tôi sẽ hướng dẫn bạn các kỹ thuật massage hiệu quả để phục hồi sau khi tập luyện.", "imageDescription": "Một chuyên gia massage đang thực hiện các động tác massage cho một cầu thủ bóng rổ trên bàn massage.", "videoDescription": "Video hướng dẫn các kỹ thuật massage cho cầu thủ bóng rổ."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:05.14841', '2026-01-27 12:58:05.14841', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Xoa bóp giúp giảm đau và căng cơ sau tập luyện.", "detailed": "Các kỹ thuật massage có thể giúp tăng cường lưu thông máu và giảm căng thẳng cơ bắp.", "comprehensive": "Massage sau tập có thể giúp giải phóng căng thẳng trong cơ bắp và tăng cường lưu thông máu. Những kỹ thuật như xoa bóp, ấn huyệt và sử dụng các dụng cụ massage có thể là những lựa chọn hiệu quả để phục hồi."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8b8413d6-cd79-4e96-b9ad-6f8050b76273', '50b9d9a2-afee-4264-8cca-8b83a3661c45', 'concept', 'Hiểu Về Chiến Lược Tiết Kiệm', 'Chiến lược tiết kiệm hiệu quả giúp bạn quản lý tài chính cá nhân tốt hơn bằng cách xác định mục tiêu tài chính và phân bổ ngân sách hợp lý.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người trẻ tuổi đang xem xét bảng ngân sách của mình.", "videoScript": "Chào mừng đến với bài học về chiến lược tiết kiệm! Hôm nay, chúng ta sẽ tìm hiểu cách thức tiết kiệm tiền một cách hiệu quả.", "imageDescription": "Một người trẻ tuổi ngồi tại bàn làm việc với laptop, giấy ghi chú và bút, thể hiện ý thức quản lý tài chính.", "videoDescription": "Video giải thích về các khái niệm tiết kiệm cơ bản và tầm quan trọng của việc lập kế hoạch tài chính."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:04:00.270392', '2026-01-27 13:04:00.270392', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tiết kiệm là việc giữ lại một phần thu nhập để sử dụng trong tương lai.", "detailed": "Chiến lược tiết kiệm hiệu quả giúp bạn quản lý tài chính cá nhân tốt hơn bằng cách xác định mục tiêu tài chính và phân bổ ngân sách hợp lý.", "comprehensive": "Chiến lược tiết kiệm hiệu quả không chỉ bao gồm việc giữ lại tiền mà còn là cách bạn lập kế hoạch tài chính để đảm bảo có đủ nguồn lực cho các mục tiêu ngắn hạn và dài hạn. Bạn cần phải hiểu rõ các loại tài khoản tiết kiệm và cách thức hoạt động của chúng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('699924db-aea7-4787-b318-5a203c0773a9', 'ba8045a4-e9e2-4488-9dba-8bad6245dd9e', 'concept', 'Nước và Sức Khỏe Da', 'Nước giúp loại bỏ độc tố và duy trì độ ẩm cho da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một chai nước và một cốc nước với chanh.", "videoScript": "Xin chào! Hôm nay chúng ta sẽ khám phá tầm quan trọng của nước đối với sức khỏe da. Hãy cùng nhau tìm hiểu cách nước giúp da bạn đẹp hơn.", "imageDescription": "Hình ảnh một chai nước và cốc nước chanh, tượng trưng cho việc duy trì đủ nước cho cơ thể.", "videoDescription": "Video giải thích vai trò của nước trong việc duy trì sức khỏe da."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:12:09.019761', '2026-01-27 13:12:09.019761', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Uống đủ nước giúp da giữ được độ ẩm.", "detailed": "Nước giúp loại bỏ độc tố và duy trì độ ẩm cho da.", "comprehensive": "Nước đóng vai trò quan trọng trong việc duy trì độ ẩm và độ đàn hồi của da. Uống đủ nước mỗi ngày giúp da bạn luôn tươi sáng và giảm nguy cơ khô da, nứt nẻ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2e5aac65-dd02-414b-afbd-e4e0bfc60bea', '57571c4c-d43c-4310-bf4f-1a28886a7083', 'concept', 'Giới thiệu về Kiểm tra và In ấn', 'Kiểm tra và in ấn là quá trình đánh giá chất lượng tài liệu trước khi in ra. Điều này bao gồm việc đọc lại nội dung, kiểm tra lỗi chính tả và định dạng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang kiểm tra tài liệu trên máy tính.", "videoScript": "Trong video này, chúng ta sẽ hướng dẫn bạn cách kiểm tra và in ấn một tài liệu. Đầu tiên, hãy mở tài liệu trên máy tính của bạn...", "imageDescription": "Một người ngồi trước màn hình máy tính, nhìn vào tài liệu và đánh dấu các lỗi.", "videoDescription": "Video hướng dẫn các bước cần thiết để kiểm tra và in ấn tài liệu một cách hiệu quả."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:39:05.950464', '2026-01-27 12:39:05.950464', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tìm hiểu những khái niệm cơ bản về kiểm tra và in ấn tài liệu.", "detailed": "Kiểm tra và in ấn là quá trình đánh giá chất lượng tài liệu trước khi in ra. Điều này bao gồm việc đọc lại nội dung, kiểm tra lỗi chính tả và định dạng.", "comprehensive": "Kiểm tra và in ấn tài liệu là một phần quan trọng trong quy trình soạn thảo văn bản. Nó bao gồm nhiều bước như xem xét lại nội dung, kiểm tra định dạng, và đảm bảo rằng tài liệu đạt tiêu chuẩn trước khi được in. Ngoài ra, việc sử dụng công cụ kiểm tra chính tả và ngữ pháp cũng là rất cần thiết."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('14ec3c70-413e-47b6-a6aa-44f62a2eaf8e', '50b9d9a2-afee-4264-8cca-8b83a3661c45', 'concept', 'Xác Định Mục Tiêu Tiết Kiệm', 'Đặt ra những mục tiêu cụ thể, như mua nhà, đi du lịch hay đầu tư cho giáo dục, sẽ giúp bạn có kế hoạch tiết kiệm rõ ràng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng mục tiêu với các ghi chú và hình ảnh minh họa.", "videoScript": "Trong video này, chúng ta sẽ cùng nhau xác định các mục tiêu tiết kiệm và cách thức để đạt được chúng.", "imageDescription": "Một bảng mục tiêu tiết kiệm với các mục tiêu cụ thể như ''Mua xe mới'', ''Du lịch châu Âu'', ''Tiết kiệm cho hưu trí''.", "videoDescription": "Video hướng dẫn cách xác định mục tiêu tiết kiệm cá nhân và tầm quan trọng của việc có mục tiêu rõ ràng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:04:00.277938', '2026-01-27 13:04:00.277938', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mục tiêu tiết kiệm giúp bạn có động lực để tiết kiệm.", "detailed": "Đặt ra những mục tiêu cụ thể, như mua nhà, đi du lịch hay đầu tư cho giáo dục, sẽ giúp bạn có kế hoạch tiết kiệm rõ ràng.", "comprehensive": "Mục tiêu tiết kiệm có thể là ngắn hạn, trung hạn hoặc dài hạn. Việc xác định rõ mục tiêu sẽ giúp bạn dễ dàng hơn trong việc theo dõi tiến trình và điều chỉnh chiến lược tiết kiệm của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4c91d57a-5d6f-420c-90f9-f1472edf49a4', '50b9d9a2-afee-4264-8cca-8b83a3661c45', 'concept', 'Nguyên Tắc 50/30/20', 'Theo nguyên tắc này, 50% thu nhập dùng cho nhu cầu thiết yếu, 30% cho sở thích và 20% cho tiết kiệm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một biểu đồ phân bổ ngân sách theo nguyên tắc 50/30/20.", "videoScript": "Hãy cùng tìm hiểu về nguyên tắc 50/30/20 và cách nó có thể giúp bạn quản lý tài chính hiệu quả hơn.", "imageDescription": "Biểu đồ thể hiện cách phân bổ ngân sách theo nguyên tắc 50/30/20 với các phần màu sắc khác nhau.", "videoDescription": "Video giải thích chi tiết về nguyên tắc 50/30/20 và cách áp dụng nó vào đời sống hàng ngày."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:04:00.28164', '2026-01-27 13:04:00.28164', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nguyên tắc 50/30/20 giúp bạn phân bổ thu nhập hợp lý.", "detailed": "Theo nguyên tắc này, 50% thu nhập dùng cho nhu cầu thiết yếu, 30% cho sở thích và 20% cho tiết kiệm.", "comprehensive": "Nguyên tắc 50/30/20 là một phương pháp quản lý tài chính cá nhân đơn giản và dễ dàng áp dụng. Bằng cách tuân thủ nguyên tắc này, bạn có thể đảm bảo rằng mình đang sống trong khả năng tài chính, đồng thời vẫn có thể tiết kiệm và tận hưởng cuộc sống."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('801a693d-0833-41ff-b6e5-274e864a41a3', '70fd8ebf-4ad8-4041-a696-1e04d14e3ff5', 'concept', 'Sử Dụng Băng Rán và Chườm Lạnh', 'Chườm lạnh có thể giúp giảm viêm và đau nhức cơ bắp sau tập luyện.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cầu thủ bóng rổ đang chườm lạnh lên vùng cơ bị đau.", "videoScript": "Chúng tôi sẽ chỉ cho bạn cách sử dụng băng rán và chườm lạnh một cách hiệu quả để phục hồi sau khi tập luyện.", "imageDescription": "Một cầu thủ bóng rổ đặt một túi đá lên chân bị đau của mình.", "videoDescription": "Hướng dẫn về việc sử dụng băng rán và chườm lạnh cho cầu thủ bóng rổ."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:05.151428', '2026-01-27 12:58:05.151428', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng băng rán giúp giảm sưng và đau.", "detailed": "Chườm lạnh có thể giúp giảm viêm và đau nhức cơ bắp sau tập luyện.", "comprehensive": "Sau khi tập luyện, việc sử dụng băng rán và chườm lạnh có thể rất hiệu quả trong việc giảm sưng và đau. Chườm lạnh trong 15 đến 20 phút có thể giúp làm giảm viêm và cải thiện quá trình phục hồi."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f9dd7939-86e7-411f-88f1-552985e7d3dc', '57571c4c-d43c-4310-bf4f-1a28886a7083', 'concept', 'Cách kiểm tra lỗi chính tả và ngữ pháp', 'Công cụ kiểm tra chính tả và ngữ pháp giúp tìm ra các lỗi cơ bản trong văn bản của bạn, từ lỗi chính tả đến cấu trúc câu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giao diện kiểm tra chính tả trong Microsoft Word.", "videoScript": "Trong video này, chúng ta sẽ xem cách sử dụng công cụ kiểm tra chính tả trong Microsoft Word. Đầu tiên, mở tài liệu và chọn tab ''Review''...", "imageDescription": "Giao diện của Microsoft Word với các lỗi chính tả được đánh dấu.", "videoDescription": "Video giải thích cách sử dụng công cụ kiểm tra chính tả và ngữ pháp trong Microsoft Word."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:39:05.959246', '2026-01-27 12:39:05.959246', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng công cụ kiểm tra chính tả để tìm lỗi trong tài liệu.", "detailed": "Công cụ kiểm tra chính tả và ngữ pháp giúp tìm ra các lỗi cơ bản trong văn bản của bạn, từ lỗi chính tả đến cấu trúc câu.", "comprehensive": "Kiểm tra lỗi chính tả và ngữ pháp là một bước quan trọng trước khi in ấn tài liệu. Bạn có thể sử dụng các công cụ như Microsoft Word hoặc Grammarly để tự động kiểm tra. Hãy chú ý đến các lỗi mà công cụ không phát hiện ra, như ngữ cảnh sử dụng từ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('78d927aa-cc58-4495-b017-6e9628faccc1', '57571c4c-d43c-4310-bf4f-1a28886a7083', 'concept', 'Cách định dạng tài liệu trước khi in', 'Định dạng tài liệu bao gồm việc chọn phông chữ, kích thước chữ, và bố trí trang. Điều này giúp tài liệu trông chuyên nghiệp hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một tài liệu với các định dạng khác nhau.", "videoScript": "Trong video này, chúng ta sẽ học cách định dạng tài liệu trước khi in. Bắt đầu với việc chọn phông chữ và kích thước chữ phù hợp...", "imageDescription": "Tài liệu được định dạng với các kiểu chữ và bố trí khác nhau.", "videoDescription": "Hướng dẫn chi tiết về cách định dạng tài liệu để chuẩn bị cho quá trình in ấn."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:39:05.963691', '2026-01-27 12:39:05.963691', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đảm bảo rằng tài liệu của bạn được định dạng đúng trước khi in.", "detailed": "Định dạng tài liệu bao gồm việc chọn phông chữ, kích thước chữ, và bố trí trang. Điều này giúp tài liệu trông chuyên nghiệp hơn.", "comprehensive": "Trước khi in tài liệu, việc định dạng là rất quan trọng. Bạn cần chú ý đến phông chữ, kích thước chữ, và khoảng cách dòng. Các yếu tố như lề trang và tiêu đề cũng cần được thiết lập một cách hợp lý để đảm bảo tài liệu dễ đọc và trông chuyên nghiệp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('359047f5-a207-4407-b1ae-f5abab24d22a', '50b9d9a2-afee-4264-8cca-8b83a3661c45', 'concept', 'Tìm Kiếm Cách Tiết Kiệm Thông Minh', 'Có nhiều cách để tiết kiệm như mua sắm thông minh, sử dụng mã giảm giá, và lên kế hoạch chi tiêu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người phụ nữ đang so sánh giá sản phẩm trên điện thoại.", "videoScript": "Trong bài học này, chúng ta sẽ khám phá các mẹo tiết kiệm thông minh giúp bạn giảm chi phí hàng tháng.", "imageDescription": "Một người phụ nữ đứng trong siêu thị, sử dụng điện thoại để so sánh giá sản phẩm trước khi quyết định mua.", "videoDescription": "Video chia sẻ các mẹo tiết kiệm thông minh và cách áp dụng chúng vào cuộc sống hàng ngày."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:04:00.285242', '2026-01-27 13:04:00.285242', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tiết kiệm thông minh là tìm cách giảm chi phí mà không ảnh hưởng đến chất lượng cuộc sống.", "detailed": "Có nhiều cách để tiết kiệm như mua sắm thông minh, sử dụng mã giảm giá, và lên kế hoạch chi tiêu.", "comprehensive": "Tiết kiệm thông minh không chỉ đơn thuần là cắt giảm chi phí mà còn là cách bạn tối ưu hóa các khoản chi tiêu hàng ngày. Việc lập danh sách mua sắm, so sánh giá cả và tìm kiếm ưu đãi có thể giúp bạn tiết kiệm đáng kể."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3df0225c-ea31-42d2-8bed-58c2bf5b3d7f', '57571c4c-d43c-4310-bf4f-1a28886a7083', 'concept', 'Chọn máy in và thiết lập in ấn', 'Máy in có nhiều loại khác nhau, từ máy in phun đến máy in laser. Việc chọn đúng loại máy in sẽ ảnh hưởng đến chất lượng in.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một máy in đang hoạt động.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu cách chọn máy in phù hợp cho nhu cầu của bạn. Đầu tiên, hãy xem xét loại tài liệu mà bạn thường xuyên in...", "imageDescription": "Máy in laser đang in tài liệu với tốc độ nhanh.", "videoDescription": "Video hướng dẫn cách chọn và thiết lập máy in cho việc in ấn tài liệu."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:39:05.967539', '2026-01-27 12:39:05.967539', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lựa chọn máy in phù hợp cho tài liệu của bạn.", "detailed": "Máy in có nhiều loại khác nhau, từ máy in phun đến máy in laser. Việc chọn đúng loại máy in sẽ ảnh hưởng đến chất lượng in.", "comprehensive": "Khi chọn máy in, bạn cần xem xét các yếu tố như loại tài liệu, số lượng bản in và chất lượng in. Máy in phun thường tốt cho in ảnh, trong khi máy in laser lại phù hợp cho in văn bản với số lượng lớn. Thiết lập máy in cũng rất quan trọng để đảm bảo rằng tài liệu được in đúng cách."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7f7f6ff0-e58b-479f-b458-38b6da54c913', '50b9d9a2-afee-4264-8cca-8b83a3661c45', 'concept', 'Theo Dõi Tiến Trình Tiết Kiệm', 'Ghi chép lại các khoản tiết kiệm hàng tháng giúp bạn đánh giá hiệu quả của chiến lược tiết kiệm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng tính với số liệu tiết kiệm.", "videoScript": "Chào mừng các bạn đến với phần theo dõi tiến trình tiết kiệm. Hôm nay, chúng ta sẽ học cách ghi chép và đánh giá tiến trình của mình.", "imageDescription": "Một người đang ngồi bên laptop với một bảng tính mở, hiển thị các khoản tiết kiệm và tiến trình đạt được mục tiêu.", "videoDescription": "Video hướng dẫn cách theo dõi tiến trình tiết kiệm và tầm quan trọng của việc này trong quản lý tài chính."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:04:00.28979', '2026-01-27 13:04:00.28979', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi tiến trình giúp bạn biết mình đã tiết kiệm được bao nhiêu.", "detailed": "Ghi chép lại các khoản tiết kiệm hàng tháng giúp bạn đánh giá hiệu quả của chiến lược tiết kiệm.", "comprehensive": "Theo dõi tiến trình tiết kiệm là một phần quan trọng trong việc đạt được mục tiêu tài chính. Bạn có thể sử dụng ứng dụng tài chính hoặc bảng tính để theo dõi các khoản tiền đã tiết kiệm, từ đó điều chỉnh kế hoạch nếu cần thiết."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ea4a9bfc-b7a2-403b-a80c-8433debddce9', 'ba8045a4-e9e2-4488-9dba-8bad6245dd9e', 'concept', 'Vitamin và Khoáng Chất Cho Da Khỏe', 'Vitamin C giúp làm sáng da, trong khi vitamin E bảo vệ da khỏi tác hại từ môi trường.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một đĩa trái cây giàu vitamin như cam và bơ.", "videoScript": "Chào các bạn! Hôm nay chúng ta sẽ cùng khám phá vai trò của vitamin và khoáng chất đối với sức khỏe da. Hãy cùng xem xét những loại thực phẩm tốt nhất cho làn da.", "imageDescription": "Hình ảnh một đĩa trái cây giàu vitamin, chứa cam và bơ, biểu thị cho những thực phẩm tốt cho sức khỏe da.", "videoDescription": "Video giới thiệu về các vitamin và khoáng chất cần thiết cho sức khỏe da."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:12:09.023046', '2026-01-27 13:12:09.023046', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Vitamin C và E rất tốt cho da.", "detailed": "Vitamin C giúp làm sáng da, trong khi vitamin E bảo vệ da khỏi tác hại từ môi trường.", "comprehensive": "Vitamin C, một chất chống oxy hóa mạnh mẽ, giúp làm sáng da và thúc đẩy sản xuất collagen, trong khi vitamin E giúp bảo vệ da khỏi tác hại của tia UV và ô nhiễm. Kết hợp hai loại vitamin này trong chế độ ăn uống sẽ mang lại lợi ích to lớn cho làn da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6be2f288-49d5-445c-84d8-5f3f179d379a', '0dd5cf1b-e8e4-431e-9aa6-32037b7d5748', 'concept', 'Giới thiệu về Mục tiêu Tài chính', 'Mục tiêu tài chính có thể là ngắn hạn, trung hạn hoặc dài hạn. Chúng giúp bạn lập kế hoạch cho tương lai tài chính của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang viết kế hoạch tài chính của mình trên giấy.", "videoScript": "Xin chào các bạn! Hôm nay chúng ta sẽ tìm hiểu về mục tiêu tài chính. Mục tiêu tài chính giúp bạn xác định hướng đi trong việc quản lý tiền bạc.", "imageDescription": "Hình ảnh một người ngồi ở bàn làm việc, viết xuống các mục tiêu tài chính trên một tờ giấy với bút.", "videoDescription": "Video giới thiệu về khái niệm và tầm quan trọng của mục tiêu tài chính trong quản lý tài chính cá nhân."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:04:36.900598', '2026-01-27 13:04:36.900598', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mục tiêu tài chính là những gì bạn muốn đạt được về tiền bạc.", "detailed": "Mục tiêu tài chính có thể là ngắn hạn, trung hạn hoặc dài hạn. Chúng giúp bạn lập kế hoạch cho tương lai tài chính của mình.", "comprehensive": "Mục tiêu tài chính là các định hướng mà cá nhân đặt ra để quản lý tài chính của mình. Chúng có thể bao gồm việc tiết kiệm cho quỹ khẩn cấp, mua nhà, hoặc đầu tư cho giáo dục con cái. Mục tiêu này cần phải cụ thể, đo lường được, có thể đạt được, thực tế và có thời hạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('aaf55165-b69b-4221-85de-3996958b2a48', '57571c4c-d43c-4310-bf4f-1a28886a7083', 'concept', 'Kiểm tra trước khi in', 'Trước khi in, hãy đọc lại tài liệu một lần nữa để đảm bảo không có lỗi nào bị bỏ sót.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang xem lại tài liệu trước khi in.", "videoScript": "Trong video này, chúng ta sẽ thực hiện kiểm tra cuối cùng trước khi in tài liệu. Hãy chắc chắn rằng bạn đã xem xét kỹ càng từng chi tiết...", "imageDescription": "Một người xem xét tài liệu cuối cùng trên màn hình máy tính.", "videoDescription": "Video hướng dẫn cách thực hiện kiểm tra cuối cùng trước khi in tài liệu."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:39:05.972371', '2026-01-27 12:39:05.972371', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thực hiện kiểm tra cuối cùng trước khi in tài liệu.", "detailed": "Trước khi in, hãy đọc lại tài liệu một lần nữa để đảm bảo không có lỗi nào bị bỏ sót.", "comprehensive": "Kiểm tra trước khi in là bước cuối cùng trong quy trình kiểm tra tài liệu. Bạn nên xem xét mọi chi tiết, từ nội dung đến định dạng. Nếu có thể, hãy in một bản thử nghiệm để kiểm tra xem mọi thứ có đúng như mong đợi hay không."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('fc9df099-6bd9-4426-a3ec-5f809401e085', '0dd5cf1b-e8e4-431e-9aa6-32037b7d5748', 'concept', 'Cách Đặt Mục tiêu Tài chính', 'Bạn có thể sử dụng phương pháp SMART để đặt mục tiêu: Cụ thể, Đo lường được, Có thể đạt được, Thực tế và Có thời gian.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng trắng với các mục tiêu tài chính được viết ra.", "videoScript": "Hãy cùng tôi khám phá cách đặt mục tiêu tài chính với phương pháp SMART. Chúng ta sẽ bắt đầu từ việc xác định điều bạn thực sự mong muốn.", "imageDescription": "Hình ảnh một bảng trắng với nhiều mục tiêu tài chính được ghi rõ ràng và có thể thấy được.", "videoDescription": "Video hướng dẫn cách đặt mục tiêu tài chính hiệu quả bằng phương pháp SMART."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:04:36.908122', '2026-01-27 13:04:36.908122', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đặt mục tiêu tài chính bằng cách xác định điều bạn muốn và thời gian bạn cần.", "detailed": "Bạn có thể sử dụng phương pháp SMART để đặt mục tiêu: Cụ thể, Đo lường được, Có thể đạt được, Thực tế và Có thời gian.", "comprehensive": "Đặt mục tiêu tài chính hiệu quả yêu cầu bạn xác định rõ ràng những gì bạn muốn đạt được. Sử dụng tiêu chí SMART sẽ giúp bạn thiết lập các mục tiêu cụ thể, có thể đo lường, khả thi, thực tế và có thời gian hoàn thành. Ví dụ, thay vì nói ''tôi muốn tiết kiệm tiền'', hãy nói ''tôi muốn tiết kiệm 10 triệu đồng trong vòng 1 năm''."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4a4a3432-2b07-4508-895b-75539e4ee5a2', '0dd5cf1b-e8e4-431e-9aa6-32037b7d5748', 'concept', 'Phân loại Mục tiêu Tài chính', 'Mục tiêu ngắn hạn thường dưới 1 năm, trung hạn từ 1 đến 5 năm, và dài hạn trên 5 năm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một biểu đồ phân loại các mục tiêu tài chính.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về các loại mục tiêu tài chính và cách phân loại chúng để dễ dàng quản lý.", "imageDescription": "Hình ảnh một biểu đồ rõ ràng phân loại các mục tiêu tài chính theo ngắn hạn, trung hạn và dài hạn.", "videoDescription": "Video giải thích các loại mục tiêu tài chính và tầm quan trọng của việc phân loại chúng."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:04:36.912286', '2026-01-27 13:04:36.912286', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có ba loại mục tiêu tài chính: ngắn hạn, trung hạn và dài hạn.", "detailed": "Mục tiêu ngắn hạn thường dưới 1 năm, trung hạn từ 1 đến 5 năm, và dài hạn trên 5 năm.", "comprehensive": "Mục tiêu tài chính được phân loại thành ba loại chính: Mục tiêu ngắn hạn (dưới 1 năm), như tiết kiệm cho kỳ nghỉ; Mục tiêu trung hạn (1-5 năm), như mua xe; và Mục tiêu dài hạn (trên 5 năm), như tiết kiệm cho hưu trí. Việc phân loại giúp bạn dễ dàng lập kế hoạch và quản lý tài chính hiệu quả hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('875d42af-a7e0-4dd5-b062-584aded49965', '0dd5cf1b-e8e4-431e-9aa6-32037b7d5748', 'concept', 'Theo dõi và Đánh giá Mục tiêu Tài chính', 'Bạn nên thường xuyên kiểm tra tiến độ và điều chỉnh kế hoạch nếu cần thiết để đạt được mục tiêu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang kiểm tra bảng theo dõi tiến độ mục tiêu tài chính.", "videoScript": "Hãy cùng tìm hiểu cách theo dõi và đánh giá các mục tiêu tài chính của bạn để đảm bảo bạn không bị lạc hướng.", "imageDescription": "Hình ảnh một người ngồi trước máy tính, kiểm tra bảng theo dõi tiến độ mục tiêu tài chính.", "videoDescription": "Video hướng dẫn cách theo dõi và đánh giá mục tiêu tài chính để đạt được thành công."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:04:36.91683', '2026-01-27 13:04:36.91683', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi mục tiêu tài chính giúp bạn biết mình đang đi đúng hướng.", "detailed": "Bạn nên thường xuyên kiểm tra tiến độ và điều chỉnh kế hoạch nếu cần thiết để đạt được mục tiêu.", "comprehensive": "Việc theo dõi và đánh giá mục tiêu tài chính là rất quan trọng để đảm bảo bạn đang đi đúng hướng. Bạn nên thiết lập các cột mốc quan trọng và thường xuyên kiểm tra tiến độ của mình. Nếu cần, điều chỉnh kế hoạch để thích ứng với sự thay đổi trong cuộc sống hoặc tình hình tài chính. Sử dụng các công cụ như bảng tính hoặc ứng dụng tài chính để hỗ trợ quá trình này."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cfe984e5-f4b2-48d5-96af-6f96dda75cf5', '0dd5cf1b-e8e4-431e-9aa6-32037b7d5748', 'concept', 'Điều chỉnh Mục tiêu Tài chính khi Cần Thiết', 'Sự thay đổi trong cuộc sống như công việc, gia đình có thể ảnh hưởng đến mục tiêu của bạn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang cân nhắc và điều chỉnh các mục tiêu tài chính.", "videoScript": "Trong video này, chúng ta sẽ nói về tầm quan trọng của việc điều chỉnh mục tiêu tài chính khi cần thiết và cách thực hiện điều này.", "imageDescription": "Hình ảnh một người đang suy nghĩ và xem xét lại các mục tiêu tài chính của mình trên máy tính.", "videoDescription": "Video thảo luận về việc điều chỉnh mục tiêu tài chính để phù hợp với thay đổi trong cuộc sống."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:04:36.921341', '2026-01-27 13:04:36.921341', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đôi khi bạn cần điều chỉnh mục tiêu tài chính của mình.", "detailed": "Sự thay đổi trong cuộc sống như công việc, gia đình có thể ảnh hưởng đến mục tiêu của bạn.", "comprehensive": "Điều chỉnh mục tiêu tài chính là một phần quan trọng trong quản lý tài chính cá nhân. Khi có sự thay đổi trong hoàn cảnh sống như thay đổi công việc, gia đình, hoặc tình hình kinh tế, bạn cần xem xét lại các mục tiêu tài chính của mình. Đừng ngần ngại điều chỉnh mục tiêu để phù hợp hơn với thực tế và khả năng tài chính hiện tại của bạn. Điều này giúp bạn duy trì động lực và sự tập trung vào các mục tiêu mà bạn có thể đạt được."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a4618d9d-abf0-488c-ae5b-1574df9b66f2', '4d65529b-adb2-410a-99ad-0c8ad3fb7867', 'concept', 'Giới Thiệu Về Kỹ Thuật Sút Phạt', 'Kỹ thuật sút phạt giúp người chơi ghi điểm từ vị trí không bị áp lực. Nó đòi hỏi sự chính xác và sự tập trung.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một cầu thủ bóng rổ đang thực hiện cú sút phạt.", "videoScript": "Cảnh 1: Giới thiệu về sút phạt. Cảnh 2: Hướng dẫn tư thế đứng. Cảnh 3: Thực hiện cú sút. Cảnh 4: Phân tích cú sút thành công.", "imageDescription": "Hình ảnh một cầu thủ bóng rổ đứng ở vạch sút phạt, chuẩn bị thực hiện cú sút.", "videoDescription": "Video này hướng dẫn chi tiết về kỹ thuật sút phạt trong bóng rổ, từ tư thế đến cách thực hiện."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:30.919377', '2026-01-27 12:58:30.919377', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sút phạt là một kỹ thuật quan trọng trong bóng rổ.", "detailed": "Kỹ thuật sút phạt giúp người chơi ghi điểm từ vị trí không bị áp lực. Nó đòi hỏi sự chính xác và sự tập trung.", "comprehensive": "Sút phạt là một trong những kỹ thuật cơ bản nhất trong bóng rổ. Khi một cầu thủ bị phạm lỗi trong khi ghi điểm, họ sẽ được thực hiện một cú sút phạt từ vạch 15 feet. Để thành công, cầu thủ cần phải luyện tập tư thế, sự tập trung và khả năng điều chỉnh lực sút."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5c145ba5-3070-4b41-86be-b3b583c38fee', '67598acc-9740-4aab-8ac6-9c4266d8a5fc', 'concept', 'Giới thiệu về Biểu Đồ Nâng Cao', 'Biểu đồ nâng cao không chỉ đơn giản là hình ảnh dữ liệu mà còn bao gồm các tùy chỉnh để làm nổi bật thông tin quan trọng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một biểu đồ nâng cao với các tùy chỉnh màu sắc và kiểu dáng.", "videoScript": "Xin chào! Hôm nay chúng ta sẽ tìm hiểu về cách tạo ra các biểu đồ nâng cao trong Excel. Đầu tiên, hãy mở Excel và nhập dữ liệu của bạn.", "imageDescription": "Một biểu đồ cột với các màu sắc khác nhau, có tiêu đề và chú thích rõ ràng.", "videoDescription": "Video hướng dẫn cách tạo và tùy chỉnh biểu đồ nâng cao trong Excel."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:39:42.756447', '2026-01-27 12:39:42.756447', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu đồ nâng cao giúp trực quan hóa dữ liệu một cách hiệu quả.", "detailed": "Biểu đồ nâng cao không chỉ đơn giản là hình ảnh dữ liệu mà còn bao gồm các tùy chỉnh để làm nổi bật thông tin quan trọng.", "comprehensive": "Biểu đồ nâng cao là công cụ mạnh mẽ trong tin học văn phòng, cho phép người dùng thể hiện dữ liệu một cách trực quan và dễ hiểu. Các biểu đồ này có thể được tùy chỉnh với nhiều tính năng như màu sắc, kiểu dáng và chú thích để người xem dễ dàng nắm bắt thông tin."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8ec6aef0-7f97-4372-a616-296237d6a765', '4d65529b-adb2-410a-99ad-0c8ad3fb7867', 'concept', 'Tư Thế Đứng Khi Sút Phạt', 'Cầu thủ cần đứng thẳng, chân rộng bằng vai, và tay giữ bóng chắc chắn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một cầu thủ đang đứng ở tư thế chuẩn bị sút phạt.", "videoScript": "Cảnh 1: Giới thiệu tư thế đứng. Cảnh 2: Phân tích từng bước đứng. Cảnh 3: Hướng dẫn cách điều chỉnh tư thế. Cảnh 4: Ví dụ thực tế.", "imageDescription": "Hình ảnh thể hiện tư thế đứng chuẩn bị sút phạt với các điểm chú ý.", "videoDescription": "Video hướng dẫn về tư thế đứng khi thực hiện cú sút phạt trong bóng rổ."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:30.927068', '2026-01-27 12:58:30.927068', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tư thế đứng rất quan trọng khi sút phạt.", "detailed": "Cầu thủ cần đứng thẳng, chân rộng bằng vai, và tay giữ bóng chắc chắn.", "comprehensive": "Tư thế đứng khi sút phạt bao gồm việc đặt chân vững vàng, điều chỉnh trọng tâm cơ thể và giữ bóng ở vị trí thuận lợi. Bạn nên đứng với chân rộng bằng vai, một chân hơi đưa về phía trước để tạo sự cân bằng, và tay phải giữ bóng ở mức ngang bụng để dễ dàng thực hiện cú sút."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1dc76206-f962-41cf-84fe-86f67b9e41cd', 'b889e673-7eb0-4577-9f57-dd4a569aa654', 'concept', 'Giới Thiệu Về Phân Tích Chi Tiêu', 'Phân tích chi tiêu hàng tháng là việc theo dõi và đánh giá các khoản chi để cải thiện quản lý tài chính cá nhân.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng biểu với các khoản chi tiêu hàng tháng.", "videoScript": "Chào mừng đến với bài học về phân tích chi tiêu hàng tháng. Hôm nay, chúng ta sẽ tìm hiểu cách theo dõi và đánh giá chi tiêu của bạn.", "imageDescription": "Một bảng biểu hiển thị các khoản chi tiêu khác nhau như thực phẩm, giải trí, hóa đơn, tiết kiệm.", "videoDescription": "Video hướng dẫn về cách phân tích chi tiêu hàng tháng, bao gồm các bước để theo dõi và tối ưu hóa chi tiêu."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:05:06.441873', '2026-01-27 13:05:06.441873', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phân tích chi tiêu giúp bạn hiểu rõ hơn về cách bạn sử dụng tiền.", "detailed": "Phân tích chi tiêu hàng tháng là việc theo dõi và đánh giá các khoản chi để cải thiện quản lý tài chính cá nhân.", "comprehensive": "Phân tích chi tiêu hàng tháng không chỉ giúp bạn nhận diện các khoản chi cần thiết và không cần thiết mà còn giúp bạn lập kế hoạch tài chính tốt hơn, từ đó đạt được các mục tiêu tài chính của bản thân."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1ab61a72-49aa-4a24-aba9-f2f654e86862', 'b889e673-7eb0-4577-9f57-dd4a569aa654', 'concept', 'Cách Theo Dõi Chi Tiêu Hàng Tháng', 'Bạn có thể sử dụng ứng dụng tài chính hoặc sổ tay để ghi chép lại tất cả các khoản chi tiêu hàng tháng của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang sử dụng ứng dụng tài chính trên điện thoại.", "videoScript": "Hôm nay, chúng ta sẽ khám phá cách theo dõi chi tiêu hàng tháng với các công cụ hiện đại.", "imageDescription": "Một người đang nhập thông tin chi tiêu vào ứng dụng tài chính trên điện thoại thông minh.", "videoDescription": "Video hướng dẫn cách sử dụng ứng dụng tài chính để theo dõi chi tiêu hàng tháng hiệu quả."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:05:06.450736', '2026-01-27 13:05:06.450736', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng ứng dụng hoặc sổ tay để ghi lại mọi khoản chi.", "detailed": "Bạn có thể sử dụng ứng dụng tài chính hoặc sổ tay để ghi chép lại tất cả các khoản chi tiêu hàng tháng của mình.", "comprehensive": "Để theo dõi chi tiêu hàng tháng, bạn nên lập danh sách các khoản chi tiêu cố định và biến đổi, sử dụng ứng dụng tài chính hoặc bảng tính, và đảm bảo ghi chép mọi khoản chi tiêu hàng ngày để có cái nhìn tổng quan hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('37496261-8940-4d6c-8b44-7f3378837c5a', 'ba8045a4-e9e2-4488-9dba-8bad6245dd9e', 'concept', 'Thói Quen Ăn Uống Lành Mạnh Cho Da', 'Lên kế hoạch bữa ăn với nhiều thực phẩm giàu chất chống oxy hóa.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bữa ăn đầy đủ với rau xanh, trái cây và protein.", "videoScript": "Xin chào và chào mừng đến với video hôm nay! Chúng ta sẽ tìm hiểu về những thói quen ăn uống lành mạnh giúp cải thiện sức khỏe da. Hãy cùng bắt đầu!", "imageDescription": "Hình ảnh một bữa ăn đầy đủ với rau xanh, trái cây và protein, thể hiện một chế độ ăn uống cân bằng.", "videoDescription": "Video hướng dẫn về thói quen ăn uống lành mạnh cho sức khỏe da."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:12:09.026937', '2026-01-27 13:12:09.026937', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Ăn nhiều rau xanh và trái cây hàng ngày.", "detailed": "Lên kế hoạch bữa ăn với nhiều thực phẩm giàu chất chống oxy hóa.", "comprehensive": "Để có làn da khỏe mạnh, bạn nên lên kế hoạch ăn uống với nhiều rau xanh, trái cây, và thực phẩm giàu chất chống oxy hóa. Thói quen ăn uống lành mạnh không chỉ giúp cải thiện sức khỏe da mà còn mang lại sức khỏe tổng thể cho cơ thể."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('55a185fd-5e96-463a-acaa-35a6312039b9', '67598acc-9740-4aab-8ac6-9c4266d8a5fc', 'concept', 'Tùy Chỉnh Màu Sắc và Kiểu Dáng', 'Việc tùy chỉnh màu sắc và kiểu dáng của biểu đồ giúp dữ liệu trở nên sinh động và hấp dẫn hơn, thu hút sự chú ý của người xem.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một biểu đồ với các màu sắc khác nhau cho từng phần.", "videoScript": "Trong video này, chúng ta sẽ xem cách thay đổi màu sắc và kiểu dáng của biểu đồ trong Excel. Hãy chọn biểu đồ mà bạn muốn chỉnh sửa và vào phần ''Format''.", "imageDescription": "Một biểu đồ tròn với các phần màu sắc khác nhau, thể hiện rõ ràng tỷ lệ phần trăm.", "videoDescription": "Video hướng dẫn cách tùy chỉnh màu sắc và kiểu dáng của biểu đồ trong Excel."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:39:42.765313', '2026-01-27 12:39:42.765313', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể thay đổi màu sắc và kiểu dáng của biểu đồ để làm nổi bật dữ liệu.", "detailed": "Việc tùy chỉnh màu sắc và kiểu dáng của biểu đồ giúp dữ liệu trở nên sinh động và hấp dẫn hơn, thu hút sự chú ý của người xem.", "comprehensive": "Tùy chỉnh màu sắc và kiểu dáng trong biểu đồ không chỉ làm cho nó đẹp hơn mà còn giúp người xem dễ dàng phân biệt các phần khác nhau của dữ liệu. Trong bài học này, chúng ta sẽ học cách thay đổi màu sắc, kiểu dáng biểu đồ và áp dụng các phong cách khác nhau để làm nổi bật các thông tin quan trọng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('eeda03b8-62a4-4d62-8cb3-4e609683763f', 'b889e673-7eb0-4577-9f57-dd4a569aa654', 'concept', 'Phân Nhóm Chi Tiêu', 'Phân nhóm chi tiêu giúp bạn dễ dàng nhận diện các lĩnh vực tiêu tốn nhiều tiền nhất.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một biểu đồ pie thể hiện tỷ lệ chi tiêu theo nhóm.", "videoScript": "Hãy cùng nhau phân loại các khoản chi tiêu để hiểu rõ hơn về thói quen chi tiêu của bạn.", "imageDescription": "Một biểu đồ pie minh họa tỷ lệ phần trăm chi tiêu cho từng nhóm như thực phẩm, giải trí và tiết kiệm.", "videoDescription": "Video giải thích cách phân nhóm chi tiêu để dễ dàng quản lý tài chính cá nhân."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:05:06.454773', '2026-01-27 13:05:06.454773', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chia chi tiêu thành các nhóm như thực phẩm, giải trí, đi lại.", "detailed": "Phân nhóm chi tiêu giúp bạn dễ dàng nhận diện các lĩnh vực tiêu tốn nhiều tiền nhất.", "comprehensive": "Bằng cách phân nhóm chi tiêu thành các danh mục như thực phẩm, hóa đơn, giải trí và đầu tư, bạn có thể dễ dàng theo dõi và điều chỉnh các khoản chi tiêu không cần thiết."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('dd983008-3c82-4ed5-869e-9124b615e0f1', '67598acc-9740-4aab-8ac6-9c4266d8a5fc', 'concept', 'Thêm Chú Thích và Ghi Chú', 'Việc thêm chú thích và ghi chú vào biểu đồ tạo điều kiện cho người xem dễ dàng nắm bắt thông tin cũng như bối cảnh của dữ liệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một biểu đồ với chú thích rõ ràng ở các phần khác nhau.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ cùng nhau tìm hiểu cách thêm chú thích vào biểu đồ trong Excel. Đầu tiên, hãy chọn biểu đồ mà bạn muốn thêm chú thích.", "imageDescription": "Một biểu đồ đường với các chú thích cho từng điểm dữ liệu quan trọng.", "videoDescription": "Video hướng dẫn cách thêm chú thích và ghi chú vào biểu đồ trong Excel."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:39:42.768996', '2026-01-27 12:39:42.768996', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chú thích và ghi chú giúp người xem hiểu rõ hơn về biểu đồ.", "detailed": "Việc thêm chú thích và ghi chú vào biểu đồ tạo điều kiện cho người xem dễ dàng nắm bắt thông tin cũng như bối cảnh của dữ liệu.", "comprehensive": "Chú thích và ghi chú là phần quan trọng trong biểu đồ, giúp tăng cường sự hiểu biết cho người xem. Trong bài học này, chúng ta sẽ học cách thêm chú thích cho các phần của biểu đồ, cũng như cách sử dụng ghi chú để giải thích thêm thông tin cần thiết."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('bac5d5da-35c2-4902-af2f-cacd769a21bc', '67598acc-9740-4aab-8ac6-9c4266d8a5fc', 'concept', 'Sử Dụng Các Loại Biểu Đồ Khác Nhau', 'Mỗi loại biểu đồ có thể phù hợp với một loại dữ liệu khác nhau, ví dụ như biểu đồ cột, biểu đồ đường, hoặc biểu đồ tròn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại biểu đồ khác nhau như cột, đường, tròn.", "videoScript": "Xin chào! Hôm nay, chúng ta sẽ xem xét các loại biểu đồ khác nhau trong Excel và cách chọn loại phù hợp cho dữ liệu của bạn.", "imageDescription": "Một bức tranh thể hiện nhiều loại biểu đồ khác nhau, mỗi biểu đồ có cách thể hiện riêng.", "videoDescription": "Video hướng dẫn về các loại biểu đồ và cách sử dụng chúng trong Excel."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:39:42.773879', '2026-01-27 12:39:42.773879', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại biểu đồ khác nhau để thể hiện dữ liệu.", "detailed": "Mỗi loại biểu đồ có thể phù hợp với một loại dữ liệu khác nhau, ví dụ như biểu đồ cột, biểu đồ đường, hoặc biểu đồ tròn.", "comprehensive": "Trong tin học văn phòng, việc chọn loại biểu đồ phù hợp với dữ liệu là rất quan trọng. Trong bài học này, chúng ta sẽ khám phá các loại biểu đồ khác nhau như biểu đồ cột, biểu đồ đường, biểu đồ vùng, và cách chọn loại phù hợp cho dữ liệu của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a14adcc3-32a6-4efe-8a5f-841eb496cbb4', 'b889e673-7eb0-4577-9f57-dd4a569aa654', 'concept', 'Đánh Giá Chi Tiêu Hàng Tháng', 'Vào cuối tháng, hãy dành thời gian để xem xét và đánh giá các khoản chi của bạn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người ngồi bên bàn làm việc với tài liệu chi tiêu.", "videoScript": "Hãy cùng nhau đánh giá các khoản chi tiêu của tháng vừa qua và lập kế hoạch cho tháng tới.", "imageDescription": "Một người đang xem xét các khoản chi tiêu hàng tháng trên bàn làm việc của mình.", "videoDescription": "Video hướng dẫn cách đánh giá và điều chỉnh các khoản chi tiêu hàng tháng."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:05:06.458271', '2026-01-27 13:05:06.458271', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Xem lại các khoản chi vào cuối tháng.", "detailed": "Vào cuối tháng, hãy dành thời gian để xem xét và đánh giá các khoản chi của bạn.", "comprehensive": "Đánh giá chi tiêu hàng tháng là một bước quan trọng trong quản lý tài chính cá nhân. Bạn nên xem xét các khoản chi tiêu, xác định những khoản chi nào cần cắt giảm và lên kế hoạch cho tháng tới."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4bcb48a9-f862-4043-a7a9-e598a73587ff', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'example', 'Dribble Ngắn Giới Thiệu', 'Hãy tưởng tượng bạn đang chơi bóng rổ trong sân ở khu phố cùng bạn bè. Bạn nhận bóng và muốn đưa nó đến khu vực rổ. Đầu tiên, bạn dùng tay phải để dribble bóng. Bằng cách chạm nhẹ vào bóng, bạn đẩy nó về phía trước. Dần dần, bạn sẽ thấy bóng bắt đầu nhảy lên và xuống, điều này nghĩa là bạn đã bắt đầu làm quen với kỹ thuật này. Bạn có thể di chuyển sang trái, sang phải mà không để bóng rơi ra ngoài tầm kiểm soát. Nếu đột nhiên có một người bạn chạy đến để cướp bóng, bạn có thể dễ dàng thực hiện động tác crossover để đưa bóng sang tay bên kia và tiếp tục di chuyển. Điều này giống như khi bạn đang lái xe máy và phải lái qua một chướng ngại vật, bạn cần phải nhanh nhẹn và linh hoạt!', '{"caption": "jgh", "imageUrl": "https://res.cloudinary.com/dmoiixotu/image/upload/bo_3px_solid_rgb:4A90E2,r_8/v1769328980/content-edits/images/xog1v1v3iy5z7gzbcetp.jpg", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "description": "gjh", "imagePrompt": "A simple and colorful illustration showing a young person playing basketball on an outdoor court. The character is dribbling a basketball with their right hand, showcasing a playful and energetic pose. There are bright colors and minimal details to make it easy to understand, with a cheerful and educational style appropriate for beginners learning basketball techniques.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ học về kỹ thuật dribble bóng trong bóng rổ! Hãy tưởng tượng bạn đang ở sân bóng cùng bạn bè. Bạn nhận bóng và muốn đưa nó đến khu vực rổ. Đầu tiên, hãy dùng tay phải của bạn để dribble. Nhẹ nhàng chạm vào bóng và đẩy nó về phía trước.\n\nNhìn xem, bóng bắt đầu nhảy lên và xuống! Điều này nghĩa là bạn đã làm quen với kỹ thuật này rồi đấy! Giờ bạn có thể di chuyển sang trái, sang phải mà không để bóng rơi ra ngoài tầm kiểm soát.\n\nNếu đột nhiên có một người bạn đến để cướp bóng, bạn hãy thực hiện động tác crossover! Chuyển bóng sang tay bên kia và tiếp tục di chuyển. Hãy tưởng tượng như bạn đang lái xe máy qua một chướng ngại vật. Bạn cần nhanh nhẹn và linh hoạt. Thật là thú vị phải không nào?", "videoDuration": "30-60 giây", "imageDescription": "Hình ảnh minh họa một người trẻ đang chơi bóng rổ và thực hiện kỹ thuật dribble. Đây là một cách giới thiệu đơn giản về khái niệm chơi bóng rổ cho người mới bắt đầu.", "videoDescription": "Video này giúp bạn làm quen với kỹ thuật dribble cơ bản trong bóng rổ một cách đơn giản và vui nhộn."}', 1, '{"xp": 12, "coin": 1}', NULL, '2026-01-15 22:11:48.524941', '2026-01-25 15:17:26.663392', 'mixed', 'easy', NULL, 'published', NULL, '2f55e271-35c0-4178-a2ed-dd0f219ab4bc', '2026-01-25 15:17:26.661', NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('53964075-11e0-4690-9c69-2d5dc354724d', '67598acc-9740-4aab-8ac6-9c4266d8a5fc', 'concept', 'Xuất và Chia Sẻ Biểu Đồ', 'Xuất biểu đồ sang định dạng hình ảnh hoặc PDF là cách hiệu quả để chia sẻ thông tin với đồng nghiệp hoặc khách hàng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một biểu đồ được xuất sang định dạng hình ảnh.", "videoScript": "Chào các bạn! Trong video này, chúng ta sẽ học cách xuất biểu đồ từ Excel sang các định dạng khác nhau để chia sẻ dễ dàng hơn.", "imageDescription": "Một biểu đồ được xuất dưới dạng PNG, sẵn sàng để chia sẻ.", "videoDescription": "Video hướng dẫn cách xuất và chia sẻ biểu đồ trong Excel."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:39:42.777654', '2026-01-27 12:39:42.777654', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể xuất và chia sẻ biểu đồ với người khác.", "detailed": "Xuất biểu đồ sang định dạng hình ảnh hoặc PDF là cách hiệu quả để chia sẻ thông tin với đồng nghiệp hoặc khách hàng.", "comprehensive": "Chia sẻ biểu đồ với đồng nghiệp hoặc khách hàng là một phần quan trọng trong việc trình bày dữ liệu. Trong bài học này, chúng ta sẽ học cách xuất biểu đồ sang định dạng hình ảnh, PDF và cách chia sẻ chúng qua email hoặc mạng xã hội."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('db63948f-38f1-4b6c-b227-8b68f85c2b3b', '4d65529b-adb2-410a-99ad-0c8ad3fb7867', 'concept', 'Kỹ Thuật Cầm Bóng Khi Sút Phạt', 'Đặt bóng ở tay thuận, ngón tay mở rộng để kiểm soát tốt hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Cận cảnh tay cầu thủ cầm bóng trong tư thế sút phạt.", "videoScript": "Cảnh 1: Giới thiệu kỹ thuật cầm bóng. Cảnh 2: Hướng dẫn chi tiết cách cầm bóng. Cảnh 3: Ví dụ thực hành. Cảnh 4: Phân tích lỗi thường gặp.", "imageDescription": "Hình ảnh mô tả cách cầm bóng đúng cách trước khi sút phạt.", "videoDescription": "Video này sẽ hướng dẫn bạn cách cầm bóng đúng cách khi thực hiện cú sút phạt trong bóng rổ."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:30.930748', '2026-01-27 12:58:30.930748', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cầm bóng đúng cách là rất quan trọng.", "detailed": "Đặt bóng ở tay thuận, ngón tay mở rộng để kiểm soát tốt hơn.", "comprehensive": "Khi cầm bóng để sút phạt, bạn cần phải đảm bảo rằng tay bạn được đặt ở vị trí chính xác. Đặt bóng trên một tay, với các ngón tay mở rộng để tạo sự kiểm soát tốt nhất. Ngón tay cái nên được đặt ở phía dưới bóng, giúp bạn dễ dàng điều chỉnh lực khi sút."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e9c5664d-a913-4ca1-8ca3-6878b8ddc357', 'b889e673-7eb0-4577-9f57-dd4a569aa654', 'concept', 'Lập Kế Hoạch Chi Tiêu Hàng Tháng', 'Lập kế hoạch chi tiêu hàng tháng giúp bạn quản lý tài chính hiệu quả hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng ngân sách chi tiêu hàng tháng.", "videoScript": "Hãy cùng lập kế hoạch chi tiêu cho tháng tới để đạt được các mục tiêu tài chính của bạn.", "imageDescription": "Một bảng ngân sách thể hiện các mục tiêu chi tiêu cho tháng với các nhóm như thực phẩm, giải trí và tiết kiệm.", "videoDescription": "Video hướng dẫn cách lập kế hoạch chi tiêu hàng tháng hiệu quả."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:05:06.463158', '2026-01-27 13:05:06.463158', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Xác định ngân sách cho từng nhóm chi tiêu.", "detailed": "Lập kế hoạch chi tiêu hàng tháng giúp bạn quản lý tài chính hiệu quả hơn.", "comprehensive": "Lập kế hoạch chi tiêu hàng tháng yêu cầu bạn xác định ngân sách cho từng nhóm chi tiêu, cân nhắc các khoản chi cố định và biến đổi, và điều chỉnh ngân sách khi cần thiết để đảm bảo bạn không vượt quá giới hạn tài chính của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b5313bbf-7e47-46c2-8e3a-659df0f32712', '0b3b104b-6815-43c1-92c8-8a83d8c6d405', 'concept', 'Giới thiệu về liệu pháp chăm sóc da', 'Liệu pháp chăm sóc da là một phần quan trọng trong việc duy trì sức khỏe làn da. Nó bao gồm các phương pháp như làm sạch, tẩy tế bào chết, cấp ẩm và điều trị các vấn đề về da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một phòng spa với các sản phẩm chăm sóc da chuyên nghiệp.", "videoScript": "Chào mừng đến với liệu pháp chăm sóc da! Hôm nay, chúng ta sẽ khám phá các liệu pháp chăm sóc da hiệu quả nhất hiện nay.", "imageDescription": "Một không gian spa sang trọng với các sản phẩm chăm sóc da sắp xếp gọn gàng.", "videoDescription": "Video giới thiệu về các liệu pháp chăm sóc da và lợi ích của chúng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:12:36.141588', '2026-01-27 13:12:36.141588', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Liệu pháp chăm sóc da bao gồm các phương pháp điều trị để cải thiện sức khỏe và vẻ đẹp của làn da.", "detailed": "Liệu pháp chăm sóc da là một phần quan trọng trong việc duy trì sức khỏe làn da. Nó bao gồm các phương pháp như làm sạch, tẩy tế bào chết, cấp ẩm và điều trị các vấn đề về da.", "comprehensive": "Liệu pháp chăm sóc da chuyên nghiệp bao gồm nhiều phương pháp điều trị khác nhau, từ cơ bản đến nâng cao, nhằm cải thiện tình trạng da. Các liệu pháp này không chỉ giúp làm sạch và cung cấp độ ẩm mà còn có thể điều trị các vấn đề như mụn, nám da, và lão hóa da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7c9f60d6-cb02-420f-baf9-7392dbdc17ba', '4d65529b-adb2-410a-99ad-0c8ad3fb7867', 'concept', 'Kỹ Thuật Thực Hiện Cú Sút Phạt', 'Dùng lực từ chân và tay để sút bóng vào rổ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Cầu thủ đang thực hiện cú sút phạt trong lúc bay lên.", "videoScript": "Cảnh 1: Giới thiệu kỹ thuật thực hiện cú sút. Cảnh 2: Hướng dẫn từng bước thực hiện. Cảnh 3: Ví dụ về cú sút phạt thành công. Cảnh 4: Phân tích kỹ thuật.", "imageDescription": "Hình ảnh cầu thủ trong quá trình thực hiện cú sút phạt với động tác tay và chân rõ ràng.", "videoDescription": "Video này cung cấp hướng dẫn về cách thực hiện cú sút phạt hiệu quả trong bóng rổ."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:30.934114', '2026-01-27 12:58:30.934114', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thực hiện cú sút phạt bằng cách đẩy bóng lên.", "detailed": "Dùng lực từ chân và tay để sút bóng vào rổ.", "comprehensive": "Khi thực hiện cú sút phạt, bạn cần phải sử dụng lực từ chân để tạo đà. Bắt đầu với việc hạ thấp cơ thể, sau đó dồn trọng lượng vào chân, và dùng tay đẩy bóng lên cao. Điều quan trọng là phải giữ mắt nhìn vào rổ để tăng khả năng ghi điểm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9db4f570-bcc8-49e9-8cd5-166e01071344', '4d65529b-adb2-410a-99ad-0c8ad3fb7867', 'concept', 'Luyện Tập Kỹ Thuật Sút Phạt', 'Tạo thói quen luyện tập sút phạt hàng ngày.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Cầu thủ đang luyện tập sút phạt trên sân.", "videoScript": "Cảnh 1: Giới thiệu tầm quan trọng của luyện tập. Cảnh 2: Hướng dẫn bài tập luyện sút phạt. Cảnh 3: Ví dụ về lịch trình luyện tập. Cảnh 4: Phân tích tiến bộ qua thời gian.", "imageDescription": "Hình ảnh cầu thủ đang thực hiện các cú sút phạt để luyện tập.", "videoDescription": "Video này sẽ hướng dẫn bạn cách luyện tập kỹ thuật sút phạt hiệu quả trong bóng rổ."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:30.938833', '2026-01-27 12:58:30.938833', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Luyện tập thường xuyên để cải thiện kỹ năng.", "detailed": "Tạo thói quen luyện tập sút phạt hàng ngày.", "comprehensive": "Để cải thiện kỹ thuật sút phạt, bạn nên luyện tập đều đặn. Hãy dành ít nhất 15-30 phút mỗi ngày để thực hiện các cú sút phạt. Bạn cũng có thể ghi lại số lần thành công và thất bại để theo dõi tiến bộ của mình. Ngoài ra, nên luyện tập trong các tình huống áp lực để cải thiện khả năng chịu đựng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e690c936-bfaf-463b-b7e8-9c64e1c62f07', '0c8b67ca-ab66-40ab-b34b-68f7d82b8543', 'concept', 'Giới Thiệu Về Lập Ngân Sách Hàng Tháng', 'Lập ngân sách hàng tháng là một công cụ quan trọng giúp bạn theo dõi và kiểm soát chi tiêu của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang ngồi trước máy tính với tờ giấy và bút, lập ngân sách.", "videoScript": "Mở đầu với cảnh một người trẻ tuổi ngồi trước máy tính. Giới thiệu về tầm quan trọng của việc lập ngân sách. Chia sẻ các bước cơ bản để bắt đầu lập ngân sách.", "imageDescription": "Hình ảnh một người trẻ tuổi đang chăm chú lập kế hoạch ngân sách hàng tháng trên máy tính.", "videoDescription": "Video hướng dẫn người xem cách lập ngân sách hàng tháng một cách đơn giản và hiệu quả."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:05:40.099212', '2026-01-27 13:05:40.099212', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lập ngân sách giúp bạn quản lý tài chính cá nhân hiệu quả.", "detailed": "Lập ngân sách hàng tháng là một công cụ quan trọng giúp bạn theo dõi và kiểm soát chi tiêu của mình.", "comprehensive": "Lập ngân sách hàng tháng không chỉ giúp bạn theo dõi chi tiêu mà còn cho phép bạn xác định các mục tiêu tài chính, tiết kiệm cho tương lai và giảm nợ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4d01e2cd-e101-4b35-8208-a75f71693d7a', '0c8b67ca-ab66-40ab-b34b-68f7d82b8543', 'concept', 'Cách Xác Định Thu Nhập Hàng Tháng', 'Xác định thu nhập hàng tháng là bước đầu tiên trong việc lập ngân sách. Bạn cần tính toán mọi nguồn thu nhập từ lương, thưởng, và các khoản thu khác.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ thu nhập hàng tháng với các nguồn thu khác nhau.", "videoScript": "Giới thiệu về các nguồn thu nhập khác nhau. Hướng dẫn cách tổng hợp và tính toán thu nhập hàng tháng.", "imageDescription": "Hình ảnh biểu đồ thể hiện các nguồn thu nhập khác nhau của một cá nhân.", "videoDescription": "Video giải thích cách xác định và tổng hợp thu nhập hàng tháng để lập ngân sách chính xác."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:05:40.108249', '2026-01-27 13:05:40.108249', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biết được thu nhập hàng tháng giúp bạn lập ngân sách chính xác.", "detailed": "Xác định thu nhập hàng tháng là bước đầu tiên trong việc lập ngân sách. Bạn cần tính toán mọi nguồn thu nhập từ lương, thưởng, và các khoản thu khác.", "comprehensive": "Để lập ngân sách hiệu quả, bạn cần xác định rõ ràng tổng thu nhập hàng tháng của mình, bao gồm lương cố định, thu nhập từ công việc tự do, và các khoản thu khác như tiền cho thuê tài sản."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0f846767-9bdd-466a-a57c-6a0680e3db7b', '0c8b67ca-ab66-40ab-b34b-68f7d82b8543', 'concept', 'Phân Tích Chi Tiêu Hàng Tháng', 'Phân tích chi tiêu hàng tháng giúp bạn nhận diện các khoản chi cần thiết và những khoản chi có thể cắt giảm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng phân tích chi tiêu với nhiều danh mục khác nhau.", "videoScript": "Mở đầu với việc trình bày các khoản chi tiêu hàng tháng. Giới thiệu cách phân tích và phân loại chi tiêu để tối ưu hóa ngân sách.", "imageDescription": "Hình ảnh một bảng phân tích chi tiêu hàng tháng với các danh mục như ăn uống, giải trí, và tiết kiệm.", "videoDescription": "Video hướng dẫn cách phân tích chi tiêu hàng tháng và cách cải thiện tình hình tài chính cá nhân."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:05:40.112402', '2026-01-27 13:05:40.112402', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phân tích chi tiêu giúp bạn hiểu rõ hơn về tài chính cá nhân.", "detailed": "Phân tích chi tiêu hàng tháng giúp bạn nhận diện các khoản chi cần thiết và những khoản chi có thể cắt giảm.", "comprehensive": "Để lập ngân sách hiệu quả, bạn cần phân tích chi tiêu hàng tháng của mình, phân loại thành các nhóm như chi phí cố định, chi phí linh hoạt và chi phí cá nhân để tìm ra cách tối ưu hóa chúng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f9b4f3ef-f657-4ac3-ad08-a61f42472392', '0c8b67ca-ab66-40ab-b34b-68f7d82b8543', 'concept', 'Đặt Mục Tiêu Tài Chính', 'Việc đặt mục tiêu tài chính rõ ràng sẽ giúp bạn duy trì động lực trong việc lập ngân sách hàng tháng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng ghi chú với các mục tiêu tài chính khác nhau.", "videoScript": "Giới thiệu tầm quan trọng của việc đặt mục tiêu tài chính. Chia sẻ các bước để xác định và theo dõi mục tiêu của bạn.", "imageDescription": "Hình ảnh một bảng ghi chú với các mục tiêu tài chính như tiết kiệm cho chuyến du lịch, mua nhà, và quỹ hưu trí.", "videoDescription": "Video hướng dẫn cách đặt và theo dõi mục tiêu tài chính cá nhân để duy trì động lực trong việc lập ngân sách."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:05:40.115742', '2026-01-27 13:05:40.115742', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đặt mục tiêu giúp bạn có động lực quản lý ngân sách.", "detailed": "Việc đặt mục tiêu tài chính rõ ràng sẽ giúp bạn duy trì động lực trong việc lập ngân sách hàng tháng.", "comprehensive": "Đặt mục tiêu tài chính là một phần quan trọng trong lập ngân sách, bao gồm các mục tiêu ngắn hạn, trung hạn và dài hạn. Điều này giúp bạn có kế hoạch rõ ràng hơn cho tương lai tài chính của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('774358b1-1355-4efc-b918-af56dcc22389', '9f04f3a2-d5a2-4f72-b8fe-309ea8790d33', 'concept', 'Giới thiệu về báo cáo tài chính cá nhân', 'Báo cáo tài chính cá nhân giúp bạn hiểu rõ thu nhập, chi tiêu và tài sản của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang xem báo cáo tài chính trên máy tính.", "videoScript": "Chào mừng bạn đến với bài học về báo cáo tài chính cá nhân. Hôm nay, chúng ta sẽ khám phá cách mà báo cáo tài chính có thể giúp bạn quản lý tiền bạc.", "imageDescription": "Một người trẻ tuổi ngồi trước máy tính với các biểu đồ tài chính trên màn hình.", "videoDescription": "Video này giới thiệu về báo cáo tài chính cá nhân và tầm quan trọng của nó trong việc quản lý tài chính cá nhân."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:55.646074', '2026-01-27 12:58:55.646074', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Báo cáo tài chính cá nhân là một công cụ giúp bạn theo dõi tài chính.", "detailed": "Báo cáo tài chính cá nhân giúp bạn hiểu rõ thu nhập, chi tiêu và tài sản của mình.", "comprehensive": "Báo cáo tài chính cá nhân không chỉ giúp bạn theo dõi tình hình tài chính mà còn hỗ trợ lập kế hoạch tài chính dài hạn, quản lý nợ và đầu tư hiệu quả."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('21301864-1a5d-4a78-a8bc-833bcbce3671', '0c8b67ca-ab66-40ab-b34b-68f7d82b8543', 'concept', 'Theo Dõi Và Điều Chỉnh Ngân Sách', 'Theo dõi ngân sách hàng tháng là bước quan trọng để đảm bảo bạn đang đi đúng hướng với mục tiêu tài chính của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một ứng dụng quản lý ngân sách trên điện thoại di động.", "videoScript": "Giới thiệu về tầm quan trọng của việc theo dõi ngân sách. Hướng dẫn cách sử dụng các công cụ và ứng dụng để theo dõi và điều chỉnh ngân sách hàng tháng.", "imageDescription": "Hình ảnh một người đang sử dụng ứng dụng quản lý ngân sách trên điện thoại để theo dõi chi tiêu.", "videoDescription": "Video hướng dẫn cách theo dõi và điều chỉnh ngân sách hàng tháng để đảm bảo bạn đạt được các mục tiêu tài chính."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:05:40.119775', '2026-01-27 13:05:40.119775', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi ngân sách giúp bạn điều chỉnh kịp thời.", "detailed": "Theo dõi ngân sách hàng tháng là bước quan trọng để đảm bảo bạn đang đi đúng hướng với mục tiêu tài chính của mình.", "comprehensive": "Để lập ngân sách hiệu quả, bạn cần thường xuyên theo dõi và điều chỉnh ngân sách hàng tháng dựa trên các thay đổi trong thu nhập và chi tiêu. Việc này giúp bạn duy trì sự ổn định tài chính."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0ccbd439-5528-428e-9fb3-60873effa6db', '9f04f3a2-d5a2-4f72-b8fe-309ea8790d33', 'concept', 'Cách lập báo cáo tài chính cá nhân', 'Bắt đầu bằng cách liệt kê tất cả nguồn thu nhập và chi phí hàng tháng, sau đó tổng hợp chúng lại.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng tính với các số liệu tài chính.", "videoScript": "Trong video này, chúng ta sẽ hướng dẫn bạn cách lập báo cáo tài chính cá nhân từ đầu đến cuối. Hãy bắt đầu với việc ghi chép thu nhập và chi tiêu.", "imageDescription": "Một bảng tính Excel mở với các mục thu nhập và chi tiêu được liệt kê rõ ràng.", "videoDescription": "Video hướng dẫn từng bước cách lập báo cáo tài chính cá nhân, bao gồm các mẹo và lưu ý quan trọng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:55.654448', '2026-01-27 12:58:55.654448', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể lập báo cáo tài chính bằng cách ghi chép thu nhập và chi tiêu.", "detailed": "Bắt đầu bằng cách liệt kê tất cả nguồn thu nhập và chi phí hàng tháng, sau đó tổng hợp chúng lại.", "comprehensive": "Để lập báo cáo tài chính cá nhân, bạn cần thu thập dữ liệu về thu nhập từ lương, đầu tư, và các khoản thu khác, sau đó phân loại chi phí theo từng nhóm như ăn uống, giải trí, và tiết kiệm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('74e0d870-1522-4083-9720-1db0fbb58d53', '0b3b104b-6815-43c1-92c8-8a83d8c6d405', 'concept', 'Tẩy tế bào chết hóa học', 'Tẩy tế bào chết hóa học sử dụng các loại acid như AHA và BHA để làm sạch da, loại bỏ tế bào chết và thúc đẩy quá trình tái tạo da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một lọ sản phẩm tẩy tế bào chết hóa học.", "videoScript": "Hôm nay, chúng ta sẽ tìm hiểu về tẩy tế bào chết hóa học và cách sử dụng hiệu quả nó.", "imageDescription": "Lọ sản phẩm tẩy tế bào chết hóa học với nhãn hiệu nổi tiếng.", "videoDescription": "Video hướng dẫn về tẩy tế bào chết hóa học và những lợi ích của nó cho làn da."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:12:36.14944', '2026-01-27 13:12:36.14944', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tẩy tế bào chết hóa học giúp loại bỏ tế bào da chết bằng các loại acid.", "detailed": "Tẩy tế bào chết hóa học sử dụng các loại acid như AHA và BHA để làm sạch da, loại bỏ tế bào chết và thúc đẩy quá trình tái tạo da.", "comprehensive": "Tẩy tế bào chết hóa học là một bước quan trọng trong quy trình chăm sóc da. Các loại acid có trong sản phẩm giúp làm sạch sâu lỗ chân lông, giảm mụn và làm sáng da. Việc sử dụng đúng cách sẽ mang lại hiệu quả tối ưu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b88f853b-310a-4ac4-92d7-5b0f2c550468', '0b3b104b-6815-43c1-92c8-8a83d8c6d405', 'concept', 'Liệu pháp ánh sáng LED', 'Liệu pháp ánh sáng LED sử dụng các loại ánh sáng khác nhau để điều trị các vấn đề về da như mụn, lão hóa và sắc tố không đều.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một thiết bị liệu pháp ánh sáng LED.", "videoScript": "Khám phá liệu pháp ánh sáng LED và cách nó có thể cải thiện làn da của bạn.", "imageDescription": "Thiết bị liệu pháp ánh sáng LED với các bóng đèn màu sắc khác nhau.", "videoDescription": "Video giải thích về liệu pháp ánh sáng LED và các loại ánh sáng khác nhau."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:12:36.15286', '2026-01-27 13:12:36.15286', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Liệu pháp ánh sáng LED giúp cải thiện tình trạng da bằng cách sử dụng ánh sáng màu.", "detailed": "Liệu pháp ánh sáng LED sử dụng các loại ánh sáng khác nhau để điều trị các vấn đề về da như mụn, lão hóa và sắc tố không đều.", "comprehensive": "Liệu pháp ánh sáng LED là một công nghệ hiện đại trong chăm sóc da. Ánh sáng đỏ giúp kích thích collagen, ánh sáng xanh giúp điều trị mụn, và ánh sáng vàng giúp làm sáng da. Mỗi loại ánh sáng có những lợi ích riêng, và việc kết hợp chúng có thể mang lại kết quả tốt nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('dfb83f50-18cb-4d42-b065-5652bd180802', '9f04f3a2-d5a2-4f72-b8fe-309ea8790d33', 'concept', 'Phân tích báo cáo tài chính cá nhân', 'Phân tích báo cáo tài chính giúp bạn đánh giá được điểm mạnh và điểm yếu trong quản lý tài chính.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang phân tích báo cáo tài chính với bút và giấy.", "videoScript": "Trong phần này, chúng ta sẽ xem xét cách phân tích báo cáo tài chính cá nhân để đưa ra quyết định tài chính thông minh hơn.", "imageDescription": "Một cá nhân đang ngồi với một tập tài liệu tài chính và bút, có vẻ như đang ghi chú những điểm quan trọng.", "videoDescription": "Video này hướng dẫn cách phân tích báo cáo tài chính cá nhân và đưa ra những nhận định có giá trị."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:55.657664', '2026-01-27 12:58:55.657664', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phân tích giúp bạn hiểu rõ tình hình tài chính của mình.", "detailed": "Phân tích báo cáo tài chính giúp bạn đánh giá được điểm mạnh và điểm yếu trong quản lý tài chính.", "comprehensive": "Qua phân tích báo cáo tài chính cá nhân, bạn có thể nhận diện các xu hướng chi tiêu, xác định những lĩnh vực cần cải thiện và lập kế hoạch tài chính tốt hơn cho tương lai."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('148f24fb-5922-4879-9d57-e7e26d08c1fb', '0b3b104b-6815-43c1-92c8-8a83d8c6d405', 'concept', 'Thư giãn với liệu pháp massage mặt', 'Liệu pháp massage mặt không chỉ giúp thư giãn mà còn kích thích lưu thông máu, giúp da trở nên khỏe mạnh và rạng rỡ hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một chuyên viên thực hiện massage mặt cho khách hàng.", "videoScript": "Hãy cùng tôi tìm hiểu về liệu pháp massage mặt và những lợi ích của nó.", "imageDescription": "Một chuyên viên massage mặt cho khách hàng trong không gian spa thư giãn.", "videoDescription": "Video hướng dẫn kỹ thuật massage mặt và lợi ích của nó cho làn da."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:12:36.156107', '2026-01-27 13:12:36.156107', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Massage mặt giúp thư giãn và cải thiện tuần hoàn máu.", "detailed": "Liệu pháp massage mặt không chỉ giúp thư giãn mà còn kích thích lưu thông máu, giúp da trở nên khỏe mạnh và rạng rỡ hơn.", "comprehensive": "Massage mặt là một phương pháp chăm sóc da tuyệt vời giúp cải thiện tình trạng da và mang lại cảm giác thư giãn. Kỹ thuật massage đúng cách có thể giúp giảm căng thẳng, làm giảm nếp nhăn và thúc đẩy quá trình tái tạo da. Kết hợp với các sản phẩm chăm sóc da sẽ mang lại hiệu quả tối ưu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c4ff7fb7-2c07-4f95-aa3e-292412d7291e', '9f04f3a2-d5a2-4f72-b8fe-309ea8790d33', 'concept', 'Lập kế hoạch tài chính dựa trên báo cáo', 'Lập kế hoạch tài chính giúp bạn đặt ra mục tiêu và đạt được sự ổn định tài chính.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một kế hoạch tài chính với các mục tiêu được ghi chú.", "videoScript": "Hãy cùng khám phá cách lập kế hoạch tài chính dựa trên báo cáo tài chính cá nhân của bạn. Điều này sẽ giúp bạn đạt được những mục tiêu tài chính của mình.", "imageDescription": "Một tài liệu kế hoạch tài chính với các mục tiêu như mua nhà, tiết kiệm hưu trí, và quỹ khẩn cấp được ghi rõ.", "videoDescription": "Video này cung cấp hướng dẫn lập kế hoạch tài chính từ báo cáo tài chính cá nhân để đạt được mục tiêu tài chính."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:55.663387', '2026-01-27 12:58:55.663387', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Dựa vào báo cáo, bạn có thể lập kế hoạch tài chính cho tương lai.", "detailed": "Lập kế hoạch tài chính giúp bạn đặt ra mục tiêu và đạt được sự ổn định tài chính.", "comprehensive": "Dựa trên phân tích báo cáo tài chính cá nhân, bạn có thể lập kế hoạch tài chính cho từng giai đoạn, bao gồm tiết kiệm, đầu tư, và chi tiêu hợp lý để đạt được các mục tiêu tài chính dài hạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f06897e3-b1b9-4cb2-9694-e3e711a28d95', '0b3b104b-6815-43c1-92c8-8a83d8c6d405', 'concept', 'Mặt nạ dưỡng da chuyên sâu', 'Mặt nạ dưỡng da là sản phẩm chăm sóc da được sử dụng để cung cấp độ ẩm và các dưỡng chất cần thiết cho làn da, giúp da trở nên mềm mại và khỏe mạnh.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang đắp mặt nạ dưỡng da.", "videoScript": "Hãy khám phá các loại mặt nạ dưỡng da chuyên sâu và cách chúng có thể cải thiện làn da của bạn.", "imageDescription": "Người phụ nữ thư giãn khi đắp mặt nạ dưỡng da trong không gian yên tĩnh.", "videoDescription": "Video giới thiệu về các loại mặt nạ dưỡng da và cách sử dụng hiệu quả."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:12:36.16069', '2026-01-27 13:12:36.16069', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mặt nạ dưỡng da giúp cung cấp độ ẩm và nuôi dưỡng làn da.", "detailed": "Mặt nạ dưỡng da là sản phẩm chăm sóc da được sử dụng để cung cấp độ ẩm và các dưỡng chất cần thiết cho làn da, giúp da trở nên mềm mại và khỏe mạnh.", "comprehensive": "Mặt nạ dưỡng da chuyên sâu là một phần thiết yếu trong quy trình chăm sóc da. Chúng có thể chứa các thành phần tự nhiên và các chất dinh dưỡng khác nhau nhằm mục đích cung cấp độ ẩm, làm sáng da và điều trị các vấn đề về da. Việc sử dụng mặt nạ đúng cách sẽ giúp làn da bạn luôn khỏe mạnh và rạng rỡ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0dc0be60-f077-4c7b-9951-1bc98a5902e2', '9f04f3a2-d5a2-4f72-b8fe-309ea8790d33', 'concept', 'Theo dõi và điều chỉnh báo cáo tài chính', 'Điều chỉnh báo cáo tài chính là cần thiết khi có thay đổi trong thu nhập hoặc chi tiêu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang kiểm tra báo cáo tài chính hàng tháng.", "videoScript": "Trong video này, chúng ta sẽ thảo luận về tầm quan trọng của việc theo dõi và điều chỉnh báo cáo tài chính cá nhân theo thời gian.", "imageDescription": "Một cá nhân đang xem xét báo cáo tài chính trên máy tính, có vẻ như đang thực hiện các điều chỉnh cần thiết.", "videoDescription": "Video này giải thích cách theo dõi và điều chỉnh báo cáo tài chính cá nhân để duy trì sự ổn định tài chính."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:58:55.666749', '2026-01-27 12:58:55.666749', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi báo cáo tài chính giúp bạn luôn cập nhật tình hình tài chính.", "detailed": "Điều chỉnh báo cáo tài chính là cần thiết khi có thay đổi trong thu nhập hoặc chi tiêu.", "comprehensive": "Việc theo dõi và điều chỉnh báo cáo tài chính cá nhân là rất quan trọng để phản ánh chính xác tình hình tài chính hiện tại, giúp bạn có những quyết định kịp thời và phù hợp với mục tiêu tài chính đã đề ra."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7a38988d-50d6-4558-b8e7-caf6e444e400', '00bbe76d-6014-485e-890f-1398b6100e68', 'example', 'Kỹ Thuật Ném Bóng Từ Vị Trí Khó', 'Đôi khi, bạn có thể gặp tình huống khó khăn, như ném bóng từ một góc rất hẹp hoặc khi có người đối phương chắn trước mặt bạn. Đừng lo, hãy suy nghĩ về những kỹ thuật này:\n\n1. **Tìm khoảng trống**: Khi bạn thấy một cơ hội ném, hãy nhanh chóng đánh giá khoảng trống. Có thể bạn cần phải di chuyển một chút để tìm vị trí tốt hơn.\n\n2. **Sử dụng tay không ném**: Nếu bạn không thể ném bóng thẳng, hãy sử dụng tay không ném để kiểm soát hướng bóng. Giống như khi bạn đang hướng dẫn ai đó đi theo một hướng nào đó.\n\n3. **Một cú ném đặc biệt**: Thay vì ném bóng ngang, bạn có thể thử ném bóng cao lên trời. Đây là cách thú vị để lừa đối phương. Hãy tưởng tượng bạn đang đẩy một quả bóng cao lên như một bông hoa hướng dương.\n\n4. **Luôn luyện tập**: Bạn cần phải luyện tập những kỹ thuật này nhiều lần. Hãy thử thực hành một mình hoặc cùng bạn bè để cảm thấy tự tin hơn.\n\nKhi bạn thành thạo các kỹ thuật này, không có góc độ nào là khó. Bạn sẽ trở thành một cầu thủ nổi bật trên sân!', '{"imageUrl": "https://placehold.co/800x600/dcfce7/166534?text=%F0%9F%8C%B1+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+%C4%90%C6%A1n+Gi%E1%BA%A3n)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A simplified, colorful illustration showing a basketball player throwing a ball from a difficult angle. The player is depicted in a bright, cartoonish style with an exaggerated expression of concentration. The background is a basketball court with simplified lines and a hoop. Include visual cues like arrows to suggest movements such as finding space and using the non-throwing hand, alongside a cartoon flower to symbolize the ''high throw'' technique.", "videoScript": "Chào mừng các bạn đến với bài học về kỹ thuật ném bóng từ vị trí khó! Đôi khi, bạn sẽ phải đối mặt với những góc hẹp hoặc có đối thủ chắn trước. Đầu tiên, hãy nhanh chóng tìm khoảng trống! Di chuyển để có vị trí tốt hơn. Nếu không thể ném thẳng, sử dụng tay không ném để giúp điều khiển hướng bóng. Và hãy thử cú ném cao như đẩy một bông hoa hướng dương! Cuối cùng, hãy nhớ rằng luyện tập là chìa khóa. Cứ tập đi tập lại để cảm thấy tự tin hơn trong mọi tình huống. Chúc các bạn thành công!", "videoDuration": "30-60 giây", "imageDescription": "Hình ảnh minh họa một cầu thủ bóng rổ đang ném bóng từ vị trí khó, với các mũi tên chỉ đường di chuyển và hình ảnh hoa biểu tượng cho cú ném cao.", "videoDescription": "Video này hướng dẫn bạn kỹ thuật ném bóng từ các vị trí khó trong bóng rổ một cách đơn giản và dễ hiểu."}', 7, '{"xp": 12, "coin": 1}', NULL, '2026-01-15 22:09:46.429475', '2026-01-19 23:39:17.534787', 'mixed', 'easy', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4f273462-fab8-48ca-9751-6ce0914fb5c3', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'concept', 'Khái Niệm Dribble', 'Dribble là một kỹ thuật rất quan trọng trong bóng rổ, cho phép người chơi di chuyển trong khi vẫn kiểm soát bóng. Bạn có thể tưởng tượng như việc đi bộ và giữ một quả bóng trong tay; tuy nhiên, thay vì giữ nó trong tay, bạn sẽ để nó nhảy lên xuống trên mặt đất. Khi bạn dribble, bạn sẽ dùng tay để chạm vào bóng và đẩy nó ra phía trước khi nó chạm đất. Điều này giúp bạn di chuyển nhanh chóng và linh hoạt hơn trên sân. Mục tiêu của dribble là không để bóng rơi vào tay đối thủ, và giúp bạn tạo khoảng trống để thực hiện các đường chuyền hoặc ghi điểm. Hãy nhớ rằng, một kỹ thuật dribble tốt sẽ giúp bạn trở thành một cầu thủ bóng rổ xuất sắc!', '{"imageUrl": "https://placehold.co/800x600/dcfce7/166534?text=%F0%9F%8C%B1+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+%C4%90%C6%A1n+Gi%E1%BA%A3n)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A simple and colorful illustration of a young child dribbling a basketball on a court. The child is smiling and wearing a basketball jersey. The basketball is clearly bouncing on the ground, showing motion. The background features a basketball court with bright colors and limited details. The style is cartoonish and playful, aimed at beginners in basketball education.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ cùng khám phá khái niệm dribble trong bóng rổ.\nDribble là một kỹ thuật quan trọng giúp bạn di chuyển và kiểm soát bóng. Hãy tưởng tượng bạn đang đi bộ và giữ một quả bóng. Nhưng thay vì cầm nó, bạn sẽ để bóng nhảy lên xuống trên mặt đất.\nKhi dribble, bạn dùng tay chạm vào bóng và đẩy nó ra phía trước. Điều này giúp bạn nhanh nhẹn hơn trên sân! \nHãy luôn nhớ rằng, một kỹ thuật dribble tốt sẽ giúp bạn không chỉ giữ bóng mà còn tạo khoảng trống để ghi điểm hoặc chuyền bóng cho đồng đội!\nHẹn gặp lại các bạn trong bài học sau nhé!", "videoDuration": "30-60 giây", "imageDescription": "Hình ảnh minh họa một đứa trẻ đang thực hiện kỹ thuật dribble bóng rổ trên sân, với nụ cười tươi và trang phục thể thao, giúp dễ dàng hiểu về kỹ thuật bóng rổ cơ bản.", "videoDescription": "Video này giới thiệu khái niệm dribble trong bóng rổ, giúp người xem hiểu rõ về kỹ thuật di chuyển và kiểm soát bóng cơ bản."}', 6, '{"xp": 8, "coin": 1}', NULL, '2026-01-15 22:11:48.480801', '2026-01-19 23:39:17.542286', 'mixed', 'easy', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('73e17cfc-7b9b-4fd3-8fed-e5dcf5e5400b', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'concept', 'Các Loại Dribble Cơ Bản', 'Có nhiều loại dribble mà bạn có thể sử dụng trong bóng rổ, nhưng ở đây, chúng ta sẽ nói về ba loại cơ bản. Đầu tiên là ''Stationary Dribble'', tức là bạn đứng tại chỗ và dribble bóng lên xuống quanh người. Điều này giúp bạn làm quen với cảm giác bóng. Thứ hai là ''Crossover Dribble'', bạn sẽ di chuyển bóng từ tay này sang tay khác, như thể bóng đang nhảy múa giữa hai tay của bạn. Kỹ thuật này rất hay được dùng để đánh lừa đối thủ. Cuối cùng, có ''Speed Dribble'', khi bạn chạy nhanh và dribble bóng cùng một lúc. Loại dribble này thường giúp bạn vượt qua các đối thủ và di chuyển nhanh chóng trên sân. Luyện tập cả ba loại này sẽ giúp bạn linh hoạt và tự tin hơn khi chơi bóng rổ.', '{"imageUrl": "https://placehold.co/800x600/dcfce7/166534?text=%F0%9F%8C%B1+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+%C4%90%C6%A1n+Gi%E1%BA%A3n)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A simple and colorful illustration depicting three basic basketball dribble techniques. 1. Stationary Dribble: A cartoon character dribbling a basketball up and down while standing still. 2. Crossover Dribble: The same character moving the ball from one hand to the other with a playful expression. 3. Speed Dribble: The character running while dribbling the ball, showing movement and energy. The style should be bright, engaging, and easy for children to understand.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ khám phá ba loại dribble cơ bản trong bóng rổ! Đầu tiên là ''Stationary Dribble''. Đây là khi bạn đứng tại chỗ và dribble bóng lên xuống quanh người. Nó giúp bạn làm quen với cảm giác bóng. Tiếp theo là ''Crossover Dribble''. Ở đây, bạn sẽ di chuyển bóng từ tay này sang tay khác, như thể bóng đang nhảy múa giữa hai tay của bạn. Kỹ thuật này rất hữu ích để đánh lừa đối thủ! Cuối cùng là ''Speed Dribble''. Bạn sẽ chạy nhanh và dribble bóng cùng lúc. Đây là cách tuyệt vời để vượt qua đối thủ! Hãy luyện tập cả ba loại dribble này để trở nên linh hoạt và tự tin hơn khi chơi bóng rổ nhé!", "videoDuration": "30-60 giây", "imageDescription": "Hình ảnh minh họa ba kỹ thuật dribble cơ bản trong bóng rổ: đứng tại chỗ, chuyển bóng, và chạy với bóng.", "videoDescription": "Video giới thiệu ba loại dribble cơ bản trong bóng rổ, giúp người xem nắm rõ kỹ thuật và cải thiện khả năng chơi bóng."}', 8, '{"xp": 8, "coin": 1}', NULL, '2026-01-15 22:11:48.516274', '2026-01-19 23:39:17.547128', 'mixed', 'easy', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ab93df44-b2b2-4898-b874-6e6ace4ff35b', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'example', 'Tích Hợp Dribble Trong Các Chiến Thuật Tấn Công: Trường Hợp Của Kobe Bryant', '# Tích Hợp Dribble Trong Các Chiến Thuật Tấn Công: Trường Hợp Của Kobe Bryant\n\n## Giới thiệu\nKobe Bryant, một trong những cầu thủ xuất sắc nhất trong lịch sử bóng rổ, đã chứng minh rằng dribble là một phần thiết yếu trong các chiến thuật tấn công. Bài viết này sẽ phân tích cách mà Kobe tích hợp kỹ thuật dribble trong cách tiếp cận tấn công đa dạng của mình để khám phá sự thành công của anh trong các trận đấu.\n\n## 1. Phân Tích Phong Cách Chơi Của Kobe\nKobe nổi tiếng với phong cách chơi độc lập, có nghĩa là anh thường tự quyết định khi nào nên thực hiện dribble. Điều này cho phép anh tạo ra không gian và phương pháp dứt điểm rất riêng, từ ném xa đến ném điểm thuận tay.\n\n## 2. Tình Huống Dribble Đặc Biệt\n### 2.1 Dribble Nhanh Trước Khi Dứt Điểm\nTrong trận đấu với Miami Heat, Kobe đã sử dụng một động tác dribble nhanh trước khi thực hiện cú ném. Việc này không chỉ giúp anh tạo khoảng trống mà còn đánh lừa những người phòng ngự theo kịp anh.\n\n### 2.2 Crossover và Fadeaway\nMột tính năng thú vị khác là việc sử dụng crossover trước khi thực hiện cú ném fadeaway. Kỹ thuật này thể hiện khả năng phối hợp giữa dribble và dứt điểm, tạo nên một cú ghi điểm khó cản phá. \n\n## 3. Động Tác Dribble và Tình Huống Cụ Thể\n### 3.1 Đọc Kẻ Đuổi\nViệc đọc kẻ đuổi là điểm mạnh của Kobe. Ông chính là một bậc thầy trong việc điều chỉnh động tác dribble và linh hoạt thay đổi tốc độ. Bằng cách này, Kobe không chỉ hạn chế sự tiếp cận của kẻ đuổi mà còn tạo ra khoảng trống cho các pha bóng khác.\n\n## 4. Các Phương Pháp Thực Hiện\n### 4.1 Cải Tiến Kỹ Thuật Dribble\nNgười chơi nên cải thiện kỹ thuật dribble bằng cách thực hiện nhiều bài tập tập trung vào tốc độ và kiểm soát. Một mẫu luyện tập hiệu quả có thể là giảm tốc độ sau một cú dribble mạnh sẽ giúp phát triển kỹ năng xử lý bóng trong tình huống áp lực.\n\n## 5. Tổng Kết\nTích hợp kỹ thuật dribble vào các chiến thuật tấn công giúp nâng cao hiệu quả ghi điểm cho đội. Việc nghiên cứu và phân tích các tình huống như Kobe Bryant không chỉ giúp cầu thủ trẻ phát triển bản thân mà còn tăng cường khả năng tương tác và phối hợp với đồng đội.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "An educational basketball scene depicting the advanced dribble techniques of Kobe Bryant. Showcase detailed illustrations of Kobe dribbling, including images of him executing quick dribbles before shots, crossover moves, and fadeaway shots. Use technical diagrams to highlight the mechanics of dribbling and footwork, incorporating arrows and annotations illustrating player movements, defensive maneuvers, and scoring methods. The style should be professional, detailed, and suitable for a basketball coaching tutorial.", "videoScript": "Xin chào các bạn đến với bài học ngày hôm nay về việc tích hợp kỹ thuật dribble vào chiến thuật tấn công qua trường hợp của Kobe Bryant. Trong video này, chúng ta sẽ cùng nhau phân tích phong cách chơi độc đáo của Kobe, người đã sử dụng dribble như một công cụ mạnh mẽ trong mọi tình huống tấn công. Đầu tiên, hãy cùng nói về cách mà Kobe đã tạo không gian bằng những cú dribble nhanh. Như trong trận đấu với Miami Heat, anh ấy đã tải tốc độ và thực hiện cú ném sút. Tiếp theo, chúng ta sẽ xem xét kỹ thuật crossover và fadeaway - một trong những động tác độc đáo của Kobe, cho phép anh ghi điểm trong những tình huống khó khăn. Cùng nhau, chúng ta sẽ học cách đọc kẻ đuổi để điều chỉnh động tác dribble một cách linh hoạt. Cuối cùng, tôi sẽ chia sẻ một số phương pháp luyện tập để cải thiện kỹ thuật dribble của bạn. Hy vọng video này sẽ giúp bạn nâng cao kỹ năng chơi bóng của mình!", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa các kỹ thuật dribble nâng cao của Kobe Bryant, thể hiện các động tác và cơ chế chính xác trong bóng rổ.", "videoDescription": "Video phân tích các kỹ thuật dribble của Kobe Bryant và cách tích hợp chúng vào các chiến thuật tấn công trong bóng rổ."}', 2, '{"xp": 20, "coin": 4}', NULL, '2026-01-15 22:13:29.45924', '2026-01-19 23:39:17.561979', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f4f762bf-80d3-4c03-a680-23c1a86f8e6d', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'example', 'Sử Dụng Nhiều Loại Dribble', 'Khi bạn đã quen với dribble cơ bản, bạn có thể thử nghiệm với nhiều loại khác nhau. Ví dụ, trong một trận đấu, nếu bạn chạy và thấy đối thủ cản đường, bạn có thể sử dụng chiến thuật crossover dribble. Hãy tưởng tượng như bạn đang chơi một trò chơi trong đó có rất nhiều người và bạn muốn tránh ai đó. Bằng cách nhanh chóng chuyển bóng từ tay trái sang tay phải, bạn có thể khiến đối thủ lúng túng và tạo ra khoảng trống cho mình. Sau đó, nếu không có ai chặn, bạn có thể chạy với tốc độ và dùng speed dribble để vượt qua đối thủ. Mỗi loại dribble đều như một công cụ trong túi của bạn, giúp bạn linh hoạt và khó bị đoán trước trong trò chơi!', '{"imageUrl": "https://placehold.co/800x600/dcfce7/166534?text=%F0%9F%8C%B1+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+%C4%90%C6%A1n+Gi%E1%BA%A3n)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A simple, colorful illustration showing a basketball player dribbling with both hands on a basketball court. The player is in motion, transitioning from a left-handed dribble to a right-handed crossover dribble, with a cartoonish style and minimal details. The background should include a bright basketball court with a hoop, and some animated opponents looking confused. Overall, the image should be educational and engaging, suitable for young learners.", "videoScript": "Chào các bạn! Hôm nay chúng ta sẽ cùng khám phá nhiều loại dribble trong bóng rổ nhé! Khi bạn đã nắm vững dribble cơ bản, bạn có thể thử nghiệm với nhiều loại khác nhau. Như bạn thấy đây, khi gặp đối thủ cản đường, bạn hãy thử dùng crossover dribble! Hãy tưởng tượng bạn đang chạy và cần phải né tránh người khác. Bằng cách chuyển bóng nhanh từ tay trái sang tay phải, bạn có thể làm cho đối thủ bối rối và tạo ra một khoảng trống cho mình. Nếu không có ai chặn bạn, hãy tăng tốc và dùng speed dribble để vượt qua đối thủ nhé! Nhớ rằng mỗi loại dribble đều là một công cụ giúp bạn linh hoạt và thật khó đoán trong trận đấu. Chúc các bạn chơi bóng vui vẻ!", "videoDuration": "30-60 giây", "imageDescription": "Hình ảnh minh họa một cầu thủ bóng rổ đang thực hiện động tác dribble. Cầu thủ chuyển bóng từ tay trái sang tay phải với phong cách vui nhộn, tạo sự thu hút cho các bạn học sinh.", "videoDescription": "Video này sẽ giúp bạn hiểu rõ về các loại dribble trong bóng rổ, với các ví dụ thú vị và trực quan."}', 3, '{"xp": 12, "coin": 1}', NULL, '2026-01-15 22:11:48.548629', '2026-01-19 23:39:17.567043', 'mixed', 'easy', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d6e8e59a-4e76-4edd-ad60-9fad1f56fbaf', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'concept', 'Tích Hợp Kỹ Thuật Dribble Trong Chiến Thuật Tấn Công', '# Tích Hợp Kỹ Thuật Dribble Trong Chiến Thuật Tấn Công\n\n## Giới thiệu\nViệc áp dụng kỹ thuật dribble một cách chính xác không chỉ giúp người chơi tự tin hơn mà còn tạo ra những cú ghi điểm bất ngờ cho đội. Bài viết này sẽ phân tích sâu về cách tích hợp kỹ thuật dribble vào các chiến thuật tấn công trong bóng rổ, bao gồm cả các trường hợp điển hình và phương pháp nâng cao.\n\n## 1. Khái Niệm Về Tấn Công Trong Bóng Rổ\n### 1.1 Chiến Thuật Tấn Công\n- **Chiến thuật tấn công**: Là cách mà một đội sử dụng các kỹ thuật, vị trí và di chuyển để tạo ra cơ hội ghi điểm. Trong bóng rổ, sự phối hợp giữa dribble và các kỹ thuật khác như chuyền và dứt điểm là cực kỳ quan trọng.\n\n### 1.2 Vai Trò Của Dribble\nDribble không chỉ là công cụ để di chuyển mà còn để phá vỡ hàng phòng ngự và tạo khoảng trống cho các đồng đội. Việc hiểu được giá trị của kỹ thuật này giúp người chơi quyết định khi nào nên sử dụng dribble và khi nào nên chuyền.\n\n## 2. Các Chiến Thuật Tấn Công Nâng Cao\n### 2.1 Pick and Roll\nTrong chiến thuật pick and roll, người chơi cầm bóng sẽ thực hiện dribble qua một đồng đội (người cược) để tạo ra không gian. Kỹ thuật dribble này cần phải được triển khai một cách tinh tế và linh hoạt để tạo điều kiện cho đồng đội thực hiện cú dứt điểm hoặc tùy thuộc vào tình huống.\n\n### 2.2 Isolation (Iso)\nChiến thuật iso thường được sử dụng cho những cầu thủ có khả năng dribble tốt. Trong trường hợp này, cầu thủ cầm bóng tạo khoảng trống cho mình bằng cách sử dụng dribble để đối đầu với một cầu thủ phòng ngự, đồng thời cần chú ý đến các đồng đội ở vị trí không bóng. Tuy nhiên, nếu không có tính toán hợp lý, điều này có thể tạo ra sự rối rắm trong tấn công.\n\n### 2.3 Fast Break\nDribble trong tình huống fast break mang lại cơ hội tuyệt vời để ghi điểm một cách nhanh chóng. Người chơi cần phải có khả năng đọc tình huống và nhanh chóng quyết định giữa việc giữ bóng và chuyền cho một đồng đội trong vị trí tốt.\n\n## 3. Lý Thuyết Liên Quan\n### 3.1 Phân Tích Tình Huống\nViệc đọc tình huống chiến thuật có thể ảnh hưởng lớn đến quyết định khi dribble. Hãy phân tích cách đối thủ di chuyển và khoảng cách giữa họ và đồng đội để đưa ra những quyết định hợp lý.\n\n### 3.2 Sự Đồng Bộ Trong Nhóm\nSự đồng bộ giữa các cầu thủ là cực kỳ quan trọng trong việc thực hiện các chiến thuật tấn công. Khi một cầu thủ kiểm soát bóng thông qua dribble, các đồng đội cần phải di chuyển hợp lý để tạo ra cơ hội ghi điểm.\n\n## 4. Best Practices và Anti-Patterns\n### 4.1 Best Practices\n- **Luôn chú ý đến vị trí của đồng đội**: Người chơi nên luôn nhìn về phía trước trong khi dribble để tìm kiếm khoảng trống hoặc cơ hội chuyền bóng.\n- **Thực hành kỹ thuật dribble**: Việc nâng cao kỹ thuật dribble sẽ giúp cải thiện khả năng xử lý trong các tình huống áp lực.\n\n### 4.2 Anti-Patterns\n- **Bỏ qua quyết định chuyền**: Nhiều cầu thủ, khi đã có bóng trong tay, thường có xu hướng chỉ dribble mà không nghĩ đến việc chuyền. Điều này có thể làm giảm hiệu quả tấn công của đội.\n- **Dribble quá dài mà không có kế hoạch**: Việc dribble mà không có mục tiêu cụ thể có thể dẫn đến việc mất bóng hay gặp khó khăn khi bị tấn công bởi đối thủ.\n\n## 5. Tổng Kết\nKỹ thuật dribble mạnh mẽ không chỉ mang lại khả năng ghi điểm cá nhân mà còn tăng cường nền tảng cho các chiến thuật tấn công trong bóng rổ. Sự kết hợp giữa kỹ thuật, giao tiếp và phân tích tình huống sẽ giúp nâng cao hiệu quả sử dụng dribble, từ đó tạo ra cơ hội chiến thắng cho đội.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "An advanced educational diagram illustrating the integration of dribbling techniques in basketball offensive strategies. The diagram should visually represent the pick and roll, isolation, and fast break tactics with detailed annotations for each technique. Include arrows showing player movement, highlights of dribbling actions, and strategic positioning of players on the court. The style should be professional and informative, suitable for a basketball coaching class.", "videoScript": "Chào mừng các bạn đến với bài học về tích hợp kỹ thuật dribble trong chiến thuật tấn công trong bóng rổ. Hôm nay, chúng ta sẽ khám phá vai trò quan trọng của dribble trong ba chiến thuật tấn công chính: pick and roll, isolation, và fast break.\n\nĐầu tiên, hãy tìm hiểu về chiến thuật pick and roll. Khi cầu thủ cầm bóng di chuyển qua một đồng đội để tạo khoảng không, việc sử dụng dribble một cách linh hoạt sẽ quyết định thành công trong tình huống này. Hãy chú ý rằng cầu thủ cầm bóng cần có khả năng đọc tình huống nhanh chóng để đưa ra quyết định chính xác.\n\nTiếp theo là chiến thuật isolation. Đây là một chiến thuật tuyệt vời cho những cầu thủ cầm bóng giỏi trong việc dribble. Họ cần tạo không gian cho mình và đồng thời theo dõi các động thái của đồng đội. Nhưng hãy cẩn thận, vì nếu không tính toán hợp lý, sự rối rắm có thể xảy ra trong tấn công.\n\nCuối cùng, chiến thuật fast break cho phép chúng ta tận dụng những cơ hội ghi điểm nhanh chóng. Dribble trong tình huống này cực kỳ quan trọng, và người chơi cần quyết định ngay khi nào nên giữ bóng và khi nào nên chuyền cho đồng đội.\n\nHy vọng rằng với những phân tích này, các bạn sẽ áp dụng tốt các kỹ thuật dribble trong các trận đấu thực tế. Cảm ơn các bạn đã theo dõi!", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa các chiến thuật tấn công trong bóng rổ, bao gồm pick and roll, isolation, và fast break. Mỗi kỹ thuật được mô tả chi tiết với các mũi tên chỉ hướng di chuyển của cầu thủ.", "videoDescription": "Video phân tích sâu về việc tích hợp kỹ thuật dribble trong các chiến thuật tấn công bóng rổ như pick and roll, isolation và fast break."}', 7, '{"xp": 15, "coin": 3}', NULL, '2026-01-15 22:13:29.442894', '2026-01-19 23:39:17.572451', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('21bd8913-8bb0-442b-9a76-0ecc36b754be', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Tư Thế Cơ Thể Khi Ném Bóng', '# Tư Thế Cơ Thể Khi Ném Bóng\n\nTư thế cơ thể là yếu tố quyết định đến sức mạnh và độ chính xác của cú ném. Một tư thế tốt sẽ giúp bạn tối ưu hóa sức mạnh từ chân và thân trên.\n\n## Các Bước Để Tạo Tư Thế Tốt\n1. **Chân**: Đặt chân rộng bằng vai, chân thuận (chân ném) nên đứng hơi lùi về phía sau.\n2. **Đầu Gối**: Gập nhẹ đầu gối để tạo lực ném, điều này sẽ giúp bạn có được sức mạnh tối ưu từ cơ thể.\n3. **Thân**: Giữ thẳng lưng và hơi nghiêng người về phía trước để tăng cường sức mạnh cho cú ném.\n4. **Tay**: Đưa tay ném lên cao và giữ thẳng với bóng.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Nhiều người chơi không gập đầu gối, dẫn đến việc thiếu sức mạnh cho cú ném.\n- **Lời Khuyên**: Hãy luyện tập đứng đúng tư thế và luôn kiểm tra lại trước khi ném.\n\n## Lưu Ý Quan Trọng\n- Không nên đứng quá xa hoặc gần so với rổ.\n- Thường xuyên luyện tập tư thế để tạo thói quen tốt.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A detailed illustration showing the proper body posture for basketball shooting. Include labels for the feet placement, knee bend, torso alignment, and hand position. The character should be in a dynamic stance, showcasing the right athletic posture. Use a professional style, and ensure it is educational with clear annotations.", "videoScript": "Chào mừng các bạn đến với bài học hôm nay về tư thế cơ thể khi ném bóng rổ. Tư thế rất quan trọng trong việc nâng cao sức mạnh và độ chính xác của cú ném. Hãy cùng tìm hiểu các bước để có một tư thế tốt nhé!\n\nĐầu tiên, đặt chân rộng bằng vai, hãy chú ý rằng chân thuận của bạn nên đứng hơi lùi về phía sau. Điều này giúp bạn có được sự cân bằng tốt hơn khi chuẩn bị ném.\n\nTiếp theo, hãy gập nhẹ đầu gối. Việc này không chỉ giúp bạn tạo lực ném mà còn mang lại sức mạnh tối ưu từ cơ thể.\n\nSau đó, giữ thẳng lưng và hơi nghiêng người về phía trước. Điều này sẽ tăng cường sức mạnh cho cú ném của bạn.\n\nCuối cùng, hãy đưa tay ném lên cao và giữ thẳng với bóng. Đảm bảo rằng bạn có cảm giác thoải mái và tự tin khi ném.\n\nNhớ rằng, một sai lầm thường gặp là không gập đầu gối, điều này dẫn đến việc thiếu sức mạnh cho cú ném. Hãy luyện tập đứng đúng tư thế và thường xuyên kiểm tra lại trước khi ném. \n\nCảm ơn các bạn đã theo dõi! Hãy cùng tập luyện để cải thiện cú ném của mình nhé!", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa tư thế đúng khi ném bóng rổ, bao gồm vị trí chân, gập đầu gối, tư thế thân, và cách đặt tay ném.", "videoDescription": "Video này hướng dẫn các bạn về tư thế cơ thể khi ném bóng trong bóng rổ, bao gồm các bước để tạo ra tư thế ném tốt."}', 3, '{"xp": 10, "coin": 1}', NULL, '2026-01-12 14:19:41.961249', '2026-01-27 11:31:13.73243', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Tư Thế Cơ Thể Khi Ném Bóng\n\nKhi bạn ném bóng, tư thế cơ thể rất quan trọng. Một tư thế đúng giúp bạn ném mạnh và chính xác hơn.\n\n## Các Bước Để Tạo Tư Thế Tốt\n- **Chân**: Đặt chân rộng bằng vai, chân ném lùi lại một chút.\n- **Đầu Gối**: Gập đầu gối nhẹ để có thêm sức mạnh.\n- **Thân**: Giữ lưng thẳng và nghiêng người về phía trước.\n- **Tay**: Đưa tay ném lên cao và giữ thẳng với bóng.\n\n## Lưu Ý\n- Không đứng quá gần hoặc xa rổ.\n- Luyện tập thường xuyên để có tư thế tốt.", "detailed": "# Tư Thế Cơ Thể Khi Ném Bóng\n\nTư thế cơ thể đóng vai trò quan trọng trong việc quyết định sức mạnh và độ chính xác của cú ném bóng rổ. Tư thế đúng không chỉ giúp bạn ném bóng hiệu quả mà còn tạo thói quen tốt.\n\n## Các Bước Để Tạo Tư Thế Tốt\n1. **Chân**: Đặt chân rộng bằng vai, với chân thuận (chân ném) hơi lùi về phía sau để tạo cân bằng.\n2. **Đầu Gối**: Gập nhẹ đầu gối để tích lũy sức mạnh cho cú ném. Điều này tương tự như việc bạn chuẩn bị nhảy lên cao.\n3. **Thân**: Giữ thẳng lưng và hơi nghiêng người về phía trước, điều này giúp tăng cường sức mạnh cho cú ném.\n4. **Tay**: Đưa tay ném lên cao và giữ thẳng với bóng, đảm bảo bóng nằm trong lòng bàn tay.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Nhiều người chơi thường không gập đầu gối, dẫn đến việc thiếu sức mạnh cho cú ném.\n- **Lời Khuyên**: Hãy luyện tập đứng đúng tư thế và kiểm tra lại trước khi ném để tối ưu hóa cú ném của bạn.\n\n## Lưu Ý Quan Trọng\n- Không nên đứng quá xa hoặc gần so với rổ, điều này ảnh hưởng đến độ chính xác.\n- Thường xuyên luyện tập để tạo thói quen tốt trong tư thế ném.", "comprehensive": "# Tư Thế Cơ Thể Khi Ném Bóng\n\nTư thế cơ thể là một trong những yếu tố quyết định đến sức mạnh và độ chính xác của cú ném bóng trong bóng rổ. Nắm vững tư thế đúng không chỉ giúp bạn cải thiện kỹ năng cá nhân mà còn tạo nền tảng vững chắc cho sự phát triển trong môn thể thao này.\n\n## Nguyên Tắc Cơ Bản Của Tư Thế Ném Bóng\nTư thế ném bóng bao gồm các yếu tố từ chân, đầu gối, thân và tay. Mỗi yếu tố đều có ảnh hưởng nhất định đến cú ném.\n\n### 1. Chân\n- Đặt chân rộng bằng vai, với chân thuận (chân ném) hơi lùi lại phía sau. Điều này tạo ra một nền tảng vững chắc và cân bằng cho cơ thể. \n\n### 2. Đầu Gối\n- Gập nhẹ đầu gối để tích lũy sức mạnh cho cú ném. Hãy tưởng tượng như bạn đang chuẩn bị để nhảy, việc gập đầu gối sẽ giúp bạn có thêm lực đẩy.\n\n### 3. Thân\n- Giữ thẳng lưng và hơi nghiêng người về phía trước. Tư thế này không chỉ giúp tối ưu hóa sức mạnh mà còn tạo cảm giác thoải mái khi ném bóng.\n\n### 4. Tay\n- Đưa tay ném lên cao và giữ thẳng với bóng. Đảm bảo rằng bóng nằm trong lòng bàn tay và các ngón tay hướng về phía rổ.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Một số người chơi thường không gập đầu gối, dẫn đến việc thiếu sức mạnh cho cú ném. Điều này thường thấy ở những người mới bắt đầu.\n- **Lời Khuyên**: Hãy luyện tập đứng đúng tư thế và luôn kiểm tra lại trước khi ném. Điều này giúp bạn phát triển thói quen tốt và cải thiện kỹ năng.\n\n## Lưu Ý Quan Trọng\n- Không nên đứng quá xa hoặc gần so với rổ. Khoảng cách là rất quan trọng; đứng quá xa có thể khiến bạn không đủ sức mạnh, trong khi đứng quá gần có thể làm giảm độ chính xác.\n- Thường xuyên luyện tập tư thế ném để tạo thói quen tốt. Đừng ngần ngại nhờ huấn luyện viên hoặc đồng đội kiểm tra tư thế của bạn để có phản hồi kịp thời.\n\n## Kết Luận\nViệc nắm vững tư thế cơ thể khi ném bóng không chỉ giúp bạn ném chính xác hơn mà còn giúp bạn tự tin hơn trong trận đấu. Hãy dành thời gian luyện tập để cải thiện kỹ năng của mình, từ đó nâng cao thành tích trong bóng rổ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('13af6ef0-1ef8-4845-9af1-d76d7b32768e', '00bbe76d-6014-485e-890f-1398b6100e68', 'example', 'Kỹ Thuật Ném Bóng Từ Vị Trí Khó', '# Kỹ Thuật Ném Bóng Từ Vị Trí Khó\n\n## Tình Huống\nTrong một trận đấu, bạn đang đứng ở góc sân và có một đối thủ đang áp sát. Bạn cần thực hiện cú ném từ vị trí khó khăn.\n\n## Vấn Đề Cần Giải Quyết\nBạn phải tìm cách ném bóng một cách chính xác dù không có nhiều không gian và thời gian.\n\n## Giải Pháp Từng Bước\n1. **Quan Sát Đối Thủ**: Nhận diện vị trí của đối thủ và rổ.\n2. **Tư Thế Cơ Thể**: Đứng thẳng, chân rộng bằng vai. Gập đầu gối để tạo lực.\n3. **Cầm Bóng**: Cầm bóng đúng cách nhưng cũng cần nhanh chóng đưa bóng lên cao.\n4. **Thực Hiện Cú Ném**: Sử dụng sức mạnh từ chân và thân để thực hiện cú ném, đồng thời giữ mắt trên bóng và rổ.\n5. **Theo Dõi Kết Quả**: Theo dõi bóng trong khi nó bay vào rổ để kiểm tra độ chính xác.\n\n## Kết Quả Và Phân Tích Kết Quả\nNếu cú ném không thành công, hãy xem xét lại tư thế và kỹ thuật của mình. Có thể bạn đã không đủ mạnh hoặc không giữ đúng tư thế.\n\n## Bài Học Rút Ra\nKỹ thuật ném bóng trong các tình huống khó là một kỹ năng cần luyện tập để cải thiện khả năng ghi điểm trong các trận đấu thực tế.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "Create an educational illustration showing a basketball player in a challenging shooting position at the corner of a court. The player should be in an athletic stance, observing the opponent and the hoop. Include labeled annotations indicating body posture, leg position, ball grip, and focus points for shooting accuracy. Use a professional and educational style.", "videoScript": "Chào mừng các bạn đến với bài học ngày hôm nay về kỹ thuật ném bóng từ vị trí khó trong bóng rổ. Trong một trận đấu, có thể bạn sẽ gặp phải những tình huống cần ném bóng khi có đối thủ áp sát. Bây giờ, hãy cùng tìm hiểu các bước thực hiện nhé!\n\nBước đầu tiên, quan sát đối thủ và xác định vị trí của rổ. Hãy chắc chắn rằng bạn biết mình cần ném vào đâu.\n\nTiếp theo, hãy điều chỉnh tư thế cơ thể của bạn. Bạn nên đứng thẳng, với chân rộng bằng vai và gập đầu gối để tạo lực.\n\nỞ bước thứ ba, hãy cầm bóng đúng cách. Đảm bảo rằng bạn cầm bóng chắc chắn nhưng cũng nhanh chóng đưa bóng lên phía trên.\n\nBây giờ đến bước thực hiện cú ném. Dùng sức mạnh từ chân và thân của bạn để ném, đừng quên giữ mắt nhìn vào bóng và rổ để đảm bảo độ chính xác.\n\nCuối cùng, theo dõi kết quả. Theo dõi bóng trong khi nó bay vào rổ để đánh giá cú ném của bạn. Nếu cú ném không thành công, hãy xem xét lại tư thế và kỹ thuật của mình để cải thiện trong lần tiếp theo.\n\nĐó là tất cả các bước bạn cần nhớ khi ném bóng từ vị trí khó. Hãy luyện tập thường xuyên để nâng cao kỹ năng của mình trong các trận đấu nhé!", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa một cầu thủ bóng rổ trong tư thế ném bóng từ vị trí khó ở góc sân, với các chú thích về tư thế cơ thể, cách cầm bóng và điểm nhìn.", "videoDescription": "Video này hướng dẫn chi tiết các bước thực hiện kỹ thuật ném bóng từ vị trí khó trong bóng rổ."}', 2, '{"xp": 15, "coin": 2}', NULL, '2026-01-12 14:19:41.982266', '2026-01-27 11:33:13.254484', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Kỹ Thuật Ném Bóng Từ Vị Trí Khó\n\n## Tình Huống\nBạn đứng ở góc sân và có đối thủ áp sát. Bạn cần ném bóng nhưng không có nhiều không gian.\n\n## Giải Pháp\n1. **Quan Sát**: Nhìn kỹ vị trí của đối thủ và rổ.\n2. **Tư Thế**: Đứng thẳng, chân rộng bằng vai. Gập đầu gối.\n3. **Cầm Bóng**: Cầm bóng chắc chắn và nâng lên cao.\n4. **Ném**: Sử dụng sức mạnh từ chân và thân, mắt luôn nhìn bóng và rổ.\n5. **Theo Dõi**: Nhìn theo bóng khi nó bay vào rổ.\n\n## Bài Học\nCần luyện tập kỹ thuật ném trong tình huống khó để ghi điểm tốt hơn trong trận đấu.", "detailed": "# Kỹ Thuật Ném Bóng Từ Vị Trí Khó\n\n## Tình Huống\nTrong một trận đấu bóng rổ, bạn đang đứng ở góc sân với một đối thủ đang bám sát. Bạn cần thực hiện cú ném từ vị trí khó khăn, điều này đòi hỏi kỹ thuật và sự khéo léo.\n\n## Giải Pháp Từng Bước\n1. **Quan Sát Đối Thủ**: Trước khi ném, hãy nhìn xem đối thủ đứng ở đâu và rổ nằm ở vị trí nào. Điều này giúp bạn quyết định góc ném phù hợp.\n2. **Tư Thế Cơ Thể**: Đứng thẳng, với chân rộng bằng vai. Gập đầu gối một chút để tạo lực cho cú ném.\n3. **Cầm Bóng**: Cầm bóng bằng cả hai tay, giữ bóng ở gần ngực và nhanh chóng đưa bóng lên cao.\n4. **Thực Hiện Cú Ném**: Sử dụng sức mạnh từ chân và phần thân trên để ném bóng. Đảm bảo mắt bạn luôn theo dõi bóng và rổ.\n5. **Theo Dõi Kết Quả**: Sau khi ném, hãy theo dõi bóng để xem nó có vào rổ hay không.\n\n## Tips Quan Trọng\n- Tập luyện thường xuyên để cải thiện kỹ thuật ném.\n- Thực hành các tình huống khó để nâng cao khả năng phản xạ và chính xác.\n\n## Bài Học Rút Ra\nKỹ thuật ném bóng trong các tình huống khó rất quan trọng, bạn cần tập luyện để cải thiện khả năng ghi điểm trong các trận đấu thực tế.", "comprehensive": "# Kỹ Thuật Ném Bóng Từ Vị Trí Khó\n\n## Tình Huống\nTrong một trận đấu bóng rổ, bạn có thể sẽ gặp phải những tình huống khó khăn, chẳng hạn như phải ném bóng từ góc sân khi có đối thủ áp sát. Để thực hiện cú ném thành công trong những tình huống này, bạn cần có sự kết hợp giữa kỹ thuật và chiến thuật.\n\n## Nguyên Tắc Cơ Bản\n- **Quan Sát Đối Thủ**: Trước khi thực hiện cú ném, việc đầu tiên là xác định vị trí của đối thủ và rổ. Hãy chú ý đến khoảng cách và góc ném. Điều này giúp bạn hiểu rõ tình huống và đưa ra quyết định tốt nhất.\n- **Tư Thế Cơ Thể**: Tư thế là yếu tố quyết định độ chính xác của cú ném. Đứng thẳng, chân rộng bằng vai, gập đầu gối để tạo lực. Tư thế vững chắc giúp bạn có một cú ném mạnh mẽ và chính xác hơn.\n- **Cầm Bóng**: Việc cầm bóng đúng cách rất quan trọng. Hãy cầm bóng bằng cả hai tay, giữ gần ngực để dễ dàng nâng lên cao khi ném.\n\n## Quy Trình Thực Hiện Cú Ném\n1. **Chuẩn Bị**: Khi đã xác định được tư thế và cầm bóng chắc chắn, hãy chuẩn bị cho cú ném. Giữ bình tĩnh và tập trung.\n2. **Ném**: Sử dụng sức mạnh từ chân để tạo đà, cùng với việc đẩy bóng lên cao bằng tay. Đảm bảo mắt bạn luôn nhìn vào bóng và rổ để điều chỉnh cú ném.\n3. **Theo Dõi Kết Quả**: Sau khi ném, hãy theo dõi bóng để xem nó có vào rổ hay không. Nếu không, hãy đánh giá lại kỹ thuật của mình.\n\n## Các Tình Huống Phức Tạp\nTrong bóng rổ, có những tình huống cực kỳ khó mà bạn có thể phải đối mặt, chẳng hạn như:\n- Ném trong khi bị cản trở.\n- Ném từ xa hoặc khi đang di chuyển.\n\n## Best Practices\n- **Tập Luyện Đều Đặn**: Cách tốt nhất để cải thiện kỹ thuật ném là luyện tập thường xuyên. Hãy dành thời gian để rèn luyện cú ném từ các vị trí khác nhau.\n- **Phân Tích Kết Quả**: Sau mỗi trận đấu, hãy phân tích các cú ném của bạn. Điều này giúp bạn hiểu rõ điểm mạnh và yếu của kỹ thuật ném.\n\n## Kết Luận\nKỹ thuật ném bóng từ vị trí khó không chỉ là một kỹ năng, mà còn là một nghệ thuật. Nó yêu cầu sự tập trung, kỹ thuật và luyện tập liên tục. Qua thời gian và công sức, bạn sẽ cải thiện khả năng ghi điểm của mình trong các trận đấu thực tế."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('078d1cfb-68e7-46cb-8c1d-72f1666fe1fc', '00bbe76d-6014-485e-890f-1398b6100e68', 'example', 'Tình Huống Ném Bóng Trong Trận Đấu', 'Trong một trận đấu bóng rổ, tình huống ném bóng rất thường xuyên xảy ra. Hãy tưởng tượng bạn đang cùng với nhóm bạn ở trường, mọi người đang chơi một trận, và bạn có cơ hội ném bóng vào rổ. Đây là cách bạn có thể thực hiện:\n\n1. **Tập trung**: Khi bạn có bóng, hãy nhìn về phía rổ, bạn nên hình dung đường bóng sẽ đi và điểm ghi điểm.\n\n2. **Chọn khoảng cách**: Nếu bạn ở gần rổ, một cú ném đơn giản sẽ là tốt. Nếu ở xa hơn, bạn có thể cần phải lấy đà.\n\n3. **Ném bóng**: Cầm bóng đúng cách, giữ chắc và thực hiện mới tư thế mà bạn đã luyện tập. Hãy thả bóng lên và đẩy tay về phía trước. Hãy nhớ, như một cú nhảy, sức mạnh đến từ chân bạn.\n\n4. **Nhìn bóng**: Sau khi ném, hãy theo dõi bóng để xem nó có vào rổ không. Nếu bạn thành công, bạn sẽ cảm thấy rất vui mừng! Nếu không, không sao cả, hãy tập luyện thêm. \n\nMỗi lúc như vậy, bạn sẽ trở nên tự tin hơn và có thể ghi điểm nhiều hơn.', '{"imageUrl": "https://placehold.co/800x600/dcfce7/166534?text=%F0%9F%8C%B1+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+%C4%90%C6%A1n+Gi%E1%BA%A3n)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A simple and colorful illustration of a young basketball player preparing to shoot a basketball into a hoop during a game. The player is focused, with clear facial expressions, showcasing the shooting posture. Bright colors, minimal details, and a cheerful background suitable for children. The scene should convey a feeling of excitement and sportsmanship.", "videoScript": "Chào mừng các bạn đến với bài học về kỹ thuật ném bóng trong bóng rổ! Hôm nay, chúng ta sẽ cùng khám phá một tình huống thú vị trong trận đấu. Khi bạn có bóng, bước đầu tiên là hãy tập trung nhìn vào rổ. Hãy hình dung đường đi của bóng! (âm thanh vui nhộn) Nếu bạn gần rổ, chỉ cần một cú ném đơn giản. (hoạt hình hình ảnh ném bóng) Còn nếu bạn ở xa, hãy lấy đà nhé! (hiệu ứng hoạt hình cho thấy lấy đà) Tiếp theo, cầm bóng chắc chắn, thả bóng lên và đẩy tay về phía trước. Nhớ rằng sức mạnh đến từ đôi chân của bạn! (hoạt hình thể hiện động tác) Cuối cùng, hãy theo dõi bóng sau khi ném! Nếu bạn ghi điểm, hãy vui mừng! (hình ảnh vui tươi) Rèn luyện và bạn sẽ ngày càng tự tin hơn. Cảm ơn các bạn đã theo dõi!", "videoDuration": "30-60 giây", "imageDescription": "Hình ảnh minh họa một cầu thủ bóng rổ trẻ tuổi đang chuẩn bị ném bóng vào rổ, thể hiện sự tập trung và quyết tâm.", "videoDescription": "Video này hướng dẫn các bạn cách ném bóng trong bóng rổ với những bước đơn giản và dễ hiểu."}', 6, '{"xp": 12, "coin": 1}', NULL, '2026-01-15 22:09:46.420737', '2026-01-19 23:39:19.130953', 'mixed', 'easy', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0ac3ad84-1103-4276-b000-af84a6526703', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Cách Cầm Bóng Đúng Cách', 'Khi ném bóng rổ, cách bạn cầm bóng rất quan trọng. Hãy tưởng tượng rằng bạn đang giữ một quả trứng. Bạn không muốn làm bể quả trứng, đúng không? Vì vậy, bạn cần phải cầm bóng một cách nhẹ nhàng nhưng chắc chắn.\n\n1. **Ngón tay**: Sử dụng các ngón tay của bạn để giữ bóng. Đặt ngón tay cái ở phía dưới bóng và các ngón tay còn lại quanh bóng, không để lòng bàn tay chạm vào bóng.\n\n2. **Tay không ném**: Đặt một tay ở phía bên bóng, tay này sẽ giúp bạn kiểm soát bóng khi bạn ném.\n\n3. **Tư thế**: Đứng thẳng, đầu gối hơi khuỵu, và giữ bóng ở gần đầu. Một số người có thể nghĩ rằng bạn cần nhấc bóng lên cao hơn, nhưng thực ra, giữ bóng ở mức độ này sẽ giúp bạn dễ dàng quan sát rổ hơn.\n\nKhi bạn đã nắm vững cách cầm bóng, việc ném bóng sẽ trở nên dễ dàng hơn nhiều. Hãy nhớ tập luyện nhiều để quen tay nhé!', '{"imageUrl": "https://placehold.co/800x600/dcfce7/166534?text=%F0%9F%8C%B1+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+%C4%90%C6%A1n+Gi%E1%BA%A3n)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A simple and colorful illustration showing a child holding a basketball properly. The child should be holding the ball gently with their fingers, while the thumb is underneath. The other hand is positioned at the side of the ball. The background is bright and cheerful, illustrating a basketball court. The style should be cartoonish and easy to understand for kids.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ học cách cầm bóng rổ đúng cách. Khi ném bóng, cầm bóng rất quan trọng. Hãy tưởng tượng bạn đang giữ một quả trứng. Bạn không muốn làm bể nó! Đặt ngón tay cái dưới bóng và các ngón tay khác quanh bóng. Nhớ không để lòng bàn tay chạm vào bóng nhé! Tay không ném sẽ giúp bạn kiểm soát bóng. Tư thế khi đứng cũng quan trọng. Đứng thẳng với đầu gối hơi khuỵu và giữ bóng gần đầu. Nhớ rằng khi cầm bóng đúng cách, ném sẽ dễ hơn rất nhiều. Hãy tập luyện nhé!", "videoDuration": "30-60 giây", "imageDescription": "Hình ảnh minh họa một em bé cầm bóng rổ đúng cách, thể hiện cách sử dụng ngón tay và tay không ném để kiểm soát bóng.", "videoDescription": "Video này hướng dẫn cách cầm bóng rổ đúng cách với những hình ảnh minh họa sống động."}', 7, '{"xp": 8, "coin": 1}', NULL, '2026-01-15 22:09:46.36478', '2026-01-19 23:39:19.135642', 'mixed', 'easy', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8f78cf4e-46d6-44cd-b7bb-d1ecb7177ad6', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Tư Thế Cơ Thể Khi Ném Bóng', 'Tư thế cơ thể cũng rất quan trọng khi bạn ném bóng. Hãy tưởng tượng rằng bạn đang chuẩn bị thực hiện một cú nhảy cao, bạn cần cơ thể được căng cứng và sẵn sàng. Dưới đây là cách để có tư thế ném bóng đúng:\n\n1. **Chân**: Đứng với chân trái (nếu bạn là người thuận tay phải) hoặc chân phải (nếu bạn là người thuận tay trái) nhích một chút về phía trước. Chân còn lại nên đứng thẳng và vững chãi, tạo cảm giác vững vàng.\n\n2. **Đầu gối**: Hơi khuỵu chân, điều này giúp bạn có thêm sức mạnh khi ném. Bạn có thể nghĩ rằng tư thế khuỵu như đang ngồi trên ghế vậy.\n\n3. **Cơ thể**: Giữ cơ thể thẳng, lưng không cong, đầu nhìn về phía rổ. Hãy tưởng tượng rằng bạn đang tập trung nhìn vào cái gì đó rất thú vị ngay trước mặt.\n\nKhi bạn đứng đúng tư thế, bạn sẽ ném bóng dễ hơn và có khả năng ghi điểm cao hơn.', '{"imageUrl": "https://placehold.co/800x600/dcfce7/166534?text=%F0%9F%8C%B1+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+%C4%90%C6%A1n+Gi%E1%BA%A3n)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A bright and colorful illustration showing a simple basketball shooting stance. The character stands with one foot slightly forward, knees bent as if sitting in a chair, and back straight. The character should be looking intently at a basketball hoop, with a cheerful expression. The background should be minimalistic and vibrant, suitable for educational purposes.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ cùng khám phá tư thế cơ thể khi ném bóng trong bóng rổ. Đầu tiên, hãy đứng với chân thuận một chút về phía trước. Chân còn lại nên vững chãi. Nhớ khuỵu gối một chút nhé! Điều này sẽ giúp bạn có sức mạnh khi ném. Cuối cùng, giữ lưng thẳng và cúi nhìn vào rổ. Khi bạn đứng đúng tư thế, bạn sẽ ném bóng dễ hơn và ghi điểm cao hơn. Hãy chuẩn bị cho cú ném hoàn hảo nào!", "videoDuration": "30-60 giây", "imageDescription": "Hình ảnh minh họa tư thế cơ thể khi ném bóng, thể hiện chân đứng vững chãi, đầu gối khuỵu và cơ thể thẳng.", "videoDescription": "Video giới thiệu tư thế cơ thể đúng khi ném bóng trong bóng rổ, giúp người xem nắm rõ cách đứng vững và đúng kỹ thuật."}', 8, '{"xp": 8, "coin": 1}', NULL, '2026-01-15 22:09:46.40965', '2026-01-19 23:39:19.201817', 'mixed', 'easy', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('0bc2a120-d678-4e04-8ffa-4a33274e265f', '00bbe76d-6014-485e-890f-1398b6100e68', 'example', 'Case Study: Cú Ném Quyết Định Trong Trận Chung Kết', 'Trong trận chung kết NBA 2022, Stephen Curry đã thực hiện một cú ném 3 điểm quyết định trong những giây cuối cùng. Các yếu tố góp phần vào thành công của cú ném này bao gồm: 1. Sự chuẩn bị tâm lý vững vàng trước khi ném. 2. Kỹ thuật ném hoàn hảo với việc sử dụng sức mạnh từ chân và động tác tay đúng cách. 3. Khả năng đọc tình huống và lựa chọn thời điểm ném chính xác. Phân tích cú ném này cho thấy tầm quan trọng của việc kết hợp kỹ thuật với tâm lý trong những tình huống áp lực cao.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "An intricate diagram illustrating Stephen Curry''s final 3-point shot in the 2022 NBA Finals. The image should feature detailed annotations pinpointing key elements such as his foot positioning, arm mechanics, and mental preparation. Include arrows to show motion dynamics and a scoreboard indicating the game''s intense last seconds. The style should be educational, professional, and visually engaging, capturing the essence of basketball technique.", "videoScript": "Chào mừng các bạn đến với bài học hôm nay. Chúng ta sẽ phân tích cú ném quyết định của Stephen Curry trong trận chung kết NBA 2022. Trong những giây cuối cùng của trận đấu, Curry đã thực hiện một cú ném 3 điểm cực kỳ quan trọng. Đầu tiên, sự chuẩn bị tâm lý vững vàng đã giúp anh ấy giữ được sự bình tĩnh trong áp lực. Tiếp theo, chúng ta sẽ xem xét kỹ thuật ném của anh. \n\nCurry đứng vững với đôi chân, sử dụng sức mạnh từ chân để nâng cao độ cao của cú ném, cùng với động tác tay được thực hiện chuẩn xác. Điểm quan trọng tiếp theo là khả năng đọc tình huống. Anh ấy đã chọn đúng thời điểm để thực hiện cú ném khi thấy đối phương không kịp phản ứng. \n\nCuối cùng, sự kết hợp giữa kỹ thuật và tâm lý vững vàng đã mang lại thành công cho cú ném này. Điều này nhấn mạnh tầm quan trọng của việc luyện tập không chỉ kỹ thuật mà còn cả tinh thần trong những tình huống căng thẳng. Cảm ơn các bạn đã tham gia bài học hôm nay!", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa kỹ thuật ném bóng của Stephen Curry trong trận chung kết NBA 2022, với các yếu tố như vị trí chân, động tác tay và sự chuẩn bị tâm lý được phân tích chi tiết.", "videoDescription": "Video phân tích cú ném 3 điểm quyết định của Stephen Curry trong trận chung kết NBA 2022, tập trung vào kỹ thuật và tâm lý trong thể thao."}', 8, '{"xp": 23, "coin": 4}', NULL, '2026-01-16 15:13:55.171756', '2026-01-19 23:39:19.227619', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b36ea412-2628-461d-9e65-f97f5fa70e68', '00bbe76d-6014-485e-890f-1398b6100e68', 'example', 'Tình Huống Khó: Ném Bóng Khi Bị Kẹp Giữa Hai Đối Thủ', 'Một tình huống phổ biến trong bóng rổ là bị kẹp giữa hai đối thủ và cần thực hiện cú ném. Để xử lý tình huống này, cầu thủ cần: 1. Giữ bóng gần cơ thể để bảo vệ bóng khỏi bị cướp. 2. Sử dụng động tác giả để tạo khoảng trống. 3. Quyết định nhanh chóng giữa việc ném bóng hoặc chuyền cho đồng đội. 4. Sử dụng cú ném với một tay nếu cần thiết, và đảm bảo hướng bóng đến rổ. Tình huống này không chỉ yêu cầu kỹ thuật mà còn cả sự quyết đoán và khả năng đọc tình huống, phản ứng nhanh.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A highly detailed and professional-style illustration of a basketball player in a challenging situation, caught between two defenders on the court. The player is protecting the ball closely to their body, showcasing various technical elements such as dribbling, feints, and shooting posture. The background includes a basketball court with fans in the stands, illustrating the intensity of the game. Include labels to highlight key techniques and decision-making moments, like ''Protect the Ball'', ''Create Space'', ''Quick Decision'', and ''One-Handed Shot''. The style should be educational, vibrant, and dynamic.", "videoScript": "Chào mừng các bạn đến với bài học hôm nay. Trong bóng rổ, việc bị kẹp giữa hai đối thủ là một tình huống khó khăn mà nhiều cầu thủ thường gặp. Để xử lý tốt tình huống này, trước tiên, cầu thủ phải giữ bóng gần cơ thể. Kỹ thuật bảo vệ bóng không chỉ giúp bạn tránh bị cướp bóng mà còn tạo cảm giác an toàn hơn. Tiếp theo, hãy sử dụng động tác giả để tạo khoảng trống. Bạn có thể hạ thấp vai hoặc thực hiện một động tác giả để làm đối thủ nhầm lẫn, tạo điều kiện cho cú ném của bạn hoặc chuyền bóng cho đồng đội. Trong tình huống bị kẹp, quyết định nhanh chóng là rất quan trọng. Bạn cần xác định liệu mình có nên ném bóng hay tìm đồng đội để chuyền. Cuối cùng, nếu cần thiết, hãy sử dụng cú ném một tay. Đảm bảo hướng bóng đến rổ và tập trung vào kỹ thuật. Nhớ rằng, không chỉ là kỹ thuật, mà còn là khả năng đọc tình huống và phản ứng nhanh sẽ giúp bạn thành công. Hãy luyện tập những kỹ năng này để trở thành một cầu thủ xuất sắc hơn!", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa một cầu thủ bóng rổ trong tình huống khó khăn bị kẹp giữa hai đối thủ, thể hiện các kỹ thuật bảo vệ bóng và ném bóng.", "videoDescription": "Video trình bày chi tiết cách xử lý tình huống khó trong bóng rổ khi bị kẹp giữa hai đối thủ và các kỹ thuật ném bóng hiệu quả."}', 10, '{"xp": 23, "coin": 4}', NULL, '2026-01-16 15:13:55.180998', '2026-01-19 23:39:19.252011', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('328660df-987b-4a84-8186-e1847470727b', '00bbe76d-6014-485e-890f-1398b6100e68', 'example', 'Phân Tích Cú Ném Bóng Từ Vị Trí Khó Của Klay Thompson', 'Klay Thompson nổi tiếng với khả năng ghi điểm từ những vị trí khó. Một cú ném 3 điểm từ góc sân trong một trận đấu playoff là một ví dụ điển hình. Trong cú ném này, các yếu tố quyết định bao gồm: 1. Tư thế đứng vững và sự cân bằng. 2. Sự nhanh nhẹn trong việc di chuyển và tìm kiếm khoảng trống. 3. Kỹ thuật ném chính xác với khả năng kiểm soát lực ném. Phân tích kỹ thuật của Klay Thompson cung cấp cái nhìn sâu sắc về cách cầu thủ có thể tối ưu hóa kỹ thuật ném trong những tình huống khó khăn.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A detailed and complex illustration depicting Klay Thompson executing a difficult three-point shot from the corner of the court during a playoff game. The image should show multiple angles of his stance, balance, and shooting technique, with arrows indicating movement and force vectors. Include annotations for key technical elements such as foot placement, arm positioning, and eye focus. The background should be a vibrant basketball court setting with fans in the stands, capturing the intensity of a playoff atmosphere. Professional and educational style.", "videoScript": "Chào mừng các bạn đến với bài học hôm nay. Trong video này, chúng ta sẽ phân tích cú ném bóng từ vị trí khó của Klay Thompson, một trong những cầu thủ ghi điểm xuất sắc nhất NBA. Đầu tiên, hãy xem xét tư thế đứng vững và sự cân bằng của Klay. Điều này giúp anh vào tư thế ném chuẩn xác hơn. Khi di chuyển trên sân, Klay không ngừng tìm kiếm khoảng trống để có thể thực hiện cú ném. Sự nhanh nhẹn trong di chuyển là rất quan trọng. Tiếp theo, chúng ta sẽ phân tích kỹ thuật ném của anh. Lực ném được điều khiển một cách chuẩn xác, giúp bóng bay ra với độ cong và sức mạnh tối ưu. Cuối cùng, chúng ta sẽ cùng nhau xem lại cú ném này và những bài học mà nó mang lại cho kỹ thuật ném bóng trong những tình huống khó khăn. Hãy cùng xem!", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa Klay Thompson thực hiện cú ném 3 điểm khó từ góc sân trong một trận đấu playoff, nhấn mạnh kỹ thuật ném bóng và các yếu tố quyết định như tư thế đứng và sự cân bằng.", "videoDescription": "Video phân tích cú ném bóng của Klay Thompson từ vị trí khó, nêu bật các kỹ thuật và yếu tố quan trọng quyết định thành công của cú ném."}', 11, '{"xp": 23, "coin": 4}', NULL, '2026-01-16 15:13:55.186726', '2026-01-19 23:39:19.504542', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3779c4db-1a83-44c6-8cca-2d973c27a07f', '00bbe76d-6014-485e-890f-1398b6100e68', 'example', 'Tình Huống Ném Bóng Trong Trận Đấu', '[{"insert": "# Tình Huống Ném Bóng Trong Trận Đấu\n\n## Tình Huống\nTrong một trận đấu quan trọng, đội của bạn đang dẫn trước 2 điểm và chỉ còn 10 giây trên đồng hồ. Bạn có cơ hội ném phạt sau khi đối thủ phạm lỗi. Đây là thời điểm quyết định có thể ảnh hưởng đến kết quả trận đấu.\n\n## Vấn Đề Cần Giải Quyết\nBạn cảm thấy áp lực rất lớn vì đây là cú ném có thể giúp đội bạn giành chiến thắng. Bạn cần giữ bình tĩnh và thực hiện cú ném chính xác.\n\n## Giải Pháp Từng Bước\n1. **Chuẩn Bị Tinh Thần**: Hít thở sâu và tập trung vào mục tiêu.\n2. **Tư Thế Đứng**: Đứng đúng tư thế, chân rộng bằng vai và gập nhẹ đầu gối.\n3. **Cầm Bóng**: Cầm bóng đúng cách, cảm nhận được sự thoải mái và chắc chắn.\n4. **Thực Hiện Cú Ném**: Nhấc bóng lên và thực hiện cú ném theo kỹ thuật đã học, giữ tập trung vào rổ.\n5. **Theo Dõi Kết Quả**: Nhìn theo bóng bay vào rổ, đảm bảo theo dõi cú ném cho đến khi bóng rời tay.\n\n## Kết Quả Và Phân Tích Kết Quả\nNếu cú ném thành công, bạn có thể giúp đội của mình giành chiến thắng. Nếu không, hãy xem xét lại từng bước đã thực hiện để tìm hiểu lý do.\n\n## Bài Học Rút Ra\nTình huống này cho thấy tầm quan trọng của việc chuẩn bị tâm lý và nắm vững kỹ thuật ném bóng. Hãy luyện tập trong các tình huống thực tế để cải thiện khả năng xử lý áp lực.\n=======================\nsdasdasdsd\n=============\n"}]', '{"caption": "tình huống ném trong trận đấu", "imageUrl": "https://res.cloudinary.com/dmoiixotu/image/upload/bo_3px_solid_rgb:4A90E2,r_8/v1768901820/content-edits/migrated/osjsjgvoenweavy5ggfs.jpg", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imageUrls": ["https://res.cloudinary.com/dmoiixotu/image/upload/bo_3px_solid_rgb:4A90E2,r_8/v1768902022/content-edits/images/vazcw7p84t48skgvjirj.jpg"], "description": "bài học chưa có hình ảnh", "imagePrompt": "A detailed educational illustration depicting a basketball player preparing to take a free throw in a crucial game situation. The player is standing with a focused expression, feet shoulder-width apart, knees slightly bent. The basketball is held correctly, with a clear view of the shooting form. The background shows a cheering crowd and a scoreboard displaying the remaining time. The image includes labels for key elements: ''Cầm Bóng'' (Holding the Ball), ''Tư Thế Đứng'' (Stance), ''Thực Hiện Cú Ném'' (Shooting Technique), and ''Theo Dõi Kết Quả'' (Follow Through). The style is professional and educational, suitable for basketball training materials.", "videoScript": "Chào mừng các bạn đến với bài học về tình huống ném bóng trong trận đấu bóng rổ! Hôm nay, chúng ta sẽ cùng nhau tìm hiểu về cú ném phạt trong một trận đấu quan trọng, khi chỉ còn 10 giây trên đồng hồ và đội của bạn đang dẫn trước 2 điểm.\n\nĐầu tiên, hãy chuẩn bị tinh thần. Hãy hít thở sâu và tập trung vào mục tiêu – rổ bóng.\n\nTiếp theo, tư thế đứng. Hãy đứng với chân rộng bằng vai và gập nhẹ đầu gối, tạo nền tảng vững chắc cho cú ném của bạn.\n\nBây giờ, cầm bóng đúng cách. Đảm bảo rằng bạn cảm thấy thoải mái và chắc chắn khi cầm bóng. Hãy nắm bóng với cả hai tay.\n\nKhi bạn thực hiện cú ném, hãy nhấc bóng lên và thả ra theo đúng kỹ thuật đã học, giữ tập trung vào rổ. Nhớ là theo dõi bóng bay vào rổ cho đến khi bóng rời tay bạn.\n\nCuối cùng, nếu cú ném thành công, đó sẽ là một chiến thắng cho đội bạn. Nếu không, hãy xem xét lại từng bước đã thực hiện để tìm hiểu lý do. Điều quan trọng là luyện tập để cải thiện khả năng xử lý áp lực trong các tình huống thực tế.\n\nHẹn gặp lại các bạn trong các bài học tiếp theo!", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa một cầu thủ bóng rổ đang chuẩn bị thực hiện cú ném phạt trong thời điểm quyết định của trận đấu. Các yếu tố kỹ thuật ném bóng được chú thích rõ ràng.", "videoDescription": "Video này hướng dẫn các kỹ thuật ném bóng trong tình huống căng thẳng của trận đấu bóng rổ, giúp bạn cải thiện khả năng xử lý áp lực và thực hiện cú ném thành công."}', 1, '{"xp": 15, "coin": 2}', NULL, '2026-01-12 14:19:41.979307', '2026-01-27 11:32:49.434163', 'mixed', 'medium', '[{"insert": "# Tình Huống Ném Bóng Trong Trận Đấu\n\n## Tình Huống\nTrong một trận đấu quan trọng, đội của bạn đang dẫn trước 2 điểm và chỉ còn 10 giây trên đồng hồ. Bạn có cơ hội ném phạt sau khi đối thủ phạm lỗi. Đây là thời điểm quyết định có thể ảnh hưởng đến kết quả trận đấu.\n\n## Vấn Đề Cần Giải Quyết\nBạn cảm thấy áp lực rất lớn vì đây là cú ném có thể giúp đội bạn giành chiến thắng. Bạn cần giữ bình tĩnh và thực hiện cú ném chính xác.\n\n## Giải Pháp Từng Bước\n1. **Chuẩn Bị Tinh Thần**: Hít thở sâu và tập trung vào mục tiêu.\n2. **Tư Thế Đứng**: Đứng đúng tư thế, chân rộng bằng vai và gập nhẹ đầu gối.\n3. **Cầm Bóng**: Cầm bóng đúng cách, cảm nhận được sự thoải mái và chắc chắn.\n4. **Thực Hiện Cú Ném**: Nhấc bóng lên và thực hiện cú ném theo kỹ thuật đã học, giữ tập trung vào rổ.\n5. **Theo Dõi Kết Quả**: Nhìn theo bóng bay vào rổ, đảm bảo theo dõi cú ném cho đến khi bóng rời tay.\n\n## Kết Quả Và Phân Tích Kết Quả\nNếu cú ném thành công, bạn có thể giúp đội của mình giành chiến thắng. Nếu không, hãy xem xét lại từng bước đã thực hiện để tìm hiểu lý do.\n\n## Bài Học Rút Ra\nTình huống này cho thấy tầm quan trọng của việc chuẩn bị tâm lý và nắm vững kỹ thuật ném bóng. Hãy luyện tập trong các tình huống thực tế để cải thiện khả năng xử lý áp lực.\n=======================\nsdasdasdsd\n=============\n"}]', 'published', NULL, '2f55e271-35c0-4178-a2ed-dd0f219ab4bc', '2026-01-20 16:23:57.064', '{"simple": "# Tình Huống Ném Bóng Trong Trận Đấu\n\n## Tình Huống\nBạn đang trong một trận đấu bóng rổ quan trọng. Đội của bạn dẫn trước 2 điểm và chỉ còn 10 giây. Bạn được ném phạt vì đối thủ phạm lỗi.\n\n## Vấn Đề\nBạn cảm thấy áp lực vì cú ném này có thể quyết định trận đấu. Bạn cần phải bình tĩnh và ném bóng thật chính xác.\n\n## Giải Pháp\n1. **Hít thở sâu**: Giúp bạn bình tĩnh lại.\n2. **Tư thế đứng**: Đứng vững với chân bằng vai.\n3. **Cầm bóng**: Cảm thấy thoải mái khi cầm bóng.\n4. **Ném bóng**: Thực hiện cú ném với kỹ thuật đã học.\n5. **Theo dõi bóng**: Nhìn theo bóng cho đến khi nó rời tay.\n\n## Kết Quả\nNếu thành công, bạn giúp đội mình thắng. Nếu không, hãy tự xem lại cách ném của mình để cải thiện.", "detailed": "# Tình Huống Ném Bóng Trong Trận Đấu\n\n## Tình Huống\nTrong một trận đấu bóng rổ quan trọng, đội của bạn đang dẫn trước 2 điểm. Chỉ còn 10 giây trên đồng hồ và bạn có cơ hội ném phạt sau khi đối thủ phạm lỗi. Đây là thời điểm quyết định có thể ảnh hưởng đến kết quả trận đấu của bạn.\n\n## Vấn Đề\nCảm giác áp lực có thể khiến bạn lo lắng. Cú ném này không chỉ quan trọng cho trận đấu mà còn cho tinh thần của toàn đội. Giữ bình tĩnh và tập trung là rất quan trọng.\n\n## Giải Pháp Từng Bước\n1. **Chuẩn Bị Tinh Thần**: Hít thở sâu và tập trung vào mục tiêu. Hãy nhớ rằng bạn đã luyện tập rất nhiều cho khoảnh khắc này.\n2. **Tư Thế Đứng**: Đứng đúng tư thế với chân rộng bằng vai và gập nhẹ đầu gối. Tư thế đúng giúp bạn có sức mạnh khi ném.\n3. **Cầm Bóng**: Cầm bóng chắc chắn, cảm thấy thoải mái và tự tin. Hãy đảm bảo bạn có sự kiểm soát tốt với bóng.\n4. **Thực Hiện Cú Ném**: Nhấc bóng lên và thực hiện cú ném theo kỹ thuật đã học. Giữ tập trung vào rổ trong suốt quá trình ném.\n5. **Theo Dõi Kết Quả**: Nhìn theo bóng bay vào rổ. Theo dõi cú ném cho đến khi bóng rời tay để có thể điều chỉnh trong tương lai.\n\n## Kết Quả Và Phân Tích\nNếu cú ném thành công, bạn đã giúp đội mình giành chiến thắng. Nếu không, hãy xem xét lại từng bước để tìm hiểu lý do và cải thiện trong những lần sau.\n\n## Bài Học Rút Ra\nTình huống này cho thấy tầm quan trọng của việc chuẩn bị tâm lý và nắm vững kỹ thuật. Hãy thường xuyên luyện tập trong các tình huống thực tế để cải thiện khả năng xử lý áp lực.", "comprehensive": "# Tình Huống Ném Bóng Trong Trận Đấu\n\n## Tình Huống\nTrong một trận đấu bóng rổ căng thẳng, đội của bạn đang dẫn trước 2 điểm, nhưng chỉ còn 10 giây trên đồng hồ. Đối thủ phạm lỗi và bạn được quyền ném phạt. Đây là thời điểm quyết định, có thể làm thay đổi cục diện trận đấu.\n\n## Vấn Đề\nÁp lực từ trận đấu có thể khiến bạn cảm thấy hồi hộp. Cú ném này không chỉ là điểm số mà còn là yếu tố quyết định tinh thần của toàn đội. Để vượt qua áp lực, bạn cần phải có sự chuẩn bị về tâm lý và kỹ thuật.\n\n## Giải Pháp Từng Bước\n1. **Chuẩn Bị Tinh Thần**: Hít thở sâu, tập trung vào mục tiêu. Hãy nhớ lại những giờ luyện tập và cố gắng biến áp lực thành động lực.\n2. **Tư Thế Đứng**: Đứng với tư thế vững chắc, chân rộng bằng vai và gập nhẹ đầu gối. Tư thế này giúp duy trì sự cân bằng và sức mạnh khi ném.\n3. **Cầm Bóng**: Cầm bóng bằng cả hai tay, cảm nhận sự thoải mái và tự tin. Hãy chắc chắn rằng bạn có sự kiểm soát tốt đối với bóng.\n4. **Thực Hiện Cú Ném**: Nhấc bóng lên ngang đầu và thực hiện cú ném theo kỹ thuật đã học. Giữ mắt nhìn vào rổ và thực hiện cú ném một cách mượt mà.\n5. **Theo Dõi Kết Quả**: Nhìn theo bóng cho đến khi nó rời tay và theo dõi xem nó có vào rổ hay không. Việc theo dõi này giúp bạn rút kinh nghiệm cho các cú ném sau.\n\n## Kết Quả Và Phân Tích Kết Quả\nNếu cú ném thành công, bạn đã góp phần vào chiến thắng của đội. Nếu không, hãy xem xét lại từng bước để tìm ra lý do và cải thiện kỹ năng. Hãy ghi nhớ rằng thất bại cũng là một phần của quá trình học tập.\n\n## Bài Học Rút Ra\nTình huống này nhấn mạnh tầm quan trọng của sự chuẩn bị về tâm lý và kỹ thuật ném bóng. Đừng quên luyện tập trong các tình huống thực tế để cải thiện khả năng xử lý áp lực. Hãy tìm hiểu về các chiến lược tâm lý và kỹ thuật ném khác nhau để nâng cao hiệu suất cá nhân và đội nhóm. Việc hiểu rõ về cách mà áp lực ảnh hưởng đến tâm lý của bạn cũng là một yếu tố quan trọng trong thể thao."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c8445f5a-f0ad-4dea-882b-ac63d47051ca', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'concept', 'Tư thế và cách kiểm soát bóng', '# Tư Thế và Cách Kiểm Soát Bóng\n\nKiểm soát bóng là yếu tố quyết định trong kỹ thuật dribble và có thể được cải thiện thông qua tư thế và cách thức thực hiện.\n\n## Giới thiệu khái niệm\n\nTư thế đứng và cách kiểm soát bóng khi dribble là hai khía cạnh quan trọng giúp bạn thực hiện các pha dribble chính xác và hiệu quả. Việc giữ bóng chắc chắn và linh hoạt sẽ tạo ra nhiều cơ hội hơn trong trận đấu.\n\n## Giải thích chi tiết với các bước/điểm chính\n\n### 1. Tư thế đứng\n- **Đứng vững**: Hai chân nên được đặt ở khoảng cách bằng vai, giúp bạn dễ dàng di chuyển và thay đổi hướng.\n- **Gối hơi khuỵu**: Giúp tạo lực và thăng bằng.\n- **Cơ thể hơi nghiêng về phía trước**: Giúp bạn dễ dàng chuyển động và phản ứng nhanh chóng.\n\n### 2. Cách kiểm soát bóng\n- **Giữ bóng bằng ngón tay**: Sử dụng các ngón tay để kiểm soát bóng, không chỉ đơn thuần là lòng bàn tay.\n- **Điều chỉnh áp lực**: Tùy thuộc vào tình huống, bạn cần điều chỉnh lực đập bóng để giữ bóng không bị rơi.\n- **Sử dụng cơ thể để bảo vệ bóng**: Khi dribble, nên sử dụng cơ thể để chắn bóng khỏi đối thủ.\n\n### 3. Kỹ thuật kiểm soát bóng\n- **Dribble chậm**: Luyện tập dribble chậm để cảm nhận bóng và cải thiện khả năng kiểm soát.\n- **Dribble nhanh**: Khi đã tự tin, hãy thử dribble nhanh để tăng tốc độ và phản xạ.\n\n## Ví dụ minh họa cụ thể và chi tiết\n- **Tư thế đúng**: Hình ảnh mô tả tư thế đứng vững, gối khuỵu, và cánh tay tự nhiên.\n- **Ví dụ thực tế**: Khi đối thủ đến gần, sử dụng một tay để giữ bóng và tay còn lại để bảo vệ.\n\n## Lưu ý quan trọng và tips\n- **Thực hành với nhiều tình huống khác nhau**: Tập luyện dribble trong các tình huống thực tế để cải thiện khả năng phản xạ.\n- **Không hoảng loạn**: Trong tình huống khó khăn, hãy giữ bình tĩnh và tìm cách vượt qua.\n\n## Ứng dụng thực tế và case studies\nNhiều cầu thủ chuyên nghiệp đã áp dụng những phương pháp này để cải thiện kỹ năng dribble của họ. Ví dụ, Stephen Curry sử dụng kỹ thuật kiểm soát bóng hoàn hảo để tạo ra khoảng trống và ghi điểm từ xa.\n\n## Tóm tắt và bài tập tự luyện\nKỹ thuật kiểm soát bóng có thể cải thiện đáng kể chất lượng dribble của bạn. Luyện tập thường xuyên và thử nghiệm với nhiều kỹ thuật khác nhau sẽ giúp bạn trở thành một cầu thủ bóng rổ xuất sắc.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "An educational illustration showing a basketball player in the correct standing position for dribbling. The player should have their feet shoulder-width apart, knees slightly bent, and body leaning forward. Include annotations pointing to important features: ''Feet Position'', ''Knees Bent'', ''Body Lean'', and ''Ball Control with Fingers''. The background should be a basketball court, styled in a professional and educational manner.", "videoScript": "Chào mừng các bạn đến với bài học hôm nay về tư thế và cách kiểm soát bóng trong kỹ thuật dribble. Đầu tiên, điều quan trọng nhất là tư thế đứng. Hai chân nên được đặt ở khoảng cách bằng vai, gối hơi khuỵu để tạo lực và thăng bằng. Nhớ nghiêng người về phía trước để dễ dàng phản ứng nhanh chóng. \n\nTiếp theo là cách kiểm soát bóng. Bạn nên sử dụng các ngón tay để giữ bóng thay vì chỉ dùng lòng bàn tay. Điều chỉnh lực đập bóng tùy theo tình huống để tránh việc bóng bị rơi. Khi thực hiện dribble, hãy sử dụng cơ thể để bảo vệ bóng khỏi đối thủ. \n\nMột kỹ thuật quan trọng là luyện tập với cả dribble chậm và dribble nhanh. Hãy bắt đầu với dribble chậm để cảm nhận bóng trước khi tăng tốc. \n\nCuối cùng, hãy thực hành trong những tình huống khác nhau để cải thiện khả năng phản xạ. Giữ bình tĩnh trong các tình huống khó khăn và luôn tìm cách vượt qua. Hãy nhớ rằng nhiều cầu thủ chuyên nghiệp đã áp dụng những kỹ thuật này, ví dụ như Stephen Curry. Chúc các bạn luyện tập thành công!", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa tư thế đứng chính xác khi dribble, với ghi chú giải thích từng yếu tố về tư thế và cách kiểm soát bóng.", "videoDescription": "Video này giải thích tư thế và cách kiểm soát bóng trong kỹ thuật dribble, với các bước cụ thể và ví dụ thực tế."}', 2, '{"xp": 10, "coin": 1}', NULL, '2026-01-12 14:20:48.995251', '2026-01-27 11:36:50.783146', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Tư Thế và Cách Kiểm Soát Bóng trong Bóng Rổ\n\nKiểm soát bóng là điều quan trọng khi bạn dribble trong bóng rổ. Dưới đây là một số điểm chính bạn cần nhớ:\n\n## Tư thế đứng\n- **Đứng vững**: Đặt hai chân ngang vai để dễ di chuyển.\n- **Gối khuỵu**: Giúp bạn giữ thăng bằng tốt hơn.\n- **Nghiêng người**: Hơi nghiêng về phía trước để dễ dàng phản ứng.\n\n## Cách kiểm soát bóng\n- **Sử dụng ngón tay**: Giữ bóng bằng các ngón tay, không chỉ lòng bàn tay.\n- **Điều chỉnh lực đập**: Thay đổi lực đập bóng tùy theo tình huống.\n- **Bảo vệ bóng**: Dùng cơ thể để chắn bóng khỏi đối thủ.\n\n## Lưu ý\n- **Thực hành nhiều tình huống**: Tập luyện trong nhiều điều kiện khác nhau.\n- **Giữ bình tĩnh**: Không hoảng loạn khi gặp khó khăn.\n\nLuyện tập thường xuyên sẽ giúp bạn trở thành một cầu thủ giỏi hơn!", "detailed": "# Tư Thế và Cách Kiểm Soát Bóng trong Bóng Rổ\n\nKiểm soát bóng là một trong những yếu tố quyết định thành công khi bạn dribble. Để cải thiện kỹ thuật này, bạn cần chú ý đến tư thế đứng và cách kiểm soát bóng.\n\n## Giới thiệu\nTư thế và cách kiểm soát bóng là hai yếu tố then chốt giúp bạn thực hiện các pha dribble hiệu quả. Việc giữ bóng chắc chắn giúp bạn tạo ra nhiều cơ hội trong trận đấu.\n\n## 1. Tư thế đứng\n- **Đứng vững**: Hai chân nên cách nhau bằng vai để tạo sự ổn định và dễ dàng di chuyển.\n- **Gối khuỵu**: Khi gối hơi khuỵu, bạn có thể tạo lực và giữ thăng bằng tốt hơn.\n- **Cơ thể nghiêng về phía trước**: Giúp bạn phản ứng nhanh chóng với tình huống trên sân.\n\n## 2. Cách kiểm soát bóng\n- **Giữ bóng bằng ngón tay**: Sử dụng ngón tay để kiểm soát bóng thay vì lòng bàn tay.\n- **Điều chỉnh áp lực**: Tùy vào tình huống, thay đổi lực đập bóng để giữ bóng an toàn.\n- **Bảo vệ bóng**: Sử dụng cơ thể để chắn bóng khỏi đối thủ trong quá trình dribble.\n\n## 3. Kỹ thuật kiểm soát bóng\n- **Dribble chậm**: Bắt đầu bằng việc dribble chậm để cảm nhận bóng và cải thiện khả năng kiểm soát.\n- **Dribble nhanh**: Khi đã tự tin, hãy thử dribble nhanh để tăng tốc độ và phản xạ.\n\n## Ví dụ minh họa\nKhi bạn đứng đúng tư thế, cơ thể được giữ thăng bằng và dễ dàng di chuyển. Ví dụ, khi đối thủ đến gần, hãy dùng một tay giữ bóng và tay còn lại để bảo vệ.\n\n## Lưu ý quan trọng\n- **Thực hành nhiều tình huống khác nhau**: Giúp bạn cải thiện khả năng phản xạ.\n- **Giữ bình tĩnh**: Trong những tình huống khó khăn, hãy tìm cách vượt qua mà không hoảng loạn.\n\n## Tóm tắt\nKỹ thuật kiểm soát bóng là rất quan trọng để nâng cao chất lượng dribble của bạn. Hãy luyện tập thường xuyên và thử nghiệm với nhiều kỹ thuật khác nhau để trở thành cầu thủ giỏi hơn.", "comprehensive": "# Tư Thế và Cách Kiểm Soát Bóng trong Bóng Rổ\n\nKỹ thuật dribble là một phần thiết yếu trong bóng rổ, và kiểm soát bóng chính là chìa khóa để thực hiện các pha dribble hiệu quả. Trong nội dung này, chúng ta sẽ đi sâu vào tư thế đứng và cách kiểm soát bóng, cũng như các nguyên lý, lý thuyết và ứng dụng thực tế.\n\n## Giới thiệu khái niệm\nKiểm soát bóng không chỉ đơn thuần là việc giữ bóng, mà còn là khả năng điều khiển nó trong các tình huống khác nhau. Tư thế đứng và cách kiểm soát bóng là hai yếu tố quyết định đến sự thành công của bạn trên sân. Việc giữ bóng chắc chắn sẽ tạo ra nhiều cơ hội cho bạn và đội bóng.\n\n## 1. Tư thế đứng\n### a. Đứng vững\n- Đặt hai chân ngang vai để tạo sự ổn định và dễ dàng di chuyển.\n### b. Gối khuỵu\n- Khi bạn khuỵu gối, cơ thể sẽ tạo ra được lực và giữ thăng bằng tốt hơn.\n### c. Nghiêng người\n- Hơi nghiêng về phía trước giúp bạn dễ dàng chuyển động và phản ứng nhanh với tình huống trên sân.\n\n## 2. Cách kiểm soát bóng\n### a. Giữ bóng bằng ngón tay\n- Sử dụng các ngón tay để kiểm soát bóng thay vì chỉ dùng lòng bàn tay. Điều này giúp bạn có sự linh hoạt hơn.\n### b. Điều chỉnh áp lực\n- Tùy thuộc vào tình huống, bạn cần điều chỉnh lực đập bóng để giữ bóng không bị rơi.\n### c. Sử dụng cơ thể để bảo vệ bóng\n- Khi dribble, bạn nên dùng cơ thể để chắn bóng khỏi đối thủ, tạo ra khoảng trống cho mình.\n\n## 3. Kỹ thuật kiểm soát bóng\n### a. Dribble chậm\n- Luyện tập dribble chậm để cảm nhận bóng và cải thiện khả năng kiểm soát.\n### b. Dribble nhanh\n- Khi đã tự tin, hãy thử dribble nhanh để tăng tốc độ và phản xạ, điều này giúp bạn có thể vượt qua đối thủ.\n\n## Ví dụ minh họa cụ thể\n- **Tư thế đúng**: Hình ảnh mô tả tư thế đứng vững, gối khuỵu, và cánh tay tự nhiên.\n- **Ví dụ thực tế**: Khi đối thủ đến gần, sử dụng một tay để giữ bóng và tay còn lại để bảo vệ.\n\n## Lưu ý quan trọng và tips\n- **Thực hành với nhiều tình huống khác nhau**: Tập luyện dribble trong các tình huống thực tế sẽ giúp bạn cải thiện khả năng phản xạ.\n- **Không hoảng loạn**: Trong tình huống khó khăn, hãy giữ bình tĩnh và tìm cách vượt qua.\n\n## Ứng dụng thực tế và case studies\nNhiều cầu thủ chuyên nghiệp đã áp dụng những phương pháp này để cải thiện kỹ năng dribble của họ. Ví dụ, Stephen Curry sử dụng kỹ thuật kiểm soát bóng hoàn hảo để tạo ra khoảng trống và ghi điểm từ xa. Việc nghiên cứu cách mà những cầu thủ hàng đầu này thực hiện kỹ thuật có thể giúp bạn rút ra được những bài học quý giá.\n\n## Tóm tắt và bài tập tự luyện\nKỹ thuật kiểm soát bóng có thể cải thiện đáng kể chất lượng dribble của bạn. Hãy luyện tập thường xuyên và thử nghiệm với nhiều kỹ thuật khác nhau. Đừng quên ghi chú lại sự tiến bộ của bản thân và điều chỉnh các phương pháp luyện tập cho phù hợp với phong cách chơi của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3b5150dc-d58b-40c3-ada4-ccf019ac1971', '00bbe76d-6014-485e-890f-1398b6100e68', 'example', 'Ví Dụ: Luyện Tập Kỹ Thuật Ném Bóng Tại Nhà', 'Một trong những cách hiệu quả để cải thiện kỹ thuật ném bóng là luyện tập tại nhà. Sử dụng một cái rổ mini hoặc một bức tường, cầu thủ có thể thực hiện các bài tập như: 1. Ném bóng từ vị trí đứng yên để cải thiện kỹ thuật cầm bóng và tư thế cơ thể. 2. Thực hiện các cú ném từ nhiều góc độ khác nhau để phát triển khả năng điều chỉnh kỹ thuật. 3. Ghi lại video các cú ném để tự phân tích và nhận diện các điểm cần cải thiện. Việc tự luyện tập không chỉ giúp nâng cao kỹ năng mà còn tạo cơ hội để cầu thủ tự phản hồi và điều chỉnh kỹ thuật của mình.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "Create a detailed educational illustration showing advanced basketball shooting techniques. Illustrate a player practicing shooting with a mini hoop and against a wall. Include diagrams highlighting body posture, ball grip, and foot positioning for shooting from different angles. Annotations should indicate key points for improving shooting form, with emphasis on biomechanics and technique analysis, set in a professional, instructional style.", "videoScript": "Chào các bạn, trong video hôm nay, chúng ta sẽ cùng khám phá các kỹ thuật ném bóng nâng cao có thể luyện tập tại nhà. Đầu tiên, chúng ta hãy tập trung vào việc ném bóng từ vị trí đứng yên. Hãy đảm bảo rằng bạn cầm bóng đúng cách, thái độ cơ thể thẳng hàng với rổ. Sau khi ném bóng thành thạo từ một vị trí, hãy thử từ nhiều góc độ khác nhau. Điều này giúp bạn phát triển khả năng điều chỉnh kỹ thuật một cách hiệu quả hơn. Một trong những phương pháp tuyệt vời là ghi lại video các cú ném của bạn, sau đó xem lại để tự phân tích. Chú ý tới những điểm nào cần cải thiện, hãy tự đưa ra phản hồi để điều chỉnh kỹ thuật cho tốt hơn. Nhớ rằng, việc tự luyện tập là chìa khóa giúp bạn nâng cao kỹ năng của mình. Cuối cùng, hãy tạo thói quen luyện tập hàng ngày và không ngừng cố gắng. Chúc bạn luyện tập thành công!", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa các kỹ thuật ném bóng nâng cao, với cầu thủ đang luyện tập ném bóng vào rổ mini và vào tường, điều chỉnh tư thế cơ thể và cầm bóng.", "videoDescription": "Video này hướng dẫn các kỹ thuật ném bóng nâng cao mà bạn có thể luyện tập tại nhà, bao gồm các bài tập và tự phân tích kỹ thuật."}', 9, '{"xp": 23, "coin": 4}', NULL, '2026-01-16 15:13:55.176356', '2026-01-19 23:39:20.491784', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4ed65126-e645-4293-bda2-d8b92e34b9ad', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Kỹ Thuật Ném Bóng Từ Vị Trí Khó', 'Ném bóng từ vị trí khó như góc 45 độ hoặc khi bị kẹp giữa hai cầu thủ là một thử thách lớn. Kỹ thuật ném trong những tình huống này cần phải linh hoạt và nhanh chóng. Các bước thực hiện bao gồm: 1. Đánh giá vị trí của đối thủ và khoảng cách đến rổ. 2. Sử dụng động tác giả để đánh lừa đối thủ và tạo khoảng trống. 3. Thực hiện cú ném với một tay trong khi tay kia giữ bóng ở vị trí ổn định. 4. Theo dõi đường đi của bóng và điều chỉnh hướng cú ném dựa trên phản ứng của đối thủ. Việc thành thạo kỹ thuật này không chỉ giúp cầu thủ đa dạng hóa cú ném mà còn nâng cao khả năng ghi điểm trong các tình huống khó.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A high-level educational illustration showing advanced basketball techniques for shooting from difficult positions, including a player shooting from a 45-degree angle and another player surrounded by defenders. The image should include detailed diagrams of body positioning, ball grip, and follow-through techniques. The style should be professional and educational, focusing on complexity and clarity.", "videoScript": "Xin chào các bạn, hôm nay chúng ta sẽ cùng tìm hiểu về kỹ thuật ném bóng từ vị trí khó trong bóng rổ. Điều này bao gồm việc ném bóng từ góc 45 độ hoặc khi bị kẹp giữa hai cầu thủ. Đầu tiên, bạn cần đánh giá vị trí của đối thủ và khoảng cách đến rổ. Khi bạn xác định được vị trí của mình, hãy bắt đầu với động tác giả để đánh lừa đối phương và tạo ra khoảng trống cho cú ném. Sau đó, bạn sẽ sử dụng một tay để thực hiện cú ném, trong khi tay còn lại giữ bóng ở vị trí ổn định, tránh để bóng rời khỏi tầm kiểm soát của bạn. Cuối cùng, hãy theo dõi đường đi của bóng và điều chỉnh hướng ném dựa trên phản ứng của đối thủ. Kỹ thuật này không chỉ giúp bạn nâng cao khả năng ghi điểm mà còn tạo sự đa dạng trong lối chơi của bạn. Hãy thực hành thường xuyên để trở thành cầu thủ xuất sắc! Cảm ơn các bạn đã theo dõi và chúc các bạn thành công trong những cú ném khó nhé!", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa kỹ thuật ném bóng trong bóng rổ từ những vị trí khó, bao gồm góc 45 độ và khi bị kẹp giữa hai cầu thủ, thể hiện chi tiết về tư thế cơ thể và cách giữ bóng.", "videoDescription": "Video này hướng dẫn kỹ thuật ném bóng từ các vị trí khó trong bóng rổ, bao gồm các bước chi tiết và mẹo hữu ích."}', 11, '{"xp": 18, "coin": 3}', NULL, '2026-01-16 15:13:55.162214', '2026-01-19 23:39:20.507574', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('343b0c8e-3ad4-43b0-9947-934a75df955f', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Phân Tích Kỹ Thuật Ném Bóng Từ Các Cầu Thủ Chuyên Nghiệp', 'Phân tích kỹ thuật ném bóng của các cầu thủ chuyên nghiệp như Stephen Curry hay Klay Thompson có thể cung cấp những bài học quý giá. Điểm nổi bật trong kỹ thuật của họ bao gồm: 1. Tư thế đứng vững chắc và sự cân bằng trong cú ném. 2. Khả năng sử dụng sức mạnh từ cơ thể một cách hiệu quả. 3. Tinh thần kiên nhẫn và kiên định trong việc thực hiện cú ném. 4. Sự linh hoạt trong việc điều chỉnh kỹ thuật tùy theo tình huống trận đấu. Việc học hỏi từ những cầu thủ này không chỉ giúp cải thiện kỹ thuật cá nhân mà còn cung cấp cái nhìn sâu sắc về cách áp dụng các chiến thuật khác nhau trong trận đấu.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A highly detailed and technical illustration showcasing the shooting techniques of professional basketball players like Stephen Curry and Klay Thompson. The image should include annotations highlighting their balanced stance, body strength utilization, patience in execution, and adaptability to game situations. Diagram elements should be included, demonstrating the biomechanics of the shooting motion, with a professional and educational style.", "videoScript": "Chào mừng các bạn đến với bài học về kỹ thuật ném bóng từ các cầu thủ chuyên nghiệp. Hôm nay, chúng ta sẽ phân tích chi tiết về những cầu thủ nổi tiếng như Stephen Curry và Klay Thompson. Đầu tiên, chúng ta hãy nói về tư thế đứng vững chắc. Họ tạo dựng một nền tảng vững chắc với đôi chân rộng mở, giúp giữ thăng bằng trong từng cú ném. Tiếp theo, sức mạnh từ cơ thể được sử dụng hiệu quả, từ chân lên đến tay, dẫn đến một cú ném mạnh mẽ và chính xác. Thêm vào đó, tinh thần kiên nhẫn là một yếu tố quan trọng. Họ không vội vàng trong việc thực hiện cú ném. Cuối cùng, sự linh hoạt trong kỹ thuật rất quan trọng. Khi cầu thủ gặp những tình huống khác nhau trong trận đấu, họ có khả năng điều chỉnh kỹ thuật ném để phù hợp với từng hoàn cảnh. Hãy cùng tiếp tục theo dõi và rút ra những bài học giá trị từ các kỹ thuật này để cải thiện khả năng ném bóng của riêng bạn.", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa kỹ thuật ném bóng của các cầu thủ chuyên nghiệp, với các chi tiết kỹ thuật và ghi chú giải thích rõ tư thế và biện pháp thực hiện cú ném.", "videoDescription": "Video phân tích sâu về kỹ thuật ném bóng của các cầu thủ bóng rổ chuyên nghiệp, mang lại cái nhìn chuyên sâu về chiến thuật và kỹ thuật ném bóng."}', 12, '{"xp": 18, "coin": 3}', NULL, '2026-01-16 15:13:55.16649', '2026-01-19 23:39:20.513187', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1cef0efb-7c27-49a7-8d8e-3fb944183db9', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Nguyên Tắc Kỹ Thuật Ném Bóng Đứng', 'Kỹ thuật ném bóng đứng là một trong những phương pháp cơ bản nhưng cực kỳ quan trọng trong bóng rổ. Nguyên tắc chính của kỹ thuật này bao gồm việc sử dụng trọng lực và sức mạnh từ chân để tăng cường lực ném. Để thực hiện cú ném hiệu quả, cầu thủ cần phải: 1. Đứng thẳng với chân rộng bằng vai. 2. Đưa bóng lên phía trước, giữ bóng bằng tay không, tay ném nằm bên dưới. 3. Sử dụng động tác hạ thấp cơ thể để tạo lực, sau đó bật lên và ném bóng theo đường thẳng. 4. Lưu ý rằng lực ném không chỉ đến từ cánh tay mà còn từ cơ thể, đặc biệt là từ chân và hông. Việc sử dụng cơ thể một cách hiệu quả trong cú ném có thể tạo ra sự khác biệt lớn trong khả năng ghi điểm.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "An educational diagram illustrating the principles of the standing shot technique in basketball. The diagram should include detailed depictions of each key step: 1. A player standing straight with feet shoulder-width apart; 2. The player holding the ball in front, with the non-throwing hand supporting and the throwing hand positioned beneath the ball; 3. A sequence showing the body lowering and then launching upwards while throwing straight; 4. Annotations highlighting the importance of using not just the arm, but also the legs and hips for effective shot mechanics. The style should be professional, with clear labels and color coding for easy understanding.", "videoScript": "Chào mừng các bạn đến với bài học về kỹ thuật ném bóng đứng trong bóng rổ. Hôm nay, chúng ta sẽ phân tích nguyên tắc cơ bản của kỹ thuật này. Kỹ thuật ném bóng đứng là một trong những phương pháp quan trọng giúp bạn ghi điểm hiệu quả. Đầu tiên, hãy đứng thẳng với chân rộng bằng vai để tạo sự ổn định. Bước tiếp theo, đưa bóng lên phía trước, tay không giữ bóng hỗ trợ và tay ném nằm bên dưới. Đặc biệt, không chỉ cánh tay, mà lực ném còn đến từ cơ thể, chân và hông. Bây giờ, hãy nhìn vào động tác hạ thấp cơ thể để tạo lực ném. Khi bạn bật lên, hãy nhớ theo dõi đường ném của bóng, nó nên đi theo phương thẳng đứng. Phân tích từng bước này giúp bạn nhận ra tầm quan trọng của việc sử dụng cơ thể hiệu quả trong cú ném. Cuối cùng, hãy thực hành nhiều lần để cải thiện kỹ năng này. Cảm ơn các bạn đã theo dõi bài học hôm nay!", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa kỹ thuật ném bóng đứng trong bóng rổ, giới thiệu các bước thực hiện và vai trò của cơ thể trong cú ném.", "videoDescription": "Video này phân tích sâu về nguyên tắc kỹ thuật ném bóng đứng trong bóng rổ và hướng dẫn từng bước thực hiện."}', 9, '{"xp": 18, "coin": 3}', NULL, '2026-01-16 15:13:55.135931', '2026-01-19 23:39:20.49718', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('404934f0-5624-4ff1-acd6-fb98aca7084a', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Cảm Nhận và Tâm Lý Trong Ném Bóng', 'Tâm lý và cảm giác khi ném bóng là một yếu tố quyết định đến thành công của cú ném. Nhiều cầu thủ gặp khó khăn trong việc duy trì sự tự tin và tập trung khi đứng trước rổ. Một số kỹ thuật để cải thiện tâm lý bao gồm: 1. Hít thở sâu để giảm căng thẳng trước khi ném. 2. Tập trung vào mục tiêu và hình dung cú ném thành công trước khi thực hiện. 3. Sử dụng những âm thanh tích cực hoặc từ ngữ khích lệ để tạo động lực. 4. Phân tích các cú ném trước đó để học hỏi từ những sai lầm và thành công. Tâm lý vững vàng không chỉ giúp cầu thủ tự tin hơn mà còn ảnh hưởng đến cách mà họ điều chỉnh cú ném trong những tình huống khó khăn.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A detailed and complex illustration depicting the psychology and feelings involved in basketball shooting. Show a basketball player in a dynamic motion, taking a deep breath before a shot, with visual elements like focus lines leading to the basket. Include thought bubbles representing concentration, confidence, and positive affirmations. Use a professional and educational style with annotations explaining each psychological element. The background should have a basketball court and spectators to create an immersive environment.", "videoScript": "Chào mừng bạn đến với bài học về Cảm Nhận và Tâm Lý Trong Ném Bóng! Trong bóng rổ, tâm lý và cảm giác khi ném bóng là rất quan trọng. Khi cầu thủ đứng trước rổ, nhiều người thường cảm thấy lo lắng và mất tự tin. Điều này có thể ảnh hưởng trực tiếp đến cú ném của họ. Trong video này, chúng ta sẽ khám phá những kỹ thuật giúp cải thiện tâm lý khi ném bóng. Đầu tiên, hãy thực hành hít thở sâu. Việc này giúp giảm căng thẳng và kích thích cảm giác bình tĩnh. Tiếp theo, hãy tập trung vào mục tiêu của bạn và hình dung một cú ném thành công. Hãy tưởng tượng quả bóng bay thẳng vào rổ. Ngoài ra, sử dụng những câu nói tích cực, như ''Tôi có thể làm được!'' để tạo động lực. Cuối cùng, đừng quên phân tích những cú ném trước đó. Học hỏi từ những sai lầm và thành công là chìa khóa để cải thiện kỹ năng. Hãy nhớ rằng, tâm lý vững vàng chính là chìa khóa giúp bạn vượt qua những tình huống khó khăn. Cảm ơn bạn đã theo dõi bài học hôm nay!", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa cho tâm lý và cảm nhận trong quá trình ném bóng. Nó thể hiện cầu thủ bóng rổ đang hít thở sâu, tập trung vào mục tiêu với các yếu tố tâm lý xung quanh.", "videoDescription": "Khám phá tâm lý và cảm nhận quan trọng trong ném bóng rổ, cùng các kỹ thuật giúp cầu thủ tự tin hơn."}', 10, '{"xp": 18, "coin": 3}', NULL, '2026-01-16 15:13:55.156817', '2026-01-19 23:39:20.502765', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('11cb163a-b88e-4003-93b7-aabf03a7e24b', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'concept', 'Khái niệm dribble', '# Khái Niệm Dribble\n\nDribble là một kỹ thuật cơ bản và thiết yếu trong bóng rổ, cho phép người chơi kiểm soát bóng khi di chuyển. Kỹ thuật này không chỉ đơn thuần là việc đập bóng xuống mặt đất mà còn bao gồm cách sử dụng cơ thể và các phần của tay để giữ bóng và thay đổi hướng một cách hiệu quả.\n\n## Giới thiệu khái niệm\n\nDribble là kỹ thuật giúp người chơi di chuyển với bóng mà không bị mất kiểm soát. Nó cho phép bạn tạo ra khoảng trống, vượt qua đối thủ và thiết lập các tình huống tấn công. Kỹ thuật dribble có thể được thực hiện với nhiều phong cách khác nhau, từ việc sử dụng một tay đến việc thay đổi tốc độ và hướng di chuyển.\n\n## Giải thích chi tiết với các bước/điểm chính\n\n### 1. Tư thế cơ bản khi dribble\n- **Chân đứng rộng**: Để giữ thăng bằng, bạn cần đứng với chân rộng bằng vai.\n- **Gối hơi khuỵu**: Giúp bạn dễ dàng di chuyển và thay đổi hướng.\n- **Cánh tay tự nhiên**: Cánh tay không nên quá căng thẳng, giữ bóng bằng tay thuận với các ngón tay mở rộng.\n\n### 2. Kỹ thuật đập bóng\n- **Đập bóng với lực vừa phải**: Không nên quá mạnh để bóng bay xa hoặc quá yếu không thể kiểm soát.\n- **Sử dụng ngón tay**: Đập bóng bằng phần ngón tay, không phải lòng bàn tay để có độ chính xác cao.\n- **Nhìn về phía trước**: Khi dribble, bạn phải nhìn về phía trước để quan sát các đối thủ và tình huống xung quanh.\n\n### 3. Thay đổi hướng và tốc độ\n- **Sử dụng chân**: Để thay đổi hướng, bạn cần di chuyển chân theo hướng mới và dùng cơ thể để tạo ra động lực.\n- **Sử dụng các kỹ thuật dribble khác nhau**: Như crossover, behind-the-back, hoặc spin move để đánh lừa đối thủ.\n\n### 4. Tập luyện và cải thiện\n- **Luyện tập hàng ngày**: Dành thời gian hàng ngày để luyện tập dribble với các bài tập khác nhau.\n- **Sử dụng gương hoặc video**: Theo dõi cách bạn thực hiện kỹ thuật để điều chỉnh và cải thiện.\n\n## Ví dụ minh họa cụ thể và chi tiết\n- **Dribble đơn giản**: Luyện tập dribble đơn giản bằng cách đập bóng từ trái sang phải trong khi di chuyển về phía trước.\n- **Dribble nâng cao**: Thử các kỹ thuật dribble phức tạp như crossover khi bị đối thủ áp sát.\n\n## Lưu ý quan trọng và tips\n- **Giữ đầu cao**: Đừng nhìn xuống bóng, hãy quan sát xung quanh.\n- **Thực hành thường xuyên**: Luyện tập với một đối tác hoặc trên sân để nâng cao kỹ năng.\n\n## Ứng dụng thực tế và case studies\nKỹ thuật dribble rất quan trọng trong các trận đấu thực tế. Nhiều cầu thủ nổi tiếng như Allen Iverson hay Kyrie Irving đã xây dựng sự nghiệp của họ dựa trên khả năng dribble xuất sắc. Họ thường xuyên sử dụng các kỹ thuật dribble để vượt qua đối thủ và tạo ra cơ hội ghi điểm.\n\n## Tóm tắt và bài tập tự luyện\nDribble là kỹ thuật thiết yếu trong bóng rổ, cần được luyện tập thường xuyên để cải thiện kỹ năng. Một bài tập đơn giản là thực hiện 10 phút dribble liên tục, thay đổi hướng và tốc độ.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A detailed educational illustration depicting the concept of dribbling in basketball. The image should show a player in a basketball stance with wide legs, slightly bent knees, and a relaxed arm holding a basketball. Include annotations for the proper stance, ball handling technique using fingers rather than palms, and looking forward. The background should be a basketball court with a hoop and other players in action, capturing the dynamic nature of the game.", "videoScript": "Chào mừng các bạn đến với bài học về khái niệm dribble trong bóng rổ. Hôm nay, chúng ta sẽ tìm hiểu về kỹ thuật đập bóng, một kỹ thuật cơ bản và quan trọng giúp bạn kiểm soát bóng khi di chuyển trên sân. Đầu tiên, hãy xem xét tư thế cơ bản khi dribble. Bạn nên đứng với chân rộng bằng vai, gối hơi khuỵu để có thể dễ dàng thay đổi hướng. Cánh tay cần giữ tự nhiên, dùng tay thuận để cầm bóng và mở rộng ngón tay để kiểm soát tốt hơn. Tiếp theo, hãy chú ý đến cách đập bóng. Đập bóng với lực vừa phải là rất quan trọng, đừng quá mạnh hoặc quá yếu. Sử dụng ngón tay để đảm bảo độ chính xác khi tạo lực đập. Một yếu tố quan trọng khác là bạn phải nhìn về phía trước để quan sát các đối thủ và tình huống xung quanh khi dribble. Bây giờ chúng ta sẽ bàn về việc thay đổi hướng và tốc độ. Khi cần thay đổi hướng, di chuyển chân theo hướng mới và sử dụng cơ thể để tạo động lực. Bạn cũng có thể sử dụng các kỹ thuật dribble khác nhau như crossover, behind-the-back, hoặc spin move để đánh lừa đối thủ. Cuối cùng, hãy nhớ rằng luyện tập hàng ngày là chìa khóa để cải thiện kỹ năng dribble của bạn. Hãy dành thời gian mỗi ngày để luyện tập với các bài tập khác nhau để hoàn thiện kỹ thuật. Cảm ơn các bạn đã theo dõi bài học hôm nay!", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa kỹ thuật dribble trong bóng rổ, thể hiện tư thế cơ bản khi đập bóng và các yếu tố quan trọng để kiểm soát bóng trong khi di chuyển.", "videoDescription": "Video này giúp bạn hiểu rõ hơn về khái niệm dribble trong bóng rổ và cách thực hiện kỹ thuật này một cách hiệu quả."}', 1, '{"xp": 10, "coin": 1}', NULL, '2026-01-12 14:20:48.989425', '2026-01-27 11:36:22.20521', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Khái Niệm Dribble\n\nDribble là một kỹ thuật cơ bản trong bóng rổ, giúp bạn kiểm soát bóng khi di chuyển. Giống như một người đi bộ có thể thay đổi hướng mà không cần dừng lại, dribble cho phép bạn di chuyển với bóng mà không bị mất kiểm soát. Để dribble hiệu quả, bạn cần đứng với chân rộng, gối khuỵu và giữ bóng bằng tay thuận. Hãy nhớ đập bóng với lực vừa phải và luôn nhìn về phía trước để quan sát xung quanh.\n\nKhi bạn đã làm quen với dribble cơ bản, hãy thử thay đổi hướng và tốc độ, sử dụng các kỹ thuật như crossover. Thực hành thường xuyên là chìa khóa để cải thiện kỹ năng. Bạn có thể tập 10 phút mỗi ngày, thay đổi hướng và tốc độ để trở nên linh hoạt hơn trong các trận đấu thực tế.", "detailed": "# Khái Niệm Dribble\n\nDribble là một kỹ thuật quan trọng trong bóng rổ, cho phép người chơi kiểm soát bóng trong khi di chuyển. Kỹ thuật này không chỉ đơn giản là đập bóng xuống đất mà còn liên quan đến cách sử dụng cơ thể để thay đổi hướng và tốc độ.\n\n## Giới thiệu khái niệm\nDribble giúp người chơi tạo không gian và vượt qua đối thủ. Bạn có thể thực hiện dribble với nhiều phong cách khác nhau, từ việc sử dụng một tay đến thay đổi tốc độ.\n\n## Các bước thực hiện dribble\n### 1. Tư thế cơ bản\n- **Chân đứng rộng**: Giữ thăng bằng tốt hơn.\n- **Gối khuỵu**: Giúp di chuyển dễ dàng hơn.\n- **Cánh tay tự nhiên**: Giữ bóng bằng tay thuận với ngón tay mở rộng.\n\n### 2. Kỹ thuật đập bóng\n- **Đập bóng với lực vừa phải**: Không quá mạnh hoặc quá yếu.\n- **Sử dụng ngón tay**: Đập bóng bằng ngón tay để có độ chính xác.\n- **Nhìn về phía trước**: Quan sát đối thủ và tình huống xung quanh.\n\n### 3. Thay đổi hướng và tốc độ\n- **Sử dụng chân**: Di chuyển chân theo hướng mới.\n- **Kỹ thuật khác**: Crossover, behind-the-back, spin move để đánh lừa đối thủ.\n\n### 4. Tập luyện và cải thiện\n- **Luyện tập hàng ngày**: Thực hiện các bài tập dribble khác nhau.\n- **Theo dõi bản thân**: Sử dụng gương hoặc video để cải thiện kỹ thuật.\n\n## Lưu ý quan trọng\n- **Giữ đầu cao**: Quan sát xung quanh, không nhìn xuống bóng.\n- **Thực hành thường xuyên**: Luyện tập với đối tác để nâng cao kỹ năng.\n\nDribble là kỹ thuật thiết yếu cho mọi cầu thủ. Hãy luyện tập để cải thiện kỹ năng của bạn!", "comprehensive": "# Khái Niệm Dribble Trong Bóng Rổ\n\nDribble là một trong những kỹ thuật cơ bản và quan trọng nhất trong bóng rổ, cho phép người chơi kiểm soát bóng trong khi di chuyển. Kỹ thuật này không chỉ giúp người chơi giữ bóng mà còn tạo cơ hội tấn công và vượt qua đối thủ. Trong bài viết này, chúng ta sẽ khám phá sâu hơn về kỹ thuật dribble, từ tư thế cơ bản đến các kỹ thuật nâng cao, cũng như tầm quan trọng của việc luyện tập.\n\n## Giới thiệu chi tiết về dribble\nDribble không chỉ là việc đập bóng xuống đất mà còn bao gồm nhiều yếu tố như tư thế cơ thể, cách di chuyển và kỹ thuật kiểm soát bóng. Việc nắm vững kỹ thuật dribble sẽ giúp người chơi trở nên linh hoạt hơn trong các tình huống trên sân.\n\n## Các bước thực hiện kỹ thuật dribble\n### 1. Tư thế cơ bản khi dribble\n- **Chân đứng rộng**: Giúp bạn giữ thăng bằng và ổn định.\n- **Gối hơi khuỵu**: Tạo điều kiện cho việc di chuyển nhanh chóng và dễ dàng thay đổi hướng.\n- **Cánh tay tự nhiên**: Giữ bóng bằng tay thuận, các ngón tay mở rộng để kiểm soát tốt hơn.\n\n### 2. Kỹ thuật đập bóng\n- **Đập bóng với lực vừa phải**: Điều này giúp bạn kiểm soát bóng tốt hơn, tránh việc bóng bay quá xa hoặc quá gần.\n- **Sử dụng ngón tay**: Đập bóng bằng ngón tay thay vì lòng bàn tay để đảm bảo độ chính xác.\n- **Nhìn về phía trước**: Giữ cái nhìn lên, quan sát đối thủ và không gian xung quanh.\n\n### 3. Thay đổi hướng và tốc độ\n- **Sử dụng chân**: Để thay đổi hướng, bạn cần di chuyển chân và cơ thể theo hướng mới.\n- **Kỹ thuật khác nhau**: Sử dụng các kỹ thuật dribble như crossover, behind-the-back, hoặc spin move để tạo sự bất ngờ cho đối thủ.\n\n### 4. Tập luyện và cải thiện\n- **Luyện tập hàng ngày**: Dành thời gian hàng ngày để luyện tập các kỹ thuật dribble khác nhau.\n- **Theo dõi và điều chỉnh**: Sử dụng gương hoặc video để theo dõi kỹ thuật của bản thân và điều chỉnh cho phù hợp.\n\n## Ví dụ minh họa\n- **Dribble đơn giản**: Thực hiện dribble từ trái sang phải trong khi di chuyển về phía trước để làm quen với cảm giác bóng.\n- **Dribble nâng cao**: Thử nghiệm với các kỹ thuật phức tạp như crossover khi bị đối thủ áp sát.\n\n## Lưu ý quan trọng và tips\n- **Giữ đầu cao**: Đừng chỉ nhìn xuống bóng, hãy quan sát xung quanh để có thể xử lý tình huống tốt hơn.\n- **Thực hành thường xuyên**: Luyện tập với đối tác hoặc trên sân để nâng cao kỹ năng dribble.\n\n## Ứng dụng thực tế\nKỹ thuật dribble rất quan trọng trong các trận đấu thực tế. Nhiều cầu thủ nổi tiếng như Allen Iverson hay Kyrie Irving đã sử dụng dribble để vượt qua đối thủ và tạo cơ hội ghi điểm. Kỹ năng này không chỉ giúp họ ghi bàn mà còn là một phần quan trọng trong chiến lược tấn công của cả đội.\n\n## Tóm tắt và bài tập tự luyện\nDribble là kỹ thuật thiết yếu trong bóng rổ, cần được luyện tập thường xuyên để cải thiện kỹ năng. Một bài tập đơn giản là thực hiện 10 phút dribble liên tục, thay đổi hướng và tốc độ. Hãy nhớ rằng, kỹ thuật dribble không chỉ là một phần của trò chơi, mà còn là một nghệ thuật cần sự kiên nhẫn và luyện tập."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1b2e05f3-1497-4f29-96c1-af2d31072964', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'example', 'Tư Thế Dribble Vào Lúc Chơi', 'Bạn đang trong một trận đấu bóng rổ sôi động. Khi nhận bóng từ đồng đội, bạn luôn cần phải có tư thế tốt. Hãy nhớ đứng với chân hơi rộng, đầu gối vẫn khuỵu xuống, giống như khi bạn chuẩn bị nhảy lên một cái ghế cao. Bạn đặt bóng ở gần người và dribble tiếp tục. Khi bạn di chuyển, thường có nhiều cầu thủ khác đang chạy xung quanh. Giữ bóng gần mình là điều quan trọng, giống như khi bạn ôm một món đồ yêu quý, không muốn ai khác chạm vào. Nếu bạn thấy một khoảng trống trên sân, đừng ngần ngại mà hãy chạy qua chỗ đó và dribble bóng theo sau. Cảm giác như bạn đang nhảy hết mình trong một điệu múa mà chỉ có bạn và bóng!', '{"imageUrl": "https://placehold.co/800x600/dcfce7/166534?text=%F0%9F%8C%B1+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+%C4%90%C6%A1n+Gi%E1%BA%A3n)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A simple and bright illustration of a basketball player in a dribbling stance. The player has wide legs, slightly bent knees, and holds the basketball close to their body, as if protecting it. The background shows a colorful basketball court with minimal details, ensuring the focus is on the player''s posture. The style is fun and educational, suitable for children.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ học về tư thế dribble trong bóng rổ. Khi nhận bóng từ đồng đội, hãy đứng với chân hơi rộng và đầu gối khuỵu xuống nhé! Như khi bạn chuẩn bị nhảy lên một cái ghế cao. Giữ bóng gần người, giống như ôm món đồ yêu thích của bạn! Nếu bạn thấy khoảng trống, hãy chạy tới đó và dribble bóng theo sau. Cảm giác như đang nhảy múa với bóng vậy! Hãy luyện tập tư thế này để trở thành cầu thủ giỏi nhé!", "videoDuration": "30-60 giây", "imageDescription": "Hình ảnh minh họa một cầu thủ bóng rổ đang trong tư thế dribble, với đôi chân rộng và đầu gối khuỵu xuống, giúp học sinh dễ hiểu về tư thế chuẩn bị.", "videoDescription": "Video này hướng dẫn các bạn về tư thế dribble trong bóng rổ, giúp bạn có được sự tự tin và kỹ năng cần thiết trong trận đấu."}', 2, '{"xp": 12, "coin": 1}', NULL, '2026-01-15 22:11:48.53681', '2026-01-19 23:39:21.430361', 'mixed', 'easy', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('fa728633-ec0a-4b7f-8d20-15af938c4809', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'concept', 'Kiểm Soát Bóng Khi Dribble: Từ Cơ Bản Đến Nâng Cao', '# Kiểm Soát Bóng Khi Dribble: Từ Cơ Bản Đến Nâng Cao\n\n## Giới thiệu\nTrong bóng rổ, kiểm soát bóng trong khi dribble là một kỹ năng thiết yếu quyết định khả năng chuyền, ghi điểm và tránh được các đối thủ. Kỹ thuật này không chỉ đòi hỏi sự khéo léo mà còn cần tính toán chiến lược để tạo ra lợi thế trên sân. Bài viết này sẽ đi sâu vào cách kiểm soát bóng hiệu quả khi dribble, thông qua các phân tích kỹ lưỡng về thuật ngữ, best practices và các trường hợp ngoại lệ.\n\n## 1. Khái Niệm Kiểm Soát Bóng\n- **Kiểm soát bóng**: là khả năng điều khiển bóng để thực hiện các chiến thuật một cách hiệu quả, bao gồm việc duy trì bóng trong tay đồng thời di chuyển, bao quát sân, và đọc tình huống trận đấu. Đây là một trong những yếu tố quan trọng nhất trong kỹ thuật dribble.\n- **Tốp độ** và **nhịp điệu**: Là hai khái niệm liên quan đến việc kiểm soát bóng. Tốp độ đề cập đến tốc độ di chuyển trong khi nhịp điệu là cách bạn phối hợp các động tác với nhau.\n\n## 2. Tư Thế Căn Bản Khi Dribble\nMột tư thế tốt sẽ giúp bạn kiểm soát bóng hiệu quả hơn. Các yếu tố cần chú ý bao gồm:\n- **Chân**: Đặt chân rộng, một chân trước một chân sau.\n- **Đầu gối**: Hơi khuỵu xuống để có thể di chuyển linh hoạt.\n- **Cánh tay**: Cánh tay không cầm bóng cần phải mở rộng để bảo vệ bóng khỏi đối thủ.\n\n## 3. Các Kỹ Thuật Kiểm Soát Bóng Nâng Cao\n### 3.1 Dribble với Tay Không Chiếm\nKhi bạn dribble, hãy đảm bảo rằng bạn luôn giữ bóng gần cơ thể mà tay không chiếm giữ đủ không gian cho phép đối thủ tấn công. Kỹ thuật này cho phép người chơi nhanh chóng phát hiện và phản ứng với đe dọa từ đối thủ.\n\n### 3.2 Dribble Bằng Một Tay\nDribble bằng một tay cho phép bạn sử dụng tay còn lại để tạo khoảng trống hoặc cản trở đối thủ. Kỹ thuật này cũng yêu cầu sự chú ý về lực tác động để duy trì kiểm soát mà không làm mất bóng.\n\n### 3.3 Điều Chỉnh Tốp Độ Dribble\nViệc điều chỉnh tốc độ dribble không chỉ giúp bạn tránh được sự theo đuổi của đối thủ mà còn dễ dàng chuyển sang các động tác khác như chuyền hoặc dứt điểm. Đặc biệt, hãy chú ý đến timing để tấn công hoặc lùi lại khi cần thiết.\n\n## 4. Các Trường Hợp Ngoại Lệ\n### 4.1 Dribble Ngược Phía\nKhi bị áp lực từ đối thủ, một dribble ngược có thể là lựa chọn tốt để thiết lập lại khoảng cách và đánh lừa đối thủ. Tuy nhiên, đây cũng là lúc dễ dàng mất bóng nếu không thực hiện chính xác.\n\n### 4.2 Dribble Khi Bị Chặn\nKhi bị chặn, cần phải có cách xử lý thông minh. Phân tích bóng, theo dõi vị trí của đồng đội và lựa chọn một thời điểm để chuyền bóng hoặc thực hiện bước nhảy kịp thời.\n\n## 5. Tổng Kết\nKiểm soát bóng khi dribble không chỉ là một kỹ năng cần thiết mà còn là một nghệ thuật. Người chơi không chỉ phải có kỹ năng cá nhân tốt mà còn phải biết áp dụng các chiến thuật vào từng trận đấu cụ thể. Thực hành thường xuyên và phân tích quá trình chơi sẽ giúp nâng cao khả năng này.', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "Create an educational infographic showcasing advanced dribbling techniques in basketball. Include detailed diagrams demonstrating ball control concepts like body posture, two-handed dribbling, and dribbling with one hand while protecting the ball. Use a professional and instructional style with annotations explaining each technique. The background should feature a basketball court with players in action, visually highlighting the tactical maneuvers involved in maintaining ball control during a game.", "videoScript": "Xin chào các bạn, hôm nay chúng ta sẽ cùng khám phá một khía cạnh quan trọng trong kỹ thuật bóng rổ - kiểm soát bóng khi dribble. Đầu tiên, hãy tìm hiểu về khái niệm cơ bản của kiểm soát bóng. Kiểm soát bóng không chỉ đơn thuần là việc giữ bóng trong tay mà còn là khả năng điều chỉnh tốc độ và nhịp điệu trong các cú dribble.\n\nTiếp theo, tư thế căn bản khi dribble rất quan trọng. Đặt chân rộng, khuỵu gối một chút và giữ cánh tay mở để bảo vệ bóng. Điều này giúp bạn linh hoạt hơn khi di chuyển và khó bị đối thủ chặn.\n\nBây giờ, chúng ta sẽ đi vào các kỹ thuật nâng cao, bắt đầu với dribble với tay không chiếm. Kỹ thuật này cho phép bạn giữ bóng gần thân trong khi vẫn kiểm soát được không gian xung quanh.\n\nTiếp theo là dribble bằng một tay. Điều này không chỉ mang lại khả năng kiểm soát tốt hơn mà còn tạo không gian cho các động tác tấn công khác. Hãy nhớ rằng, điều chỉnh tốc độ của bạn có thể giúp bạn dễ dàng chuyển đổi giữa các pha hành động khác nhau.\n\nCuối cùng, hãy luyện tập thường xuyên để nâng cao khả năng kiểm soát bóng của bạn. Điều này sẽ giúp bạn trở thành một cầu thủ bóng rổ xuất sắc hơn trong mọi tình huống. Cảm ơn các bạn đã theo dõi, hy vọng các kỹ thuật này sẽ hữu ích cho sự nghiệp bóng rổ của bạn!", "videoDuration": "3-5 phút", "imageDescription": "Hình minh họa thể hiện các kỹ thuật dribble nâng cao trong bóng rổ, nhấn mạnh cách kiểm soát bóng thông qua tư thế cơ thể và vị trí tay. Sử dụng biểu đồ chi tiết để giải thích các khái niệm trong thực tế.", "videoDescription": "Video này phân tích các kỹ thuật kiểm soát bóng nâng cao trong bóng rổ, bao gồm tư thế, kỹ thuật dribble và cách duy trì kiểm soát bóng hiệu quả trong trận đấu."}', 6, '{"xp": 15, "coin": 3}', NULL, '2026-01-15 22:13:29.415893', '2026-01-19 23:39:21.435569', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ba912c19-7bf1-4261-b0e0-3a2112ccb5bf', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'example', 'Phân Tích Các Tình Huống Dribble Khó Khăn: Case Study Về Stephen Curry', '# Phân Tích Các Tình Huống Dribble Khó Khăn: Case Study Về Stephen Curry\n\n## Giới thiệu\nStephen Curry, một trong những cầu thủ vĩ đại nhất mọi thời đại, không chỉ nổi bật với khả năng ném bóng mà còn với kỹ thuật dribble tinh tế. Bài viết này sẽ phân tích một số tình huống dribble khó khăn mà Curry đã gặp phải trong các trận đấu để rút ra những bài học quý giá cho người chơi bóng rổ.\n\n## 1. Tình Huống Dribble Khi Kẻ Đuổi Gần\nMột trong những tình huống điển hình là khi Curry bị kẻ đuổi gần. Với khả năng dribble bằng tay thuận, anh thường chuyển hướng nhanh để tạo khoảng trống. Trường hợp này minh họa cho việc cần có khả năng kiểm soát bóng tốt khi đối mặt với áp lực.\n\n### 1.1 Phân Tích Di Chuyển\nCurry sử dụng kỹ thuật crossover (chuyển tay) để đánh lừa kẻ đuổi và thậm chí đôi khi là sử dụng nhịp độ chậm để tạo khoảng cách trước khi tăng tốc chạy. Điều này cho phép anh ta có thời gian và không gian cần thiết để lựa chọn dứt điểm hoặc chuyền bóng.\n\n## 2. Tình Huống Crossover Đột Phát\nCurry nổi tiếng với các động tác crossover đột phát. Một lần trong trận gặp LA Lakers, anh đã thực hiện một cú crossover để tách rời khỏi đối thủ, tạo ra một khoảng trống để thực hiện cú ném ba điểm. Sự kết hợp giữa kỹ năng dribble và khả năng đọc tình huống sẽ quyết định sự thành công của cú ném trong tình huống này.\n\n### 2.1 Kỹ Thuật Thực Hiện\n- **Sử dụng lực**: Lực truyền động từ cơ thể phải được phân phối đều vào chân và tay.\n- **Sự chú ý**: Đôi mắt phải luôn hướng về phía đồng đội và khu vực ghi điểm, giúp đưa ra quyết định tốt hơn.\n- **Cước độ**: An toàn hơn là giảm tốc độ khi tiến vào giao tranh, thay vào đó là tạo ra nhiều sự lựa chọn cho việc dứt điểm.\n\n## 3. Tình Huống Khi Khó Khăn Trong Phòng Ngự\nTrong một số tình huống khác, Curry cũng đã gặp phải phòng ngự mạnh mẽ. Trong trận đấu quan trọng giữa giai đoạn playoffs, anh đã bị bao vây bởi nhiều tuyển thủ đẳng cấp. Điều mà anh thực hiện là tận dụng các kỹ thuật dribble để dời đối thủ ra xa và tạo khoảng trống.\n\n### 3.1 Khả Năng Điều Chỉnh\nKhả năng điều chỉnh của Curry nổi bật khi anh sử dụng các động tác fake (giả vờ) để đánh lừa đối thủ. Những kỹ thuật này không chỉ giúp anh điều chỉnh tốc độ mà còn tạo ra các lựa chọn có lợi cho đồng đội.\n\n## 4. Học Hỏi Từ Stephen Curry\nPhân tích các tình huống dribble khó khăn của Curry cho thấy: \n- **Kiểm soát bóng**: Luôn duy trì kiểm soát bóng ở mức cao là rất quan trọng.\n- **Tốp độ và nhịp điệu**: Điều chỉnh tốp độ phù hợp với tình huống có thể tạo ra cơ hội chiến thắng.\n- **Di chuyển của đội**: Các đồng đội luôn có vai trò hỗ trợ trong mọi tình huống.\n\n## 5. Tổng Kết\nCase study này cho thấy rằng dribble không chỉ đơn giản là một kỹ thuật cá nhân mà còn là một phần không thể tách rời trong chiến thuật tổng thể của đội. Các cầu thủ trẻ nên học hỏi từ các trường hợp điển hình như Stephen Curry để cải thiện bản thân trong việc kiểm soát bóng và tạo ra các cơ hội trên sân.\n', '{"imageUrl": "https://placehold.co/800x600/fce7f3/9d174d?text=%F0%9F%8E%93+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chuy%C3%AAn+S%C3%A2u)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A professional and detailed infographic showing advanced basketball dribbling techniques inspired by Stephen Curry. Include dynamic positions of Curry executing difficult crossover moves, dribbling against defenders, and visual aids explaining the biomechanics and footwork involved. The style should be clean, modern, and educational focused.", "videoScript": "Chào mừng các bạn tới bài học phân tích các tình huống dribble khó khăn qua case study về Stephen Curry. Hôm nay, chúng ta sẽ xem xét cách mà Curry ứng phó với những tình huống ghi điểm khó khăn và những kỹ thuật vượt trội mà anh sử dụng. Đầu tiên, hãy cùng tìm hiểu tình huống khi Curry bị kẻ đuổi gần. Như các bạn đã thấy, anh ấy không chỉ kiểm soát bóng tốt mà còn có thể chuyển hướng nhanh bất ngờ nhờ vào kỹ thuật crossover tinh tế. Thứ hai, khi chúng ta đi vào những cú crossover đột phát, bạn có thể thấy, trong trận đấu với LA Lakers, cách Curry tạo ra khoảng trống để thực hiện cú ném ba điểm. Lanz trước mặt là một thử thách lớn nhưng với sự kết hợp giữa tốc độ và khả năng đọc tình huống, anh đã thành công. Cuối cùng, các bạn hãy xem cách Curry xử lý khi bị phòng ngự mạnh mẽ. Xuất sắc trong việc điều chỉnh kỹ thuật dribble, anh đã tạo ra khoảng trống cần thiết cho những quyết định quan trọng. Tóm lại, các bạn hãy nhớ rằng khả năng đọc tình huống và kiểm soát bóng là rất quan trọng trong bóng rổ, đặc biệt khi đối thủ tấn công. Cảm ơn các bạn đã theo dõi!", "videoDuration": "3-5 phút", "imageDescription": "Hình ảnh minh họa các kỹ thuật dribble tinh vi của Stephen Curry, bao gồm các động tác crossover và phản ứng khi bị kẻ đuổi gần.", "videoDescription": "Video phân tích các tình huống dribble khó khăn của Stephen Curry, nêu rõ kỹ thuật và chiến thuật mà anh sử dụng trong bóng rổ."}', 1, '{"xp": 20, "coin": 4}', NULL, '2026-01-15 22:13:29.451547', '2026-01-19 23:39:17.557248', 'mixed', 'hard', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('77521ae3-6e90-42ff-b5e9-545450a09802', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'concept', 'Tư Thế và Cách Kiểm Soát Bóng', 'Để thực hiện kỹ thuật dribble hiệu quả, bạn cần có tư thế đúng. Hãy đứng ở tư thế thoải mái, đầu gối hơi khuỵu và thân mình hơi nghiêng về phía trước, giống như khi bạn chuẩn bị nhảy. Đặt một tay lên bóng và tay kia ở bên cạnh. Bạn không nên dồn hết lực lên bóng mà hãy để bàn tay cầm nhẹ nhàng, như thể bạn đang ôm một quả chanh. Điều này giúp bạn dễ dàng điều khiển bóng hơn. Khi bạn dribble, hãy nhớ cách chạm bóng cũng rất quan trọng. Nhẹ nhàng nhưng đủ mạnh để bóng nhảy lên cao, giúp bạn di chuyển dễ dàng. Nếu bạn cảm thấy bóng lăn đi xa, hãy lấy lại tốc độ và kiểm soát bóng một lần nữa. Kiểm soát tốt bóng là chìa khóa để bạn có thể thực hiện các kỹ thuật khác trong bóng rổ.', '{"imageUrl": "https://placehold.co/800x600/dcfce7/166534?text=%F0%9F%8C%B1+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+%C4%90%C6%A1n+Gi%E1%BA%A3n)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A simple and bright illustration of a basketball player demonstrating the correct dribble posture. The player is shown in a relaxed stance, knees bent, leaning slightly forward, with one hand on the basketball and the other hand at the side. The background is colorful and engaging with minimal details, making it easy to understand for beginners. Use a cartoonish style to attract young aspiring basketball players.", "videoScript": "Chào các bạn! Hôm nay chúng ta sẽ cùng khám phá cách kiểm soát bóng khi dribble. Đầu tiên, hãy đứng ở tư thế thoải mái, đầu gối hơi khuỵu và thân mình nghiêng về phía trước. Nhớ rằng, một tay cầm bóng còn tay kia để bên cạnh. Đừng dồn hết lực vào bóng nhé! Giữ cho tay bạn thật nhẹ nhàng như đang ôm một quả chanh. Khi bạn dribble, chạm bóng nhẹ nhàng nhưng đủ mạnh để bóng có thể nhảy lên cao. Nếu bóng lăn xa, hãy nhanh chóng lấy lại kiểm soát. Nhớ rằng, kiểm soát bóng là chìa khóa để thực hiện các kỹ thuật khác trong bóng rổ. Cảm ơn các bạn đã theo dõi!", "videoDuration": "30-60 giây", "imageDescription": "Hình ảnh minh họa một cầu thủ bóng rổ đang thực hiện tư thế đúng khi dribble, với đầu gối khuỵu và tay cầm bóng nhẹ nhàng.", "videoDescription": "Video hướng dẫn cách thực hiện tư thế đúng và kiểm soát bóng khi dribble trong bóng rổ."}', 7, '{"xp": 8, "coin": 1}', NULL, '2026-01-15 22:11:48.50494', '2026-01-19 23:39:21.444747', 'mixed', 'easy', NULL, 'published', NULL, NULL, NULL, NULL);
