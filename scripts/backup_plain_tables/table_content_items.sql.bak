INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('55c5a73d-a2b3-4c2c-88f1-c46e89194b07', '26d9bb19-e34e-44b6-83c2-a0a0527e727d', 'concept', 'Giới thiệu về Phân tích Tiến độ', 'Phân tích tiến độ là quá trình theo dõi và đánh giá tiến độ thực hiện dự án so với kế hoạch ban đầu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ Gantt hiển thị tiến độ dự án.", "videoScript": "Chào mừng đến với bài học về phân tích tiến độ. Hôm nay, chúng ta sẽ tìm hiểu cách sử dụng biểu đồ Gantt để theo dõi tiến độ dự án.", "imageDescription": "Biểu đồ Gantt thể hiện các giai đoạn và thời gian thực hiện của dự án.", "videoDescription": "Video hướng dẫn cách sử dụng biểu đồ Gantt để phân tích tiến độ dự án."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:40:13.271426', '2026-01-27 12:40:13.271426', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phân tích tiến độ giúp theo dõi và điều chỉnh dự án.", "detailed": "Phân tích tiến độ là quá trình theo dõi và đánh giá tiến độ thực hiện dự án so với kế hoạch ban đầu.", "comprehensive": "Phân tích tiến độ là một phần quan trọng trong quản lý dự án. Nó giúp xác định sự khác biệt giữa tiến độ thực tế và tiến độ kế hoạch, từ đó đưa ra các biện pháp điều chỉnh cần thiết để đảm bảo dự án hoàn thành đúng hạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b93427db-08e0-4171-b33f-db6d42e377d4', '829a56a0-cf00-4477-b21e-d3ac5fbcecd7', 'concept', 'Giới Thiệu Về Mặt Nạ Đắp Mặt', 'Mặt nạ đắp mặt có nhiều loại, mỗi loại có công dụng riêng như cung cấp độ ẩm, làm sáng da hay chống lão hóa.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các loại mặt nạ đắp mặt khác nhau trên bàn trang điểm.", "videoScript": "Trong video này, chúng ta sẽ khám phá các loại mặt nạ đắp mặt và lợi ích của từng loại.", "imageDescription": "Một bàn trang điểm với nhiều loại mặt nạ đắp mặt như mặt nạ giấy, mặt nạ đất sét, và mặt nạ tự chế.", "videoDescription": "Video giới thiệu về các loại mặt nạ đắp mặt và cách sử dụng chúng để chăm sóc da hiệu quả."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:13:11.097781', '2026-01-27 13:13:11.097781', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mặt nạ đắp mặt giúp cải thiện làn da.", "detailed": "Mặt nạ đắp mặt có nhiều loại, mỗi loại có công dụng riêng như cung cấp độ ẩm, làm sáng da hay chống lão hóa.", "comprehensive": "Mặt nạ đắp mặt là sản phẩm chăm sóc da được sử dụng để cung cấp dưỡng chất trực tiếp cho da. Chúng có thể giúp làm sạch, cấp ẩm, làm sáng da, và cải thiện kết cấu da. Có nhiều loại mặt nạ như mặt nạ giấy, mặt nạ đất sét, và mặt nạ tự chế từ nguyên liệu thiên nhiên."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cadf9c03-1e54-4116-9622-09a38ec64757', '829a56a0-cf00-4477-b21e-d3ac5fbcecd7', 'concept', 'Lợi Ích Của Việc Đắp Mặt Nạ', 'Việc đắp mặt nạ định kỳ có thể giúp cải thiện độ ẩm, làm sáng da, và giảm nếp nhăn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh trước và sau khi đắp mặt nạ.", "videoScript": "Chào mừng bạn đến với video hôm nay! Chúng ta sẽ cùng tìm hiểu về những lợi ích của việc đắp mặt nạ cho da.", "imageDescription": "Hình ảnh so sánh làn da trước và sau khi sử dụng mặt nạ, cho thấy sự cải thiện rõ rệt.", "videoDescription": "Video nói về các lợi ích của mặt nạ đắp mặt và cách chúng có thể cải thiện sức khỏe làn da."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:13:11.113347', '2026-01-27 13:13:11.113347', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đắp mặt nạ giúp da khỏe mạnh và sáng hơn.", "detailed": "Việc đắp mặt nạ định kỳ có thể giúp cải thiện độ ẩm, làm sáng da, và giảm nếp nhăn.", "comprehensive": "Đắp mặt nạ mang lại nhiều lợi ích cho làn da như cung cấp độ ẩm, làm mềm da, làm sáng màu da, và thu nhỏ lỗ chân lông. Các thành phần trong mặt nạ như vitamin, khoáng chất và chiết xuất thiên nhiên có thể giúp nuôi dưỡng và tái tạo làn da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f6fff723-0934-4fef-8aa9-15798e9be48c', '829a56a0-cf00-4477-b21e-d3ac5fbcecd7', 'concept', 'Cách Chọn Mặt Nạ Phù Hợp Với Da', 'Người có da dầu nên chọn mặt nạ kiểm soát dầu, trong khi người có da khô nên chọn mặt nạ cấp ẩm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh người đang thử nghiệm các loại mặt nạ trên da.", "videoScript": "Hôm nay, chúng ta sẽ cùng nhau tìm hiểu cách chọn mặt nạ phù hợp với từng loại da.", "imageDescription": "Một người phụ nữ đang kiểm tra nhiều loại mặt nạ khác nhau để tìm ra sản phẩm phù hợp với làn da của mình.", "videoDescription": "Video hướng dẫn người xem cách lựa chọn mặt nạ đắp mặt phù hợp cho từng loại da khác nhau."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:13:11.118833', '2026-01-27 13:13:11.118833', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn mặt nạ dựa trên loại da của bạn.", "detailed": "Người có da dầu nên chọn mặt nạ kiểm soát dầu, trong khi người có da khô nên chọn mặt nạ cấp ẩm.", "comprehensive": "Để chọn mặt nạ phù hợp với loại da, bạn cần xác định tình trạng da của mình. Da dầu nên sử dụng mặt nạ đất sét để kiểm soát bã nhờn, trong khi da khô nên chọn mặt nạ chứa hyaluronic acid hoặc glycerin để cấp nước. Da nhạy cảm nên chọn mặt nạ chiết xuất tự nhiên, không chứa hóa chất độc hại."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('357897c4-cdcd-4df4-9ae9-558e8cd41a66', '3bcfb897-2b3a-4c34-a205-ab1231f31988', 'concept', 'Giới Thiệu Về Cấu Trúc Da', 'Da có ba lớp chính: biểu bì, trung bì và hạ bì. Mỗi lớp có vai trò quan trọng trong việc bảo vệ cơ thể.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa cấu trúc da với các lớp khác nhau.", "videoScript": "Chào mừng đến với bài học về cấu trúc da. Hôm nay, chúng ta sẽ khám phá ba lớp chính của da và vai trò của chúng.", "imageDescription": "Một bức tranh mô tả cấu trúc da với các lớp biểu bì, trung bì và hạ bì.", "videoDescription": "Video trình bày các lớp của da với hình ảnh minh họa và giải thích chi tiết."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:18:15.926038', '2026-01-27 13:18:15.926038', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cấu trúc da bao gồm nhiều lớp khác nhau.", "detailed": "Da có ba lớp chính: biểu bì, trung bì và hạ bì. Mỗi lớp có vai trò quan trọng trong việc bảo vệ cơ thể.", "comprehensive": "Cấu trúc da gồm ba lớp: biểu bì là lớp ngoài cùng giúp bảo vệ, trung bì chứa các mạch máu và tuyến dầu, hạ bì là lớp mỡ giúp cách nhiệt và hỗ trợ cấu trúc da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('00e35a74-dc01-40f6-b9c5-ce6faa4d08f6', '26d9bb19-e34e-44b6-83c2-a0a0527e727d', 'concept', 'Các Công cụ Phân tích Tiến độ', 'Các công cụ như Microsoft Excel, Trello và Asana rất hữu ích trong việc phân tích và theo dõi tiến độ dự án.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh giao diện phần mềm quản lý dự án.", "videoScript": "Trong video này, chúng ta sẽ xem xét các công cụ phân tích tiến độ và cách sử dụng chúng hiệu quả.", "imageDescription": "Giao diện phần mềm Trello với các thẻ nhiệm vụ và tiến độ thực hiện.", "videoDescription": "Video hướng dẫn sử dụng các công cụ phân tích tiến độ trong quản lý dự án."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:40:13.280635', '2026-01-27 12:40:13.280635', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng Excel hoặc phần mềm quản lý dự án để phân tích tiến độ.", "detailed": "Các công cụ như Microsoft Excel, Trello và Asana rất hữu ích trong việc phân tích và theo dõi tiến độ dự án.", "comprehensive": "Trong bài học này, chúng ta sẽ khám phá nhiều công cụ khác nhau giúp phân tích tiến độ, bao gồm Excel với các biểu đồ, phần mềm quản lý dự án như Trello và Asana, và cách tích hợp chúng vào quy trình làm việc hàng ngày."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ca81fe67-1c27-4757-ad00-8174f9b71dce', '829a56a0-cf00-4477-b21e-d3ac5fbcecd7', 'concept', 'Cách Sử Dụng Mặt Nạ Đắp Mặt Đúng Cách', 'Trước khi đắp mặt nạ, hãy làm sạch da và thoa một lớp mỏng. Để mặt nạ trên da trong khoảng 15-20 phút.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh người đang đắp mặt nạ với các bước hướng dẫn.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ hướng dẫn cách sử dụng mặt nạ đắp mặt đúng cách để đạt được hiệu quả tốt nhất.", "imageDescription": "Một người phụ nữ đang thực hiện các bước đắp mặt nạ, bao gồm làm sạch da, thoa mặt nạ và thời gian chờ đợi.", "videoDescription": "Video hướng dẫn từng bước để sử dụng mặt nạ đắp mặt đúng cách, giúp da hấp thụ dưỡng chất tốt nhất."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:13:11.124093', '2026-01-27 13:13:11.124093', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đắp mặt nạ đúng cách giúp da hấp thụ tốt hơn.", "detailed": "Trước khi đắp mặt nạ, hãy làm sạch da và thoa một lớp mỏng. Để mặt nạ trên da trong khoảng 15-20 phút.", "comprehensive": "Để sử dụng mặt nạ đắp mặt hiệu quả, trước tiên bạn cần rửa mặt sạch sẽ để loại bỏ bụi bẩn và dầu thừa. Sau đó, thoa một lớp mặt nạ đều lên da, tránh vùng mắt và môi. Để mặt nạ ở lại trên da khoảng 15-20 phút để các dưỡng chất thẩm thấu vào da, sau đó rửa sạch bằng nước ấm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5343b559-5607-4989-9309-a2bbcb5b51bf', '3bcfb897-2b3a-4c34-a205-ab1231f31988', 'concept', 'Vai Trò Của Biểu Bì', 'Biểu bì giúp bảo vệ cơ thể khỏi vi khuẩn và các yếu tố bên ngoài.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của biểu bì với các tế bào da.", "videoScript": "Trong phần này, chúng ta sẽ tìm hiểu sâu hơn về biểu bì và tầm quan trọng của nó trong việc bảo vệ da.", "imageDescription": "Một bức tranh thể hiện lớp biểu bì và cấu trúc tế bào của nó.", "videoDescription": "Video giải thích về biểu bì, các chức năng của nó và cách chăm sóc lớp da này."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:18:15.933531', '2026-01-27 13:18:15.933531', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu bì là lớp ngoài cùng của da.", "detailed": "Biểu bì giúp bảo vệ cơ thể khỏi vi khuẩn và các yếu tố bên ngoài.", "comprehensive": "Biểu bì là lớp da ngoài cùng, có chức năng bảo vệ các lớp bên dưới khỏi sự xâm nhập của vi khuẩn, đồng thời giúp giữ ẩm và tạo ra các tế bào mới."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5bd9d73a-19a9-4f40-8027-4936a14eb8f3', '3bcfb897-2b3a-4c34-a205-ab1231f31988', 'concept', 'Chức Năng Của Trung Bì', 'Trung bì cung cấp độ ẩm và dinh dưỡng cho da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh cấu trúc trung bì với các mạch máu và tuyến dầu.", "videoScript": "Hãy cùng tìm hiểu về trung bì, lớp da quan trọng giúp cung cấp độ ẩm và nuôi dưỡng cho da.", "imageDescription": "Một bức tranh mô tả lớp trung bì với các chi tiết về mạch máu và tuyến dầu.", "videoDescription": "Video trình bày về trung bì, vai trò của mạch máu và tuyến dầu trong việc chăm sóc da."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:18:15.937714', '2026-01-27 13:18:15.937714', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Trung bì chứa các mạch máu và tuyến dầu.", "detailed": "Trung bì cung cấp độ ẩm và dinh dưỡng cho da.", "comprehensive": "Trung bì là lớp giữa của da, chứa mạch máu nuôi dưỡng các tế bào da, tuyến dầu giúp giữ ẩm và các sợi collagen giúp duy trì độ đàn hồi cho da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4d967d9c-6448-443e-b977-6def0afd8114', '26d9bb19-e34e-44b6-83c2-a0a0527e727d', 'concept', 'Kỹ thuật Điều chỉnh Tiến độ', 'Khi tiến độ dự án bị chậm, có nhiều kỹ thuật để điều chỉnh, chẳng hạn như điều chỉnh nguồn lực hoặc thay đổi lịch trình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhóm làm việc cùng nhau để điều chỉnh tiến độ.", "videoScript": "Chào mừng bạn đến với bài học về kỹ thuật điều chỉnh tiến độ. Chúng ta sẽ cùng nhau khám phá các bước cần thiết để điều chỉnh tiến độ khi gặp phải khó khăn.", "imageDescription": "Một nhóm các nhà quản lý dự án đang thảo luận để điều chỉnh tiến độ dự án.", "videoDescription": "Video hướng dẫn thực tế về các kỹ thuật điều chỉnh tiến độ trong quản lý dự án."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:40:13.284781', '2026-01-27 12:40:13.284781', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Điều chỉnh tiến độ khi có sự chậm trễ hoặc thay đổi trong dự án.", "detailed": "Khi tiến độ dự án bị chậm, có nhiều kỹ thuật để điều chỉnh, chẳng hạn như điều chỉnh nguồn lực hoặc thay đổi lịch trình.", "comprehensive": "Bài học này sẽ cung cấp cho bạn các kỹ thuật và chiến lược để điều chỉnh tiến độ dự án một cách hiệu quả, bao gồm việc sử dụng phân tích nguyên nhân gốc rễ để xác định lý do chậm trễ và cách điều chỉnh nguồn lực một cách hợp lý."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c2ec24f3-f00c-4f7b-897b-da43cb420acf', '26d9bb19-e34e-44b6-83c2-a0a0527e727d', 'concept', 'Phân tích Rủi ro và Ảnh hưởng đến Tiến độ', 'Việc phân tích rủi ro là rất quan trọng để xác định các yếu tố có thể ảnh hưởng đến tiến độ dự án.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ phân tích rủi ro.", "videoScript": "Trong video này, chúng ta sẽ cùng nhau phân tích các rủi ro trong dự án và cách chúng có thể ảnh hưởng đến tiến độ.", "imageDescription": "Một biểu đồ phân tích rủi ro thể hiện các yếu tố và mức độ tác động của chúng đến tiến độ.", "videoDescription": "Video hướng dẫn về phân tích rủi ro và tác động của nó đến tiến độ dự án."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:40:13.289137', '2026-01-27 12:40:13.289137', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Rủi ro có thể ảnh hưởng lớn đến tiến độ dự án.", "detailed": "Việc phân tích rủi ro là rất quan trọng để xác định các yếu tố có thể ảnh hưởng đến tiến độ dự án.", "comprehensive": "Trong bài học này, chúng ta sẽ tìm hiểu cách phân tích rủi ro và ảnh hưởng của chúng đến tiến độ, bao gồm các phương pháp xác định rủi ro tiềm ẩn và cách lập kế hoạch ứng phó để giảm thiểu tác động đến tiến độ dự án."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9a3148d6-4429-4940-aef6-51ac96f151d7', '26d9bb19-e34e-44b6-83c2-a0a0527e727d', 'concept', 'Đánh giá Tiến độ Dự án', 'Đánh giá tiến độ dự án là việc so sánh tiến độ thực tế với kế hoạch và đưa ra các biện pháp cần thiết.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh báo cáo tiến độ dự án.", "videoScript": "Chào mừng bạn đến với bài học về đánh giá tiến độ dự án. Chúng ta sẽ cùng nhau tìm hiểu cách đánh giá tiến độ một cách chính xác và hiệu quả.", "imageDescription": "Một báo cáo tiến độ dự án với các chỉ số và biểu đồ thể hiện tình trạng hiện tại.", "videoDescription": "Video hướng dẫn về quy trình đánh giá tiến độ trong quản lý dự án."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:40:13.29406', '2026-01-27 12:40:13.29406', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đánh giá tiến độ giúp xác định tình trạng hiện tại của dự án.", "detailed": "Đánh giá tiến độ dự án là việc so sánh tiến độ thực tế với kế hoạch và đưa ra các biện pháp cần thiết.", "comprehensive": "Bài học này sẽ hướng dẫn bạn cách thực hiện đánh giá tiến độ dự án, từ việc thu thập dữ liệu cho đến phân tích và báo cáo kết quả đánh giá, đồng thời đưa ra các khuyến nghị để cải thiện hiệu quả dự án."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2e82a3dd-5f6a-42ec-b17d-56ddf6dae277', '3bcfb897-2b3a-4c34-a205-ab1231f31988', 'concept', 'Hạ Bì: Lớp Da Quan Trọng', 'Hạ bì giúp cách nhiệt và hỗ trợ cấu trúc da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của hạ bì với các tế bào mỡ.", "videoScript": "Chúng ta sẽ khám phá lớp hạ bì, tìm hiểu vai trò của nó trong việc bảo vệ và duy trì sức khỏe của da.", "imageDescription": "Một bức tranh thể hiện lớp hạ bì và các tế bào mỡ bên trong.", "videoDescription": "Video giải thích về hạ bì, các chức năng và ảnh hưởng của nó đến sức khỏe làn da."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:18:15.941249', '2026-01-27 13:18:15.941249', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hạ bì là lớp mỡ bên dưới cùng của da.", "detailed": "Hạ bì giúp cách nhiệt và hỗ trợ cấu trúc da.", "comprehensive": "Hạ bì là lớp sâu nhất của da, chứa mỡ giúp cách nhiệt, lưu trữ năng lượng và bảo vệ các cơ quan bên dưới, đồng thời hỗ trợ sự linh hoạt của da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1c475273-36a1-4d7e-bf64-297a0e036519', '83bd90f8-4b1f-4ccf-831d-c499713a5ff7', 'concept', 'Giới Thiệu Về Âm Nhạc Jazz', 'Âm nhạc jazz ra đời vào đầu thế kỷ 20, kết hợp giữa nhạc châu Phi và châu Âu. Nó nổi bật với các yếu tố như nhịp điệu swing, improvisation và hòa âm phong phú.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một buổi hòa nhạc jazz với nhạc công đang chơi piano.", "videoScript": "Chào mừng đến với thế giới âm nhạc jazz! Hôm nay, chúng ta sẽ khám phá nguồn gốc và sự phát triển của jazz.", "imageDescription": "Một nhạc công đang say sưa chơi piano trên sân khấu trong một buổi hòa nhạc jazz.", "videoDescription": "Video giới thiệu về âm nhạc jazz, lịch sử và sự phát triển của nó qua các thập kỷ."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:51:11.245434', '2026-01-27 12:51:11.245434', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Âm nhạc jazz là một thể loại âm nhạc nổi bật với nhịp điệu tự do và sự sáng tạo.", "detailed": "Âm nhạc jazz ra đời vào đầu thế kỷ 20, kết hợp giữa nhạc châu Phi và châu Âu. Nó nổi bật với các yếu tố như nhịp điệu swing, improvisation và hòa âm phong phú.", "comprehensive": "Jazz là một thể loại âm nhạc phức tạp, phát triển từ các cộng đồng người da đen ở Mỹ. Sự kết hợp giữa nhạc cụ như trumpet, saxophone và piano tạo nên âm thanh độc đáo. Jazz đã có nhiều biến thể như bebop, cool jazz, và fusion, phản ánh sự đa dạng trong cách biểu diễn và sáng tác."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7a93d8e0-7146-4496-998f-92854c8d7e65', '83bd90f8-4b1f-4ccf-831d-c499713a5ff7', 'concept', 'Vai Trò Của Piano Trong Jazz', 'Trong jazz, piano không chỉ đóng vai trò là nhạc cụ hòa âm mà còn là công cụ để biểu diễn solo. Nó mang lại chiều sâu cho âm nhạc thông qua các hợp âm và giai điệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bức ảnh một nghệ sĩ piano đang biểu diễn trong một quán bar jazz.", "videoScript": "Hãy cùng khám phá vai trò của piano trong jazz, từ những hợp âm đến những giai điệu độc đáo.", "imageDescription": "Nghệ sĩ piano đang chơi một bản nhạc jazz sôi động trong không khí ấm cúng của quán bar.", "videoDescription": "Video giải thích vai trò của piano trong jazz, bao gồm các kỹ thuật và phong cách chơi khác nhau."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:51:11.253262', '2026-01-27 12:51:11.253262', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Piano là một trong những nhạc cụ quan trọng nhất trong jazz.", "detailed": "Trong jazz, piano không chỉ đóng vai trò là nhạc cụ hòa âm mà còn là công cụ để biểu diễn solo. Nó mang lại chiều sâu cho âm nhạc thông qua các hợp âm và giai điệu.", "comprehensive": "Piano trong jazz thường được sử dụng để tạo ra các cấu trúc hòa âm phức tạp và cung cấp nền tảng cho các nhạc cụ khác. Những nghệ sĩ piano như Thelonious Monk và Bill Evans đã định hình cách chơi piano trong jazz, biến nó thành một phần không thể thiếu trong mỗi buổi biểu diễn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('739b50de-a37e-4eb0-9e68-34be7a1e3d30', '3bcfb897-2b3a-4c34-a205-ab1231f31988', 'concept', 'Cách Chăm Sóc Da Theo Cấu Trúc', 'Sử dụng sản phẩm phù hợp giúp bảo vệ và nuôi dưỡng từng lớp da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các sản phẩm chăm sóc da phù hợp với từng lớp.", "videoScript": "Cuối cùng, chúng ta sẽ tìm hiểu cách chăm sóc da theo cấu trúc để đạt hiệu quả tốt nhất.", "imageDescription": "Một bức tranh minh họa các sản phẩm chăm sóc da như kem dưỡng ẩm, serum, và dầu dưỡng.", "videoDescription": "Video hướng dẫn cách chọn sản phẩm chăm sóc da dựa trên cấu trúc và nhu cầu của từng lớp da."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:18:15.946195', '2026-01-27 13:18:15.946195', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chăm sóc da cần phù hợp với từng lớp.", "detailed": "Sử dụng sản phẩm phù hợp giúp bảo vệ và nuôi dưỡng từng lớp da.", "comprehensive": "Để chăm sóc da hiệu quả, cần hiểu rõ về cấu trúc da và sử dụng các sản phẩm phù hợp cho từng lớp, từ kem dưỡng ẩm cho biểu bì đến các sản phẩm nuôi dưỡng cho trung bì và hạ bì."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('250ab247-d743-4831-8805-343b3b3df683', 'b1825ba3-0bb4-4639-ac09-dafd43dc869c', 'concept', 'Giới Thiệu về Hòa Âm Cơ Bản', 'Hòa âm cơ bản bao gồm việc sử dụng các hợp âm và nốt nhạc đơn giản để tạo ra một giai điệu dễ nhớ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bản nhạc piano với các hợp âm cơ bản.", "videoScript": "Chào mừng các bạn đến với bài học hòa âm cơ bản. Hôm nay, chúng ta sẽ tìm hiểu về các hợp âm cơ bản và cách áp dụng chúng vào piano.", "imageDescription": "Bảng hợp âm cơ bản cho piano với các nốt và vị trí tay.", "videoDescription": "Video hướng dẫn về hòa âm cơ bản cho người mới bắt đầu học piano."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:40:43.720513', '2026-01-27 12:40:43.720513', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hòa âm cơ bản là cách kết hợp các nốt nhạc để tạo ra âm thanh đẹp.", "detailed": "Hòa âm cơ bản bao gồm việc sử dụng các hợp âm và nốt nhạc đơn giản để tạo ra một giai điệu dễ nhớ.", "comprehensive": "Hòa âm cơ bản là nền tảng của âm nhạc, bao gồm việc sử dụng các hợp âm và tiến trình hợp âm để tạo ra sự phong phú về âm thanh. Nó giúp người chơi piano phát triển khả năng cảm thụ âm nhạc và sáng tạo trong việc chơi nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('36a52083-f471-45af-9b68-3d7b36ba5ab7', 'b1825ba3-0bb4-4639-ac09-dafd43dc869c', 'concept', 'Khám Phá Các Hợp Âm Chính', 'Các hợp âm chính như C, G, Am, F là những hợp âm thường gặp trong nhiều bài hát.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các hợp âm chính trên piano.", "videoScript": "Hôm nay, chúng ta sẽ cùng nhau khám phá các hợp âm chính trên piano và cách chơi chúng một cách hiệu quả.", "imageDescription": "Các vị trí ngón tay cho hợp âm C, G, Am, F trên đàn piano.", "videoDescription": "Video hướng dẫn chơi các hợp âm chính trên piano cho người mới."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:40:43.728219', '2026-01-27 12:40:43.728219', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm chính là nền tảng cho mọi bản nhạc.", "detailed": "Các hợp âm chính như C, G, Am, F là những hợp âm thường gặp trong nhiều bài hát.", "comprehensive": "Các hợp âm chính là những hợp âm cơ bản mà bất kỳ ai học piano cũng cần phải biết. Chúng bao gồm các hợp âm trưởng và thứ, và mỗi hợp âm có thể tạo ra những cảm xúc khác nhau trong âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b585d017-0866-438b-a065-bd8a0dcd574d', 'b1825ba3-0bb4-4639-ac09-dafd43dc869c', 'concept', 'Tiến Trình Hợp Âm Cơ Bản', 'Một số tiến trình hợp âm phổ biến như I-IV-V hoặc I-vi-ii-V giúp tạo ra giai điệu hấp dẫn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Sơ đồ tiến trình hợp âm cơ bản.", "videoScript": "Chúng ta sẽ cùng nhau tìm hiểu về các tiến trình hợp âm cơ bản và cách áp dụng chúng vào việc chơi piano.", "imageDescription": "Sơ đồ minh họa các tiến trình hợp âm I-IV-V và I-vi-ii-V.", "videoDescription": "Video giải thích về tiến trình hợp âm cơ bản cho người học piano."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:40:43.732018', '2026-01-27 12:40:43.732018', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tiến trình hợp âm là cách các hợp âm được kết nối với nhau.", "detailed": "Một số tiến trình hợp âm phổ biến như I-IV-V hoặc I-vi-ii-V giúp tạo ra giai điệu hấp dẫn.", "comprehensive": "Tiến trình hợp âm cơ bản là cách mà các hợp âm được sắp xếp để tạo ra một dòng nhạc liên tục. Việc nắm vững các tiến trình này sẽ giúp bạn dễ dàng sáng tác và chơi nhạc hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('be1e7637-a0cb-4dce-92ed-31505a0e7b6e', 'b1825ba3-0bb4-4639-ac09-dafd43dc869c', 'concept', 'Sáng Tác với Hòa Âm Cơ Bản', 'Sử dụng các hợp âm cơ bản để tạo ra giai điệu riêng của bạn là một cách thú vị để học đàn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bản nhạc tự sáng tác trên piano.", "videoScript": "Hôm nay, chúng ta sẽ cùng nhau sáng tác một bản nhạc đơn giản sử dụng các hợp âm cơ bản.", "imageDescription": "Bảng nhạc với các hợp âm và giai điệu tự sáng tác.", "videoDescription": "Video hướng dẫn cách sáng tác nhạc bằng các hợp âm cơ bản cho người học piano."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:40:43.735948', '2026-01-27 12:40:43.735948', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn có thể sáng tác nhạc bằng cách kết hợp các hợp âm đã học.", "detailed": "Sử dụng các hợp âm cơ bản để tạo ra giai điệu riêng của bạn là một cách thú vị để học đàn.", "comprehensive": "Sáng tác nhạc với hòa âm cơ bản không chỉ giúp bạn thực hành mà còn phát triển khả năng sáng tạo. Bằng cách kết hợp các hợp âm đã học, bạn có thể tạo ra những bản nhạc độc đáo của riêng mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b9570d5d-e659-4028-afcf-4a67583c3c93', 'b1825ba3-0bb4-4639-ac09-dafd43dc869c', 'concept', 'Thực Hành Hòa Âm với Bài Hát Cổ Điển', 'Chọn một bài hát cổ điển và thực hành hòa âm theo các hợp âm cơ bản.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bài hát cổ điển với các hợp âm chỉ dẫn.", "videoScript": "Trong bài học này, chúng ta sẽ thực hành hòa âm với một bài hát cổ điển và tìm hiểu cách kết hợp hợp âm với giai điệu.", "imageDescription": "Bảng nhạc của một bài hát cổ điển với các hợp âm được đánh dấu.", "videoDescription": "Video hướng dẫn thực hành hòa âm với bài hát cổ điển cho người học piano."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:40:43.739202', '2026-01-27 12:40:43.739202', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách hòa âm với một bài hát đơn giản.", "detailed": "Chọn một bài hát cổ điển và thực hành hòa âm theo các hợp âm cơ bản.", "comprehensive": "Thực hành hòa âm với bài hát cổ điển là một cách tuyệt vời để áp dụng kiến thức về hòa âm cơ bản. Chúng ta sẽ chọn một bài hát dễ và thực hành cách chơi hợp âm cùng với giai điệu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b051c81a-2bec-4410-adda-8ec452177151', '5f36b291-05f3-4155-b8b3-0910221c4da6', 'concept', 'Giới Thiệu Về Bảo Hiểm', 'Bảo hiểm giúp bảo vệ tài chính cá nhân trước những rủi ro không lường trước được, như tai nạn hay bệnh tật, thông qua việc đóng phí bảo hiểm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa về một gia đình hạnh phúc đang thảo luận về bảo hiểm.", "videoScript": "Chào mừng đến với bài học về bảo hiểm! Hôm nay, chúng ta sẽ khám phá những khái niệm cơ bản về bảo hiểm và tầm quan trọng của nó trong cuộc sống hàng ngày.", "imageDescription": "Một bức ảnh của một gia đình ngồi tại bàn ăn, đang xem xét tài liệu về bảo hiểm.", "videoDescription": "Video giới thiệu về bảo hiểm, bao gồm khái niệm, loại hình và lợi ích của bảo hiểm đối với cá nhân và gia đình."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:06:25.852756', '2026-01-27 13:06:25.852756', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bảo hiểm là một hợp đồng giữa người mua và công ty bảo hiểm nhằm bảo vệ tài chính.", "detailed": "Bảo hiểm giúp bảo vệ tài chính cá nhân trước những rủi ro không lường trước được, như tai nạn hay bệnh tật, thông qua việc đóng phí bảo hiểm.", "comprehensive": "Bảo hiểm là một biện pháp tài chính quan trọng, giúp cá nhân bảo vệ tài sản và sức khỏe của mình. Qua hợp đồng bảo hiểm, người mua sẽ trả một khoản phí định kỳ cho công ty bảo hiểm, và trong trường hợp xảy ra sự kiện không mong muốn, công ty sẽ bồi thường tài chính theo quy định trong hợp đồng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d2bd2426-0242-4274-b756-b0a9ed8ae6ec', '7fa5b8b2-f70f-4e5f-bb08-e02636b929ec', 'concept', 'Giới Thiệu Về Các Loại Da', 'Các loại da được phân loại dựa trên lượng dầu, độ ẩm và độ nhạy cảm. Da thường có độ cân bằng tốt, da khô thiếu ẩm, da dầu sản xuất nhiều dầu, da hỗn hợp kết hợp cả hai và da nhạy cảm dễ bị kích ứng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các loại da khác nhau.", "videoScript": "Chào mừng bạn đến với bài học về các loại da. Hôm nay, chúng ta sẽ khám phá năm loại da chính mà bạn cần biết.", "imageDescription": "Một bức tranh với bốn mẫu da: da thường, da khô, da dầu, da nhạy cảm.", "videoDescription": "Video hướng dẫn về các loại da và cách nhận biết chúng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:18:44.215996', '2026-01-27 13:18:44.215996', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có 5 loại da chính: da thường, da khô, da dầu, da hỗn hợp, và da nhạy cảm.", "detailed": "Các loại da được phân loại dựa trên lượng dầu, độ ẩm và độ nhạy cảm. Da thường có độ cân bằng tốt, da khô thiếu ẩm, da dầu sản xuất nhiều dầu, da hỗn hợp kết hợp cả hai và da nhạy cảm dễ bị kích ứng.", "comprehensive": "Phân loại da rất quan trọng trong việc chăm sóc da. Da thường được coi là lý tưởng, trong khi da khô có thể gây ra cảm giác khó chịu. Da dầu thường gặp phải mụn trứng cá, da hỗn hợp cần chăm sóc cẩn thận để cân bằng, và da nhạy cảm yêu cầu sản phẩm dịu nhẹ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7437bf7b-d79b-4c7f-a6d8-944f89fb6f3d', '83bd90f8-4b1f-4ccf-831d-c499713a5ff7', 'concept', 'Những Nghệ Sĩ Piano Nổi Tiếng Trong Lịch Sử Jazz', 'Duke Ellington, một trong những nhạc trưởng vĩ đại nhất của jazz, đã sử dụng piano để tạo ra những bản nhạc bất hủ. Oscar Peterson được biết đến với kỹ thuật chơi nhanh và tinh tế, trong khi Herbie Hancock kết hợp jazz với các thể loại khác.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh chân dung của Duke Ellington, Oscar Peterson và Herbie Hancock.", "videoScript": "Trong video này, chúng ta sẽ điểm qua những nghệ sĩ piano nổi tiếng nhất trong lịch sử jazz và ảnh hưởng của họ đến âm nhạc.", "imageDescription": "Chân dung của ba nghệ sĩ piano jazz nổi tiếng: Duke Ellington, Oscar Peterson và Herbie Hancock.", "videoDescription": "Video tôn vinh những nghệ sĩ piano jazz vĩ đại và sự đóng góp của họ cho thể loại này."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:51:11.257134', '2026-01-27 12:51:11.257134', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Một số nghệ sĩ piano jazz nổi tiếng bao gồm Duke Ellington, Oscar Peterson và Herbie Hancock.", "detailed": "Duke Ellington, một trong những nhạc trưởng vĩ đại nhất của jazz, đã sử dụng piano để tạo ra những bản nhạc bất hủ. Oscar Peterson được biết đến với kỹ thuật chơi nhanh và tinh tế, trong khi Herbie Hancock kết hợp jazz với các thể loại khác.", "comprehensive": "Duke Ellington, Oscar Peterson, và Herbie Hancock là những nghệ sĩ piano đã để lại dấu ấn sâu sắc trong lịch sử jazz. Ellington không chỉ là một nghệ sĩ piano mà còn là một nhà soạn nhạc vĩ đại. Peterson được ngưỡng mộ vì khả năng chơi nhạc nhanh và sáng tạo, trong khi Hancock đã mở rộng ranh giới của jazz bằng cách kết hợp với funk và rock."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f30060d3-9a3a-4a56-8a1f-e05ef0c25e18', '108e984e-99ea-4c84-b06d-6bc340f42b17', 'concept', 'Khái Niệm Cơ Bản Về Giai Điệu', 'Giai điệu là yếu tố chính trong âm nhạc, bao gồm một chuỗi các nốt nhạc được sắp xếp theo cao độ và thời gian. Nó có thể biểu đạt nhiều cảm xúc khác nhau và là phần dễ nhớ nhất trong một bài hát.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bản nhạc với các nốt được đánh dấu nổi bật", "videoScript": "Xin chào! Hôm nay chúng ta sẽ khám phá khái niệm về giai điệu trong âm nhạc. Giai điệu là gì? Hãy cùng tìm hiểu!", "imageDescription": "Bản nhạc với các giai điệu chính được làm nổi bật để minh họa", "videoDescription": "Video giải thích khái niệm giai điệu và tầm quan trọng của nó trong âm nhạc."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:41:27.069778', '2026-01-27 12:41:27.069778', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giai điệu là chuỗi âm thanh được sắp xếp theo trật tự. Nó tạo nên cảm xúc và không khí cho bản nhạc.", "detailed": "Giai điệu là yếu tố chính trong âm nhạc, bao gồm một chuỗi các nốt nhạc được sắp xếp theo cao độ và thời gian. Nó có thể biểu đạt nhiều cảm xúc khác nhau và là phần dễ nhớ nhất trong một bài hát.", "comprehensive": "Giai điệu là phần âm nhạc nổi bật, thường là yếu tố chính mà người nghe nhớ đến. Nó bao gồm một chuỗi các nốt nhạc được tổ chức theo quy tắc âm nhạc, thể hiện cảm xúc và ý nghĩa của tác phẩm. Giai điệu có thể là đơn giản hoặc phức tạp, và thường được kết hợp với hòa âm để tạo ra một tác phẩm âm nhạc hoàn chỉnh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d2d51f0f-bc80-42aa-8372-addb56cb5191', 'ae369e64-6c5e-4daf-b676-221dbf5c67b2', 'concept', 'Tại sao cần điều chỉnh ngân sách?', 'Điều chỉnh ngân sách là quá trình theo dõi và điều chỉnh chi tiêu của bạn để đảm bảo bạn không vượt quá số tiền bạn có. Điều này giúp bạn tiết kiệm tiền và đầu tư cho tương lai.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang xem xét ngân sách của mình với máy tính và sổ tay.", "videoScript": "Mở đầu với hình ảnh một người trẻ tuổi đang nhìn vào màn hình máy tính. Giọng nói lồng ghép giải thích tầm quan trọng của việc điều chỉnh ngân sách.", "imageDescription": "Hình ảnh một người đàn ông ngồi tại bàn làm việc, sử dụng máy tính và ghi chú trong sổ tay.", "videoDescription": "Video giải thích về lý do tại sao điều chỉnh ngân sách lại quan trọng trong quản lý tài chính cá nhân."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:59:21.21828', '2026-01-27 12:59:21.21828', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Điều chỉnh ngân sách giúp bạn quản lý chi tiêu hiệu quả hơn.", "detailed": "Điều chỉnh ngân sách là quá trình theo dõi và điều chỉnh chi tiêu của bạn để đảm bảo bạn không vượt quá số tiền bạn có. Điều này giúp bạn tiết kiệm tiền và đầu tư cho tương lai.", "comprehensive": "Việc điều chỉnh ngân sách là một yếu tố quan trọng trong tài chính cá nhân. Nó không chỉ giúp bạn kiểm soát chi tiêu mà còn giúp bạn tạo ra một kế hoạch tài chính bền vững, từ đó đạt được các mục tiêu tài chính dài hạn như mua nhà, nghỉ hưu sớm hay du lịch."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('884ae9c9-a3db-4d0f-a772-551a03109a8b', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Kỹ Thuật Ném Bóng Cơ Bản', '# Kỹ Thuật Ném Bóng Cơ Bản\n\nKỹ thuật ném bóng cơ bản bao gồm các bước quan trọng từ cầm bóng đến khi bóng rời khỏi tay bạn. Hiểu rõ từng bước sẽ giúp bạn thực hiện cú ném chính xác hơn.\n\n## Các Bước Cụ Thể\n1. **Chuẩn Bị**: Bắt đầu với bóng trong tay, đứng đúng tư thế.\n2. **Nhấc Bóng**: Nhấc bóng lên khỏi mặt đất, giữ tay ở vị trí ngang ngực.\n3. **Gập Đầu Gối**: Gập đầu gối để tạo lực, đồng thời nâng bóng lên cao hơn.\n4. **Ném**: Khi đã chuẩn bị sẵn sàng, đẩy bóng lên bằng tay ném, đồng thời duỗi chân và thân để tạo lực.\n5. **Thả Bóng**: Khi bóng rời tay, hãy giữ tay ném ở vị trí cao và theo dõi bóng bay vào rổ.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Nhiều người chơi không kiểm soát được lực ném, dẫn đến việc bóng không vào rổ. Hãy chú ý đến thời điểm đẩy bóng.\n- **Lời Khuyên**: Luyện tập từng bước một cho đến khi bạn cảm thấy thoải mái với toàn bộ quy trình.\n\n## Lưu Ý Quan Trọng\n- Hãy giữ mắt trên bóng và rổ trong suốt quá trình ném.\n- Đừng quên kiểm tra lại tư thế trước khi ném.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "Create an educational illustration that demonstrates the basic basketball shooting technique. It should include a detailed diagram showing five main steps: 1. Preparing with the ball in hand, 2. Lifting the ball to chest level, 3. Bending the knees for power, 4. Shooting the ball with an upward motion using the shooting hand, and 5. Releasing the ball while following through. Use clear labels and annotations for each step, and maintain a professional educational style.", "videoScript": "Xin chào các bạn! Hôm nay, chúng ta sẽ học kỹ thuật ném bóng cơ bản trong bóng rổ. Bước đầu tiên, hãy chuẩn bị với bóng trong tay và đứng thẳng. Tiếp theo, nhấc bóng lên ngang ngực. Gập đầu gối để tạo lực đồng thời nâng bóng lên cao hơn. Khi đã sẵn sàng, hãy đẩy bóng lên bằng tay ném, nhớ duỗi chân và thân để tạo ra lực. Cuối cùng, khi bóng rời tay, hãy giữ tay ném ở vị trí cao và theo dõi bóng bay mất. Hãy lưu ý rằng, điều quan trọng là phải giữ mắt trên bóng và rổ trong suốt quá trình ném. Luyện tập từng bước sẽ giúp bạn thực hiện cú ném chính xác hơn.", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa quy trình ném bóng cơ bản trong bóng rổ với năm bước chính, giúp người xem dễ dàng nắm bắt kỹ thuật.", "videoDescription": "Video này hướng dẫn kỹ thuật ném bóng cơ bản trong bóng rổ, bao gồm năm bước quan trọng."}', 4, '{"xp": 10, "coin": 1}', NULL, '2026-01-12 14:19:41.96526', '2026-01-27 11:31:32.666077', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Kỹ Thuật Ném Bóng Cơ Bản\n\nKỹ thuật ném bóng trong bóng rổ rất quan trọng để bạn ghi điểm. Dưới đây là các bước cơ bản:\n\n## Các Bước Thực Hiện\n1. **Chuẩn Bị**: Cầm bóng và đứng đúng tư thế.\n2. **Nhấc Bóng**: Đưa bóng lên ngang ngực.\n3. **Gập Đầu Gối**: Gập đầu gối để tạo sức mạnh.\n4. **Ném**: Đẩy bóng lên và duỗi chân ra.\n5. **Thả Bóng**: Giữ tay cao khi bóng rời tay và theo dõi bóng.\n\n## Lời Khuyên\n- Luyện tập từng bước để cảm thấy thoải mái hơn.\n- Nhớ giữ mắt trên bóng và rổ khi ném.", "detailed": "# Kỹ Thuật Ném Bóng Cơ Bản\n\nKỹ thuật ném bóng là một trong những yếu tố quyết định thành công trong bóng rổ. Việc ném đúng kỹ thuật không chỉ giúp bạn ghi điểm mà còn làm tăng sự tự tin khi thi đấu.\n\n## Các Bước Cụ Thể\n1. **Chuẩn Bị**: Đứng thẳng với chân rộng bằng vai. Cầm bóng bằng một tay, tay kia hỗ trợ ở bên hông.\n2. **Nhấc Bóng**: Nhấc bóng lên ngang ngực. Đảm bảo rằng cổ tay của bạn được giữ thẳng.\n3. **Gập Đầu Gối**: Gập đầu gối xuống để tích lũy sức mạnh trước khi ném, đồng thời nâng bóng lên cao hơn.\n4. **Ném**: Khi đã sẵn sàng, đẩy bóng lên bằng tay ném. Đừng quên duỗi chân và thân để tạo lực tối đa.\n5. **Thả Bóng**: Giữ tay ở vị trí cao khi bóng rời tay và theo dõi đường bay của bóng vào rổ.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Nhiều người không kiểm soát lực ném dẫn đến việc bóng không vào rổ. Hãy chú ý đến thời điểm đẩy bóng.\n- **Lời Khuyên**: Luyện tập từng bước một cho đến khi bạn cảm thấy thoải mái với toàn bộ quy trình.\n\n## Lưu Ý Quan Trọng\n- Giữ mắt trên bóng và rổ trong suốt quá trình ném.\n- Kiểm tra lại tư thế của bạn trước khi ném.", "comprehensive": "# Kỹ Thuật Ném Bóng Cơ Bản\n\nKỹ thuật ném bóng là nền tảng quan trọng trong bóng rổ. Bài viết này sẽ đi sâu vào từng bước của kỹ thuật ném bóng, lý do tại sao nó lại quan trọng và những lưu ý cần thiết để trở thành một người chơi bóng rổ giỏi.\n\n## Nguyên Tắc Cơ Bản\nKỹ thuật ném bóng không chỉ là việc đẩy bóng vào rổ mà còn liên quan đến tư thế, lực phát và độ chính xác. Một cú ném tốt phải có sự kết hợp hoàn hảo giữa các yếu tố này.\n\n## Các Bước Cụ Thể\n1. **Chuẩn Bị**: Đứng vững với chân cách xa nhau, cầm bóng bằng tay ném và giữ tay kia hỗ trợ ở bên hông. Tư thế này giúp bạn dễ dàng điều chỉnh trước khi ném.\n2. **Nhấc Bóng**: Đưa bóng lên ngang ngực, đảm bảo cổ tay thẳng. Điều này giúp bạn kiểm soát bóng tốt hơn.\n3. **Gập Đầu Gối**: Gập đầu gối để tạo ra lực ném. Đây là phần rất quan trọng vì nó quyết định sức mạnh của cú ném. Hãy tưởng tượng như bạn đang chuẩn bị nhảy lên.\n4. **Ném**: Đẩy bóng lên bằng tay ném, đồng thời duỗi chân và thân để tạo lực. Hãy chắc chắn rằng bạn đã có đủ sức mạnh và thời điểm.\n5. **Thả Bóng**: Khi bóng rời tay, giữ tay ở vị trí cao và theo dõi bóng bay về phía rổ. Đây là lúc bạn cảm nhận được kết quả của cú ném.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Một trong những lỗi phổ biến là không kiểm soát được lực ném. Điều này thường xảy ra khi bạn quá hồi hộp hoặc không tập trung.\n- **Lời Khuyên**: Hãy luyện tập từng bước một cho đến khi bạn cảm thấy tự tin và thoải mái với toàn bộ quy trình ném.\n\n## Lưu Ý Quan Trọng\n- Giữ mắt trên bóng và rổ trong suốt quá trình ném để tăng khả năng chính xác.\n- Kiểm tra lại tư thế trước khi ném. Một tư thế tốt sẽ tạo ra cú ném hiệu quả hơn.\n\n## Các Khái Niệm Liên Quan\n- **Lực Phát**: Hiểu và kiểm soát lực phát là yếu tố quan trọng. Luyện tập với bóng nhẹ hơn có thể giúp bạn cải thiện sức mạnh và kỹ thuật.\n- **Tâm Lý**: Tâm lý là một phần không thể thiếu trong bóng rổ. Hãy tự tin và tập trung vào từng cú ném.\n\n## Kết Luận\nKỹ thuật ném bóng trong bóng rổ không chỉ là kỹ năng thể chất mà còn là nghệ thuật. Hãy kiên nhẫn luyện tập và bạn sẽ thấy sự tiến bộ của mình qua từng trận đấu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5d2ec8e9-63cb-4a67-9a49-173e64091b66', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Thực Hành Và Luyện Tập Kỹ Thuật Ném Bóng', '# Thực Hành Và Luyện Tập Kỹ Thuật Ném Bóng\n\nThực hành là phần quan trọng nhất để hoàn thiện kỹ thuật ném bóng. Dưới đây là một số bài tập bạn có thể thực hiện để cải thiện khả năng ném bóng của mình.\n\n## Các Bài Tập Cơ Bản\n1. **Ném Bóng Từ Gần**: Bắt đầu ném bóng từ khoảng cách gần rổ để làm quen với kỹ thuật.\n2. **Ném Bóng Từ Xa**: Tăng dần khoảng cách từ rổ để thử thách bản thân.\n3. **Tập Luyện Trong Tình Huống Thực Tế**: Thực hiện các cú ném dưới áp lực, ví dụ như trong trận đấu hoặc cùng với đội bạn.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Không dành đủ thời gian cho việc luyện tập, dẫn đến việc không cải thiện kỹ năng.\n- **Lời Khuyên**: Hãy đặt ra mục tiêu cụ thể cho mỗi buổi tập để theo dõi tiến bộ của bản thân.\n\n## Lưu Ý Quan Trọng\n- Ghi lại số lượng cú ném thành công và thất bại để tự đánh giá.\n- Thực hiện các bài tập với sự tập trung cao độ để đạt được hiệu quả tối đa.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A detailed illustration of a basketball player practicing their shooting technique. The player is standing on the court near the basket, with labels indicating key elements such as body posture, ball grip, and follow-through. The background features a basketball court with markings and a hoop. The style is educational and professional, suitable for a sports training guide.", "videoScript": "Chào mừng các bạn đến với bài học về thực hành và luyện tập kỹ thuật ném bóng trong bóng rổ! Trong video này, chúng ta sẽ khám phá các bài tập cơ bản giúp cải thiện khả năng ném bóng của bạn. \n\nĐầu tiên, hãy bắt đầu với việc ném bóng từ gần. Đứng cách rổ khoảng 2 mét và thực hiện cú ném. Hãy chú ý đến tư thế và cách cầm bóng. Sau đó, tăng dần khoảng cách để thử thách bản thân. \n\nTiếp theo, chúng ta sẽ tập luyện trong tình huống thực tế. Hãy thực hiện cú ném dưới áp lực, có thể là trong trận đấu hoặc cùng với đội bạn. Đừng quên ghi lại số lượng cú ném thành công và thất bại để đánh giá tiến bộ của bạn. \n\nCuối cùng, hãy nhớ là luyện tập tập trung cao độ để đạt hiệu quả tối đa. Chúc bạn thành công và hãy đặt mục tiêu rõ ràng cho mỗi buổi tập!", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa một cầu thủ bóng rổ đang thực hành kỹ thuật ném bóng, với các chỉ dẫn về tư thế cơ thể, cách cầm bóng và động tác theo dõi.", "videoDescription": "Video này hướng dẫn các bạn các bài tập thực hành ném bóng trong bóng rổ, giúp cải thiện kỹ thuật và năng lực ném bóng."}', 6, '{"xp": 10, "coin": 1}', NULL, '2026-01-12 14:19:41.975266', '2026-01-27 11:32:23.130723', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Thực Hành Kỹ Thuật Ném Bóng\n\nNém bóng là một kỹ năng quan trọng trong bóng rổ. Để ném bóng tốt hơn, bạn có thể thực hiện một số bài tập sau:\n\n## Các Bài Tập Cơ Bản\n- **Ném Bóng Từ Gần**: Bắt đầu từ khoảng cách gần rổ để làm quen.\n- **Ném Bóng Từ Xa**: Tăng dần khoảng cách để thử thách bản thân.\n- **Tập Luyện Trong Tình Huống Thực Tế**: Thực hiện ném bóng như trong trận đấu.\n\n## Lưu Ý\n- Ghi lại kết quả để theo dõi tiến bộ.\n- Tập trung cao độ khi luyện tập để đạt hiệu quả tốt nhất.", "detailed": "# Thực Hành Và Luyện Tập Kỹ Thuật Ném Bóng\n\nKỹ thuật ném bóng là một phần không thể thiếu trong bóng rổ. Để cải thiện khả năng ném bóng, bạn cần thực hành thường xuyên. Dưới đây là một số bài tập và lưu ý quan trọng để bạn có thể nâng cao kỹ năng của mình.\n\n## Các Bài Tập Cơ Bản\n1. **Ném Bóng Từ Gần**: Bắt đầu bằng cách đứng gần rổ và ném bóng. Điều này giúp bạn làm quen với cách cầm bóng và tư thế ném.\n2. **Ném Bóng Từ Xa**: Khi bạn đã tự tin với khoảng cách gần, hãy tăng dần khoảng cách để thử thách bản thân. Điều này sẽ giúp bạn phát triển sức mạnh và kỹ thuật.\n3. **Tập Luyện Trong Tình Huống Thực Tế**: Hãy ném bóng trong các tình huống giống như trong trận đấu. Có thể luyện tập cùng đội hoặc dưới áp lực của thời gian.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Nhiều người không dành đủ thời gian cho việc luyện tập dẫn đến việc kỹ năng không được cải thiện.\n- **Lời Khuyên**: Đặt ra mục tiêu cụ thể mỗi buổi tập, ví dụ như số lượng cú ném thành công trong 30 phút.\n\n## Lưu Ý Quan Trọng\n- Ghi lại số lượng cú ném thành công và thất bại để đánh giá sự tiến bộ của bạn.\n- Tập trung cao độ trong suốt buổi tập để đạt được hiệu quả tối đa.", "comprehensive": "# Thực Hành Và Luyện Tập Kỹ Thuật Ném Bóng Trong Bóng Rổ\n\nKỹ thuật ném bóng là yếu tố quyết định thành công của một cầu thủ bóng rổ. Để thành thạo kỹ thuật này, bạn cần phải thực hành một cách có hệ thống và tập trung. Bài viết này sẽ cung cấp cho bạn những kiến thức sâu sắc về các bài tập ném bóng, nguyên lý và những lưu ý cần thiết.\n\n## Nguyên Lý Cơ Bản Của Kỹ Thuật Ném Bóng\nNém bóng không chỉ đơn thuần là hành động ném mà còn cần sự phối hợp giữa nhiều yếu tố như: tư thế, lực đẩy, và điểm rơi của bóng. Khi ném, bạn cần sử dụng cả cơ thể để tạo ra sức mạnh và độ chính xác.\n\n## Các Bài Tập Cơ Bản Để Cải Thiện Kỹ Thuật\n### 1. Ném Bóng Từ Gần\n- **Mục Tiêu**: Làm quen với kỹ thuật cầm bóng và tư thế ném. \n- **Cách Thực Hiện**: Đứng gần rổ, cầm bóng với cả hai tay và ném bóng vào rổ. Tập trung vào việc giữ bóng ổn định và điều chỉnh lực ném.\n\n### 2. Ném Bóng Từ Xa\n- **Mục Tiêu**: Xây dựng sức mạnh và kỹ thuật ném từ khoảng cách xa.\n- **Cách Thực Hiện**: Từ vị trí đứng cách rổ khoảng 5-7m, thực hiện các cú ném. Hãy chú ý đến việc duy trì tư thế và độ chính xác của cú ném.\n\n### 3. Tập Luyện Trong Tình Huống Thực Tế\n- **Mục Tiêu**: Giúp bạn làm quen với áp lực thực tế trong trận đấu.\n- **Cách Thực Hiện**: Tổ chức các buổi luyện tập với đội, thực hiện các cú ném trong thời gian giới hạn hoặc dưới sự theo dõi của huấn luyện viên.\n\n## Ví Dụ Minh Họa Và Lời Khuyên\n### Sai Lầm Thường Gặp\n- Không dành đủ thời gian cho việc luyện tập sẽ dẫn đến việc kỹ năng không được cải thiện.\n\n### Lời Khuyên\n- Đặt ra mục tiêu cụ thể cho mỗi buổi tập, chẳng hạn như số lượng cú ném thành công trong một khoảng thời gian nhất định.\n\n## Lưu Ý Quan Trọng\n- Ghi lại số lượng cú ném thành công và thất bại để tự đánh giá. Điều này giúp bạn nhận ra sự tiến bộ và những điểm cần cải thiện.\n- Tập trung cao độ trong suốt buổi tập để đạt được hiệu quả tối đa. Hãy nhớ rằng ném bóng là một kỹ năng cần thời gian và nỗ lực để hoàn thiện.\n\n## Kết Luận\nViệc thực hành và luyện tập kỹ thuật ném bóng không chỉ giúp bạn nâng cao khả năng cá nhân mà còn góp phần vào thành công chung của đội bóng. Hãy kiên trì và chăm chỉ để trở thành một cầu thủ ném bóng xuất sắc!"}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('33bac6d5-8582-43fc-8035-b89d1d53b9a8', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Cảm Giác Và Tâm Lý Khi Ném Bóng', '# Cảm Giác Và Tâm Lý Khi Ném Bóng\n\nCảm giác và tâm lý là những yếu tố không thể thiếu trong kỹ thuật ném bóng. Đôi khi, kỹ thuật có thể hoàn hảo nhưng nếu tâm lý không ổn định, cú ném vẫn có thể thất bại.\n\n## Các Yếu Tố Tâm Lý\n- **Tự Tin**: Tự tin vào khả năng của bản thân là điều cần thiết giúp bạn thực hiện những cú ném tốt hơn.\n- **Tập Trung**: Tập trung vào mục tiêu và quá trình thực hiện cú ném.\n- **Kiểm Soát Cảm Xúc**: Giữ bình tĩnh khi đối mặt với áp lực.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Nhiều cầu thủ khi đối mặt với áp lực lớn thường mất tập trung và không thực hiện đúng kỹ thuật.\n- **Lời Khuyên**: Hãy tập luyện trong các tình huống áp lực để cải thiện khả năng giữ bình tĩnh.\n\n## Lưu Ý Quan Trọng\n- Hãy thường xuyên tự nhắc nhở bản thân về khả năng của mình.\n- Thực hành các bài tập tâm lý để nâng cao khả năng tập trung.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "An educational illustration focusing on basketball shooting techniques, featuring a player in a dynamic shooting pose with annotations indicating key psychological factors like Confidence, Concentration, and Emotion Control. The background includes a basketball court with a hoop, and visual cues like arrows showing the shooting motion. The style is professional, clear, and informative.", "videoScript": "Xin chào các bạn! Hôm nay, chúng ta sẽ cùng tìm hiểu về cảm giác và tâm lý khi ném bóng trong bóng rổ. Đầu tiên, tự tin là yếu tố rất quan trọng. Khi bạn tin vào khả năng của mình, cơ hội thực hiện cú ném chính xác sẽ cao hơn. Tiếp theo, tập trung là cần thiết. Hãy luôn chú ý vào mục tiêu và các bước thực hiện cú ném. Cuối cùng, kiểm soát cảm xúc cũng rất quan trọng. Khi bạn đối mặt với áp lực, hãy giữ bình tĩnh. Một mẹo hữu ích là thực hành trong các tình huống căng thẳng để chuẩn bị tốt hơn cho những trận đấu thực tế. Cảm ơn các bạn đã theo dõi!", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa kỹ thuật ném bóng trong bóng rổ, với các yếu tố tâm lý như tự tin, tập trung và kiểm soát cảm xúc được chú thích rõ ràng.", "videoDescription": "Video này giới thiệu về những yếu tố tâm lý trong kỹ thuật ném bóng, giúp người chơi nắm vững các yếu tố cần thiết để cải thiện kỹ năng của mình."}', 5, '{"xp": 10, "coin": 1}', NULL, '2026-01-12 14:19:41.96917', '2026-01-27 11:31:58.072775', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Cảm Giác Và Tâm Lý Khi Ném Bóng\n\nKhi ném bóng, cảm giác và tâm lý đóng một vai trò rất quan trọng. Ngay cả khi bạn ném đúng kỹ thuật, nếu tâm lý không ổn định, cú ném có thể không thành công.\n\n## Các Yếu Tố Tâm Lý\n- **Tự Tin**: Tin tưởng vào khả năng của mình giúp bạn ném tốt hơn.\n- **Tập Trung**: Nhìn vào mục tiêu và chú ý vào cú ném.\n- **Kiểm Soát Cảm Xúc**: Giữ bình tĩnh, đặc biệt khi có áp lực.\n\n## Lời Khuyên\n- Tập luyện trong các tình huống áp lực để học cách giữ bình tĩnh.\n- Nhắc nhở bản thân về khả năng của mình thường xuyên.", "detailed": "# Cảm Giác Và Tâm Lý Khi Ném Bóng\n\nKhi ném bóng, cảm giác và tâm lý là những yếu tố không thể thiếu. Đôi khi, dù bạn có kỹ thuật hoàn hảo, nhưng nếu tâm lý không ổn định, cú ném vẫn có thể không thành công. Dưới đây là một số yếu tố tâm lý quan trọng mà bạn cần lưu ý.\n\n## Các Yếu Tố Tâm Lý\n- **Tự Tin**: Tự tin vào khả năng của bản thân là điều cần thiết. Nếu bạn tin rằng bạn có thể ném bóng vào rổ, khả năng thành công sẽ cao hơn.\n- **Tập Trung**: Tập trung vào mục tiêu và quá trình thực hiện cú ném là rất quan trọng. Hãy cố gắng không để những yếu tố bên ngoài làm bạn phân tâm.\n- **Kiểm Soát Cảm Xúc**: Giữ bình tĩnh là chìa khóa khi đối mặt với áp lực. Khi bạn cảm thấy hồi hộp, hãy hít thở sâu và cố gắng lấy lại sự bình tĩnh.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Nhiều cầu thủ khi đối mặt với áp lực lớn thường mất tập trung và không thực hiện đúng kỹ thuật ném. Ví dụ, trong một trận đấu quan trọng, cầu thủ có thể cảm thấy lo lắng và ném hỏng.\n- **Lời Khuyên**: Hãy tập luyện trong các tình huống áp lực, ví dụ như ném bóng trong những giây cuối cùng của trận đấu, để cải thiện khả năng giữ bình tĩnh.\n\n## Lưu Ý Quan Trọng\n- Thường xuyên tự nhắc nhở bản thân về khả năng của mình.\n- Thực hành các bài tập tâm lý để nâng cao khả năng tập trung.", "comprehensive": "# Cảm Giác Và Tâm Lý Khi Ném Bóng\n\nCảm giác và tâm lý là hai yếu tố quyết định trong kỹ thuật ném bóng, đặc biệt trong môn bóng rổ. Dù bạn có sở hữu kỹ thuật ném bóng hoàn hảo, nhưng nếu tâm lý không ổn định, kết quả sẽ không như mong đợi. Trong bài viết này, chúng ta sẽ đi sâu vào các yếu tố tâm lý và cách nó ảnh hưởng đến hiệu suất ném bóng.\n\n## Các Yếu Tố Tâm Lý\n- **Tự Tin**: Tự tin là yếu tố then chốt giúp bạn thực hiện những cú ném chính xác. Khi bạn tin tưởng vào khả năng của mình, não bộ sẽ phát tín hiệu tích cực giúp bạn thực hiện cú ném một cách tự nhiên hơn.\n- **Tập Trung**: Việc tập trung vào mục tiêu và quá trình ném bóng giúp bạn tránh bị phân tâm bởi các yếu tố bên ngoài như tiếng cổ vũ hoặc áp lực từ đồng đội. Hãy luyện tập kỹ năng tập trung, ví dụ như thiền hoặc các bài tập chú ý.\n- **Kiểm Soát Cảm Xúc**: Cảm xúc có thể ảnh hưởng lớn đến hiệu suất của bạn. Khi gặp áp lực, nhiều cầu thủ có xu hướng lo lắng, dẫn đến sai sót trong kỹ thuật. Hãy học cách kiểm soát cảm xúc của mình, bằng cách phát triển những phương pháp như hít thở sâu hoặc tưởng tượng thành công trước khi thực hiện cú ném.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Một ví dụ điển hình là khi cầu thủ ném phạt trong một trận đấu quyết định. Áp lực từ khán giả và tầm quan trọng của cú ném có thể khiến họ mất tập trung, dẫn đến việc ném hỏng. Để cải thiện, hãy tập luyện trong các tình huống tương tự để làm quen với áp lực.\n- **Lời Khuyên**: Tập luyện thường xuyên trong các tình huống áp lực là cách tốt nhất để nâng cao khả năng giữ bình tĩnh. Bạn có thể tạo ra các tình huống giả lập để rèn luyện tâm lý.\n\n## Nguyên Lý Tâm Lý Trong Thể Thao\nTâm lý học thể thao đã chứng minh rằng tâm lý ảnh hưởng đến hiệu suất thể thao như thế nào. Các nghiên cứu cho thấy rằng việc phát triển kỹ năng tâm lý không chỉ giúp bạn cải thiện kỹ năng ném bóng mà còn nâng cao toàn bộ hiệu suất thi đấu.\n\n## Best Practices\n- **Thiết lập Mục Tiêu**: Đặt ra các mục tiêu rõ ràng và cụ thể cho từng buổi tập và trận đấu. Điều này sẽ giúp bạn theo dõi tiến bộ và tăng cường động lực.\n- **Phản Hồi Và Tự Đánh Giá**: Sau mỗi trận đấu hoặc buổi tập, hãy dành thời gian để đánh giá hiệu suất của bản thân. Hãy xem xét những gì đã làm tốt và những gì cần cải thiện.\n- **Tìm Kiếm Sự Hỗ Trợ**: Đừng ngần ngại tìm kiếm sự hỗ trợ từ huấn luyện viên hoặc chuyên gia tâm lý thể thao. Họ có thể mang đến cái nhìn chuyên sâu và phương pháp luyện tập hiệu quả.\n\n## Kết Luận\nTóm lại, cảm giác và tâm lý đóng một vai trò quan trọng trong kỹ thuật ném bóng. Khi bạn có thể kiểm soát tốt các yếu tố tâm lý, bạn sẽ có khả năng ném bóng chính xác hơn, từ đó nâng cao hiệu suất trong các trận đấu. Hãy nhớ rằng, tâm lý không chỉ là một phần của thể thao mà còn là một phần quan trọng trong cuộc sống hàng ngày."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5753acf2-c0c1-49ae-ab4e-0a7248e56953', '108e984e-99ea-4c84-b06d-6bc340f42b17', 'concept', 'Cách Viết Giai Điệu Đơn Giản', 'Để viết giai điệu đơn giản, bạn có thể bắt đầu với một nốt nhạc chính và tạo ra các nốt tiếp theo bằng cách sử dụng khoảng cách giữa các nốt. Hãy thử nghiệm với các nhịp điệu khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các nốt nhạc đơn giản được sắp xếp trên khuông nhạc", "videoScript": "Chào các bạn! Hôm nay chúng ta sẽ học cách viết một giai điệu đơn giản. Hãy cùng bắt đầu với một nốt nhạc chính.", "imageDescription": "Các nốt nhạc đơn giản thể hiện một giai điệu dễ nhớ", "videoDescription": "Video hướng dẫn cách viết giai điệu đơn giản cho người mới bắt đầu."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:41:27.077274', '2026-01-27 12:41:27.077274', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bắt đầu bằng việc chọn một nốt nhạc và thêm các nốt tiếp theo theo thứ tự.", "detailed": "Để viết giai điệu đơn giản, bạn có thể bắt đầu với một nốt nhạc chính và tạo ra các nốt tiếp theo bằng cách sử dụng khoảng cách giữa các nốt. Hãy thử nghiệm với các nhịp điệu khác nhau.", "comprehensive": "Viết giai điệu đơn giản là bước đầu tiên để tạo ra âm nhạc. Chọn một nốt nhạc chính làm điểm khởi đầu, sau đó thêm các nốt tiếp theo theo các khoảng cách như quãng ngũ hoặc quãng bát. Thay đổi nhịp điệu và tốc độ để tạo ra sự đa dạng cho giai điệu. Thực hành là chìa khóa để phát triển khả năng sáng tạo của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c1198736-0a8c-4185-97c5-13cc2f47e49f', '829a56a0-cf00-4477-b21e-d3ac5fbcecd7', 'concept', 'Mặt Nạ Tự Chế Từ Nguyên Liệu Thiên Nhiên', 'Bạn có thể tự chế mặt nạ từ bơ, mật ong và sữa chua để cấp ẩm cho da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh nguyên liệu thiên nhiên để làm mặt nạ.", "videoScript": "Hôm nay, chúng ta sẽ cùng nhau làm mặt nạ tự chế từ nguyên liệu thiên nhiên tại nhà.", "imageDescription": "Bảng nguyên liệu thiên nhiên như bơ, mật ong, và sữa chua để làm mặt nạ tự chế.", "videoDescription": "Video hướng dẫn cách làm mặt nạ tự chế từ bơ, mật ong và sữa chua, giúp cấp ẩm cho da hiệu quả."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:13:11.128825', '2026-01-27 13:13:11.128825', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Mặt nạ tự chế an toàn và hiệu quả.", "detailed": "Bạn có thể tự chế mặt nạ từ bơ, mật ong và sữa chua để cấp ẩm cho da.", "comprehensive": "Mặt nạ tự chế từ nguyên liệu thiên nhiên rất an toàn và dễ làm. Chẳng hạn, bạn có thể sử dụng bơ, mật ong và sữa chua để tạo ra mặt nạ cấp ẩm. Bơ cung cấp vitamin E, mật ong có tính kháng khuẩn và sữa chua chứa axit lactic giúp làm mềm da. Hãy trộn các nguyên liệu lại với nhau và thoa lên mặt trong 15-20 phút."}');
