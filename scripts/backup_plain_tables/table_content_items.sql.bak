INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7a35fea3-a0fb-47ce-8adb-5a7a95d2ff6a', 'e8af6e5c-9dca-4b86-8d90-ad70c8d1e8f5', 'concept', 'Cách cải thiện quy trình theo dõi tiến độ dự án', 'Để cải thiện quy trình theo dõi tiến độ dự án, bạn có thể áp dụng các công cụ tự động hóa, thường xuyên tổ chức họp nhóm và khuyến khích báo cáo tiến độ định kỳ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các biện pháp cải thiện quy trình theo dõi tiến độ dự án.", "videoScript": "Trong video này, chúng ta sẽ khám phá những cách hiệu quả để cải thiện quy trình theo dõi tiến độ dự án và làm cho nó trở nên hiệu quả hơn.", "imageDescription": "Minh họa các biện pháp như tự động hóa và họp nhóm để cải thiện quy trình theo dõi tiến độ.", "videoDescription": "Video giới thiệu các biện pháp và công cụ để cải thiện quy trình theo dõi tiến độ trong các dự án."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:28:43.459095', '2026-01-27 12:28:43.459095', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng công cụ tự động hóa có thể giúp cải thiện quy trình theo dõi.", "detailed": "Để cải thiện quy trình theo dõi tiến độ dự án, bạn có thể áp dụng các công cụ tự động hóa, thường xuyên tổ chức họp nhóm và khuyến khích báo cáo tiến độ định kỳ.", "comprehensive": "Cải thiện quy trình theo dõi tiến độ dự án đòi hỏi một chiến lược toàn diện, bao gồm việc sử dụng các công cụ tự động hóa để giảm thiểu sai sót, tổ chức các cuộc họp định kỳ để cập nhật tiến độ và khuyến khích văn hóa báo cáo cởi mở trong nhóm. Những biện pháp này sẽ giúp nâng cao hiệu quả và độ chính xác trong theo dõi tiến độ."}');
