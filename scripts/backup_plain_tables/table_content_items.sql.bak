INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('91d15e48-5628-40e0-9cf2-bb2a3b549f2a', 'e8af6e5c-9dca-4b86-8d90-ad70c8d1e8f5', 'concept', 'Lợi ích của việc theo dõi tiến độ dự án', 'Việc theo dõi tiến độ dự án mang lại nhiều lợi ích như phát hiện sớm các vấn đề, tối ưu hóa tài nguyên và cải thiện sự giao tiếp trong nhóm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa lợi ích của việc theo dõi tiến độ dự án.", "videoScript": "Trong video này, chúng ta sẽ thảo luận về những lợi ích đáng kể mà việc theo dõi tiến độ dự án mang lại cho nhóm và tổ chức.", "imageDescription": "Biểu đồ thể hiện lợi ích của việc theo dõi tiến độ dự án như tiết kiệm thời gian và cải thiện hiệu suất làm việc.", "videoDescription": "Video giải thích về các lợi ích của việc theo dõi tiến độ dự án, bao gồm cách cải thiện hiệu suất và tối ưu hóa quy trình làm việc."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:28:43.450664', '2026-01-27 12:28:43.450664', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi tiến độ dự án giúp phát hiện sớm vấn đề và điều chỉnh kịp thời.", "detailed": "Việc theo dõi tiến độ dự án mang lại nhiều lợi ích như phát hiện sớm các vấn đề, tối ưu hóa tài nguyên và cải thiện sự giao tiếp trong nhóm.", "comprehensive": "Theo dõi tiến độ dự án không chỉ giúp nhận diện sớm các vấn đề mà còn cho phép quản lý dự án điều chỉnh kế hoạch một cách hiệu quả. Nó giúp tăng cường sự minh bạch, tạo động lực cho nhóm và cải thiện khả năng giao tiếp giữa các bên liên quan."}');
