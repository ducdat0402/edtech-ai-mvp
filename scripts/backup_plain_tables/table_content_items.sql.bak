INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c7b49fe2-8102-4070-ae45-19db291a8d06', 'e8af6e5c-9dca-4b86-8d90-ad70c8d1e8f5', 'concept', 'Giới thiệu về Theo dõi tiến độ dự án', 'Theo dõi tiến độ dự án là quá trình theo dõi và ghi nhận các bước thực hiện của dự án. Nó giúp đảm bảo rằng dự án đi đúng hướng và kịp thời phát hiện các vấn đề.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa về biểu đồ Gantt và các bước theo dõi tiến độ dự án.", "videoScript": "Chào mừng đến với bài học về theo dõi tiến độ dự án. Trong video này, chúng ta sẽ tìm hiểu về tầm quan trọng của việc theo dõi tiến độ và các công cụ hữu ích.", "imageDescription": "Biểu đồ Gantt với các mốc thời gian và tiến độ thực hiện các công việc trong dự án.", "videoDescription": "Video giải thích về các khái niệm cơ bản và tầm quan trọng của theo dõi tiến độ dự án trong quản lý dự án."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:28:43.432113', '2026-01-27 12:28:43.432113', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Theo dõi tiến độ dự án giúp đảm bảo rằng các mục tiêu được thực hiện đúng hạn.", "detailed": "Theo dõi tiến độ dự án là quá trình theo dõi và ghi nhận các bước thực hiện của dự án. Nó giúp đảm bảo rằng dự án đi đúng hướng và kịp thời phát hiện các vấn đề.", "comprehensive": "Theo dõi tiến độ dự án là một phần quan trọng trong quản lý dự án, bao gồm việc lập kế hoạch, theo dõi và đánh giá các hoạt động và kết quả của dự án. Quá trình này giúp quản lý tài nguyên, thời gian và ngân sách, đảm bảo rằng dự án đạt được các mục tiêu đề ra."}');
