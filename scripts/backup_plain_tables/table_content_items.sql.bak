INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ce45fc57-4211-4225-b3aa-8cbe45725fec', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'boss_quiz', 'Boss Quiz: Khám Phá Kỹ Thuật Dribble Trong Bóng Rổ', 'Kiểm tra kiến thức về Khám Phá Kỹ Thuật Dribble Trong Bóng Rổ', NULL, 100, '{"xp": 50, "coin": 10}', '{"options": ["A. Giúp kiểm soát bóng khi di chuyển", "B. Tạo ra điểm số ngay lập tức", "C. Giúp bạn phòng ngự tốt hơn", "D. Làm cho bạn nổi bật hơn trong trận đấu"], "question": "Kỹ thuật dribble có vai trò gì trong bóng rổ?", "explanation": "Kỹ thuật dribble rất quan trọng trong bóng rổ vì nó giúp người chơi kiểm soát bóng khi di chuyển, từ đó tạo ra khoảng trống và cơ hội cho các tình huống tấn công. Nếu không có kỹ thuật dribble tốt, bạn sẽ gặp khó khăn trong việc vượt qua đối thủ và duy trì kiểm soát bóng.", "correctAnswer": 0}', '2026-01-12 14:20:49.014514', '2026-01-12 14:20:49.014514', NULL, 'medium', NULL, 'published', NULL, NULL, NULL, NULL);
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('574178fa-840d-46bb-9d3b-f242724955b3', '83bd90f8-4b1f-4ccf-831d-c499713a5ff7', 'concept', 'Các Kỹ Thuật Chơi Piano Trong Jazz', 'Improvisation là một yếu tố quan trọng trong jazz. Piano thường sử dụng các hợp âm phức và giai điệu để tạo ra âm thanh độc đáo. Các kỹ thuật như stride piano và comping cũng rất phổ biến.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bức ảnh một bản nhạc piano jazz với các hợp âm phức tạp.", "videoScript": "Hãy cùng tìm hiểu về các kỹ thuật chơi piano trong jazz và cách chúng tạo nên âm thanh đặc trưng của thể loại này.", "imageDescription": "Một bản nhạc piano với các ghi chú và hợp âm phức tạp, thể hiện sự đa dạng trong kỹ thuật chơi jazz.", "videoDescription": "Video hướng dẫn các kỹ thuật chơi piano trong jazz, từ improvisation đến các hợp âm phức tạp."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:51:11.262223', '2026-01-27 12:51:11.262223', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều kỹ thuật chơi piano trong jazz, bao gồm improvisation và dùng hợp âm phức.", "detailed": "Improvisation là một yếu tố quan trọng trong jazz. Piano thường sử dụng các hợp âm phức và giai điệu để tạo ra âm thanh độc đáo. Các kỹ thuật như stride piano và comping cũng rất phổ biến.", "comprehensive": "Kỹ thuật chơi piano trong jazz rất đa dạng và phong phú. Improvisation cho phép nghệ sĩ tự do sáng tạo âm nhạc ngay tại chỗ. Các hợp âm phức và cách sử dụng syncopation, cùng với các phong cách như stride piano và comping, tạo ra sự phong phú trong âm thanh. Những nghệ sĩ như Art Tatum đã phát triển kỹ thuật chơi phức tạp, mang lại sự mới mẻ cho jazz."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c4e0e421-6f19-4d3d-847d-d2433aef80ba', 'ae369e64-6c5e-4daf-b676-221dbf5c67b2', 'concept', 'Xác định các khoản chi cần thiết và không cần thiết', 'Bạn cần phân loại các khoản chi của mình thành hai nhóm: cần thiết và không cần thiết. Các khoản cần thiết bao gồm tiền thuê nhà, thực phẩm, và hóa đơn, trong khi các khoản không cần thiết là các khoản chi cho giải trí và mua sắm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng phân loại các khoản chi cần thiết và không cần thiết.", "videoScript": "Video giới thiệu về cách phân loại các khoản chi hàng tháng, bắt đầu với một bảng phân loại và các ví dụ thực tế.", "imageDescription": "Hình ảnh một bảng trắng với các danh sách phân loại các khoản chi, như ''Cần thiết'' và ''Không cần thiết''.", "videoDescription": "Video hướng dẫn phân loại các khoản chi trong ngân sách cá nhân."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:59:21.230342', '2026-01-27 12:59:21.230342', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biết được chi nào cần thiết và chi nào không giúp bạn tiết kiệm.", "detailed": "Bạn cần phân loại các khoản chi của mình thành hai nhóm: cần thiết và không cần thiết. Các khoản cần thiết bao gồm tiền thuê nhà, thực phẩm, và hóa đơn, trong khi các khoản không cần thiết là các khoản chi cho giải trí và mua sắm.", "comprehensive": "Phân loại các khoản chi thành cần thiết và không cần thiết là một bước quan trọng trong quản lý ngân sách. Các khoản chi cần thiết là những khoản bạn phải chi trả để sống, như tiền thuê nhà và thực phẩm, trong khi các khoản không cần thiết là những khoản có thể cắt giảm hoặc điều chỉnh để tiết kiệm hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d1f741ca-9a23-406f-a42d-4557626e9c3e', 'e23d9c85-8084-4ad5-a6ca-e75b21e2d6f6', 'concept', 'Giới Thiệu Về Phong Cách Chơi Nhạc Piano', 'Tìm hiểu về các phong cách chơi nhạc piano như cổ điển, jazz, và pop.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ piano đang chơi nhạc cổ điển trên một cây đàn grand.", "videoScript": "Chào mừng bạn đến với bài học về phong cách chơi nhạc piano. Hôm nay, chúng ta sẽ khám phá những phong cách khác nhau và cách chúng ảnh hưởng đến âm nhạc.", "imageDescription": "Hình ảnh của một nghệ sĩ với ánh đèn vàng ấm áp, đang say sưa trong từng phím đàn piano.", "videoDescription": "Video này sẽ giới thiệu các phong cách chơi nhạc piano, cung cấp ví dụ minh họa và phân tích những yếu tố khác nhau trong từng phong cách."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:03.016149', '2026-01-27 12:42:03.016149', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khám phá các phong cách chơi nhạc piano cơ bản.", "detailed": "Tìm hiểu về các phong cách chơi nhạc piano như cổ điển, jazz, và pop.", "comprehensive": "Bài học này sẽ giới thiệu cho bạn về các phong cách chơi nhạc piano khác nhau, từ cổ điển đến hiện đại, và cách chúng ảnh hưởng đến cách chơi và cảm nhận âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4023f7b6-e8cf-42a7-88b0-a134da296402', '83bd90f8-4b1f-4ccf-831d-c499713a5ff7', 'concept', 'Sự Phát Triển Của Jazz Piano Qua Các Thời Kỳ', 'Từ những năm 1920, jazz piano đã trải qua nhiều thay đổi, từ phong cách stride đến bebop và fusion. Mỗi giai đoạn đều có những đặc điểm riêng và những nghệ sĩ tiêu biểu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nghệ sĩ piano jazz từ những năm 1920 đến nay.", "videoScript": "Hãy cùng khám phá sự phát triển của jazz piano qua các thời kỳ và những thay đổi trong phong cách chơi.", "imageDescription": "Một nghệ sĩ piano thể hiện sự phát triển của phong cách jazz qua các thập kỷ.", "videoDescription": "Video tóm tắt sự phát triển của jazz piano từ những năm 1920 đến nay, với các phong cách và nghệ sĩ tiêu biểu."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:51:11.266631', '2026-01-27 12:51:11.266631', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Jazz piano đã phát triển từ những năm 1920 đến nay.", "detailed": "Từ những năm 1920, jazz piano đã trải qua nhiều thay đổi, từ phong cách stride đến bebop và fusion. Mỗi giai đoạn đều có những đặc điểm riêng và những nghệ sĩ tiêu biểu.", "comprehensive": "Sự phát triển của jazz piano bắt đầu từ những năm 1920 với phong cách stride, nơi nghệ sĩ sử dụng những hợp âm mạnh mẽ. Sau đó, bebop vào những năm 1940 đã mang đến một phong cách chơi nhanh và phức tạp hơn. Đến những năm 1970, fusion kết hợp jazz với rock và funk, mở ra một kỷ nguyên mới cho jazz piano với các nghệ sĩ như Chick Corea và Keith Jarrett."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ffd175d0-8a98-4a3a-a8b5-657e0c04f0f1', 'ae369e64-6c5e-4daf-b676-221dbf5c67b2', 'concept', 'Lập kế hoạch tiết kiệm cho các mục tiêu tài chính', 'Bạn nên xác định mục tiêu tài chính cụ thể và lập kế hoạch tiết kiệm hàng tháng để đạt được chúng. Điều này có thể bao gồm việc tiết kiệm cho kỳ nghỉ, mua nhà hoặc quỹ khẩn cấp.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang viết kế hoạch tiết kiệm trên giấy.", "videoScript": "Video bắt đầu với hình ảnh một người đang viết ra các mục tiêu tài chính. Giọng nói lồng ghép hướng dẫn cách lập kế hoạch tiết kiệm hiệu quả.", "imageDescription": "Hình ảnh một người phụ nữ đang ngồi tại bàn, ghi chú các mục tiêu tài chính và kế hoạch tiết kiệm của mình.", "videoDescription": "Video hướng dẫn lập kế hoạch tiết kiệm cho các mục tiêu tài chính cá nhân."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:59:21.233886', '2026-01-27 12:59:21.233886', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tiết kiệm giúp bạn đạt được các mục tiêu tài chính.", "detailed": "Bạn nên xác định mục tiêu tài chính cụ thể và lập kế hoạch tiết kiệm hàng tháng để đạt được chúng. Điều này có thể bao gồm việc tiết kiệm cho kỳ nghỉ, mua nhà hoặc quỹ khẩn cấp.", "comprehensive": "Lập kế hoạch tiết kiệm cho các mục tiêu tài chính là bước quan trọng trong việc đạt được sự ổn định tài chính. Bạn cần xác định rõ các mục tiêu ngắn hạn và dài hạn, từ đó lập một kế hoạch tiết kiệm cụ thể với số tiền tiết kiệm hàng tháng phù hợp để thực hiện các mục tiêu đó."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6b3d862b-dce5-4797-873c-1efd7fe42664', 'ae369e64-6c5e-4daf-b676-221dbf5c67b2', 'concept', 'Kiểm tra và điều chỉnh ngân sách định kỳ', 'Bạn nên kiểm tra ngân sách của mình hàng tháng hoặc hàng quý để đảm bảo nó vẫn phù hợp với tình hình tài chính hiện tại và điều chỉnh nếu cần.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang phân tích ngân sách của mình với biểu đồ và đồ thị.", "videoScript": "Video bắt đầu với hình ảnh một người đang phân tích ngân sách. Giọng nói lồng ghép giải thích tầm quan trọng của việc kiểm tra ngân sách định kỳ.", "imageDescription": "Hình ảnh một người đàn ông đang xem xét các biểu đồ ngân sách trên màn hình máy tính.", "videoDescription": "Video hướng dẫn cách kiểm tra và điều chỉnh ngân sách cá nhân định kỳ."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:59:21.237771', '2026-01-27 12:59:21.237771', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Điều chỉnh ngân sách định kỳ giúp bạn theo kịp tình hình tài chính.", "detailed": "Bạn nên kiểm tra ngân sách của mình hàng tháng hoặc hàng quý để đảm bảo nó vẫn phù hợp với tình hình tài chính hiện tại và điều chỉnh nếu cần.", "comprehensive": "Việc kiểm tra và điều chỉnh ngân sách định kỳ là rất quan trọng để đảm bảo rằng bạn đang đi đúng hướng với các mục tiêu tài chính của mình. Bạn nên thiết lập một lịch trình kiểm tra ngân sách hàng tháng hoặc hàng quý để xem xét các khoản chi, thu nhập và điều chỉnh ngân sách khi cần thiết để phù hợp với sự thay đổi trong cuộc sống."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9b3da90d-1496-4c3a-bee7-26a8ad75298c', 'aa097387-3253-40d6-8817-a725adc8f9e3', 'concept', 'Giới thiệu về Tài chính cá nhân', 'Tài chính cá nhân bao gồm việc lập kế hoạch và quản lý thu nhập, chi tiêu, tiết kiệm và đầu tư.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang lập kế hoạch tài chính với sổ ghi chép và máy tính.", "videoScript": "Video bắt đầu với một lời giới thiệu về tầm quan trọng của tài chính cá nhân, sau đó là các bước cơ bản để quản lý tài chính.", "imageDescription": "Một người ngồi tại bàn làm việc, xung quanh là sách tài chính và tài liệu.", "videoDescription": "Một video giải thích về tài chính cá nhân với các ví dụ thực tế và lời khuyên hữu ích."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:59:41.144845', '2026-01-27 12:59:41.144845', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tài chính cá nhân là quản lý tiền bạc của bạn.", "detailed": "Tài chính cá nhân bao gồm việc lập kế hoạch và quản lý thu nhập, chi tiêu, tiết kiệm và đầu tư.", "comprehensive": "Tài chính cá nhân không chỉ là việc kiểm soát chi tiêu mà còn liên quan đến việc lập kế hoạch cho các mục tiêu tài chính trong tương lai như mua nhà, nghỉ hưu hay giáo dục con cái."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5b63f6b4-1f4e-417c-ab74-1636267ff7eb', 'aa097387-3253-40d6-8817-a725adc8f9e3', 'concept', 'Lập ngân sách cá nhân', 'Một ngân sách cá nhân chi tiết sẽ giúp bạn theo dõi thu nhập và chi phí hàng tháng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bảng ngân sách với các danh mục chi tiêu.", "videoScript": "Video hướng dẫn cách lập ngân sách cá nhân, bao gồm các công cụ và ứng dụng hữu ích.", "imageDescription": "Một bảng ngân sách với các mục như tiền thuê nhà, thực phẩm, giải trí và tiết kiệm.", "videoDescription": "Video đào tạo về lập ngân sách cá nhân với minh họa sinh động và các bước cụ thể."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:59:41.153081', '2026-01-27 12:59:41.153081', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Lập ngân sách giúp bạn biết chi tiêu như thế nào.", "detailed": "Một ngân sách cá nhân chi tiết sẽ giúp bạn theo dõi thu nhập và chi phí hàng tháng.", "comprehensive": "Lập ngân sách không chỉ là ghi lại các khoản thu chi mà còn là một công cụ để bạn có thể điều chỉnh hành vi chi tiêu và tiết kiệm cho các mục tiêu tài chính."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f37c83e6-841e-43ee-8659-c2ccfbdaceab', 'e23d9c85-8084-4ad5-a6ca-e75b21e2d6f6', 'concept', 'Phong Cách Cổ Điển Trong Chơi Nhạc Piano', 'Phong cách cổ điển bao gồm các tác phẩm của Beethoven, Mozart và Chopin. Hãy cùng tìm hiểu cách thể hiện chúng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bản nhạc cổ điển và một cây đàn piano cổ điển.", "videoScript": "Hôm nay, chúng ta sẽ tập trung vào phong cách cổ điển. Bạn sẽ được nghe một số tác phẩm nổi bật và cách chúng được thể hiện trên piano.", "imageDescription": "Một bản nhạc cổ điển và một cây đàn piano gỗ cổ điển, tạo nên không khí trang trọng và nghệ thuật.", "videoDescription": "Video này sẽ phân tích phong cách cổ điển trong âm nhạc piano, cùng với các ví dụ thực tế từ những tác phẩm nổi tiếng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:03.024351', '2026-01-27 12:42:03.024351', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khám phá phong cách cổ điển với những tác phẩm nổi tiếng.", "detailed": "Phong cách cổ điển bao gồm các tác phẩm của Beethoven, Mozart và Chopin. Hãy cùng tìm hiểu cách thể hiện chúng.", "comprehensive": "Bài học này sẽ đưa bạn vào thế giới của âm nhạc cổ điển, nơi bạn sẽ học về các tác phẩm vĩ đại và kỹ thuật chơi piano cần thiết để thể hiện chúng một cách hoàn hảo."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('bfc39b03-ca7a-462f-9940-cb9052c90376', 'e23d9c85-8084-4ad5-a6ca-e75b21e2d6f6', 'concept', 'Phong Cách Jazz Trong Chơi Nhạc Piano', 'Phong cách jazz đặc trưng bởi sự ngẫu hứng và cảm xúc. Hãy cùng tìm hiểu cách chơi piano jazz.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ jazz đang chơi piano trong một quán bar nhỏ.", "videoScript": "Chào mừng bạn đến với thế giới của jazz! Hôm nay, chúng ta sẽ khám phá cách chơi piano jazz và những đặc điểm tạo nên phong cách này.", "imageDescription": "Hình ảnh một nghệ sĩ đang say sưa chơi piano jazz, với ánh đèn mờ và khán giả thưởng thức.", "videoDescription": "Video này sẽ giới thiệu về phong cách jazz trong âm nhạc piano với các kỹ thuật ngẫu hứng và ví dụ từ các nghệ sĩ nổi tiếng."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:03.027718', '2026-01-27 12:42:03.027718', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tìm hiểu về phong cách jazz và cách chơi piano jazz.", "detailed": "Phong cách jazz đặc trưng bởi sự ngẫu hứng và cảm xúc. Hãy cùng tìm hiểu cách chơi piano jazz.", "comprehensive": "Bài học này sẽ khám phá phong cách jazz trong âm nhạc piano, bao gồm các kỹ thuật ngẫu hứng, hợp âm phức tạp và cách tạo ra cảm xúc trong âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9d7e31b6-c99d-4464-bf71-f4d970a06eed', 'e23d9c85-8084-4ad5-a6ca-e75b21e2d6f6', 'concept', 'Phong Cách Pop Trong Chơi Nhạc Piano', 'Phong cách pop thường dễ nghe và gần gũi. Hãy tìm hiểu cách chơi piano cho các bản hit nổi tiếng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ đang biểu diễn một bài hát pop trên piano.", "videoScript": "Hôm nay, chúng ta sẽ tìm hiểu về phong cách pop trong âm nhạc piano. Bạn sẽ khám phá các bản hit và cách chơi chúng một cách thú vị.", "imageDescription": "Hình ảnh một nghệ sĩ trẻ đang biểu diễn một bài hát pop trên sân khấu với ánh đèn rực rỡ.", "videoDescription": "Video này sẽ trình bày phong cách pop trong âm nhạc piano với các ví dụ cụ thể và cách chơi cho các bài hát nổi tiếng."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:03.031209', '2026-01-27 12:42:03.031209', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khám phá phong cách pop và cách áp dụng vào piano.", "detailed": "Phong cách pop thường dễ nghe và gần gũi. Hãy tìm hiểu cách chơi piano cho các bản hit nổi tiếng.", "comprehensive": "Bài học này sẽ đưa bạn vào thế giới âm nhạc pop, hướng dẫn bạn cách chơi piano cho các bản nhạc hit và cách tạo ra phong cách riêng của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1212fd97-a0fe-48d3-bec1-2b01b36b90a5', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Giới Thiệu Về Kỹ Thuật Ném Bóng', '# Giới Thiệu Về Kỹ Thuật Ném Bóng\n\nKỹ thuật ném bóng là một trong những kỹ năng quan trọng nhất trong bóng rổ, ảnh hưởng trực tiếp đến khả năng ghi điểm của người chơi. Ném bóng không chỉ đơn thuần là việc phóng bóng vào rổ mà còn là sự kết hợp giữa tư thế, kỹ thuật và cảm giác. Để có thể ném bóng hiệu quả, người chơi cần phải hiểu rõ về các yếu tố ảnh hưởng đến cú ném của mình.\n\n## Các Yếu Tố Ảnh Hưởng Đến Kỹ Thuật Ném Bóng\n- **Tư Thế Cầm Bóng**: Cách cầm bóng ảnh hưởng đến độ chính xác và lực ném.\n- **Tư Thế Cơ Thể**: Cách đứng, vị trí của chân và cơ thể sẽ quyết định đến cú ném.\n- **Kỹ Thuật Ném**: Các bước thực hiện cụ thể trong cú ném.\n- **Cảm Giác**: Sự tự tin và cảm giác của người chơi trong từng cú ném.\n\n## Tầm Quan Trọng Của Kỹ Thuật Ném Bóng\nViệc luyện tập kỹ thuật ném bóng không chỉ giúp người chơi ghi điểm mà còn tăng cường sự tự tin, giúp họ đối mặt với áp lực trong các trận đấu. Một cú ném tốt có thể thay đổi cả kết quả trận đấu và tạo nên những khoảnh khắc đáng nhớ trong sự nghiệp thể thao của mỗi cầu thủ.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "An educational illustration showing the technique of basketball shooting. The image should include labeled sections such as ''Ball Grip'', ''Body Position'', ''Shooting Technique'', and ''Feel''. The basketball player should be in a mid-action pose, demonstrating the shooting form with a basketball hoop in the background. The style should be clear and professional, designed for educational purposes with annotations highlighting key aspects of shooting technique.", "videoScript": "Chào các bạn đến với bài học về kỹ thuật ném bóng trong bóng rổ! Hôm nay, chúng ta sẽ tìm hiểu những yếu tố quan trọng ảnh hưởng đến cú ném của cầu thủ. Đầu tiên, tư thế cầm bóng rất quan trọng, nó ảnh hưởng đến độ chính xác và lực ném. Hãy chú ý đến cách mà cầu thủ đặt tay lên bóng. Tiếp theo là tư thế cơ thể. Đứng vững, đặt chân đúng vị trí sẽ giúp bạn có một cú ném tốt hơn. Bây giờ, đến kỹ thuật ném – hãy chắc chắn rằng bạn thực hiện đầy đủ các bước từ chân đến tay. Cuối cùng, cảm giác tự tin sẽ tạo nên sự khác biệt lớn trong mỗi cú ném. Luyện tập thường xuyên sẽ giúp bạn cải thiện kỹ thuật ném bóng và đạt được nhiều điểm hơn trong trận đấu.", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa kỹ thuật ném bóng trong bóng rổ với các phần được chú thích như ''Cách cầm bóng'', ''Tư thế cơ thể'', ''Kỹ thuật ném'', và ''Cảm giác''.", "videoDescription": "Video này giới thiệu về kỹ thuật ném bóng trong bóng rổ, nhấn mạnh các yếu tố ảnh hưởng đến cú ném và tầm quan trọng của việc luyện tập."}', 1, '{"xp": 10, "coin": 1}', NULL, '2026-01-12 14:19:41.94825', '2026-01-27 11:30:27.241932', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Giới Thiệu Về Kỹ Thuật Ném Bóng\n\nKỹ thuật ném bóng là một phần quan trọng trong bóng rổ, giúp cầu thủ ghi điểm. Ném bóng không chỉ là việc đưa bóng vào rổ mà còn liên quan đến cách cầm bóng, tư thế đứng, và cảm giác của người ném.\n\n## Các Yếu Tố Quan Trọng\n- **Cách Cầm Bóng**: Cầm bóng đúng giúp bạn ném chính xác hơn.\n- **Tư Thế Cơ Thể**: Đứng đúng tư thế sẽ giúp cú ném tốt hơn.\n- **Kỹ Thuật Ném**: Làm theo các bước cụ thể khi ném.\n- **Cảm Giác**: Tự tin sẽ giúp bạn ném tốt hơn.\n\n## Tại Sao Kỹ Thuật Ném Bóng Quan Trọng?\nTập luyện kỹ thuật ném bóng giúp bạn ghi điểm và tự tin hơn trong trận đấu. Một cú ném tốt có thể thay đổi kết quả trận đấu và tạo ra những khoảnh khắc đáng nhớ!", "detailed": "# Giới Thiệu Về Kỹ Thuật Ném Bóng\n\nKỹ thuật ném bóng là một trong những kỹ năng thiết yếu trong bóng rổ, đóng vai trò quan trọng trong việc ghi điểm. Ném bóng không phải chỉ là việc phóng bóng vào rổ, mà còn là sự kết hợp hoàn hảo giữa tư thế, kỹ thuật, và cảm giác của người chơi.\n\n## Các Yếu Tố Ảnh Hưởng Đến Kỹ Thuật Ném Bóng\n- **Tư Thế Cầm Bóng**: Cách bạn cầm bóng sẽ ảnh hưởng đến độ chính xác và lực ném. Hãy đảm bảo rằng các ngón tay của bạn có thể kiểm soát bóng tốt.\n- **Tư Thế Cơ Thể**: Đứng vững với hai chân chắc chắn, một chân hơi nhô ra phía trước. Tư thế này giúp bạn có được lực ném tối ưu.\n- **Kỹ Thuật Ném**: Cú ném bao gồm nhiều bước như nâng bóng lên, đẩy bóng ra và theo dõi bóng khi ném. Mỗi bước nên được thực hiện một cách chính xác để có được kết quả tốt.\n- **Cảm Giác**: Sự tự tin trong mỗi cú ném có thể tạo ra sự khác biệt lớn. Hãy luyện tập thường xuyên để tăng cường cảm giác này.\n\n## Tầm Quan Trọng Của Kỹ Thuật Ném Bóng\nViệc luyện tập kỹ thuật ném bóng không chỉ giúp bạn ghi điểm mà còn nâng cao sự tự tin, giúp bạn đối mặt với áp lực trong các trận đấu. Một cú ném tốt có thể thay đổi cả kết quả của trận đấu và tạo ra những khoảnh khắc đáng nhớ trong sự nghiệp của bạn.\n\n### Tips Quan Trọng\n- Luyện tập thường xuyên để cải thiện kỹ năng.\n- Quan sát các cầu thủ giỏi để học hỏi từ họ.\n- Đừng ngại thử nghiệm các tư thế và kỹ thuật khác nhau để tìm ra cách phù hợp nhất với bạn.", "comprehensive": "# Giới Thiệu Về Kỹ Thuật Ném Bóng\n\nKỹ thuật ném bóng trong bóng rổ là một trong những kỹ năng quan trọng nhất mà mỗi cầu thủ cần phải thành thạo. Điều này không chỉ quyết định khả năng ghi điểm mà còn ảnh hưởng đến toàn bộ lối chơi của cầu thủ. Ném bóng không chỉ đơn giản là hành động đưa bóng vào rổ; nó đòi hỏi sự kết hợp giữa nhiều yếu tố khác nhau, bao gồm tư thế, kỹ thuật, và cảm giác.\n\n## Các Yếu Tố Ảnh Hưởng Đến Kỹ Thuật Ném Bóng\n- **Tư Thế Cầm Bóng**: Cách cầm bóng là yếu tố đầu tiên ảnh hưởng đến cú ném. Bạn cần phải cầm bóng một cách thoải mái nhưng chắc chắn, sử dụng các ngón tay để kiểm soát bóng. Hãy tưởng tượng như bạn đang cầm một quả trứng để không làm rơi nó.\n\n- **Tư Thế Cơ Thể**: Tư thế cơ thể ảnh hưởng lớn đến khả năng ném bóng thành công. Một tư thế đứng vững với hai chân mở rộng, trọng tâm cơ thể được phân bố đều sẽ tạo ra sự ổn định cần thiết trong cú ném. Hãy chú ý đến vị trí của hai chân và đầu gối, đảm bảo rằng bạn không ngã về phía trước hoặc phía sau khi ném.\n\n- **Kỹ Thuật Ném**: Các bước trong kỹ thuật ném bao gồm việc nâng bóng lên đến vị trí ném, đưa tay ra trước và đẩy bóng bằng lực từ chân và cơ thể. Cần lưu ý rằng việc theo dõi bóng sau khi ném cũng rất quan trọng, vì nó giúp bạn hiểu rõ hơn về cách bóng di chuyển.\n\n- **Cảm Giác**: Cảm giác tự tin và thoải mái khi ném bóng sẽ giúp bạn thực hiện cú ném tốt hơn. Hãy luyện tập để phát triển cảm giác này, có thể thông qua việc tạo ra một thói quen trước khi ném, như hít thở sâu hoặc hình dung về cú ném thành công.\n\n## Tầm Quan Trọng Của Kỹ Thuật Ném Bóng\nKỹ thuật ném bóng không chỉ giúp cầu thủ ghi điểm mà còn mang lại sự tự tin cho họ trong các tình huống áp lực. Một cú ném thành công có thể thay đổi cục diện của trận đấu, tạo ra những khoảnh khắc đáng nhớ trong sự nghiệp thể thao của mỗi cầu thủ.\n\n### Thực Hành và Phát Triển Kỹ Năng\nĐể cải thiện kỹ thuật ném bóng, hãy thực hiện các bài tập luyện tập thường xuyên. Bạn có thể bắt đầu với việc ném bóng vào rổ ở khoảng cách gần và từ từ tăng khoảng cách khi bạn cảm thấy tự tin hơn. Quan sát các cầu thủ chuyên nghiệp và cố gắng mô phỏng kỹ thuật của họ để học hỏi thêm.\n\n### Các Tình Huống Đặc Biệt và Best Practices\n- Trong các tình huống áp lực, hãy giữ bình tĩnh và tập trung vào cú ném của bạn.\n- Hãy thử nghiệm với các góc ném khác nhau và tìm ra cách ném hiệu quả nhất cho riêng bạn.\n- Tham gia vào các buổi tập với nhóm để cải thiện cảm giác thi đấu và kỹ năng giao tiếp trong trận đấu.\n\n### Ví Dụ Thực Tế\nNhiều cầu thủ bóng rổ nổi tiếng như Michael Jordan và Stephen Curry đều đã từng nhấn mạnh tầm quan trọng của kỹ thuật ném. Họ đã không ngừng luyện tập để hoàn thiện kỹ năng này, và sự thành công của họ trong sự nghiệp sát cánh cùng những cú ném tuyệt vời đã tạo nên những khoảnh khắc lịch sử trong làng bóng rổ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('85b8048a-8175-4a91-9380-6d05234d923c', 'aa097387-3253-40d6-8817-a725adc8f9e3', 'concept', 'Tiết kiệm và Đầu tư', 'Tiết kiệm cung cấp sự an toàn tài chính, trong khi đầu tư có thể giúp bạn tăng trưởng tài sản theo thời gian.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang xem xét các lựa chọn đầu tư.", "videoScript": "Video thảo luận về lợi ích của việc tiết kiệm và đầu tư, cùng với các ví dụ về cách bắt đầu.", "imageDescription": "Một người đang ngồi trước máy tính với biểu đồ tài chính và các tài liệu đầu tư.", "videoDescription": "Video giải thích về sự khác nhau giữa tiết kiệm và đầu tư, với các chiến lược cụ thể cho người mới bắt đầu."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:59:41.156862', '2026-01-27 12:59:41.156862', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tiết kiệm là để dành tiền, còn đầu tư là làm cho tiền sinh lời.", "detailed": "Tiết kiệm cung cấp sự an toàn tài chính, trong khi đầu tư có thể giúp bạn tăng trưởng tài sản theo thời gian.", "comprehensive": "Sự khác biệt giữa tiết kiệm và đầu tư là rất quan trọng trong việc xây dựng một kế hoạch tài chính bền vững, từ việc chọn các tài khoản tiết kiệm cho đến các lựa chọn đầu tư như cổ phiếu và trái phiếu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4bddd155-6c17-45b8-acb7-5b71900e8c9e', 'aa097387-3253-40d6-8817-a725adc8f9e3', 'concept', 'Quản lý nợ', 'Quản lý nợ bao gồm việc theo dõi các khoản vay và lập kế hoạch trả nợ hiệu quả.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang làm việc với các hóa đơn và tài liệu liên quan đến nợ.", "videoScript": "Video hướng dẫn cách lập kế hoạch trả nợ và các mẹo để giảm thiểu nợ.", "imageDescription": "Một người đang viết ra kế hoạch trả nợ với các hóa đơn và bút.", "videoDescription": "Video cung cấp thông tin về các phương pháp quản lý nợ hiệu quả và cách xây dựng tín dụng tốt."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:59:41.160481', '2026-01-27 12:59:41.160481', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Quản lý nợ giúp bạn không bị áp lực tài chính.", "detailed": "Quản lý nợ bao gồm việc theo dõi các khoản vay và lập kế hoạch trả nợ hiệu quả.", "comprehensive": "Quản lý nợ là một phần quan trọng trong tài chính cá nhân, giúp bạn tránh các khoản nợ xấu và xây dựng tín dụng tốt, từ việc hiểu rõ các điều khoản vay đến việc tìm cách giảm nợ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3eea9121-7b81-49a0-904b-4061da17ee64', 'e23d9c85-8084-4ad5-a6ca-e75b21e2d6f6', 'concept', 'Kết Hợp Các Phong Cách Chơi Nhạc Piano', 'Hãy tìm hiểu cách kết hợp các phong cách cổ điển, jazz và pop để tạo ra âm nhạc độc đáo.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ đang chơi piano với sự kết hợp của nhiều phong cách khác nhau.", "videoScript": "Chào mừng bạn đến với bài học về sự kết hợp phong cách! Hôm nay, chúng ta sẽ khám phá cách kết hợp các phong cách để tạo ra âm nhạc độc đáo.", "imageDescription": "Hình ảnh một nghệ sĩ đang biểu diễn với sự kết hợp của các phong cách âm nhạc khác nhau trên piano.", "videoDescription": "Video này sẽ trình bày cách kết hợp các phong cách chơi nhạc piano khác nhau, cùng với các ví dụ thực tế và cách phát triển phong cách riêng của bạn."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:03.035578', '2026-01-27 12:42:03.035578', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khám phá cách kết hợp các phong cách chơi nhạc piano.", "detailed": "Hãy tìm hiểu cách kết hợp các phong cách cổ điển, jazz và pop để tạo ra âm nhạc độc đáo.", "comprehensive": "Bài học này sẽ hướng dẫn bạn cách kết hợp các phong cách chơi nhạc piano khác nhau để tạo ra âm nhạc độc đáo, giúp bạn phát triển phong cách cá nhân và sáng tạo."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3aa7a34c-b5ee-4906-9347-bbb51a4438a2', 'aa097387-3253-40d6-8817-a725adc8f9e3', 'concept', 'Đánh giá tình hình tài chính cá nhân', 'Đánh giá tình hình tài chính cá nhân bao gồm việc xem xét thu nhập, chi phí, tài sản và nợ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang xem xét báo cáo tài chính cá nhân.", "videoScript": "Video hướng dẫn cách thực hiện đánh giá tài chính cá nhân, bao gồm các chỉ số và công cụ cần thiết.", "imageDescription": "Một người đang phân tích các số liệu tài chính với biểu đồ và bảng số liệu.", "videoDescription": "Video giải thích về cách đánh giá tình hình tài chính cá nhân và lập kế hoạch cho tương lai."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:59:41.16429', '2026-01-27 12:59:41.16429', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đánh giá tài chính giúp bạn biết mình đang ở đâu.", "detailed": "Đánh giá tình hình tài chính cá nhân bao gồm việc xem xét thu nhập, chi phí, tài sản và nợ.", "comprehensive": "Một đánh giá toàn diện về tình hình tài chính cá nhân sẽ giúp bạn hiểu rõ về vị trí tài chính hiện tại và lập kế hoạch cho tương lai, từ việc phân tích bảng cân đối kế toán cá nhân đến việc thiết lập các mục tiêu tài chính cụ thể."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('93fa711a-84f5-410c-9b68-a6ce3e613304', 'ecfa0b88-c1ef-4439-8019-76df3085e14e', 'concept', 'Giới Thiệu Về Bảo Hiểm Sức Khỏe', 'Bảo hiểm sức khỏe là một loại bảo hiểm giúp bạn bảo vệ tài chính trước các chi phí y tế phát sinh do bệnh tật hoặc tai nạn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một gia đình hạnh phúc bên nhau, thể hiện sự an tâm về sức khỏe.", "videoScript": "Chào mừng bạn đến với bài học về bảo hiểm sức khỏe. Hôm nay, chúng ta sẽ tìm hiểu tại sao bảo hiểm sức khỏe lại quan trọng trong cuộc sống hàng ngày.", "imageDescription": "Một gia đình đang trò chuyện vui vẻ trong công viên, biểu tượng cho sự an toàn và sức khỏe.", "videoDescription": "Video giới thiệu về bảo hiểm sức khỏe, cách thức hoạt động và lợi ích của nó cho từng cá nhân trong xã hội."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:00:06.681986', '2026-01-27 13:00:06.681986', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bảo hiểm sức khỏe giúp bạn chi trả cho các chi phí y tế.", "detailed": "Bảo hiểm sức khỏe là một loại bảo hiểm giúp bạn bảo vệ tài chính trước các chi phí y tế phát sinh do bệnh tật hoặc tai nạn.", "comprehensive": "Bảo hiểm sức khỏe là sự bảo vệ tài chính quan trọng cho mỗi cá nhân, giúp chi trả cho các chi phí y tế như khám bệnh, điều trị nội trú và ngoại trú. Việc có bảo hiểm sức khỏe giúp người tham gia an tâm hơn khi gặp phải các tình huống y tế không mong muốn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1b5014de-f84b-426f-afaf-2692b0e15ab8', 'ecfa0b88-c1ef-4439-8019-76df3085e14e', 'concept', 'Lợi Ích Của Bảo Hiểm Sức Khỏe', 'Một trong những lợi ích lớn nhất của bảo hiểm sức khỏe là bảo vệ bạn khỏi những chi phí y tế đột xuất có thể gây ra gánh nặng tài chính.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bác sĩ đang tư vấn cho bệnh nhân.", "videoScript": "Lợi ích của bảo hiểm sức khỏe không chỉ là chi phí, mà còn là sự an tâm cho bạn và gia đình. Hãy cùng tìm hiểu thêm.", "imageDescription": "Bác sĩ và bệnh nhân đang trao đổi thông tin sức khỏe, thể hiện sự tin tưởng và hỗ trợ.", "videoDescription": "Video mô tả những lợi ích của bảo hiểm sức khỏe, bao gồm giảm bớt gánh nặng tài chính và đảm bảo tiếp cận dịch vụ y tế chất lượng."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:00:06.691929', '2026-01-27 13:00:06.691929', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bảo hiểm sức khỏe bảo vệ bạn khỏi chi phí y tế cao.", "detailed": "Một trong những lợi ích lớn nhất của bảo hiểm sức khỏe là bảo vệ bạn khỏi những chi phí y tế đột xuất có thể gây ra gánh nặng tài chính.", "comprehensive": "Bảo hiểm sức khỏe mang lại nhiều lợi ích bao gồm việc giảm thiểu rủi ro tài chính, đảm bảo bạn có thể nhận được dịch vụ y tế cần thiết mà không phải lo lắng về chi phí, và giúp bạn duy trì sức khỏe tốt hơn thông qua các chương trình khám sức khỏe định kỳ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('336a7c18-ca1b-47a2-a555-5cb410888a71', 'ea70d998-1fa8-4885-9a12-7713df8fca34', 'concept', 'Giới Thiệu Về Tẩy Tế Bào Chết', 'Tẩy tế bào chết là bước quan trọng trong skincare, giúp loại bỏ lớp da chết và bụi bẩn, cho làn da mịn màng hơn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người phụ nữ đang tẩy tế bào chết cho mặt.", "videoScript": "Chào mừng đến với video hôm nay, chúng ta sẽ khám phá tẩy tế bào chết và lợi ích của nó. Bạn có biết rằng tẩy tế bào chết giúp làm sáng da không?", "imageDescription": "Một người phụ nữ trẻ tuổi đang sử dụng sản phẩm tẩy tế bào chết trên khuôn mặt của mình trong phòng tắm.", "videoDescription": "Video giới thiệu về tẩy tế bào chết và cách thực hiện đúng cách."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:13:42.146717', '2026-01-27 13:13:42.146717', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tẩy tế bào chết giúp loại bỏ da chết, mang lại làn da sáng khỏe.", "detailed": "Tẩy tế bào chết là bước quan trọng trong skincare, giúp loại bỏ lớp da chết và bụi bẩn, cho làn da mịn màng hơn.", "comprehensive": "Tẩy tế bào chết không chỉ loại bỏ tế bào chết mà còn kích thích sản sinh collagen, cải thiện kết cấu da và làm sáng màu da. Có hai loại tẩy tế bào chết: vật lý và hóa học, mỗi loại có ưu điểm riêng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('58854ea3-0e3e-4f92-8cc0-5b12cc72cc00', '00bbe76d-6014-485e-890f-1398b6100e68', 'concept', 'Cách Cầm Bóng Đúng Cách', '# Cách Cầm Bóng Đúng Cách\n\nCách cầm bóng là bước đầu tiên và cực kỳ quan trọng trong kỹ thuật ném bóng. Nó không chỉ ảnh hưởng đến khả năng kiểm soát bóng mà còn quyết định đến độ chính xác của cú ném.\n\n## Các Bước Cầm Bóng\n1. **Đặt Bóng**: Đặt bóng vào tay không thuận, sử dụng bàn tay không để giữ bóng ở vị trí ngang ngực.\n2. **Tay Ném**: Dùng tay thuận (tay ném) để đặt bóng lên đỉnh đầu, lòng bàn tay hướng về phía trước.\n3. **Ngón Tay**: Đảm bảo rằng các ngón tay của tay ném mở rộng và đặt nhẹ nhàng trên bề mặt bóng. Không nên để bóng chạm vào lòng bàn tay.\n4. **Cảm Giác**: Cảm nhận về bóng rất quan trọng, hãy chắc chắn rằng bạn cảm thấy thoải mái và tự tin khi cầm bóng.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Nhiều cầu thủ mới thường cầm bóng quá chặt, điều này không chỉ làm giảm độ chính xác mà còn gây khó khăn trong việc điều khiển bóng.\n- **Lời Khuyên**: Hãy luyện tập việc cầm bóng nhiều lần trước khi thực hiện cú ném thực tế, điều này sẽ giúp bạn cải thiện khả năng cầm bóng và tăng cường sự tự tin.\n\n## Lưu Ý Quan Trọng\n- Tránh việc cầm bóng quá chặt.\n- Đảm bảo rằng tay ném luôn ở tư thế sẵn sàng, không để bóng rơi ra khỏi tay.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A detailed educational illustration demonstrating the correct way to hold a basketball. The image shows a person in a basketball court background. There are four steps labeled: 1. Placing the ball on the non-dominant hand at chest level, 2. Positioning the dominant hand on top of the ball with fingers spread, 3. Emphasizing finger placement lightly on the ball''s surface, and 4. Annotations showing common mistakes of gripping too tightly. The style is professional and informative.", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ cùng nhau tìm hiểu về cách cầm bóng đúng cách trong bóng rổ. Đầu tiên, hãy đặt bóng vào tay không thuận, giữ bóng ở vị trí ngang ngực. Tiếp theo, dùng tay thuận để đặt bóng lên đỉnh đầu, lòng bàn tay hướng về phía trước. Quan trọng nhất là các ngón tay của tay ném phải mở rộng và nhẹ nhàng đặt trên bề mặt bóng. Tránh để bóng chạm vào lòng bàn tay. Cảm giác về bóng rất quan trọng, vì vậy hãy chắc chắn bạn cảm thấy thoải mái khi cầm bóng. Một sai lầm thường gặp là cầm bóng quá chặt, điều này sẽ làm giảm độ chính xác. Hãy luyện tập để cải thiện khả năng cầm bóng và tăng cường sự tự tin. Cảm ơn các bạn đã theo dõi!", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa cách cầm bóng đúng cách trong bóng rổ, hiển thị các bước và lưu ý quan trọng.", "videoDescription": "Video hướng dẫn về cách cầm bóng đúng cách trong bóng rổ, với các bước chi tiết và lưu ý quan trọng."}', 2, '{"xp": 10, "coin": 1}', NULL, '2026-01-12 14:19:41.957448', '2026-01-27 11:30:51.309969', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Cách Cầm Bóng Đúng Cách\n\nCầm bóng đúng cách là bước đầu tiên rất quan trọng khi chơi bóng rổ. Nếu bạn cầm bóng không đúng, bạn sẽ khó kiểm soát và ném bóng chính xác.\n\n## Các Bước Cầm Bóng\n1. **Đặt Bóng**: Giữ bóng bằng tay không thuận ở ngang ngực.\n2. **Tay Ném**: Đặt bóng lên đỉnh đầu bằng tay thuận, lòng bàn tay hướng về phía trước.\n3. **Ngón Tay**: Đảm bảo ngón tay mở rộng và nhẹ nhàng trên bóng, không để bóng chạm lòng bàn tay.\n4. **Cảm Giác**: Cảm thấy thoải mái khi cầm bóng. \n\n## Ví Dụ Minh Họa\n- **Sai Lầm**: Cầm bóng quá chặt làm giảm độ chính xác và khó kiểm soát.\n- **Lời Khuyên**: Luyện tập cầm bóng nhiều lần trước khi ném thực tế để cải thiện sự tự tin.\n\n## Lưu Ý\n- Tránh cầm bóng quá chặt.\n- Giữ tay ném sẵn sàng để không làm rơi bóng.", "detailed": "# Cách Cầm Bóng Đúng Cách\n\nCầm bóng đúng cách là một kỹ thuật rất quan trọng trong bóng rổ. Việc cầm bóng không chỉ ảnh hưởng đến khả năng kiểm soát mà còn quyết định đến độ chính xác của cú ném.\n\n## Các Bước Cầm Bóng\n1. **Đặt Bóng**: Đầu tiên, bạn hãy đặt bóng vào tay không thuận và giữ nó ở ngang ngực. Điều này giúp bạn có thể dễ dàng điều khiển bóng khi thực hiện cú ném.\n2. **Tay Ném**: Sử dụng tay thuận (tay ném) để nâng bóng lên đỉnh đầu, lòng bàn tay hướng về phía trước. Điều này tạo ra một vị trí thuận lợi để bạn thực hiện cú ném.\n3. **Ngón Tay**: Ngón tay của tay ném nên mở rộng và nhẹ nhàng tiếp xúc với bề mặt bóng. Tránh để bóng chạm vào lòng bàn tay vì điều này có thể làm bạn mất kiểm soát.\n4. **Cảm Giác**: Cảm nhận về bóng rất quan trọng. Hãy chắc chắn rằng bạn cảm thấy thoải mái và tự tin khi cầm bóng trước khi thực hiện cú ném.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Nhiều cầu thủ mới thường cầm bóng quá chặt. Điều này không chỉ làm giảm độ chính xác mà còn gây khó khăn trong việc điều khiển bóng.\n- **Lời Khuyên**: Hãy luyện tập việc cầm bóng nhiều lần trước khi thực hiện cú ném thực tế. Điều này sẽ giúp bạn cải thiện khả năng cầm bóng và tăng cường sự tự tin.\n\n## Lưu Ý Quan Trọng\n- Tránh việc cầm bóng quá chặt.\n- Đảm bảo rằng tay ném luôn ở tư thế sẵn sàng, không để bóng rơi ra khỏi tay.", "comprehensive": "# Cách Cầm Bóng Đúng Cách\n\nCầm bóng đúng cách là một trong những kỹ thuật cơ bản nhưng cực kỳ quan trọng trong bóng rổ. Kỹ thuật này không chỉ ảnh hưởng đến khả năng kiểm soát bóng mà còn quyết định đến độ chính xác của những cú ném. Để nắm vững kỹ thuật này, người chơi cần hiểu rõ từng bước cũng như các nguyên lý liên quan.\n\n## Nguyên Tắc Cầm Bóng\nCầm bóng đúng cách giúp người chơi có thể thực hiện các cú ném một cách hiệu quả hơn. Nó giúp tạo ra lực ném tốt, đồng thời giữ bóng ở vị trí thuận lợi để có thể điều khiển dễ dàng hơn. Nếu cầm bóng sai cách, bạn có thể gặp khó khăn trong việc thực hiện các cú ném chính xác.\n\n## Các Bước Cầm Bóng Chi Tiết\n1. **Đặt Bóng**: Đầu tiên, bạn hãy đặt bóng vào tay không thuận và giữ nó ở ngang ngực. Điều này giúp bạn có thể dễ dàng điều khiển bóng.\n2. **Tay Ném**: Dùng tay thuận (tay ném) để nâng bóng lên đỉnh đầu, lòng bàn tay hướng về phía trước.\n3. **Ngón Tay**: Đảm bảo rằng các ngón tay của tay ném mở rộng và nhẹ nhàng chạm vào bề mặt bóng. Không nên để bóng chạm vào lòng bàn tay, vì điều này có thể làm giảm độ chính xác và khả năng điều khiển.\n4. **Cảm Giác**: Cảm nhận về bóng rất quan trọng. Hãy chắc chắn rằng bạn cảm thấy thoải mái và tự tin khi cầm bóng.\n\n## Ví Dụ Minh Họa\n- **Sai Lầm Thường Gặp**: Nhiều cầu thủ mới thường cầm bóng quá chặt hoặc không đúng vị trí, điều này không chỉ làm giảm độ chính xác mà còn gây khó khăn trong việc điều khiển bóng.\n- **Lời Khuyên**: Hãy luyện tập việc cầm bóng nhiều lần trước khi thực hiện cú ném thực tế. Điều này sẽ giúp bạn cải thiện khả năng cầm bóng và tăng cường sự tự tin.\n\n## Lưu Ý Quan Trọng\n- Tránh việc cầm bóng quá chặt. Điều này có thể làm giảm sức mạnh và độ linh hoạt của cú ném.\n- Đảm bảo rằng tay ném luôn ở tư thế sẵn sàng, không để bóng rơi ra khỏi tay.\n\n## Các Tình Huống Khác\nTrong một số trường hợp, bạn có thể cần điều chỉnh vị trí cầm bóng tùy thuộc vào tình huống cụ thể trong trận đấu. Ví dụ, khi bạn đang trong tình huống bị chắn, việc cầm bóng gần sát thân thể có thể giúp bạn bảo vệ bóng tốt hơn. \n\n## Best Practices\n- Cố gắng luyện tập với bạn bè hoặc huấn luyện viên để nhận được phản hồi về kỹ thuật cầm bóng của bạn.\n- Hãy chú ý đến cảm giác thoải mái và tự tin khi cầm bóng. Nếu bạn cảm thấy không thoải mái, hãy điều chỉnh lại cách cầm của mình.\n\n## Kết Luận\nCầm bóng đúng cách là một kỹ thuật căn bản nhưng vô cùng quan trọng trong bóng rổ. Bằng cách thực hiện đúng các bước cầm bóng, bạn sẽ cải thiện đáng kể khả năng kiểm soát và độ chính xác trong những cú ném của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('88d6e867-5458-454d-a397-862b94c29f1a', 'a1618b25-304d-43be-b2e3-bd7613475485', 'concept', 'Khám Phá Âm Nhạc Ngẫu Hứng', 'Chơi nhạc theo cảm hứng giúp bạn thể hiện bản thân và khám phá âm nhạc theo cách riêng của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang ngồi chơi piano, ánh sáng mờ ảo tạo nên không khí lãng mạn.", "videoScript": "Chào mừng các bạn đến với bài học về chơi nhạc theo cảm hứng. Hôm nay, chúng ta sẽ cùng nhau khám phá cách tạo ra âm nhạc tự do từ trái tim của mình.", "imageDescription": "Hình ảnh mô tả một nghệ sĩ đang chơi piano trong một không gian yên tĩnh, thể hiện sự cảm hứng và sáng tạo.", "videoDescription": "Video hướng dẫn các kỹ thuật để chơi nhạc theo cảm hứng, bao gồm các bài tập thực hành và ví dụ minh họa."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:33.681149', '2026-01-27 12:42:33.681149', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách chơi nhạc một cách tự do và sáng tạo.", "detailed": "Chơi nhạc theo cảm hứng giúp bạn thể hiện bản thân và khám phá âm nhạc theo cách riêng của mình.", "comprehensive": "Trong bài học này, chúng ta sẽ tìm hiểu về những kỹ thuật cơ bản để chơi nhạc theo cảm hứng, bao gồm cách cảm nhận âm nhạc, sử dụng hợp âm và tạo ra giai điệu tự do."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1ef033e4-f36a-42f9-b216-6916bcaa1282', 'da4e1803-cf29-46b1-ac58-160894256860', 'concept', 'Giới Thiệu Về Nhạc Cổ Điển', 'Nhạc cổ điển là một thể loại âm nhạc được sáng tác trong khoảng thời gian từ thế kỷ 9 đến thế kỷ 21, bao gồm nhiều phong cách và thể loại khác nhau. Nó thường được biểu diễn bởi các dàn nhạc giao hưởng, nhạc viện và các nghệ sĩ độc tấu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một dàn nhạc giao hưởng đang biểu diễn trên sân khấu.", "videoScript": "Chào mừng bạn đến với thế giới của nhạc cổ điển! Trong video này, chúng ta sẽ cùng khám phá lịch sử và những đặc điểm nổi bật của âm nhạc cổ điển.", "imageDescription": "Một bức ảnh chụp một dàn nhạc giao hưởng trong trang phục biểu diễn, đang chơi nhạc cổ điển với sự tập trung cao độ.", "videoDescription": "Video giới thiệu về nhạc cổ điển, bao gồm lịch sử, các phong cách chính và những nhạc sĩ nổi tiếng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:51:38.976891', '2026-01-27 12:51:38.976891', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhạc cổ điển là thể loại âm nhạc có nguồn gốc từ châu Âu, thường được sáng tác cho dàn nhạc giao hưởng.", "detailed": "Nhạc cổ điển là một thể loại âm nhạc được sáng tác trong khoảng thời gian từ thế kỷ 9 đến thế kỷ 21, bao gồm nhiều phong cách và thể loại khác nhau. Nó thường được biểu diễn bởi các dàn nhạc giao hưởng, nhạc viện và các nghệ sĩ độc tấu.", "comprehensive": "Nhạc cổ điển thường được chia thành nhiều phong cách, bao gồm Baroque, Classical, Romantic và Modern. Mỗi phong cách có những đặc điểm và nhạc cụ riêng, từ những bản giao hưởng vĩ đại của Beethoven đến những bản sonata đầy cảm xúc của Chopin. Nhạc cổ điển không chỉ mang tính nghệ thuật cao mà còn là nền tảng cho nhiều thể loại âm nhạc hiện đại."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('91858ef7-f128-42d9-97fa-e48caa112e94', 'ecfa0b88-c1ef-4439-8019-76df3085e14e', 'concept', 'Cách Chọn Bảo Hiểm Sức Khỏe Phù Hợp', 'Để chọn được bảo hiểm sức khỏe phù hợp, bạn cần xem xét các yếu tố như tuổi tác, tình trạng sức khỏe và ngân sách của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang nghiên cứu thông tin bảo hiểm trên máy tính.", "videoScript": "Chọn bảo hiểm sức khỏe là một quyết định quan trọng. Hãy cùng tìm hiểu các bước để chọn gói bảo hiểm phù hợp nhất cho bạn.", "imageDescription": "Người dùng đang tìm kiếm thông tin về các gói bảo hiểm sức khỏe khác nhau trên mạng.", "videoDescription": "Video hướng dẫn cách chọn bảo hiểm sức khỏe, bao gồm phân tích nhu cầu cá nhân và so sánh các lựa chọn khác nhau."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:00:06.696575', '2026-01-27 13:00:06.696575', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn bảo hiểm cần dựa vào nhu cầu cá nhân.", "detailed": "Để chọn được bảo hiểm sức khỏe phù hợp, bạn cần xem xét các yếu tố như tuổi tác, tình trạng sức khỏe và ngân sách của mình.", "comprehensive": "Việc chọn bảo hiểm sức khỏe phù hợp yêu cầu bạn phân tích nhu cầu y tế của bản thân và gia đình, so sánh các gói bảo hiểm hiện có, và lựa chọn một gói có mức phí hợp lý mà vẫn đảm bảo quyền lợi tốt nhất. Cần xem xét các điều khoản về quyền lợi bảo hiểm, mức thanh toán và phạm vi bảo hiểm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('be3dfd80-9e3e-4224-ae40-e9a09984220a', 'ecfa0b88-c1ef-4439-8019-76df3085e14e', 'concept', 'Những Sai Lầm Phổ Biến Khi Mua Bảo Hiểm Sức Khỏe', 'Một sai lầm phổ biến là không hiểu rõ các điều khoản và điều kiện của hợp đồng bảo hiểm, dẫn đến việc không nhận được quyền lợi như mong đợi.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người trông bối rối khi đọc hợp đồng bảo hiểm.", "videoScript": "Hãy cùng điểm qua những sai lầm phổ biến mà nhiều người mắc phải khi mua bảo hiểm sức khỏe để bạn có thể tránh xa.", "imageDescription": "Người đó đang xem xét nội dung của một bản hợp đồng bảo hiểm sức khỏe với vẻ mặt khó hiểu.", "videoDescription": "Video trình bày những sai lầm thường gặp khi mua bảo hiểm sức khỏe và cách để tránh chúng."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:00:06.703034', '2026-01-27 13:00:06.703034', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhiều người không đọc kỹ điều khoản bảo hiểm.", "detailed": "Một sai lầm phổ biến là không hiểu rõ các điều khoản và điều kiện của hợp đồng bảo hiểm, dẫn đến việc không nhận được quyền lợi như mong đợi.", "comprehensive": "Khi mua bảo hiểm sức khỏe, nhiều người thường mắc phải những sai lầm như không đọc kỹ điều khoản, chọn gói bảo hiểm không phù hợp với nhu cầu, hoặc chỉ quan tâm đến mức phí mà không xem xét các quyền lợi. Những sai lầm này có thể gây ra những hậu quả tài chính nghiêm trọng trong tương lai."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('30568b74-1531-4aa4-9442-1ce1ff740ab9', '00bbe76d-6014-485e-890f-1398b6100e68', 'example', 'Luyện Tập Kỹ Thuật Ném Bóng Tại Nhà', '# Luyện Tập Kỹ Thuật Ném Bóng Tại Nhà\n\n## Tình Huống\nBạn không có thời gian đến sân bóng nhưng vẫn muốn luyện tập kỹ thuật ném bóng.\n\n## Vấn Đề Cần Giải Quyết\nLàm thế nào để luyện tập một cách hiệu quả tại nhà mà không cần phải có một sân bóng hoàn chỉnh?\n\n## Giải Pháp Từng Bước\n1. **Sử Dụng Một Bức Tường**: Tìm một bức tường phẳng để ném bóng vào, điều này giúp bạn luyện tập kỹ thuật ném mà không cần rổ.\n2. **Thực Hiện Các Cú Ném**: Ném bóng vào tường nhiều lần, chú ý vào kỹ thuật và cảm giác khi bóng quay về.\n3. **Luyện Tập Cảm Xúc**: Tập trung vào cảm giác của mỗi cú ném và cố gắng giữ tâm lý ổn định.\n4. **Ghi Nhận Kết Quả**: Ghi lại số lần thành công và thất bại để theo dõi tiến bộ.\n\n## Kết Quả Và Phân Tích Kết Quả\nLuyện tập tại nhà có thể giúp bạn cải thiện kỹ năng mà không cần phải ra ngoài. Nếu bạn ghi nhận được sự tiến bộ, điều này sẽ giúp bạn tự tin hơn khi trở lại sân bóng.\n\n## Bài Học Rút Ra\nLuyện tập có thể diễn ra ở bất kỳ đâu, chỉ cần bạn có một trái bóng và không gian phù hợp.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "A basketball practice scene at home, featuring a person throwing a basketball against a flat wall. The wall has annotations showing the key techniques of shooting, including stance, grip, and follow-through motions. The environment is cozy, with a basketball on the floor and a checklist for tracking progress beside it. The style is educational and professional, focusing on clarity and instructional details.", "videoScript": "Chào mừng các bạn đến với bài học về kỹ thuật ném bóng tại nhà. Hôm nay, chúng ta sẽ khám phá cách luyện tập hiệu quả mà không cần phải ra sân. Bước đầu tiên, hãy tìm một bức tường phẳng. Điều này giúp bạn luyện tập cú ném mà không cần rổ. Tiếp theo, ném bóng vào tường nhiều lần, chú ý đến kỹ thuật và cảm giác khi bóng quay về. Khi thực hiện cú ném, hãy giữ cho tâm lý ổn định và cảm nhận từng cú ném. Cuối cùng, đừng quên ghi lại số lần bạn thành công và thất bại, điều này sẽ giúp bạn theo dõi tiến bộ. Luyện tập đúng cách sẽ giúp bạn tự tin hơn khi trở lại sân bóng. Hãy nhớ rằng, luyện tập có thể diễn ra ở bất kỳ đâu, chỉ cần bạn có một trái bóng và không gian phù hợp.", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa một người đang luyện tập kỹ thuật ném bóng tại nhà, ném bóng vào tường. Có các chú thích ghi rõ kỹ thuật ném và không gian dễ chịu xung quanh.", "videoDescription": "Video hướng dẫn luyện tập kỹ thuật ném bóng tại nhà, giúp bạn cải thiện kỹ năng mà không cần đến sân bóng."}', 3, '{"xp": 15, "coin": 2}', NULL, '2026-01-12 14:19:41.98729', '2026-01-27 11:33:31.600571', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Luyện Tập Kỹ Thuật Ném Bóng Tại Nhà\n\n## Tình Huống\nBạn không thể ra sân bóng nhưng vẫn muốn luyện tập ném bóng.\n\n## Giải Pháp Đơn Giản\n- **Tìm Một Bức Tường**: Chọn một bức tường phẳng để ném bóng vào.\n- **Ném Bóng**: Thực hiện nhiều cú ném vào tường, chú ý đến cách ném.\n- **Cảm Nhận**: Tập trung vào cảm giác khi bóng quay về.\n- **Theo Dõi Kết Quả**: Ghi lại số lần ném thành công và thất bại.\n\n## Kết Quả\nLuyện tập tại nhà giúp bạn cải thiện kỹ năng ném bóng mà không cần ra ngoài. Nếu bạn thấy tiến bộ, bạn sẽ tự tin hơn khi trở lại sân bóng.", "detailed": "# Luyện Tập Kỹ Thuật Ném Bóng Tại Nhà\n\n## Tình Huống\nNhiều người không có thời gian hoặc điều kiện để đến sân bóng nhưng vẫn muốn cải thiện kỹ năng ném bóng. Việc luyện tập tại nhà là một giải pháp khả thi và hiệu quả.\n\n## Giải Pháp Từng Bước\n1. **Sử Dụng Một Bức Tường**: Tìm một bức tường phẳng và chắc chắn, nơi bạn có thể ném bóng mà không sợ làm hỏng đồ đạc.\n2. **Thực Hiện Các Cú Ném**: Ném bóng nhiều lần vào tường. Chú ý vào kỹ thuật ném, như cách bạn giữ bóng, cách bạn xoay cổ tay khi ném.\n3. **Luyện Tập Cảm Xúc**: Tập trung vào cảm giác của mỗi cú ném. Cố gắng giữ tâm lý bình tĩnh và tự tin.\n4. **Ghi Nhận Kết Quả**: Theo dõi số lần ném thành công và thất bại để đánh giá sự tiến bộ của bản thân.\n\n## Kết Quả Và Phân Tích\nLuyện tập tại nhà giúp bạn cải thiện kỹ năng ném bóng mà không cần phải ra ngoài. Nếu bạn thấy mình tiến bộ, điều đó sẽ tạo động lực cho bạn trở lại sân bóng và thi đấu với tự tin hơn.\n\n## Bài Học Rút Ra\nLuyện tập có thể diễn ra ở bất kỳ đâu, chỉ cần bạn có một trái bóng và một không gian thích hợp để ném.", "comprehensive": "# Luyện Tập Kỹ Thuật Ném Bóng Tại Nhà\n\n## Tình Huống\nTrong bối cảnh bận rộn hiện nay, không phải ai cũng có thời gian hoặc cơ hội để đến sân bóng rổ. Tuy nhiên, việc luyện tập kỹ thuật ném bóng tại nhà hoàn toàn khả thi và có thể mang lại hiệu quả cao nếu thực hiện đúng cách.\n\n## Vấn Đề Cần Giải Quyết\nLàm thế nào để luyện tập ném bóng một cách hiệu quả tại nhà mà không cần rổ và không gian sân bóng hoàn chỉnh? Câu trả lời nằm ở việc sử dụng các phương tiện có sẵn để cải thiện kỹ năng ném.\n\n## Giải Pháp Từng Bước\n1. **Sử Dụng Một Bức Tường**: Tìm một bức tường phẳng và chắc chắn. Điều này cho phép bạn ném bóng vào tường và nhận lại bóng. Hãy chắc chắn rằng khu vực xung quanh không có vật cản hoặc đồ đạc dễ hỏng.\n2. **Thực Hiện Các Cú Ném**: Khi ném, chú ý đến từng phần trong kỹ thuật ném. Cách bạn giữ bóng, cách bạn xoay cổ tay, và vị trí đứng đều ảnh hưởng đến độ chính xác và lực ném. Hãy thử nhiều góc ném khác nhau để thấy cách bóng phản hồi.\n3. **Luyện Tập Cảm Xúc**: Khi ném bóng, tập trung vào cảm giác mà bạn nhận được từ từng cú ném. Giữ tâm lý ổn định và tự tin, điều này sẽ giúp bạn cải thiện khả năng kiểm soát cảm xúc khi thi đấu.\n4. **Ghi Nhận Kết Quả**: Ghi lại số lần thành công và thất bại để theo dõi tiến bộ của bản thân. Việc này không chỉ giúp bạn nhận thức rõ hơn về sự cải thiện mà còn tạo động lực cho bạn trong quá trình luyện tập.\n\n## Kết Quả Và Phân Tích Kết Quả\nViệc luyện tập tại nhà không chỉ giúp bạn cải thiện kỹ năng ném mà còn giúp bạn duy trì phong độ mà không cần ra ngoài. Nếu bạn nhận thấy sự tiến bộ qua thời gian, điều này sẽ giúp bạn tự tin hơn khi trở lại sân bóng, sẵn sàng thi đấu với kỹ năng tốt hơn.\n\n## Bài Học Rút Ra\nLuyện tập có thể diễn ra ở bất kỳ đâu, miễn là bạn có một trái bóng và không gian đủ để ném. Hãy nhớ rằng, sự kiên nhẫn và quyết tâm là chìa khóa để đạt được những kỹ năng tốt trong bóng rổ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('be6b5e49-b3ad-4171-8b8d-797d944d793a', 'ecfa0b88-c1ef-4439-8019-76df3085e14e', 'concept', 'Tương Lai Của Bảo Hiểm Sức Khỏe', 'Với sự phát triển của công nghệ và nhu cầu chăm sóc sức khỏe ngày càng cao, bảo hiểm sức khỏe sẽ có những thay đổi lớn trong tương lai.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bác sĩ sử dụng công nghệ cao trong điều trị bệnh.", "videoScript": "Bảo hiểm sức khỏe không chỉ là một sản phẩm tài chính mà còn là một phần quan trọng trong tương lai của chăm sóc sức khỏe. Hãy cùng khám phá những xu hướng mới trong ngành.", "imageDescription": "Bác sĩ đang sử dụng thiết bị công nghệ cao để theo dõi sức khỏe bệnh nhân.", "videoDescription": "Video khám phá những xu hướng và cải tiến trong bảo hiểm sức khỏe tương lai, bao gồm công nghệ và các mô hình bảo hiểm mới."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:00:06.707498', '2026-01-27 13:00:06.707498', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bảo hiểm sức khỏe đang ngày càng quan trọng.", "detailed": "Với sự phát triển của công nghệ và nhu cầu chăm sóc sức khỏe ngày càng cao, bảo hiểm sức khỏe sẽ có những thay đổi lớn trong tương lai.", "comprehensive": "Trong tương lai, bảo hiểm sức khỏe dự kiến sẽ có nhiều cải tiến với sự xuất hiện của công nghệ mới như telemedicine, trí tuệ nhân tạo trong chẩn đoán và điều trị, và các mô hình bảo hiểm linh hoạt hơn để đáp ứng nhu cầu đa dạng của người dân. Điều này sẽ giúp cải thiện khả năng tiếp cận dịch vụ y tế và chất lượng chăm sóc sức khỏe cho mọi người."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('600179e3-0704-4947-be20-4be6ba345f3f', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'concept', 'Các loại dribble cơ bản', '# Các Loại Dribble Cơ Bản\n\nCó nhiều loại dribble khác nhau, mỗi loại có ứng dụng riêng trong trận đấu. Việc nắm vững các loại dribble này sẽ giúp bạn trở thành cầu thủ linh hoạt hơn.\n\n## Giới thiệu khái niệm\n\nCác loại dribble cơ bản bao gồm: dribble đơn giản, crossover, behind-the-back, và spin move. Mỗi loại đều có cách thực hiện và ứng dụng trong các tình huống khác nhau.\n\n## Giải thích chi tiết với các bước/điểm chính\n\n### 1. Dribble đơn giản\n- **Thực hiện**: Đập bóng xuống mặt đất với một tay, giữ đầu cao và tập trung vào hướng đi.\n- **Ứng dụng**: Sử dụng trong các tình huống di chuyển thẳng hoặc khi không có áp lực từ đối thủ.\n\n### 2. Crossover\n- **Thực hiện**: Đập bóng từ tay này sang tay kia một cách nhanh chóng, tạo ra sự bất ngờ cho đối thủ.\n- **Ứng dụng**: Khi muốn đổi hướng đột ngột và đánh lừa đối thủ.\n\n### 3. Behind-the-back\n- **Thực hiện**: Đập bóng ra sau lưng với một tay và lấy bóng bằng tay còn lại.\n- **Ứng dụng**: Khi bị đối thủ áp sát từ trước và muốn vượt qua mà không để họ chặn bóng.\n\n### 4. Spin move\n- **Thực hiện**: Xoay người về phía tay còn lại trong khi giữ bóng gần cơ thể.\n- **Ứng dụng**: Khi bạn bị đối thủ chặn đường và muốn thoát ra khỏi tình huống đó.\n\n## Ví dụ minh họa cụ thể và chi tiết\n- **Dribble đơn giản**: Hình ảnh mô tả cách thực hiện dribble đơn giản.\n- **Crossover**: Ví dụ về cầu thủ thực hiện crossover để vượt qua đối thủ.\n- **Behind-the-back**: Hình ảnh mô tả cách giữ bóng khi thực hiện kỹ thuật này.\n- **Spin move**: Ví dụ về cách xoay người và di chuyển ra khỏi áp lực.\n\n## Lưu ý quan trọng và tips\n- **Thực hành từng loại một**: Tập trung vào từng loại dribble và luyện tập cho đến khi bạn cảm thấy thoải mái.\n- **Kết hợp các loại dribble**: Khi đã thành thạo, hãy thử kết hợp các loại dribble khác nhau trong một tình huống cụ thể.\n\n## Ứng dụng thực tế và case studies\nCầu thủ như Allen Iverson nổi tiếng với kỹ thuật crossover, trong khi Kobe Bryant ưa chuộng spin move. Họ đã sử dụng các kỹ thuật này để ghi điểm trong những tình huống căng thẳng nhất.\n\n## Tóm tắt và bài tập tự luyện\nViệc nắm vững các loại dribble cơ bản sẽ giúp bạn cải thiện khả năng kiểm soát bóng và tăng tính linh hoạt trong trận đấu. Hãy thực hiện từng loại dribble mỗi ngày để nâng cao kỹ năng của mình.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "Create an educational and professional style image illustrating the four basic types of basketball dribbles: simple dribble, crossover, behind-the-back, and spin move. Each dribble should be clearly labeled with annotations indicating the execution steps and applications in real game scenarios. The image should feature a basketball court background with a player demonstrating each type of dribble, and visual aids that highlight key points and movements.", "videoScript": "Chào mừng các bạn đến với bài học hôm nay về các loại dribble cơ bản trong bóng rổ. Trong video này, chúng ta sẽ cùng nhau khám phá bốn loại dribble quan trọng: dribble đơn giản, crossover, behind-the-back, và spin move.\n\nĐầu tiên, hãy bắt đầu với dribble đơn giản. Kỹ thuật này bao gồm việc đập bóng xuống mặt đất bằng một tay. Bạn cần giữ đầu cao và tập trung vào hướng đi. Hãy sử dụng kỹ thuật này khi bạn di chuyển thẳng và không có áp lực từ đối thủ.\n\nTiếp theo là crossover. Đây là một kỹ thuật đòi hỏi bạn phải đập bóng từ tay này sang tay kia một cách nhanh chóng, nhằm tạo ra sự bất ngờ cho đối thủ. Hãy tập luyện để thực hiện động tác này một cách mượt mà để có thể thay đổi hướng đột ngột.\n\nChúng ta tiếp tục với behind-the-back. Kỹ thuật này giúp bạn vượt qua đối thủ đang áp sát từ phía trước. Để thực hiện, bạn sẽ đập bóng ra sau lưng bằng một tay và bắt bóng bằng tay còn lại. Điều này giúp bạn giữ bóng an toàn hơn khi bị áp lực.\n\nCuối cùng, là spin move. Để thực hiện, bạn hãy xoay người về phía tay còn lại trong khi giữ bóng gần cơ thể. Kỹ thuật này rất hữu ích khi bạn đang bị chặn đường di chuyển.\n\nĐừng quên thực hành từng loại dribble một cách đều đặn và sau đó thử kết hợp chúng để nâng cao kỹ năng của bạn.<br> Cảm ơn các bạn đã theo dõi, và hãy bắt đầu luyện tập ngay hôm nay!", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh minh họa các loại dribble cơ bản trong bóng rổ, bao gồm dribble đơn giản, crossover, behind-the-back, và spin move, với các chú thích và hướng dẫn thực hiện.", "videoDescription": "Video này giúp bạn khám phá các loại dribble cơ bản trong bóng rổ, hướng dẫn cụ thể cách thực hiện và ứng dụng trong thực tế."}', 3, '{"xp": 10, "coin": 1}', NULL, '2026-01-12 14:20:48.998263', '2026-01-27 11:37:18.002136', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "# Các Loại Dribble Cơ Bản\n\nTrong bóng rổ, dribble là kỹ thuật quan trọng giúp bạn điều khiển bóng. Có 4 loại dribble cơ bản mà bạn cần biết:\n\n## 1. Dribble đơn giản\n- **Cách thực hiện**: Đập bóng xuống đất bằng một tay, giữ đầu cao.\n- **Khi dùng**: Khi bạn di chuyển thẳng và không bị áp lực từ đối thủ.\n\n## 2. Crossover\n- **Cách thực hiện**: Chuyển bóng từ tay này sang tay kia nhanh chóng.\n- **Khi dùng**: Khi bạn muốn đổi hướng và đánh lừa đối thủ.\n\n## 3. Behind-the-back\n- **Cách thực hiện**: Đập bóng ra sau lưng và lấy bằng tay còn lại.\n- **Khi dùng**: Khi đối thủ áp sát bạn từ trước.\n\n## 4. Spin move\n- **Cách thực hiện**: Xoay người trong khi giữ bóng gần cơ thể.\n- **Khi dùng**: Khi bạn bị chặn và muốn thoát ra.\n\nHãy thực hành từng loại để cải thiện kỹ năng của bạn!", "detailed": "# Các Loại Dribble Cơ Bản\n\nDribble là một trong những kỹ thuật quan trọng nhất trong bóng rổ. Nắm vững các loại dribble cơ bản sẽ giúp bạn trở thành cầu thủ linh hoạt hơn trên sân.\n\n## Giới thiệu khái niệm\nCác loại dribble cơ bản bao gồm: dribble đơn giản, crossover, behind-the-back, và spin move. Mỗi loại có cách thực hiện và ứng dụng khác nhau.\n\n## 1. Dribble đơn giản\n- **Cách thực hiện**: Đập bóng xuống đất với một tay, giữ đầu cao và nhìn về phía trước. Điều này giúp bạn kiểm soát hướng đi.\n- **Ứng dụng**: Thích hợp khi bạn di chuyển thẳng hoặc không có áp lực từ đối thủ.\n\n## 2. Crossover\n- **Cách thực hiện**: Đập bóng từ tay này sang tay kia một cách nhanh chóng, tạo ra sự bất ngờ cho đối thủ.\n- **Ứng dụng**: Sử dụng khi bạn muốn đổi hướng đột ngột để đánh lừa đối thủ.\n\n## 3. Behind-the-back\n- **Cách thực hiện**: Đập bóng ra sau lưng với một tay và dùng tay còn lại để lấy bóng.\n- **Ứng dụng**: Khi bạn bị đối thủ áp sát từ trước và cần vượt qua mà không để họ chặn bóng.\n\n## 4. Spin move\n- **Cách thực hiện**: Xoay người về phía tay còn lại trong khi giữ bóng gần cơ thể.\n- **Ứng dụng**: Khi bị đối thủ chặn đường và muốn thoát khỏi tình huống đó.\n\n## Ví dụ minh họa\n- **Dribble đơn giản**: Hình ảnh mô tả cách thực hiện dribble đơn giản.\n- **Crossover**: Hình ảnh cầu thủ thực hiện crossover.\n- **Behind-the-back**: Hình ảnh mô tả cách giữ bóng khi thực hiện kỹ thuật này.\n- **Spin move**: Hình ảnh thực hiện spin move.\n\n## Lưu ý quan trọng\n- **Thực hành từng loại một**: Đảm bảo bạn nắm vững từng loại dribble.\n- **Kết hợp các loại dribble**: Khi đã quen, hãy thử kết hợp chúng trong các tình huống khác nhau.\n\nViệc nắm vững các loại dribble này sẽ giúp bạn cải thiện khả năng kiểm soát bóng và nâng cao kỹ năng chơi bóng rổ của mình.", "comprehensive": "# Các Loại Dribble Cơ Bản\n\nDribble không chỉ là một kỹ thuật, mà còn là một phần thiết yếu của chiến lược trong bóng rổ. Nắm vững các loại dribble cơ bản giúp bạn tự tin hơn khi đối mặt với các tình huống khác nhau trên sân. Dưới đây là cái nhìn sâu sắc hơn về từng loại dribble và cách chúng có thể được áp dụng một cách hiệu quả.\n\n## Giới thiệu khái niệm\nCó 4 loại dribble cơ bản mà mọi cầu thủ bóng rổ nên biết: dribble đơn giản, crossover, behind-the-back, và spin move. Mỗi loại có thể được sử dụng trong các tình huống khác nhau, từ việc vượt qua đối thủ đến việc bảo vệ bóng.\n\n## 1. Dribble đơn giản\n- **Cách thực hiện**: Đập bóng xuống mặt đất bằng một tay, giữ đầu cao và mắt nhìn về phía trước. Điều này không chỉ giúp bạn kiểm soát bóng mà còn giúp bạn theo dõi đồng đội và đối thủ.\n- **Ứng dụng**: Thực hiện khi bạn di chuyển thẳng mà không có áp lực. Điều này cũng rất hữu ích trong việc thiết lập các tình huống tấn công.\n- **Tips**: Hãy thực hành dribble đơn giản khi di chuyển trong không gian rộng để cải thiện cảm giác bóng.\n\n## 2. Crossover\n- **Cách thực hiện**: Đập bóng từ tay này sang tay kia một cách nhanh chóng. Đảm bảo rằng bạn giữ bóng gần cơ thể để tránh bị chặn.\n- **Ứng dụng**: Sử dụng khi bạn cần thay đổi hướng đột ngột để vượt qua đối thủ. Đây là một kỹ thuật rất hiệu quả trong việc tạo khoảng trống để ghi điểm.\n- **Ví dụ**: Cầu thủ như Allen Iverson nổi tiếng với kỹ thuật crossover, tạo ra sự bất ngờ cho đối thủ.\n\n## 3. Behind-the-back\n- **Cách thực hiện**: Đập bóng ra sau lưng với một tay, sau đó lấy bóng bằng tay còn lại. Kỹ thuật này yêu cầu sự linh hoạt và kiểm soát tốt.\n- **Ứng dụng**: Khi bạn bị đối thủ áp sát từ trước và cần vượt qua mà không để họ chặn bóng. Đây là kỹ thuật rất hữu ích trong những tình huống khẩn cấp.\n- **Tips**: Thực hành trong khi di chuyển để cải thiện sự nhạy bén và phản xạ.\n\n## 4. Spin move\n- **Cách thực hiện**: Xoay người về phía tay còn lại trong khi giữ bóng gần cơ thể. Kỹ thuật này giúp bạn thoát khỏi sự chặn đường của đối thủ.\n- **Ứng dụng**: Khi bạn bị đối thủ chặn đường và cần tìm cách thoát ra.\n- **Ví dụ**: Kobe Bryant là một trong những cầu thủ sử dụng spin move hiệu quả để ghi điểm trong những tình huống căng thẳng.\n\n## Lưu ý quan trọng và best practices\n- **Thực hành từng loại một**: Dành thời gian cho từng loại dribble và luyện tập cho đến khi bạn cảm thấy thoải mái.\n- **Kết hợp các loại dribble**: Khi bạn đã thành thạo, hãy thử kết hợp chúng trong các tình huống cụ thể để tăng tính linh hoạt.\n- **Ghi nhớ**: Tránh lạm dụng một kỹ thuật duy nhất; hãy học cách sử dụng chúng một cách linh hoạt và sáng tạo.\n\n## Ứng dụng thực tế và case studies\nNhiều cầu thủ nổi tiếng đã sử dụng các kỹ thuật này để ghi điểm trong các trận đấu căng thẳng. Ví dụ, Allen Iverson với kỹ thuật crossover đã trở thành một biểu tượng trong bóng rổ, trong khi Kobe Bryant thường xuyên sử dụng spin move để vượt qua các đối thủ.\n\n## Tóm tắt và bài tập tự luyện\nNắm vững các loại dribble cơ bản sẽ giúp bạn cải thiện khả năng kiểm soát bóng và tăng tính linh hoạt trong trận đấu. Hãy thực hiện từng loại dribble mỗi ngày và thử nghiệm các tình huống khác nhau để nâng cao kỹ năng của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9dfacca9-6541-41a9-8bef-58d756c3f465', '5f36b291-05f3-4155-b8b3-0910221c4da6', 'concept', 'Các Loại Hình Bảo Hiểm', 'Các loại hình bảo hiểm phổ biến bao gồm bảo hiểm nhân thọ, bảo hiểm sức khỏe, bảo hiểm xe hơi và bảo hiểm nhà ở. Mỗi loại có những đặc điểm và lợi ích riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa các loại hình bảo hiểm với biểu tượng đặc trưng.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về các loại hình bảo hiểm phổ biến và cách chúng có thể bảo vệ bạn trong các tình huống khác nhau.", "imageDescription": "Bảng biểu diễn các loại hình bảo hiểm với các biểu tượng như trái tim cho bảo hiểm sức khỏe, nhà cho bảo hiểm tài sản.", "videoDescription": "Một video giải thích các loại hình bảo hiểm khác nhau, bao gồm bảo hiểm nhân thọ, bảo hiểm sức khỏe và bảo hiểm tài sản, cùng với lợi ích của từng loại."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:06:25.86115', '2026-01-27 13:06:25.86115', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại hình bảo hiểm như bảo hiểm nhân thọ, bảo hiểm sức khỏe, và bảo hiểm tài sản.", "detailed": "Các loại hình bảo hiểm phổ biến bao gồm bảo hiểm nhân thọ, bảo hiểm sức khỏe, bảo hiểm xe hơi và bảo hiểm nhà ở. Mỗi loại có những đặc điểm và lợi ích riêng.", "comprehensive": "Bảo hiểm nhân thọ giúp bảo vệ tài chính cho gia đình khi người tham gia không còn sống. Bảo hiểm sức khỏe chi trả chi phí y tế, trong khi bảo hiểm tài sản bảo vệ tài sản cá nhân khỏi thiệt hại do thiên tai hoặc tai nạn. Hiểu rõ các loại hình bảo hiểm giúp người tiêu dùng lựa chọn phù hợp với nhu cầu của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d1f6ec35-1edc-41d8-bde2-21121268e45d', 'a1618b25-304d-43be-b2e3-bd7613475485', 'concept', 'Sử Dụng Hợp Âm Ngẫu Hứng', 'Việc sử dụng hợp âm ngẫu hứng có thể giúp bạn tạo ra những âm điệu độc đáo và phong phú cho bản nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bảng hợp âm piano với các nốt nhạc được đánh dấu nổi bật.", "videoScript": "Hãy cùng nhau khám phá một số hợp âm thú vị mà bạn có thể sử dụng để làm phong phú thêm âm nhạc của mình. Chúng ta sẽ làm việc với các hợp âm cơ bản và cách kết hợp chúng.", "imageDescription": "Hình ảnh của một bảng hợp âm piano với các nốt nhạc, giúp người học dễ dàng hình dung cách kết hợp hợp âm.", "videoDescription": "Video hướng dẫn cách sử dụng các hợp âm để tạo ra âm nhạc ngẫu hứng, bao gồm ví dụ minh họa và bài tập thực hành."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:33.689789', '2026-01-27 12:42:33.689789', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách tạo ra hợp âm ngẫu hứng để làm phong phú thêm bản nhạc.", "detailed": "Việc sử dụng hợp âm ngẫu hứng có thể giúp bạn tạo ra những âm điệu độc đáo và phong phú cho bản nhạc.", "comprehensive": "Trong bài học này, chúng ta sẽ tìm hiểu về cách sử dụng hợp âm ngẫu hứng, cách kết hợp chúng với giai điệu và tạo ra những chuyển biến trong âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5ff4b9a4-3e88-4e34-a223-fe0234ce2260', '58b08221-9d48-4974-a0c0-6c59ee91e301', 'concept', 'Những sai lầm thường gặp khi dribble', '# Những Sai Lầm Thường Gặp Khi Dribble\n\nNắm rõ những sai lầm này sẽ giúp bạn cải thiện kỹ thuật dribble và tránh những vấn đề không đáng có trong trận đấu.\n\n## Giới thiệu khái niệm\n\nSai lầm trong kỹ thuật dribble có thể dẫn đến việc mất bóng hoặc không thể vượt qua đối thủ. Việc nhận diện và sửa chữa những sai lầm này là vô cùng cần thiết.\n\n## Giải thích chi tiết với các bước/điểm chính\n\n### 1. Nhìn xuống bóng\n- **Vấn đề**: Nếu bạn nhìn xuống bóng khi dribble, bạn sẽ không thể quan sát các đối thủ và tình huống xung quanh.\n- **Giải pháp**: Hãy luôn giữ đầu cao và nhìn về phía trước.\n\n### 2. Đập bóng quá mạnh hoặc quá nhẹ\n- **Vấn đề**: Đập bóng quá mạnh có thể khiến bóng bay ra xa, trong khi đập quá nhẹ có thể khiến bạn mất kiểm soát.\n- **Giải pháp**: Thực hành điều chỉnh lực đập bóng để có độ chính xác cao hơn.\n\n### 3. Không sử dụng cả hai tay\n- **Vấn đề**: Nhiều cầu thủ chỉ sử dụng một tay để dribble, điều này làm giảm khả năng linh hoạt.\n- **Giải pháp**: Hãy luyện tập dribble với cả hai tay để trở nên đa dạng hơn trong cách chơi.\n\n### 4. Đứng thẳng khi dribble\n- **Vấn đề**: Tư thế đứng thẳng không giúp bạn giữ thăng bằng và linh hoạt.\n- **Giải pháp**: Hãy khuỵu gối và giữ tư thế chuẩn khi dribble.\n\n## Ví dụ minh họa cụ thể và chi tiết\n- **Hình ảnh mô tả sai lầm**: Các hình ảnh mô tả những sai lầm thường gặp khi dribble.\n\n## Lưu ý quan trọng và tips\n- **Nhận diện sai lầm của bản thân**: Theo dõi quá trình tập luyện và nhận diện các sai lầm của bạn.\n- **Xin ý kiến từ huấn luyện viên**: Hãy hỏi ý kiến từ huấn luyện viên để có những điều chỉnh hợp lý.\n\n## Ứng dụng thực tế và case studies\nNhiều cầu thủ nổi tiếng đã phải đối mặt với những sai lầm này và đã cải thiện kỹ năng của họ thông qua việc nhận thức và tập luyện.\n\n## Tóm tắt và bài tập tự luyện\nHãy lưu ý đến những sai lầm thường gặp khi dribble và thực hiện bài tập tự luyện để cải thiện kỹ năng. Mỗi ngày, hãy tự kiểm tra kỹ thuật của mình để trở nên tốt hơn.', '{"imageUrl": "https://placehold.co/800x600/fef3c7/92400e?text=%F0%9F%93%9A+C%E1%BA%A7n+%C4%90%C3%B3ng+G%C3%B3p+H%C3%ACnh+%E1%BA%A2nh%0A(M%E1%BB%A9c+Chi+Ti%E1%BA%BFt)&font=roboto", "videoUrl": "https://commondatastorage.googleapis.com/gtv-videos-bucket/sample/BigBuckBunny.mp4", "imagePrompt": "Create an educational illustration showing common mistakes in basketball dribbling. Include labeled graphics demonstrating four key mistakes: looking down at the ball, dribbling too hard or too soft, using only one hand, and standing straight while dribbling. Use a clear, professional style suitable for a sports education context. Include annotations that explain each mistake with brief descriptions.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ khám phá những sai lầm thường gặp khi dribble trong bóng rổ và cách khắc phục chúng. Điều đầu tiên là nhìn xuống bóng. Việc này khiến bạn không thể quan sát những gì xảy ra xung quanh. Hãy luôn giữ đầu cao và nhìn về phía trước để phát hiện đối thủ và tạo cơ hội tấn công. Sai lầm thứ hai là đập bóng quá mạnh hoặc quá nhẹ. Điều này có thể dẫn đến mất kiểm soát. Hãy thực hành để điều chỉnh lực đập cho phù hợp! Tiếp theo là việc không sử dụng cả hai tay. Để trở nên linh hoạt và tránh bị bắt bài, hãy luyện tập dribble với cả hai tay. Cuối cùng, đừng đứng thẳng khi dribble, khuỵu gối để giữ thăng bằng và linh hoạt hơn. Đó là những sai lầm thường gặp, hãy tự kiểm tra kỹ thuật dribble của bạn hàng ngày và cải thiện qua từng bài tập.", "videoDuration": "1-2 phút", "imageDescription": "Hình ảnh mô tả những sai lầm thường gặp khi dribble trong bóng rổ, bao gồm việc nhìn xuống bóng, đập bóng không chính xác, chỉ sử dụng một tay, và đứng thẳng khi dribble.", "videoDescription": "Video này giới thiệu những sai lầm chung khi dribble bóng rổ và cách để cải thiện kỹ thuật."}', 5, '{"xp": 10, "coin": 1}', NULL, '2026-01-12 14:20:49.007092', '2026-01-27 11:38:04.31574', 'mixed', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "## Những Sai Lầm Thường Gặp Khi Dribble\n\nKhi chơi bóng rổ, dribble là kỹ thuật quan trọng, nhưng có một số sai lầm mà bạn có thể gặp phải. Dưới đây là những lỗi phổ biến và cách khắc phục:\n\n1. **Nhìn xuống bóng**: Nếu bạn nhìn xuống, bạn sẽ không thấy đối thủ và tình huống xung quanh. Hãy luôn nhìn về phía trước.\n2. **Đập bóng không đúng lực**: Đập quá mạnh có thể làm bóng bay xa, trong khi đập quá nhẹ có thể khiến bạn mất kiểm soát. Hãy luyện tập để điều chỉnh lực đập.\n3. **Chỉ dùng một tay**: Việc này làm bạn kém linh hoạt. Hãy luyện tập dribble với cả hai tay.\n4. **Đứng thẳng**: Tư thế này không giúp bạn giữ thăng bằng. Hãy khuỵu gối để giữ tư thế tốt hơn.\n\nHãy chú ý đến những sai lầm này để cải thiện kỹ năng dribble của bạn!", "detailed": "## Những Sai Lầm Thường Gặp Khi Dribble\n\nKỹ thuật dribble trong bóng rổ rất quan trọng, nhưng nhiều cầu thủ thường mắc phải những sai lầm khiến họ không thể chơi tốt nhất. Dưới đây là những sai lầm phổ biến và cách khắc phục chúng.\n\n### 1. Nhìn xuống bóng\n- **Vấn đề**: Khi nhìn xuống, bạn không thể quan sát đối thủ và tình huống xung quanh, dễ dẫn đến việc mất bóng.\n- **Giải pháp**: Hãy giữ đầu cao và luôn nhìn về phía trước để có thể ra quyết định tốt hơn.\n\n### 2. Đập bóng quá mạnh hoặc quá nhẹ\n- **Vấn đề**: Đập quá mạnh sẽ khiến bóng bay ra xa, trong khi đập quá nhẹ có thể làm bạn mất kiểm soát.\n- **Giải pháp**: Thực hành để điều chỉnh lực đập, đảm bảo độ chính xác khi dribble.\n\n### 3. Không sử dụng cả hai tay\n- **Vấn đề**: Nhiều cầu thủ chỉ dùng một tay, điều này làm giảm khả năng linh hoạt và dễ bị đối thủ bắt bài.\n- **Giải pháp**: Luyện tập dribble với cả hai tay để cải thiện sự đa dạng trong lối chơi.\n\n### 4. Đứng thẳng khi dribble\n- **Vấn đề**: Tư thế đứng thẳng không giúp bạn giữ thăng bằng và linh hoạt.\n- **Giải pháp**: Hãy khuỵu gối và giữ tư thế chuẩn để có thể phản ứng nhanh hơn với tình huống trên sân.\n\n### Lưu ý quan trọng\n- Theo dõi quá trình tập luyện của bạn để nhận diện các sai lầm và cải thiện.\n- Hãy xin ý kiến từ huấn luyện viên để có những điều chỉnh hợp lý.\n\nBằng cách chú ý đến những sai lầm này và thực hành thường xuyên, bạn sẽ cải thiện kỹ năng dribble của mình một cách đáng kể.", "comprehensive": "## Những Sai Lầm Thường Gặp Khi Dribble\n\nDribble là một kỹ thuật cốt lõi trong bóng rổ, và việc nhận diện sai lầm trong kỹ thuật này có thể ảnh hưởng lớn đến hiệu suất thi đấu. Trong bài viết này, chúng ta sẽ đi sâu vào những sai lầm thường gặp khi dribble và cách khắc phục chúng.\n\n### Giới thiệu khái niệm\nSai lầm trong kỹ thuật dribble không chỉ làm bạn mất bóng mà còn tạo cơ hội cho đối thủ tấn công. Việc nhận diện và sửa chữa những sai lầm này là cần thiết để nâng cao khả năng cá nhân và đội bóng.\n\n### 1. Nhìn xuống bóng\n- **Vấn đề**: Nhìn xuống có thể khiến bạn mất phương hướng và không thể quan sát được không gian xung quanh, dẫn đến việc không thể đưa ra các quyết định nhanh chóng trong trận đấu.\n- **Giải pháp**: Hãy tập thói quen giữ đầu cao và nhìn về phía trước. Hãy tưởng tượng bạn đang lái xe; nếu bạn chỉ nhìn xuống, bạn sẽ không thể tránh được chướng ngại vật.\n\n### 2. Đập bóng quá mạnh hoặc quá nhẹ\n- **Vấn đề**: Đập bóng không đúng lực có thể khiến bạn không kiểm soát được bóng. Đập quá mạnh có thể làm bóng bay xa, trong khi đập quá nhẹ có thể khiến bạn bị cướp bóng.\n- **Giải pháp**: Tập luyện điều chỉnh lực đập bóng. Bắt đầu với các bài tập đơn giản, sau đó tăng dần độ khó để làm quen với cảm giác của bóng.\n\n### 3. Không sử dụng cả hai tay\n- **Vấn đề**: Việc chỉ sử dụng một tay khi dribble sẽ làm giảm sự linh hoạt và dễ bị đối thủ bắt bài.\n- **Giải pháp**: Luyện tập dribble với cả hai tay. Một bài tập đơn giản là dribble bằng tay không thuận trong một khoảng thời gian nhất định mỗi ngày.\n\n### 4. Đứng thẳng khi dribble\n- **Vấn đề**: Tư thế đứng thẳng không tạo ra sự ổn định và linh hoạt, khiến bạn dễ bị mất thăng bằng khi đối mặt với áp lực từ đối thủ.\n- **Giải pháp**: Khuỵu gối và giữ tư thế thấp hơn. Tư thế này sẽ giúp bạn phản ứng nhanh hơn và giữ bóng an toàn hơn.\n\n### 5. Nhận diện sai lầm của bản thân\n- **Lưu ý quan trọng**: Theo dõi quá trình tập luyện của bạn để nhận diện các sai lầm và cải thiện. Hãy ghi lại từng buổi tập và phản ánh về kỹ thuật của mình.\n- **Xin ý kiến từ huấn luyện viên**: Đừng ngần ngại hỏi ý kiến từ huấn luyện viên. Họ có thể cung cấp cho bạn những thông tin quý giá giúp bạn điều chỉnh kỹ thuật.\n\n### Ứng dụng thực tế và case studies\nNhiều cầu thủ nổi tiếng như Stephen Curry hay Kyrie Irving đã từng mắc phải những sai lầm này trong quá trình tập luyện. Họ đã nhận ra và cải thiện kỹ năng dribble của mình thông qua việc luyện tập chăm chỉ và nhận sự hỗ trợ từ huấn luyện viên.\n\n### Tóm tắt và bài tập tự luyện\nHãy chú ý đến những sai lầm thường gặp khi dribble. Thực hiện các bài tập tự luyện để cải thiện kỹ năng của mình. Mỗi ngày, hãy tự kiểm tra kỹ thuật của mình và điều chỉnh theo hướng dẫn trên. Bằng cách này, bạn sẽ trở thành một cầu thủ dribble tốt hơn!"}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('5dc92544-89f0-48dd-9a59-5f40116a7b37', '076c48e4-fb7b-4f42-bcc0-56550fcf5c3b', 'concept', 'Giới Thiệu Về Bảo Hiểm Nhân Thọ', 'Bảo hiểm nhân thọ cung cấp sự bảo vệ tài chính cho người thân của bạn trong trường hợp bạn qua đời. Nó bao gồm các khoản tiền bảo hiểm và có thể tích lũy giá trị tiền mặt theo thời gian.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một gia đình hạnh phúc bên nhau.", "videoScript": "Chào mừng đến với bài học về bảo hiểm nhân thọ. Trong video này, chúng ta sẽ khám phá những lợi ích và tầm quan trọng của bảo hiểm nhân thọ trong việc bảo vệ tài chính cho gia đình bạn.", "imageDescription": "Một gia đình đang cười đùa trong công viên, tượng trưng cho sự an tâm và bảo vệ tài chính.", "videoDescription": "Video giới thiệu về bảo hiểm nhân thọ, giải thích các khái niệm cơ bản và lợi ích của việc sở hữu bảo hiểm nhân thọ."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:00:36.358323', '2026-01-27 13:00:36.358323', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bảo hiểm nhân thọ là một dạng bảo hiểm giúp bảo vệ tài chính cho gia đình bạn khi bạn không còn.", "detailed": "Bảo hiểm nhân thọ cung cấp sự bảo vệ tài chính cho người thân của bạn trong trường hợp bạn qua đời. Nó bao gồm các khoản tiền bảo hiểm và có thể tích lũy giá trị tiền mặt theo thời gian.", "comprehensive": "Bảo hiểm nhân thọ không chỉ bảo vệ gia đình bạn về mặt tài chính khi bạn không còn nữa, mà còn có thể là một công cụ tiết kiệm và đầu tư. Các loại bảo hiểm như toàn phần, một phần và tạm thời sẽ giúp đáp ứng nhu cầu tài chính khác nhau của bạn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('37bf43e1-4af3-439c-8c4b-4854c0357b01', 'ea70d998-1fa8-4885-9a12-7713df8fca34', 'concept', 'Lợi Ích Của Tẩy Tế Bào Chết', 'Lợi ích của tẩy tế bào chết bao gồm làm sạch sâu, ngăn ngừa mụn, và cải thiện độ đàn hồi của da.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh trước và sau khi tẩy tế bào chết.", "videoScript": "Trong video này, chúng ta sẽ cùng xem xét lợi ích của việc tẩy tế bào chết hàng tuần. Bạn sẽ thấy làn da của mình thay đổi thế nào.", "imageDescription": "So sánh hình ảnh làn da trước và sau khi tẩy tế bào chết, cho thấy sự khác biệt rõ rệt.", "videoDescription": "Video trình bày những lợi ích nổi bật của việc tẩy tế bào chết cho làn da."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:13:42.155907', '2026-01-27 13:13:42.155907', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tẩy tế bào chết giúp da sáng hơn và ngăn ngừa mụn.", "detailed": "Lợi ích của tẩy tế bào chết bao gồm làm sạch sâu, ngăn ngừa mụn, và cải thiện độ đàn hồi của da.", "comprehensive": "Tẩy tế bào chết giúp loại bỏ tế bào chết, ngăn ngừa mụn, làm sáng da, và cải thiện khả năng hấp thụ các sản phẩm dưỡng da khác. Nó cũng giúp da trở nên mịn màng và tươi trẻ hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d54c0b1c-ba8a-45f2-bad1-74a3583910b1', 'a1618b25-304d-43be-b2e3-bd7613475485', 'concept', 'Tạo Ra Giai Điệu Tự Do', 'Giai điệu tự do cho phép bạn thể hiện cảm xúc và ý tưởng của mình một cách tự nhiên.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ đang sáng tác giai điệu trên piano với ánh sáng vàng ấm áp.", "videoScript": "Chào mừng các bạn đến với bài học về sáng tác giai điệu tự do. Hôm nay, chúng ta sẽ cùng nhau khám phá cách phát triển và biến tấu các giai điệu.", "imageDescription": "Hình ảnh của một nghệ sĩ đang ngồi trước piano, tập trung vào việc sáng tác một giai điệu mới.", "videoDescription": "Video hướng dẫn cách sáng tác giai điệu tự do trên piano, bao gồm các kỹ thuật và bài tập giúp phát triển khả năng sáng tạo."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:33.693627', '2026-01-27 12:42:33.693627', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách sáng tạo giai điệu tự do trên piano.", "detailed": "Giai điệu tự do cho phép bạn thể hiện cảm xúc và ý tưởng của mình một cách tự nhiên.", "comprehensive": "Trong bài học này, chúng ta sẽ tìm hiểu về cách phát triển giai điệu tự do, từ việc sử dụng các nốt nhạc đến việc biến tấu chúng để tạo ra âm nhạc độc đáo."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8bbcf91a-f5db-4cd2-81eb-fd0a7adb8f87', '076c48e4-fb7b-4f42-bcc0-56550fcf5c3b', 'concept', 'Lợi Ích Của Bảo Hiểm Nhân Thọ', 'Lợi ích lớn nhất của bảo hiểm nhân thọ là sự an tâm. Khi bạn có bảo hiểm, bạn sẽ biết rằng gia đình bạn sẽ không gặp khó khăn tài chính nếu có chuyện xảy ra với bạn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh người phụ nữ đang suy nghĩ về tương lai.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu về những lợi ích của bảo hiểm nhân thọ. Hãy cùng khám phá cách mà bảo hiểm nhân thọ có thể giúp bạn và gia đình bạn.", "imageDescription": "Một người phụ nữ đang ngồi trong một công viên, nhìn xa xăm về tương lai, thể hiện sự lo lắng và mong muốn an toàn tài chính.", "videoDescription": "Video giải thích các lợi ích của bảo hiểm nhân thọ, bao gồm sự an tâm tài chính và khả năng tích lũy tài sản."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:00:36.366097', '2026-01-27 13:00:36.366097', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bảo hiểm nhân thọ giúp bạn bảo vệ tài chính cho những người thân yêu.", "detailed": "Lợi ích lớn nhất của bảo hiểm nhân thọ là sự an tâm. Khi bạn có bảo hiểm, bạn sẽ biết rằng gia đình bạn sẽ không gặp khó khăn tài chính nếu có chuyện xảy ra với bạn.", "comprehensive": "Ngoài việc bảo vệ tài chính cho gia đình, bảo hiểm nhân thọ còn có thể giúp bạn tích lũy tài sản, cung cấp các khoản tiền cho việc học hành của con cái, và thậm chí có thể dùng làm tài sản thế chấp cho các khoản vay trong tương lai."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f25a6f43-23a1-407e-bfff-cb0f699ee7f8', '076c48e4-fb7b-4f42-bcc0-56550fcf5c3b', 'concept', 'Các Loại Bảo Hiểm Nhân Thọ', 'Bảo hiểm nhân thọ có ba loại chính: bảo hiểm toàn phần, bảo hiểm một phần và bảo hiểm tạm thời. Mỗi loại có những đặc điểm và lợi ích riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Biểu đồ so sánh các loại bảo hiểm nhân thọ.", "videoScript": "Chào mừng đến với bài học về các loại bảo hiểm nhân thọ. Hãy cùng nhau phân tích sự khác biệt giữa bảo hiểm toàn phần, một phần và tạm thời.", "imageDescription": "Một biểu đồ so sánh ba loại bảo hiểm nhân thọ với các đặc điểm nổi bật của từng loại.", "videoDescription": "Video trình bày các loại bảo hiểm nhân thọ, giải thích chi tiết về từng loại và giúp người xem lựa chọn loại phù hợp."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:00:36.369953', '2026-01-27 13:00:36.369953', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại bảo hiểm nhân thọ khác nhau như toàn phần, một phần và tạm thời.", "detailed": "Bảo hiểm nhân thọ có ba loại chính: bảo hiểm toàn phần, bảo hiểm một phần và bảo hiểm tạm thời. Mỗi loại có những đặc điểm và lợi ích riêng.", "comprehensive": "Bảo hiểm toàn phần cung cấp bảo vệ suốt đời và tích lũy giá trị tiền mặt. Bảo hiểm một phần cũng cung cấp bảo vệ lâu dài nhưng với chi phí thấp hơn và ít tích lũy. Bảo hiểm tạm thời cung cấp bảo vệ trong một khoảng thời gian nhất định và thường có chi phí thấp nhất."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('dd068707-8f00-4a34-9c43-c17711904e4d', 'a1618b25-304d-43be-b2e3-bd7613475485', 'concept', 'Kết Hợp Cảm Xúc Với Âm Nhạc', 'Âm nhạc là ngôn ngữ của cảm xúc, và việc kết hợp cảm xúc vào việc chơi nhạc là rất quan trọng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ đang chơi piano với ánh mắt đầy cảm xúc, ánh sáng dịu nhẹ.", "videoScript": "Hãy cùng nhau khám phá cách để cảm xúc của bạn có thể được truyền tải qua âm nhạc. Chúng ta sẽ làm việc với các kỹ thuật thể hiện và cách chọn nốt nhạc phù hợp.", "imageDescription": "Hình ảnh của một nghệ sĩ đang thể hiện cảm xúc mạnh mẽ qua âm nhạc trên piano.", "videoDescription": "Video hướng dẫn cách kết hợp cảm xúc vào âm nhạc, bao gồm các kỹ thuật thể hiện và ví dụ minh họa."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:33.698728', '2026-01-27 12:42:33.698728', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách truyền tải cảm xúc qua âm nhạc.", "detailed": "Âm nhạc là ngôn ngữ của cảm xúc, và việc kết hợp cảm xúc vào việc chơi nhạc là rất quan trọng.", "comprehensive": "Trong bài học này, chúng ta sẽ tìm hiểu cách truyền tải cảm xúc qua âm nhạc, từ việc lựa chọn hợp âm đến cách thể hiện trên piano."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('344065a3-ca42-4e4a-ac91-bb3ade172b24', 'ea70d998-1fa8-4885-9a12-7713df8fca34', 'concept', 'Cách Chọn Sản Phẩm Tẩy Tế Bào Chết', 'Khi chọn sản phẩm tẩy tế bào chết, cần xem xét loại da của bạn. Da nhạy cảm nên chọn sản phẩm dịu nhẹ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các sản phẩm tẩy tế bào chết khác nhau.", "videoScript": "Chào các bạn! Hôm nay, chúng ta sẽ cùng tìm hiểu cách chọn sản phẩm tẩy tế bào chết thích hợp cho từng loại da.", "imageDescription": "Một bảng sản phẩm tẩy tế bào chết từ nhiều thương hiệu khác nhau, thể hiện sự đa dạng trong lựa chọn.", "videoDescription": "Video hướng dẫn cách chọn sản phẩm tẩy tế bào chết phù hợp với nhu cầu của từng loại da."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:13:42.160276', '2026-01-27 13:13:42.160276', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn sản phẩm phù hợp với loại da của bạn.", "detailed": "Khi chọn sản phẩm tẩy tế bào chết, cần xem xét loại da của bạn. Da nhạy cảm nên chọn sản phẩm dịu nhẹ.", "comprehensive": "Để chọn sản phẩm tẩy tế bào chết phù hợp, bạn cần xác định loại da của mình. Da dầu có thể thích sản phẩm có chứa AHA, trong khi da khô nên chọn sản phẩm dịu nhẹ hơn như BHA. Đừng quên kiểm tra thành phần để tránh kích ứng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c460bc08-a7ab-4500-bc9d-fbefe88861a3', '076c48e4-fb7b-4f42-bcc0-56550fcf5c3b', 'concept', 'Cách Chọn Bảo Hiểm Nhân Thọ Phù Hợp', 'Khi chọn bảo hiểm nhân thọ, bạn cần xem xét nhu cầu bảo vệ tài chính của gia đình, khả năng chi trả hàng tháng và mục tiêu tài chính trong tương lai.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh người tư vấn tài chính với khách hàng.", "videoScript": "Trong bài học này, chúng ta sẽ thảo luận về cách chọn bảo hiểm nhân thọ phù hợp với nhu cầu của bạn. Hãy cùng tìm hiểu những yếu tố quan trọng mà bạn cần xem xét.", "imageDescription": "Một chuyên gia tài chính đang tư vấn cho khách hàng về bảo hiểm nhân thọ, thể hiện sự chuyên nghiệp và tận tâm.", "videoDescription": "Video hướng dẫn cách chọn bảo hiểm nhân thọ phù hợp, đưa ra các tiêu chí cần cân nhắc để giúp người xem đưa ra quyết định đúng đắn."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:00:36.373477', '2026-01-27 13:00:36.373477', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chọn bảo hiểm nhân thọ cần cân nhắc nhu cầu và khả năng tài chính.", "detailed": "Khi chọn bảo hiểm nhân thọ, bạn cần xem xét nhu cầu bảo vệ tài chính của gia đình, khả năng chi trả hàng tháng và mục tiêu tài chính trong tương lai.", "comprehensive": "Để chọn bảo hiểm nhân thọ phù hợp, bạn nên đánh giá những yếu tố như số tiền bảo hiểm cần thiết, thời gian bảo vệ, ngân sách hàng tháng và các điều kiện sức khỏe của bản thân. Hãy tham khảo ý kiến của các chuyên gia và xem xét nhiều lựa chọn khác nhau."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('512a6569-52be-4166-9d0f-7f01789101a6', '076c48e4-fb7b-4f42-bcc0-56550fcf5c3b', 'concept', 'Những Sai Lầm Cần Tránh Khi Mua Bảo Hiểm Nhân Thọ', 'Một số sai lầm phổ biến khi mua bảo hiểm nhân thọ bao gồm không đọc kỹ các điều khoản trong hợp đồng, không xem xét nhu cầu thực tế của bản thân và chỉ chọn bảo hiểm dựa trên giá cả.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang đọc hợp đồng bảo hiểm.", "videoScript": "Trong bài học này, chúng ta sẽ điểm qua những sai lầm mà mọi người thường mắc phải khi mua bảo hiểm nhân thọ và cách để tránh chúng.", "imageDescription": "Một người đang ngồi đọc kỹ hợp đồng bảo hiểm nhân thọ, thể hiện sự chú ý và cẩn thận.", "videoDescription": "Video trình bày những sai lầm phổ biến khi mua bảo hiểm nhân thọ và cách để người xem có thể tránh được những rủi ro không đáng có."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:00:36.376906', '2026-01-27 13:00:36.376906', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tránh những sai lầm như không đọc kỹ hợp đồng hoặc không so sánh nhiều lựa chọn.", "detailed": "Một số sai lầm phổ biến khi mua bảo hiểm nhân thọ bao gồm không đọc kỹ các điều khoản trong hợp đồng, không xem xét nhu cầu thực tế của bản thân và chỉ chọn bảo hiểm dựa trên giá cả.", "comprehensive": "Khi mua bảo hiểm nhân thọ, nhiều người mắc phải sai lầm như không hiểu rõ về các điều khoản, chỉ tập trung vào giá mà không xem xét giá trị thực tế, và không tham khảo ý kiến từ các chuyên gia. Hãy chắc chắn rằng bạn đã nghiên cứu kỹ trước khi quyết định."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d8e0e672-42fa-41a8-8570-14c5b142c23c', 'ea70d998-1fa8-4885-9a12-7713df8fca34', 'concept', 'Cách Sử Dụng Tẩy Tế Bào Chết', 'Để đạt hiệu quả tối ưu, bạn nên sử dụng tẩy tế bào chết 1-2 lần mỗi tuần và massage nhẹ nhàng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang thực hiện quy trình tẩy tế bào chết.", "videoScript": "Hãy cùng xem cách thực hiện quy trình tẩy tế bào chết đúng cách để đạt được làn da khỏe mạnh nhé!", "imageDescription": "Một người phụ nữ đang nhẹ nhàng massage sản phẩm tẩy tế bào chết lên mặt, thể hiện quá trình chăm sóc da.", "videoDescription": "Video hướng dẫn từng bước trong quy trình tẩy tế bào chết."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:13:42.16414', '2026-01-27 13:13:42.16414', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Sử dụng sản phẩm theo hướng dẫn trên bao bì.", "detailed": "Để đạt hiệu quả tối ưu, bạn nên sử dụng tẩy tế bào chết 1-2 lần mỗi tuần và massage nhẹ nhàng.", "comprehensive": "Để sử dụng tẩy tế bào chết hiệu quả, bạn nên làm sạch mặt trước, sau đó thoa sản phẩm và massage nhẹ nhàng trong 2-3 phút. Rửa sạch bằng nước ấm và tiếp tục với các bước dưỡng da tiếp theo. Lưu ý không nên lạm dụng để tránh làm tổn thương da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a2f014c5-ab22-4749-959e-201c77365a32', 'a1618b25-304d-43be-b2e3-bd7613475485', 'concept', 'Thực Hành Chơi Nhạc Tự Do', 'Thực hành là chìa khóa để phát triển khả năng chơi nhạc theo cảm hứng của bạn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nhóm nhạc sĩ đang cùng nhau chơi nhạc tự do trên nhiều cây đàn piano.", "videoScript": "Chào mừng các bạn đến với bài học thực hành chơi nhạc tự do. Hôm nay, chúng ta sẽ cùng nhau áp dụng những gì đã học và tạo ra những bản nhạc độc đáo.", "imageDescription": "Hình ảnh của một nhóm nhạc sĩ đang cùng nhau chơi nhạc tự do, tạo nên một không khí vui tươi và sáng tạo.", "videoDescription": "Video hướng dẫn thực hành chơi nhạc tự do, bao gồm các bài tập và hoạt động nhóm để phát triển khả năng sáng tạo."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:33.702145', '2026-01-27 12:42:33.702145', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thực hành chơi nhạc tự do trên piano.", "detailed": "Thực hành là chìa khóa để phát triển khả năng chơi nhạc theo cảm hứng của bạn.", "comprehensive": "Trong bài học này, chúng ta sẽ thực hành chơi nhạc tự do, áp dụng tất cả các kiến thức đã học để tạo ra những bản nhạc độc đáo và đầy sáng tạo."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('86f137e8-c666-4fd1-b1b8-89bba89fff3e', '57611fb0-9b15-4aff-9874-a35e26584807', 'concept', 'Giới thiệu về Cảm Âm', 'Cảm âm là khả năng nhận diện và phân tích âm thanh, giúp bạn chơi piano một cách tự nhiên và sáng tạo.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang chơi piano với nốt nhạc xung quanh.", "videoScript": "Chào mừng đến với bài học về cảm âm. Trong bài học này, chúng ta sẽ khám phá ý nghĩa của cảm âm và cách nó giúp bạn trong việc chơi piano.", "imageDescription": "Một nghệ sĩ piano đang chơi một bản nhạc cổ điển, với nốt nhạc bay lơ lửng xung quanh, tượng trưng cho cảm âm.", "videoDescription": "Video này sẽ giải thích về cảm âm, cách nó ảnh hưởng đến khả năng chơi nhạc và một số bài tập đơn giản để phát triển kỹ năng này."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:59.455133', '2026-01-27 12:42:59.455133', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cảm âm giúp bạn nhận biết âm thanh và hòa âm.", "detailed": "Cảm âm là khả năng nhận diện và phân tích âm thanh, giúp bạn chơi piano một cách tự nhiên và sáng tạo.", "comprehensive": "Cảm âm là một kỹ năng quan trọng trong âm nhạc, cho phép bạn nhận biết cao độ, nhịp điệu và hòa âm. Kỹ năng này sẽ giúp bạn cải thiện khả năng chơi piano của mình và sáng tác các giai điệu độc đáo."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('335534e3-32ab-4ede-b809-36bd07102728', '30dc66e2-1493-4b2e-999d-6525979015b5', 'concept', 'Tại Sao Cần Lập Kế Hoạch Hưu Trí?', 'Lập kế hoạch hưu trí giúp bạn chuẩn bị tài chính cần thiết để sống thoải mái trong giai đoạn không còn làm việc. Nó bao gồm việc xác định các nguồn thu nhập, chi phí dự kiến và các khoản tiết kiệm cần thiết.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người lớn tuổi đang ngồi bên bàn làm việc, xem xét các tài liệu tài chính.", "videoScript": "Mở đầu với hình ảnh của một cặp vợ chồng lớn tuổi đang thư giãn. Giọng nói lồng ghép giải thích về tầm quan trọng của việc lập kế hoạch hưu trí.", "imageDescription": "Hình ảnh một người lớn tuổi cẩn thận kiểm tra kế hoạch tài chính của mình trên bàn làm việc, phản ánh sự chuẩn bị cho hưu trí.", "videoDescription": "Video giải thích lý do tại sao việc lập kế hoạch hưu trí là cần thiết, bao gồm các yếu tố tài chính và cuộc sống."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:01:09.833275', '2026-01-27 13:01:09.833275', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hưu trí là giai đoạn quan trọng trong cuộc đời. Việc lập kế hoạch giúp bạn đảm bảo tài chính khi về già.", "detailed": "Lập kế hoạch hưu trí giúp bạn chuẩn bị tài chính cần thiết để sống thoải mái trong giai đoạn không còn làm việc. Nó bao gồm việc xác định các nguồn thu nhập, chi phí dự kiến và các khoản tiết kiệm cần thiết.", "comprehensive": "Trong một thế giới đang thay đổi nhanh chóng, việc lập kế hoạch hưu trí không chỉ đảm bảo bạn có đủ tiền để sống, mà còn tạo ra một nền tảng vững chắc cho các hoạt động khác như du lịch, chăm sóc sức khỏe và hỗ trợ gia đình. Điều này đòi hỏi bạn phải phân tích chi tiết các yếu tố như lạm phát, thuế, và các khoản đầu tư."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('357e5dfa-4ac0-4aa5-9f05-cf0f983e933f', '57611fb0-9b15-4aff-9874-a35e26584807', 'concept', 'Bài Tập Nhận Biết Âm Cao Độ', 'Bài tập này sẽ giúp bạn luyện tập khả năng nhận biết âm cao độ bằng cách nghe và xác định các nốt nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bàn phím piano với các nốt nhạc được đánh dấu.", "videoScript": "Trong bài tập này, chúng ta sẽ thực hành nhận biết âm cao độ. Tôi sẽ chơi một nốt nhạc và bạn hãy cố gắng xác định nó.", "imageDescription": "Bàn phím piano với các nốt nhạc được tô màu khác nhau, cho thấy cách nhận diện các âm thanh.", "videoDescription": "Video sẽ hướng dẫn bạn cách thực hiện bài tập nhận biết âm cao độ, bao gồm các phương pháp và mẹo hữu ích."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:59.46357', '2026-01-27 12:42:59.46357', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hãy lắng nghe và nhận biết các nốt nhạc khác nhau.", "detailed": "Bài tập này sẽ giúp bạn luyện tập khả năng nhận biết âm cao độ bằng cách nghe và xác định các nốt nhạc.", "comprehensive": "Để cải thiện cảm âm, hãy bắt đầu bằng việc nghe các nốt nhạc đơn lẻ và thử xác định chúng. Bạn có thể sử dụng một cây đàn piano hoặc ứng dụng âm nhạc để hỗ trợ cho bài tập này."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('efb6fc0c-0b6d-4309-9227-4ca2b0da7b71', '57611fb0-9b15-4aff-9874-a35e26584807', 'concept', 'Phân Tích Hòa Âm', 'Bài học này sẽ giúp bạn phân tích và nhận diện các hợp âm khác nhau trong âm nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một bản nhạc với các hợp âm được đánh dấu.", "videoScript": "Hôm nay, chúng ta sẽ học cách phân tích hòa âm. Tôi sẽ chơi một đoạn nhạc và bạn hãy chú ý đến các hợp âm xuất hiện.", "imageDescription": "Một trang bản nhạc với các hợp âm được tô sáng, giúp dễ dàng nhận diện.", "videoDescription": "Video này sẽ giới thiệu về hòa âm, cách phân tích chúng, và một số bài tập để cải thiện khả năng nhận diện hòa âm."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:59.467597', '2026-01-27 12:42:59.467597', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tìm hiểu cách nhận diện các hợp âm.", "detailed": "Bài học này sẽ giúp bạn phân tích và nhận diện các hợp âm khác nhau trong âm nhạc.", "comprehensive": "Phân tích hòa âm là một phần quan trọng trong cảm âm. Bạn sẽ học cách nhận diện các loại hợp âm như trưởng, thứ, và các hợp âm phức tạp hơn thông qua việc nghe và chơi chúng trên piano."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d0e18378-bbd3-46b7-891e-335a798c12d2', '57611fb0-9b15-4aff-9874-a35e26584807', 'concept', 'Luyện Tập Nghe Nhạc', 'Bằng cách lắng nghe các thể loại nhạc khác nhau, bạn sẽ phát triển khả năng cảm nhận âm thanh và hòa âm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người nghe nhạc với tai nghe và một cây đàn piano bên cạnh.", "videoScript": "Trong bài tập này, chúng ta sẽ luyện tập nghe nhạc. Tôi sẽ bật một bản nhạc và bạn hãy cố gắng phân tích các yếu tố trong đó.", "imageDescription": "Một người đang lắng nghe nhạc với tai nghe, bên cạnh là một cây đàn piano, biểu thị sự kết nối giữa nghe và chơi.", "videoDescription": "Video này sẽ hướng dẫn bạn cách luyện tập nghe nhạc để cải thiện cảm âm, với các ví dụ và bài tập cụ thể."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:59.471449', '2026-01-27 12:42:59.471449', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nghe nhiều loại nhạc khác nhau để cải thiện cảm âm.", "detailed": "Bằng cách lắng nghe các thể loại nhạc khác nhau, bạn sẽ phát triển khả năng cảm nhận âm thanh và hòa âm.", "comprehensive": "Luyện tập nghe nhạc là một cách tuyệt vời để phát triển cảm âm. Hãy thử nghe các bản nhạc cổ điển, jazz, và pop, và cố gắng phân tích cao độ, nhịp điệu và cấu trúc của chúng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a21fd4bc-e48b-4cf0-8b50-89358955538a', '57611fb0-9b15-4aff-9874-a35e26584807', 'concept', 'Sáng Tác Giai Điệu Đơn Giản', 'Hãy thử sáng tác giai điệu của riêng bạn dựa trên các nốt nhạc mà bạn đã học.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người đang viết nhạc với một cây đàn piano.", "videoScript": "Trong bài học cuối cùng này, chúng ta sẽ sáng tác giai điệu đơn giản. Tôi sẽ hướng dẫn bạn từng bước để tạo ra một giai điệu mới.", "imageDescription": "Một nhạc sĩ đang viết nhạc trên giấy, bên cạnh là cây đàn piano, thể hiện sự sáng tạo trong âm nhạc.", "videoDescription": "Video này sẽ chỉ ra cách bạn có thể sáng tác giai điệu đơn giản dựa trên cảm âm và các bài học trước đó."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:42:59.476155', '2026-01-27 12:42:59.476155', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tạo ra các giai điệu đơn giản bằng cảm âm.", "detailed": "Hãy thử sáng tác giai điệu của riêng bạn dựa trên các nốt nhạc mà bạn đã học.", "comprehensive": "Bài tập này sẽ khuyến khích bạn sáng tác các giai điệu đơn giản bằng cách sử dụng cảm âm. Hãy thử nghiệm với các nốt nhạc khác nhau và tạo ra những giai điệu độc đáo."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('54e66470-f9f6-4469-a723-48eb00c2849c', 'da4e1803-cf29-46b1-ac58-160894256860', 'concept', 'Khám Phá Piano', 'Piano, được phát minh vào đầu thế kỷ 18, là một nhạc cụ phím có khả năng chơi nhiều loại âm thanh và sắc thái khác nhau. Nó đóng vai trò quan trọng trong nhạc cổ điển, thường được dùng để biểu diễn solo hoặc trong các bản hòa tấu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một nghệ sĩ đang chơi piano trong một buổi hòa nhạc.", "videoScript": "Trong video hôm nay, chúng ta sẽ tìm hiểu về lịch sử và cách chơi piano, cũng như những tác phẩm nổi tiếng mà bạn nên nghe.", "imageDescription": "Hình ảnh một nghệ sĩ đang biểu diễn trên cây piano grand, tập trung vào các phím đàn và âm nhạc.", "videoDescription": "Video khám phá về piano, bao gồm lịch sử, cách chơi và những tác phẩm kinh điển."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:51:38.98475', '2026-01-27 12:51:38.98475', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Piano là một trong những nhạc cụ phổ biến nhất trong nhạc cổ điển.", "detailed": "Piano, được phát minh vào đầu thế kỷ 18, là một nhạc cụ phím có khả năng chơi nhiều loại âm thanh và sắc thái khác nhau. Nó đóng vai trò quan trọng trong nhạc cổ điển, thường được dùng để biểu diễn solo hoặc trong các bản hòa tấu.", "comprehensive": "Piano có hai loại chính: piano cơ và piano điện. Với 88 phím và khả năng biểu diễn từ những nốt thấp nhất đến những nốt cao nhất, piano là nhạc cụ lý tưởng cho việc thể hiện cảm xúc và kỹ thuật. Các nhạc sĩ như Mozart, Beethoven và Chopin đã để lại những tác phẩm vĩ đại cho piano, ảnh hưởng đến nhiều thế hệ nhạc sĩ sau này."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('eb9c62ce-750e-41d9-89ec-21a135aa5d91', 'da4e1803-cf29-46b1-ac58-160894256860', 'concept', 'Tác Phẩm Nổi Tiếng Trong Nhạc Cổ Điển', 'Một số tác phẩm nổi tiếng trong nhạc cổ điển bao gồm ''Giao hưởng số 9'' của Beethoven, ''Khúc nhạc cho đàn piano'' của Chopin, và ''Bản giao hưởng số 40'' của Mozart. Mỗi tác phẩm đều mang đến những cảm xúc và trải nghiệm âm nhạc khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bìa album của một bản giao hưởng nổi tiếng.", "videoScript": "Chào mừng bạn đến với phần giới thiệu về những tác phẩm nổi tiếng trong nhạc cổ điển. Chúng ta sẽ cùng lắng nghe những đoạn trích và phân tích ý nghĩa của từng tác phẩm.", "imageDescription": "Hình ảnh bìa album của bản giao hưởng nổi tiếng, với tên tác giả và tên tác phẩm được thiết kế ấn tượng.", "videoDescription": "Video giới thiệu về các tác phẩm nổi tiếng trong nhạc cổ điển, bao gồm phân tích và lắng nghe các đoạn nhạc."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:51:38.988591', '2026-01-27 12:51:38.988591', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều tác phẩm nổi tiếng trong nhạc cổ điển như ''Một Đêm Mùa Hè'' của Mendelssohn.", "detailed": "Một số tác phẩm nổi tiếng trong nhạc cổ điển bao gồm ''Giao hưởng số 9'' của Beethoven, ''Khúc nhạc cho đàn piano'' của Chopin, và ''Bản giao hưởng số 40'' của Mozart. Mỗi tác phẩm đều mang đến những cảm xúc và trải nghiệm âm nhạc khác nhau.", "comprehensive": "Nhạc cổ điển có rất nhiều tác phẩm vĩ đại, từ bản giao hưởng hùng tráng đến những bản sonata tinh tế. Những tác phẩm này không chỉ thể hiện tài năng của các nhạc sĩ mà còn phản ánh những biến chuyển trong lịch sử âm nhạc. Chúng ta sẽ cùng tìm hiểu về những tác phẩm này và ảnh hưởng của chúng đối với nền âm nhạc hiện đại."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2e5ad02c-f7ee-4263-b307-9735a3df2653', '8dbb50b4-6f7c-4e4a-b00f-ca3caa1a8b64', 'concept', 'Bài Học 1: Giới Thiệu Về Piano', 'Piano là một nhạc cụ gõ có phím, thường có 88 phím, bao gồm 52 phím trắng và 36 phím đen. Nó được sử dụng trong nhiều thể loại âm nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một cây piano grand với phím bật mở, trong một căn phòng sáng sủa.", "videoScript": "Chào mừng đến với bài học đầu tiên về piano. Hôm nay, chúng ta sẽ khám phá lịch sử và cấu trúc của cây đàn piano.", "imageDescription": "Hình ảnh một cây piano grand sang trọng với ánh sáng tự nhiên chiếu vào, tạo cảm giác ấm cúng.", "videoDescription": "Video giới thiệu về piano, bao gồm cách thức hoạt động và vai trò của nó trong âm nhạc."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:43:29.240244', '2026-01-27 12:43:29.240244', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Piano là một nhạc cụ phổ biến với 88 phím.", "detailed": "Piano là một nhạc cụ gõ có phím, thường có 88 phím, bao gồm 52 phím trắng và 36 phím đen. Nó được sử dụng trong nhiều thể loại âm nhạc.", "comprehensive": "Piano là nhạc cụ gõ phổ biến nhất thế giới, với 88 phím, gồm 52 phím trắng và 36 phím đen. Nhạc cụ này được phát minh vào đầu thế kỷ 18 và đã trở thành nền tảng cho nhiều thể loại âm nhạc, từ cổ điển đến hiện đại. Việc chơi piano không chỉ giúp phát triển khả năng âm nhạc mà còn cải thiện kỹ năng tư duy và trí nhớ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4a0a4aaa-8194-456e-b061-df436b704ea2', '8dbb50b4-6f7c-4e4a-b00f-ca3caa1a8b64', 'concept', 'Bài Học 2: Các Nốt Nhạc Cơ Bản', 'Các nốt nhạc cơ bản bao gồm Đô (C), Rê (D), Mi (E), Fa (F), Sol (G), La (A), Si (B). Mỗi nốt có vị trí riêng trên phím đàn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Sơ đồ các nốt nhạc trên phím piano.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu về các nốt nhạc cơ bản và cách nhận biết chúng trên piano.", "imageDescription": "Hình ảnh mô tả vị trí của các nốt nhạc trên phím đàn piano.", "videoDescription": "Video giải thích về các nốt nhạc cơ bản và cách chúng được biểu diễn trên piano."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:43:29.247981', '2026-01-27 12:43:29.247981', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có 7 nốt nhạc cơ bản: Đô, Rê, Mi, Fa, Sol, La, Si.", "detailed": "Các nốt nhạc cơ bản bao gồm Đô (C), Rê (D), Mi (E), Fa (F), Sol (G), La (A), Si (B). Mỗi nốt có vị trí riêng trên phím đàn.", "comprehensive": "Trong âm nhạc, có 7 nốt nhạc cơ bản: Đô (C), Rê (D), Mi (E), Fa (F), Sol (G), La (A), Si (B). Những nốt này tạo thành cơ sở cho mọi bản nhạc. Với piano, các nốt này được phân bố trên các phím trắng và đen, giúp người chơi dễ dàng nhận biết và tìm kiếm âm thanh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1f02dedb-7a28-4fc4-b466-451512b7afa9', '8dbb50b4-6f7c-4e4a-b00f-ca3caa1a8b64', 'concept', 'Bài Học 3: Cách Đọc Nhạc', 'Việc đọc nhạc là một kỹ năng quan trọng. Bạn cần nắm vững các ký hiệu và cách chúng đại diện cho âm thanh.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bản nhạc piano với các nốt nhạc rõ ràng.", "videoScript": "Chào mừng đến với bài học đọc nhạc. Hôm nay, chúng ta sẽ cùng nhau khám phá các ký hiệu và cách đọc bản nhạc.", "imageDescription": "Hình ảnh một bản nhạc piano với các nốt nhạc được ghi rõ ràng, giúp người học dễ dàng theo dõi.", "videoDescription": "Video hướng dẫn cách đọc bản nhạc cho piano, bao gồm các ký hiệu quan trọng."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:43:29.251788', '2026-01-27 12:43:29.251788', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách đọc bản nhạc giúp bạn chơi piano dễ hơn.", "detailed": "Việc đọc nhạc là một kỹ năng quan trọng. Bạn cần nắm vững các ký hiệu và cách chúng đại diện cho âm thanh.", "comprehensive": "Đọc nhạc là một phần thiết yếu trong việc chơi piano. Nó bao gồm việc hiểu các ký hiệu, như nốt nhạc, khóa nhạc và nhịp độ. Bằng cách đọc nhạc, bạn có thể chơi các bản nhạc phức tạp và phát triển kỹ năng chơi đàn của mình."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d55d0d6f-50c6-4a24-9652-9ff0734f67eb', '8dbb50b4-6f7c-4e4a-b00f-ca3caa1a8b64', 'concept', 'Bài Học 4: Kỹ Thuật Chơi Bản Nhạc Ngắn', 'Chơi các bản nhạc ngắn là cách tuyệt vời để cải thiện kỹ năng chơi piano. Bắt đầu với những bản nhạc đơn giản và dần dần tăng độ khó.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người chơi piano đang thực hành một bản nhạc ngắn.", "videoScript": "Trong bài học này, chúng ta sẽ thực hành chơi một bản nhạc ngắn. Hãy chú ý đến từng nốt và nhịp điệu.", "imageDescription": "Hình ảnh một người chơi piano chăm chú luyện tập một bản nhạc ngắn, thể hiện sự tập trung.", "videoDescription": "Video hướng dẫn từng bước để chơi một bản nhạc ngắn trên piano."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:43:29.257309', '2026-01-27 12:43:29.257309', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chơi bản nhạc ngắn giúp bạn luyện tập kỹ năng.", "detailed": "Chơi các bản nhạc ngắn là cách tuyệt vời để cải thiện kỹ năng chơi piano. Bắt đầu với những bản nhạc đơn giản và dần dần tăng độ khó.", "comprehensive": "Để trở thành một người chơi piano giỏi, việc luyện tập các bản nhạc ngắn là rất quan trọng. Những bản nhạc này thường có cấu trúc đơn giản và giúp bạn làm quen với các nốt và nhịp điệu. Điều này cũng giúp tăng cường khả năng ghi nhớ và sự nhạy bén của đôi tay."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('683c76e9-3981-4186-b895-779e3943b516', '8dbb50b4-6f7c-4e4a-b00f-ca3caa1a8b64', 'concept', 'Bài Học 5: Thực Hành và Phát Triển Kỹ Năng', 'Bạn cần dành thời gian thực hành hàng ngày để phát triển kỹ năng chơi piano của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một lịch trình luyện tập piano trên bàn.", "videoScript": "Trong bài học cuối cùng này, chúng ta sẽ thảo luận về tầm quan trọng của việc thực hành và cách xây dựng một lịch trình luyện tập hiệu quả.", "imageDescription": "Hình ảnh một lịch trình luyện tập piano được ghi chú rõ ràng, giúp người học dễ dàng theo dõi.", "videoDescription": "Video hướng dẫn cách thiết lập lịch trình luyện tập piano để phát triển kỹ năng."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:43:29.261422', '2026-01-27 12:43:29.261422', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thực hành là chìa khóa để chơi piano giỏi.", "detailed": "Bạn cần dành thời gian thực hành hàng ngày để phát triển kỹ năng chơi piano của mình.", "comprehensive": "Thực hành thường xuyên là điều cần thiết để trở thành một người chơi piano giỏi. Đặt ra một lịch trình luyện tập cụ thể, bao gồm các bài tập kỹ thuật và chơi bản nhạc yêu thích. Theo dõi sự tiến bộ của bạn và không ngừng thách thức bản thân với những bản nhạc khó hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cade3762-3d73-4744-a157-6b6ecc1a9b0f', '9aef46aa-a4a4-4bec-8fcc-3bd8ce073766', 'concept', 'Giới Thiệu Về Hợp Âm Cơ Bản', 'Hợp âm cơ bản bao gồm hợp âm trưởng, hợp âm thứ và hợp âm bảy. Mỗi loại hợp âm có âm thanh và cảm xúc khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một cây piano với các phím được đánh dấu hợp âm cơ bản.", "videoScript": "Xin chào các bạn, hôm nay chúng ta sẽ cùng tìm hiểu về hợp âm cơ bản trên piano. Hợp âm là gì? Chúng ta có hợp âm trưởng, hợp âm thứ, và hợp âm bảy. Hãy cùng nhau khám phá từng loại nhé!", "imageDescription": "Hình ảnh minh họa một cây piano với các phím trắng và đen, chỉ ra các hợp âm trưởng, hợp âm thứ và hợp âm bảy.", "videoDescription": "Video giải thích các loại hợp âm cơ bản trên piano và cách chúng được sử dụng trong âm nhạc."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:01.055622', '2026-01-27 12:44:01.055622', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm là sự kết hợp của ba nốt nhạc trở lên.", "detailed": "Hợp âm cơ bản bao gồm hợp âm trưởng, hợp âm thứ và hợp âm bảy. Mỗi loại hợp âm có âm thanh và cảm xúc khác nhau.", "comprehensive": "Hợp âm là nền tảng của âm nhạc, bao gồm các nốt nhạc được chơi cùng nhau. Hợp âm trưởng tạo cảm giác vui vẻ, hợp âm thứ mang âm hưởng buồn bã, và hợp âm bảy thường được sử dụng để tạo ra sự căng thẳng và chuyển tiếp tới hợp âm khác."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('010b6165-8205-41a9-afd7-2f92ad6cf4ab', '9aef46aa-a4a4-4bec-8fcc-3bd8ce073766', 'concept', 'Cách Chơi Hợp Âm Trưởng', 'Để chơi hợp âm trưởng, bạn cần chơi nốt gốc, nốt thứ ba, và nốt thứ năm. Ví dụ: hợp âm C trưởng bao gồm các nốt C, E, G.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bảng hợp âm C trưởng trên piano.", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ học cách chơi hợp âm trưởng. Hãy cùng nhau xem nốt C trưởng và cách chơi nó trên piano.", "imageDescription": "Hình ảnh minh họa các phím C, E, G trên piano với chú thích cách chơi hợp âm C trưởng.", "videoDescription": "Video hướng dẫn cách chơi hợp âm trưởng trên piano với ví dụ cụ thể."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:01.064442', '2026-01-27 12:44:01.064442', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm trưởng thường vui tươi và sáng sủa.", "detailed": "Để chơi hợp âm trưởng, bạn cần chơi nốt gốc, nốt thứ ba, và nốt thứ năm. Ví dụ: hợp âm C trưởng bao gồm các nốt C, E, G.", "comprehensive": "Hợp âm trưởng được xây dựng bằng cách lấy nốt gốc, nốt thứ ba cách nốt gốc hai bước và nốt thứ năm cách nốt gốc bốn bước. Cách chơi hợp âm trưởng C là: nhấn phím C, E, G đồng thời."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('6dc32eb2-24b8-476e-902a-2ddc9b9c8bfb', '9aef46aa-a4a4-4bec-8fcc-3bd8ce073766', 'concept', 'Cách Chơi Hợp Âm Thứ', 'Để chơi hợp âm thứ, bạn cần lấy nốt gốc, nốt thứ ba hạ một nửa bước và nốt thứ năm. Ví dụ: hợp âm A thứ gồm A, C, E.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bảng hợp âm A thứ trên piano.", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ học cách chơi hợp âm thứ. Hãy cùng nhau xem nốt A thứ và cách chơi nó trên piano.", "imageDescription": "Hình ảnh minh họa các phím A, C, E trên piano với chú thích cách chơi hợp âm A thứ.", "videoDescription": "Video hướng dẫn cách chơi hợp âm thứ trên piano với ví dụ cụ thể."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:01.069032', '2026-01-27 12:44:01.069032', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm thứ thường mang âm điệu buồn hơn hợp âm trưởng.", "detailed": "Để chơi hợp âm thứ, bạn cần lấy nốt gốc, nốt thứ ba hạ một nửa bước và nốt thứ năm. Ví dụ: hợp âm A thứ gồm A, C, E.", "comprehensive": "Hợp âm thứ được tạo ra bằng cách lấy nốt gốc, nốt thứ ba hạ một nửa bước so với hợp âm trưởng tương ứng, và nốt thứ năm không thay đổi. Hợp âm A thứ bao gồm các nốt A, C, E."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('3689daea-5ab3-4562-80c2-7bfe1e64e82b', '9aef46aa-a4a4-4bec-8fcc-3bd8ce073766', 'concept', 'Cách Chơi Hợp Âm Bảy', 'Hợp âm bảy bao gồm nốt gốc, nốt thứ ba, nốt thứ năm và nốt bảy. Ví dụ: hợp âm C bảy bao gồm C, E, G, Bb.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bảng hợp âm C bảy trên piano.", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ học cách chơi hợp âm bảy. Hãy cùng nhau xem nốt C bảy và cách chơi nó trên piano.", "imageDescription": "Hình ảnh minh họa các phím C, E, G, Bb trên piano với chú thích cách chơi hợp âm C bảy.", "videoDescription": "Video hướng dẫn cách chơi hợp âm bảy trên piano với ví dụ cụ thể."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:01.072393', '2026-01-27 12:44:01.072393', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm bảy tạo ra âm thanh phong phú và thú vị.", "detailed": "Hợp âm bảy bao gồm nốt gốc, nốt thứ ba, nốt thứ năm và nốt bảy. Ví dụ: hợp âm C bảy bao gồm C, E, G, Bb.", "comprehensive": "Hợp âm bảy được tạo ra bằng cách thêm nốt bảy vào hợp âm trưởng hoặc thứ. Nốt bảy có thể là nốt giảm hoặc nốt tự nhiên, tạo nên cảm giác căng thẳng và cần được giải quyết. Ví dụ, hợp âm C bảy bao gồm các nốt C, E, G, Bb."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c1f4dbd8-228e-4eb2-8f71-7846351a58e9', '9aef46aa-a4a4-4bec-8fcc-3bd8ce073766', 'concept', 'Thực Hành Chơi Hợp Âm Cơ Bản', 'Hãy dành thời gian mỗi ngày để thực hành các hợp âm trưởng, thứ và bảy. Bạn có thể thử chơi các bài hát đơn giản sử dụng các hợp âm này.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang thực hành chơi piano.", "videoScript": "Chào các bạn, hôm nay chúng ta sẽ cùng nhau thực hành chơi hợp âm cơ bản. Hãy dành ít nhất 15 phút mỗi ngày để luyện tập nhé!", "imageDescription": "Hình ảnh minh họa một người đang ngồi trước piano, tập trung vào việc chơi các hợp âm cơ bản.", "videoDescription": "Video hướng dẫn thực hành các hợp âm cơ bản trên piano và cách áp dụng chúng vào các bài hát."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:01.076248', '2026-01-27 12:44:01.076248', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thực hành là cách tốt nhất để cải thiện kỹ năng chơi piano.", "detailed": "Hãy dành thời gian mỗi ngày để thực hành các hợp âm trưởng, thứ và bảy. Bạn có thể thử chơi các bài hát đơn giản sử dụng các hợp âm này.", "comprehensive": "Để trở thành một pianiste giỏi, bạn cần thực hành thường xuyên. Hãy bắt đầu với các hợp âm cơ bản, thử chơi các bài hát yêu thích của bạn và kết hợp chúng lại với nhau. Ghi nhớ rằng, thực hành chính là chìa khóa để thành công."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c7f96d35-3e8e-43d6-9e56-7eee1b63536e', '05db29b6-cf0f-40ce-904a-8ff4c3a124ff', 'concept', 'Bài Tập Bắt Đầu Với Ngón Tay', 'Bắt đầu bằng cách đặt ngón tay lên phím đàn. Chơi từng nốt một để cảm nhận sự di chuyển của ngón tay.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bàn tay đang đặt trên phím đàn piano với các nốt C, D, E được đánh dấu.", "videoScript": "Trong video này, chúng ta sẽ khám phá cách đặt ngón tay đúng cách trên phím đàn.", "imageDescription": "Hình ảnh một bàn tay trên phím đàn piano, với các nốt cơ bản được chỉ ra.", "videoDescription": "Video hướng dẫn về cách đặt ngón tay và chơi từng nốt cơ bản trên piano."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:25.43979', '2026-01-27 12:44:25.43979', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hãy làm quen với từng ngón tay của bạn trên phím đàn.", "detailed": "Bắt đầu bằng cách đặt ngón tay lên phím đàn. Chơi từng nốt một để cảm nhận sự di chuyển của ngón tay.", "comprehensive": "Trước khi chơi, hãy đảm bảo rằng bàn tay của bạn thoải mái. Sử dụng ngón cái, ngón trỏ, và ngón giữa để chơi các nốt C, D, E. Thực hiện 10 lần cho mỗi nốt."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e449b194-e3b2-48c9-a800-70bc146714ad', '05db29b6-cf0f-40ce-904a-8ff4c3a124ff', 'concept', 'Tập Luyện Ngón Tay Chạy', 'Chơi một chuỗi các nốt từ C đến G bằng cách sử dụng các ngón tay tương ứng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Sơ đồ bàn tay với các ngón tay đánh dấu tương ứng với các nốt C, D, E, F, G.", "videoScript": "Trong video này, chúng ta sẽ luyện tập chạy nốt từ C đến G và cải thiện tốc độ ngón tay.", "imageDescription": "Sơ đồ chỉ ra cách sắp xếp ngón tay cho các nốt C đến G.", "videoDescription": "Video hướng dẫn cách chạy nốt và cải thiện kỹ năng ngón tay trên piano."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:25.451293', '2026-01-27 12:44:25.451293', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chạy các nốt từ C đến G.", "detailed": "Chơi một chuỗi các nốt từ C đến G bằng cách sử dụng các ngón tay tương ứng.", "comprehensive": "Hãy thực hành chạy nốt từ C đến G, sử dụng ngón cái cho C, ngón trỏ cho D, ngón giữa cho E, ngón áp út cho F và ngón út cho G. Lặp lại 5 lần."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2e4ad381-4cb2-4655-8c61-0d190e600746', '05db29b6-cf0f-40ce-904a-8ff4c3a124ff', 'concept', 'Tập Luyện Ngón Tay Với Nhịp Điệu', 'Sử dụng metronome để giữ nhịp khi chơi các nốt từ C đến G.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một metronome bên cạnh một cây đàn piano.", "videoScript": "Trong video này, chúng ta sẽ sử dụng metronome để luyện tập nhịp điệu khi chơi piano.", "imageDescription": "Một metronome đang hoạt động, bên cạnh là một bàn tay đang chơi piano.", "videoDescription": "Video hướng dẫn cách kết hợp metronome vào luyện tập chơi piano."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:25.455963', '2026-01-27 12:44:25.455963', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chơi các nốt theo nhịp điệu.", "detailed": "Sử dụng metronome để giữ nhịp khi chơi các nốt từ C đến G.", "comprehensive": "Hãy thực hành chơi các nốt từ C đến G với metronome. Bắt đầu với tốc độ chậm và dần dần tăng tốc độ khi bạn cảm thấy thoải mái."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('12a40395-58ac-400e-888c-349a8b948967', '05db29b6-cf0f-40ce-904a-8ff4c3a124ff', 'concept', 'Tập Luyện Ngón Tay Với Các Giai Điệu Đơn Giản', 'Thực hành chơi các giai điệu dễ trên piano để tăng cường kỹ năng ngón tay.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Nốt nhạc của ''Mary Had a Little Lamb'' trên một trang nhạc.", "videoScript": "Trong video này, chúng ta sẽ học cách chơi ''Mary Had a Little Lamb'' trên piano.", "imageDescription": "Bảng nhạc với các nốt của giai điệu ''Mary Had a Little Lamb''.", "videoDescription": "Video hướng dẫn cách chơi giai điệu đơn giản trên piano."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:25.459748', '2026-01-27 12:44:25.459748', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chơi các giai điệu đơn giản như ''Mary Had a Little Lamb''.", "detailed": "Thực hành chơi các giai điệu dễ trên piano để tăng cường kỹ năng ngón tay.", "comprehensive": "Hãy chọn một giai điệu đơn giản như ''Mary Had a Little Lamb''. Thực hành từng đoạn và chú ý đến cách di chuyển ngón tay giữa các nốt."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('078dbe16-f18a-48bb-a23e-3b878dbf42ed', '05db29b6-cf0f-40ce-904a-8ff4c3a124ff', 'concept', 'Tập Luyện Ngón Tay Với Các Bài Tập Trên Phím Đen', 'Chơi các nốt trên phím đen để cải thiện kỹ năng và sự linh hoạt của ngón tay.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bàn tay đang chơi trên các phím đen của đàn piano.", "videoScript": "Trong video này, chúng ta sẽ luyện tập chơi các phím đen để tăng cường kỹ năng ngón tay.", "imageDescription": "Bàn tay đang chơi các nốt trên phím đen của piano.", "videoDescription": "Video hướng dẫn về cách chơi các phím đen và cải thiện sự linh hoạt của ngón tay."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:25.465588', '2026-01-27 12:44:25.465588', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Luyện tập với các phím đen.", "detailed": "Chơi các nốt trên phím đen để cải thiện kỹ năng và sự linh hoạt của ngón tay.", "comprehensive": "Hãy thử chơi các nốt trên phím đen như F# và G#. Sử dụng ngón tay tương ứng và thực hành chuyển động giữa các phím đen và phím trắng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('53267356-b4eb-4110-b87a-9615e16c13a7', 'da4e1803-cf29-46b1-ac58-160894256860', 'concept', 'Kỹ Thuật Chơi Piano', 'Kỹ thuật chơi piano bao gồm cách đặt ngón tay, kiểm soát nhịp điệu và cảm xúc. Việc luyện tập hàng ngày sẽ giúp bạn cải thiện kỹ năng và khả năng biểu diễn của mình.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một giáo viên đang dạy học sinh chơi piano.", "videoScript": "Trong video này, chúng ta sẽ học về các kỹ thuật cơ bản trong việc chơi piano và cách áp dụng chúng vào bài học thực tế.", "imageDescription": "Hình ảnh một giáo viên đang hướng dẫn học sinh cách chơi piano, với các nốt nhạc và sách giáo khoa trên bàn.", "videoDescription": "Video hướng dẫn kỹ thuật chơi piano, bao gồm các bài tập và mẹo hữu ích cho người mới bắt đầu."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:51:38.992225', '2026-01-27 12:51:38.992225', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Để chơi piano tốt, bạn cần luyện tập thường xuyên.", "detailed": "Kỹ thuật chơi piano bao gồm cách đặt ngón tay, kiểm soát nhịp điệu và cảm xúc. Việc luyện tập hàng ngày sẽ giúp bạn cải thiện kỹ năng và khả năng biểu diễn của mình.", "comprehensive": "Có nhiều kỹ thuật trong việc chơi piano, từ cách điều chỉnh lực nhấn phím đến việc sử dụng pedal để tạo hiệu ứng âm thanh. Học cách đọc bản nhạc và hiểu cấu trúc của nó cũng rất quan trọng. Chúng ta sẽ cùng khám phá những kỹ thuật này và cách áp dụng chúng trong việc thực hành hàng ngày."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2d47aeb8-ed50-459f-ba20-f4892b134e25', 'fdf42707-f183-418c-8977-bd0ea8dae6b9', 'concept', 'Bài Học 1: Giới Thiệu Về Ngón Tay Trên Piano', 'Giới thiệu về vị trí và cách sử dụng các ngón tay để chơi piano hiệu quả.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bàn tay đặt đúng vị trí trên phím đàn piano.", "videoScript": "Giới thiệu các ngón tay và cách sử dụng chúng. Thực hành đặt ngón tay đúng trên phím đàn.", "imageDescription": "Bàn tay với ngón tay cái đặt trên phím Đô, các ngón còn lại đặt trên các phím kế tiếp.", "videoDescription": "Video hướng dẫn cách đặt ngón tay đúng trên piano với các ví dụ minh họa."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:49.796062', '2026-01-27 12:44:49.796062', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách đặt ngón tay đúng trên phím đàn.", "detailed": "Giới thiệu về vị trí và cách sử dụng các ngón tay để chơi piano hiệu quả.", "comprehensive": "Trình bày chi tiết về cách bố trí ngón tay trên bàn phím, cách sử dụng từng ngón tay và tầm quan trọng của việc phát triển kỹ năng ngón tay cơ bản."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8ff5d42e-b26c-4479-bf56-7cb5aa706f5e', 'fdf42707-f183-418c-8977-bd0ea8dae6b9', 'concept', 'Bài Học 2: Luyện Tập Ngón Tay Cơ Bản', 'Hướng dẫn các bài tập cơ bản giúp tăng cường sức mạnh và độ linh hoạt của ngón tay.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh về một người đang luyện tập các bài tập ngón tay trên piano.", "videoScript": "Hướng dẫn từng bài tập ngón tay, cách thực hiện và lợi ích của việc luyện tập thường xuyên.", "imageDescription": "Người chơi piano đang thực hiện các bài tập ngón tay với sự tập trung cao.", "videoDescription": "Video hướng dẫn cụ thể về các bài tập ngón tay cơ bản trên piano."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:49.803192', '2026-01-27 12:44:49.803192', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thực hành các bài tập ngón tay cơ bản.", "detailed": "Hướng dẫn các bài tập cơ bản giúp tăng cường sức mạnh và độ linh hoạt của ngón tay.", "comprehensive": "Trình bày các bài tập cơ bản cho từng ngón tay, bao gồm các kỹ thuật để cải thiện khả năng chơi piano."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('8fd512c6-39c4-4c5b-a74d-1b7b67f8ee4f', 'fdf42707-f183-418c-8977-bd0ea8dae6b9', 'concept', 'Bài Học 3: Kỹ Thuật Nhấn Phím Đúng Cách', 'Phân tích kỹ thuật nhấn phím để tạo ra âm thanh chính xác và mạnh mẽ.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một ngón tay đang nhấn phím piano.", "videoScript": "Giải thích kỹ thuật nhấn phím, hướng dẫn cách thực hiện và ví dụ minh họa.", "imageDescription": "Ngón tay đang nhấn phím Đô với lực nhấn phù hợp để tạo ra âm thanh rõ ràng.", "videoDescription": "Video trình bày kỹ thuật nhấn phím đúng cách để chơi piano hiệu quả."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:49.806668', '2026-01-27 12:44:49.806668', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách nhấn phím một cách hiệu quả.", "detailed": "Phân tích kỹ thuật nhấn phím để tạo ra âm thanh chính xác và mạnh mẽ.", "comprehensive": "Trình bày chi tiết về cách nhấn phím, bao gồm tư thế tay, cách điều chỉnh lực nhấn và tầm quan trọng của việc kiểm soát âm thanh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9f6782a5-b537-43bd-a452-44c05dded2a6', 'fdf42707-f183-418c-8977-bd0ea8dae6b9', 'concept', 'Bài Học 4: Luyện Tập Quy Tắc Ngón Tay', 'Hướng dẫn quy tắc ngón tay để chơi các nốt nhạc một cách mượt mà và liên tục.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bản nhạc với các quy tắc ngón tay ghi chú.", "videoScript": "Giới thiệu quy tắc ngón tay và cách áp dụng chúng vào các bản nhạc cụ thể.", "imageDescription": "Bản nhạc piano có các quy tắc ngón tay được đánh dấu rõ ràng.", "videoDescription": "Video giải thích quy tắc ngón tay và cách luyện tập chúng trên piano."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:49.811272', '2026-01-27 12:44:49.811272', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tìm hiểu về quy tắc ngón tay khi chơi các đoạn nhạc.", "detailed": "Hướng dẫn quy tắc ngón tay để chơi các nốt nhạc một cách mượt mà và liên tục.", "comprehensive": "Trình bày chi tiết các quy tắc ngón tay cho từng loại nhạc, cách áp dụng quy tắc vào thực tế khi chơi."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d7dbc766-7863-4ec7-811c-b1a23bbbb9ae', 'fdf42707-f183-418c-8977-bd0ea8dae6b9', 'concept', 'Bài Học 5: Thực Hành Tăng Tốc Độ Ngón Tay', 'Hướng dẫn các bài tập giúp tăng tốc độ và độ chính xác khi chơi piano.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người chơi piano với tốc độ nhanh.", "videoScript": "Hướng dẫn các bài tập tăng tốc độ ngón tay, cách thiết lập mục tiêu và theo dõi tiến độ.", "imageDescription": "Một người chơi piano thể hiện tốc độ chơi nhanh với sự tập trung và chính xác.", "videoDescription": "Video hướng dẫn luyện tập để tăng tốc độ ngón tay khi chơi piano."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:44:49.815459', '2026-01-27 12:44:49.815459', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thực hành để tăng tốc độ chơi ngón tay.", "detailed": "Hướng dẫn các bài tập giúp tăng tốc độ và độ chính xác khi chơi piano.", "comprehensive": "Trình bày chi tiết về cách luyện tập để tăng tốc độ ngón tay, bao gồm các bài tập cụ thể và mẹo hữu ích."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('811cfa46-a51d-4a71-80b4-042e169dc2ad', 'da4e1803-cf29-46b1-ac58-160894256860', 'concept', 'Âm Nhạc Cổ Điển Trong Văn Hóa Hiện Đại', 'Nhiều nghệ sĩ hiện đại đã lấy cảm hứng từ âm nhạc cổ điển, tạo ra những bản nhạc kết hợp giữa các thể loại. Điều này cho thấy sự phát triển liên tục của âm nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một cảnh trong phim với nhạc cổ điển.", "videoScript": "Chào mừng bạn đến với video khám phá ảnh hưởng của âm nhạc cổ điển trong văn hóa hiện đại. Chúng ta sẽ xem xét các ví dụ và phân tích cách âm nhạc cổ điển được sử dụng trong các tác phẩm nghệ thuật ngày nay.", "imageDescription": "Hình ảnh một khung cảnh trong phim với âm nhạc cổ điển đang phát, tạo cảm xúc cho người xem.", "videoDescription": "Video phân tích ảnh hưởng của âm nhạc cổ điển đến văn hóa hiện đại, bao gồm các ví dụ từ phim ảnh và âm nhạc."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:51:38.996288', '2026-01-27 12:51:38.996288', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Âm nhạc cổ điển vẫn có ảnh hưởng lớn đến âm nhạc hiện đại.", "detailed": "Nhiều nghệ sĩ hiện đại đã lấy cảm hứng từ âm nhạc cổ điển, tạo ra những bản nhạc kết hợp giữa các thể loại. Điều này cho thấy sự phát triển liên tục của âm nhạc.", "comprehensive": "Âm nhạc cổ điển không chỉ ảnh hưởng đến các nhạc sĩ mà còn đến các lĩnh vực nghệ thuật khác như điện ảnh và khiêu vũ. Nhiều bộ phim nổi tiếng đã sử dụng nhạc cổ điển để tăng cường cảm xúc cho các cảnh quay. Chúng ta sẽ cùng khám phá những ví dụ cụ thể và sự kết nối giữa âm nhạc cổ điển và văn hóa hiện đại."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d5ac4286-ae15-4c38-8ff2-3747b0806041', 'e0a5bc0c-3420-45f5-9865-904ae84d5b3a', 'concept', 'Giới thiệu về bản nhạc', 'Bản nhạc bao gồm nhiều phần khác nhau như nhịp điệu, cao độ và âm sắc. Mỗi phần đóng vai trò quan trọng trong việc tạo ra âm nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bức tranh về khuôn nhạc với các nốt nhạc được đánh dấu.", "videoScript": "Trong video này, chúng ta sẽ khám phá cấu trúc của bản nhạc và cách đọc các ký hiệu âm nhạc.", "imageDescription": "Hình ảnh minh họa khuôn nhạc với các nốt nhạc và ký hiệu âm nhạc.", "videoDescription": "Video hướng dẫn về cách đọc bản nhạc cơ bản, bao gồm các ký hiệu và nốt nhạc."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:45:25.07177', '2026-01-27 12:45:25.07177', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bản nhạc là sự kết hợp của các nốt nhạc để tạo thành một giai điệu.", "detailed": "Bản nhạc bao gồm nhiều phần khác nhau như nhịp điệu, cao độ và âm sắc. Mỗi phần đóng vai trò quan trọng trong việc tạo ra âm nhạc.", "comprehensive": "Bản nhạc được cấu thành từ các nốt nhạc được sắp xếp theo thứ tự thời gian. Những nốt này có thể được biểu diễn trên khuôn nhạc với các ký hiệu khác nhau, bao gồm cao độ, trường độ và biểu cảm âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('69e6c293-864e-4445-b081-29fb5d543f09', 'e0a5bc0c-3420-45f5-9865-904ae84d5b3a', 'concept', 'Các ký hiệu âm nhạc cơ bản', 'Các ký hiệu âm nhạc cơ bản bao gồm nốt đen, nốt trắng, nốt tròn và các dấu lặng khác nhau. Mỗi ký hiệu có giá trị thời gian riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các ký hiệu âm nhạc cơ bản.", "videoScript": "Chào mừng bạn đến với bài học về các ký hiệu âm nhạc cơ bản. Hôm nay, chúng ta sẽ tìm hiểu về các loại nốt và giá trị của chúng.", "imageDescription": "Bảng chú giải các ký hiệu âm nhạc cơ bản như nốt đen, nốt trắng và dấu lặng.", "videoDescription": "Video giải thích các ký hiệu âm nhạc cơ bản và cách chúng hoạt động trong bản nhạc."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:45:25.07963', '2026-01-27 12:45:25.07963', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều ký hiệu trong âm nhạc, như nốt đen, nốt trắng và dấu lặng.", "detailed": "Các ký hiệu âm nhạc cơ bản bao gồm nốt đen, nốt trắng, nốt tròn và các dấu lặng khác nhau. Mỗi ký hiệu có giá trị thời gian riêng.", "comprehensive": "Ký hiệu âm nhạc là cách để biểu thị âm thanh và thời gian. Nốt đen thường có giá trị một phách, trong khi nốt trắng có giá trị hai phách. Các ký hiệu khác như nốt tròn, nốt đơn và dấu lặng cũng đóng vai trò quan trọng trong việc xác định nhịp điệu của bản nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7164cd18-05f2-4ffd-9c16-703ff2897cda', 'ea70d998-1fa8-4885-9a12-7713df8fca34', 'concept', 'Những Lỗi Thường Gặp Khi Tẩy Tế Bào Chết', 'Một số lỗi thường gặp là tẩy tế bào chết quá nhiều lần trong tuần hoặc sử dụng sản phẩm không phù hợp.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ thể hiện số lần tẩy tế bào chết được khuyến nghị.", "videoScript": "Trong video này, chúng ta sẽ nói về những lỗi thường gặp mà mọi người hay mắc phải khi tẩy tế bào chết, và cách khắc phục chúng.", "imageDescription": "Biểu đồ minh họa số lần tẩy tế bào chết được khuyến nghị cho từng loại da.", "videoDescription": "Video giới thiệu về những lỗi thường gặp khi thực hiện tẩy tế bào chết và cách tránh chúng."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:13:42.16836', '2026-01-27 13:13:42.16836', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Không tẩy tế bào chết quá thường xuyên.", "detailed": "Một số lỗi thường gặp là tẩy tế bào chết quá nhiều lần trong tuần hoặc sử dụng sản phẩm không phù hợp.", "comprehensive": "Lỗi thường gặp khi tẩy tế bào chết bao gồm tẩy quá nhiều lần, không rửa sạch sản phẩm sau khi sử dụng, và chọn sản phẩm không phù hợp với loại da. Tẩy tế bào chết quá thường xuyên có thể gây kích ứng và làm hỏng hàng rào bảo vệ da."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('630f2db3-584d-49d7-89d5-2a2eebce567b', '7fa5b8b2-f70f-4e5f-bb08-e02636b929ec', 'concept', 'Da Khô: Nguyên Nhân và Giải Pháp', 'Nguyên nhân của da khô bao gồm thời tiết lạnh, không khí khô, và thiếu nước. Để cải thiện tình trạng da khô, hãy sử dụng kem dưỡng ẩm và nước hoa hồng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh sản phẩm chăm sóc da cho da khô.", "videoScript": "Hãy cùng tìm hiểu nguyên nhân và giải pháp cho da khô. Chúng ta sẽ khám phá các sản phẩm hữu ích.", "imageDescription": "Một bộ sản phẩm chăm sóc da bao gồm kem dưỡng ẩm, serum, và mặt nạ cho da khô.", "videoDescription": "Video chia sẻ về cách chăm sóc da khô và các sản phẩm phù hợp."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:18:44.223874', '2026-01-27 13:18:44.223874', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Da khô có thể do thời tiết, thiếu độ ẩm hoặc sử dụng sản phẩm không phù hợp.", "detailed": "Nguyên nhân của da khô bao gồm thời tiết lạnh, không khí khô, và thiếu nước. Để cải thiện tình trạng da khô, hãy sử dụng kem dưỡng ẩm và nước hoa hồng.", "comprehensive": "Da khô là một vấn đề phổ biến và có thể do nhiều yếu tố như di truyền, môi trường, và chế độ ăn uống. Sử dụng các sản phẩm chứa hyaluronic acid, glycerin và ceramides sẽ giúp giữ ẩm cho làn da. Ngoài ra, hãy duy trì thói quen uống đủ nước và tránh tắm nước nóng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f119dce3-2197-45a4-97db-2c12825a9ae0', '7fa5b8b2-f70f-4e5f-bb08-e02636b929ec', 'concept', 'Da Dầu: Đặc Điểm và Cách Chăm Sóc', 'Da dầu thường có lỗ chân lông to và bóng nhờn. Để chăm sóc da dầu, nên sử dụng sản phẩm kiểm soát dầu và làm sạch sâu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh sản phẩm chăm sóc da cho da dầu.", "videoScript": "Hôm nay, chúng ta sẽ tìm hiểu về da dầu và cách chăm sóc hiệu quả. Đừng bỏ lỡ những mẹo hay nhé!", "imageDescription": "Một bộ sản phẩm chăm sóc da bao gồm sữa rửa mặt, toner và kem dưỡng cho da dầu.", "videoDescription": "Video hướng dẫn chăm sóc da dầu với các sản phẩm và thói quen phù hợp."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:18:44.228347', '2026-01-27 13:18:44.228347', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Da dầu có lượng dầu nhiều hơn bình thường và thường dễ bị mụn.", "detailed": "Da dầu thường có lỗ chân lông to và bóng nhờn. Để chăm sóc da dầu, nên sử dụng sản phẩm kiểm soát dầu và làm sạch sâu.", "comprehensive": "Da dầu là do tuyến bã nhờn hoạt động quá mức. Để có làn da khỏe mạnh, hãy sử dụng sữa rửa mặt chứa salicylic acid và các sản phẩm không chứa dầu. Đừng quên tẩy tế bào chết định kỳ để kiểm soát mụn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('90e0a867-39a7-42ef-8a30-3c97d4d7323d', 'e0a5bc0c-3420-45f5-9865-904ae84d5b3a', 'concept', 'Cách đọc nốt nhạc trên khuôn nhạc', 'Khuôn nhạc có 5 dòng và 4 khe. Nốt nhạc có thể nằm trên hoặc giữa các dòng và khe, mỗi vị trí tương ứng với một cao độ khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh khuôn nhạc với các nốt nhạc được gắn nhãn.", "videoScript": "Hãy cùng tôi tìm hiểu cách đọc nốt nhạc trên khuôn nhạc. Chúng ta sẽ xem xét các dòng và khe, và cách xác định cao độ của mỗi nốt.", "imageDescription": "Minh họa khuôn nhạc với các nốt nhạc và vị trí của chúng.", "videoDescription": "Video hướng dẫn cách đọc nốt nhạc trên khuôn nhạc, giải thích về các dòng và khe."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:45:25.083476', '2026-01-27 12:45:25.083476', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nốt nhạc được đặt trên các dòng và khe của khuôn nhạc.", "detailed": "Khuôn nhạc có 5 dòng và 4 khe. Nốt nhạc có thể nằm trên hoặc giữa các dòng và khe, mỗi vị trí tương ứng với một cao độ khác nhau.", "comprehensive": "Khuôn nhạc là hệ thống biểu diễn âm nhạc gồm 5 dòng và 4 khe. Mỗi dòng và khe tương ứng với một nốt nhạc cụ thể. Để đọc nốt nhạc, bạn cần biết tên của các nốt trên dòng và khe, ví dụ, nốt E, G, B, D, F trên dòng và nốt F, A, C, E trong khe."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2ca35ef4-db18-4585-b0d5-681e67539210', '7fa5b8b2-f70f-4e5f-bb08-e02636b929ec', 'concept', 'Da Nhạy Cảm: Nhận Biết và Chăm Sóc', 'Da nhạy cảm có thể phản ứng với nhiều loại sản phẩm và môi trường. Cần chọn sản phẩm không chứa hương liệu và hóa chất độc hại.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh sản phẩm dành cho da nhạy cảm.", "videoScript": "Chào mừng bạn đến với bài học về da nhạy cảm. Chúng ta sẽ cùng khám phá cách nhận biết và chăm sóc cho loại da này.", "imageDescription": "Một bộ sản phẩm chăm sóc da nhạy cảm bao gồm gel làm dịu và kem dưỡng ẩm.", "videoDescription": "Video hướng dẫn chăm sóc da nhạy cảm với các sản phẩm phù hợp."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:18:44.231301', '2026-01-27 13:18:44.231301', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Da nhạy cảm dễ bị kích ứng và cần sản phẩm phù hợp.", "detailed": "Da nhạy cảm có thể phản ứng với nhiều loại sản phẩm và môi trường. Cần chọn sản phẩm không chứa hương liệu và hóa chất độc hại.", "comprehensive": "Da nhạy cảm cần được chăm sóc đặc biệt. Hãy tránh các sản phẩm có chứa cồn, hương liệu và phẩm màu. Nên sử dụng sản phẩm thiên nhiên và làm dịu da như aloe vera hoặc chiết xuất trà xanh."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b223fdf4-43e0-46eb-a55b-8f3996e1d6a6', 'e0a5bc0c-3420-45f5-9865-904ae84d5b3a', 'concept', 'Nhịp điệu trong bản nhạc', 'Nhịp điệu xác định cách các nốt nhạc được chơi theo thời gian. Nó có thể nhanh hoặc chậm, và có thể sử dụng các dấu nhịp khác nhau để biểu thị.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh mô tả các dấu nhịp khác nhau.", "videoScript": "Trong bài học này, chúng ta sẽ khám phá nhịp điệu trong âm nhạc và cách nó ảnh hưởng đến cách chơi bản nhạc.", "imageDescription": "Bảng hiển thị các dấu nhịp khác nhau và ví dụ về cách chúng ảnh hưởng đến nhạc.", "videoDescription": "Video giải thích về nhịp điệu trong bản nhạc và cách đọc các dấu nhịp."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:45:25.086853', '2026-01-27 12:45:25.086853', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhịp điệu là cách mà âm thanh được sắp xếp theo thời gian.", "detailed": "Nhịp điệu xác định cách các nốt nhạc được chơi theo thời gian. Nó có thể nhanh hoặc chậm, và có thể sử dụng các dấu nhịp khác nhau để biểu thị.", "comprehensive": "Nhịp điệu là yếu tố quan trọng trong âm nhạc, quyết định cách mà các nốt nhạc được sắp xếp và chơi. Nhịp điệu có thể được biểu thị bằng các ký hiệu như 4/4, 3/4 hoặc 6/8. Mỗi loại nhịp điệu tạo ra một cảm giác âm nhạc khác nhau và ảnh hưởng đến cách người chơi thể hiện bản nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cac871d8-a60b-4023-99c8-601ba60ed96d', 'e0a5bc0c-3420-45f5-9865-904ae84d5b3a', 'concept', 'Biểu cảm trong âm nhạc', 'Biểu cảm trong âm nhạc bao gồm các dấu hiệu như crescendo, diminuendo và staccato, giúp người chơi thể hiện cảm xúc khi chơi nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các ký hiệu biểu cảm trong âm nhạc.", "videoScript": "Chào mừng các bạn đến với bài học về biểu cảm trong âm nhạc. Hôm nay, chúng ta sẽ tìm hiểu các ký hiệu và cách chúng ảnh hưởng đến cách chơi nhạc.", "imageDescription": "Minh họa các ký hiệu biểu cảm như crescendo, diminuendo, và staccato.", "videoDescription": "Video hướng dẫn về biểu cảm trong âm nhạc và cách thể hiện cảm xúc khi chơi nhạc."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:45:25.090338', '2026-01-27 12:45:25.090338', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Biểu cảm giúp âm nhạc trở nên sống động hơn.", "detailed": "Biểu cảm trong âm nhạc bao gồm các dấu hiệu như crescendo, diminuendo và staccato, giúp người chơi thể hiện cảm xúc khi chơi nhạc.", "comprehensive": "Biểu cảm là yếu tố thể hiện cảm xúc trong âm nhạc. Các ký hiệu như crescendo (tăng âm lượng), diminuendo (giảm âm lượng) và staccato (chơi nhanh và ngắn) đều là những cách để thể hiện cảm xúc và màu sắc trong bản nhạc. Hiểu và thực hành biểu cảm sẽ giúp bạn trở thành một nhạc công tốt hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('69c2a4ad-4216-42d8-836c-1198e2a2e9cf', '65c63bd1-a319-41b5-b27a-249545b94cc4', 'concept', 'Nhịp Điệu Cơ Bản', 'Nhịp điệu cơ bản là nền tảng của mọi tác phẩm âm nhạc. Chúng ta sẽ học về các loại nhịp như 4/4, 3/4 và 2/4.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh piano với các ký hiệu nhịp điệu.", "videoScript": "Chào mừng các bạn đến với bài học về nhịp điệu cơ bản. Hôm nay, chúng ta sẽ khám phá các loại nhịp khác nhau và cách chúng được sử dụng trong âm nhạc.", "imageDescription": "Một bức tranh thể hiện các ký hiệu nhịp điệu cơ bản trên một bản nhạc piano.", "videoDescription": "Video hướng dẫn nhịp điệu cơ bản trong âm nhạc piano."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:01.502634', '2026-01-27 12:52:01.502634', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khám phá nhịp điệu cơ bản trong âm nhạc piano.", "detailed": "Nhịp điệu cơ bản là nền tảng của mọi tác phẩm âm nhạc. Chúng ta sẽ học về các loại nhịp như 4/4, 3/4 và 2/4.", "comprehensive": "Nhịp điệu cơ bản không chỉ là cách đếm mà còn là cách chúng ta cảm nhận âm nhạc. Trong bài học này, chúng ta sẽ phân tích các nhịp khác nhau, cách chúng ảnh hưởng đến cảm xúc và cách thực hành chúng trên piano."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b8efb5ff-844e-48e2-b64a-b60ff733e691', '4af491f0-1a16-43d2-977e-c38883ac61f9', 'concept', 'Bài Học 1: Nhịp Điệu Cơ Bản', 'Trong bài học này, bạn sẽ tìm hiểu về nhịp 4/4, cách đếm số nhịp và cách chơi các nốt nhạc cơ bản trong nhịp đó.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bản nhạc với nhịp 4/4.", "videoScript": "Chào mừng đến với bài học về nhịp điệu cơ bản. Hôm nay, chúng ta sẽ cùng nhau khám phá nhịp 4/4. Hãy cùng đếm từng nhịp và chơi những nốt nhạc đơn giản.", "imageDescription": "Một bản nhạc với các ký hiệu nhịp 4/4, hiển thị cụ thể các nốt nhạc.", "videoDescription": "Video hướng dẫn về cách đếm nhịp 4/4 và chơi các nốt nhạc cơ bản trên piano."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:45:53.540494', '2026-01-27 12:45:53.540494', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học về nhịp 4/4 và cách đếm nhịp.", "detailed": "Trong bài học này, bạn sẽ tìm hiểu về nhịp 4/4, cách đếm số nhịp và cách chơi các nốt nhạc cơ bản trong nhịp đó.", "comprehensive": "Nhịp 4/4 là nhịp phổ biến nhất trong âm nhạc. Bạn sẽ học cách phân chia nhịp, cách đếm nhịp bằng cách sử dụng các nốt tròn, nốt trắng và nốt đen, cũng như cách áp dụng chúng vào việc chơi piano."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9fd0d30c-a572-4962-a9fb-9a671ed17ce5', '4af491f0-1a16-43d2-977e-c38883ac61f9', 'concept', 'Bài Học 2: Nhịp Điệu Với Nốt Đen', 'Bài học này sẽ giúp bạn làm quen với việc thêm nốt đen vào nhịp 4/4, tạo ra sự phong phú trong âm thanh.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bàn phím piano với các nốt đen được đánh dấu.", "videoScript": "Trong bài học này, chúng ta sẽ thêm nốt đen vào nhịp 4/4. Hãy cùng nhau thực hành từng bước để tạo ra những âm thanh hấp dẫn.", "imageDescription": "Một bàn phím piano với các nốt đen hiển thị rõ ràng, cùng với các ký hiệu nhịp.", "videoDescription": "Video hướng dẫn cách sử dụng nốt đen trong nhịp 4/4 để tạo ra nhịp điệu phong phú."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:45:53.548738', '2026-01-27 12:45:53.548738', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách sử dụng nốt đen trong nhịp 4/4.", "detailed": "Bài học này sẽ giúp bạn làm quen với việc thêm nốt đen vào nhịp 4/4, tạo ra sự phong phú trong âm thanh.", "comprehensive": "Chúng ta sẽ cùng nhau thực hành chơi các nốt đen trong nhịp 4/4, tìm hiểu cách chúng tương tác với các nốt khác và cách chúng có thể tạo ra các mẫu nhịp điệu thú vị."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d2c2d816-f148-4c49-a12a-e2162ab74959', '4af491f0-1a16-43d2-977e-c38883ac61f9', 'concept', 'Bài Học 3: Nhịp Điệu Phức Tạp Hơn', 'Bài học này sẽ giúp bạn làm quen với nhịp 3/4 và 6/8, cùng với cách chơi các mẫu nhịp khác nhau.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bản nhạc với nhịp 3/4 và 6/8.", "videoScript": "Hôm nay, chúng ta sẽ khám phá nhịp 3/4 và 6/8. Hãy cùng nhau thực hành những mẫu nhịp này trên piano.", "imageDescription": "Một bản nhạc hiển thị nhịp 3/4 và 6/8 với các nốt nhạc minh họa.", "videoDescription": "Video hướng dẫn về nhịp 3/4 và 6/8, cùng với các mẫu nhịp điệu phức tạp hơn."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:45:53.552409', '2026-01-27 12:45:53.552409', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khám phá nhịp 3/4 và nhịp 6/8.", "detailed": "Bài học này sẽ giúp bạn làm quen với nhịp 3/4 và 6/8, cùng với cách chơi các mẫu nhịp khác nhau.", "comprehensive": "Chúng ta sẽ phân tích sự khác biệt giữa nhịp 4/4, 3/4 và 6/8. Bạn sẽ học cách tạo ra những mẫu nhịp điệu phức tạp hơn và cách áp dụng chúng vào piano."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9b972bd1-d884-42ef-9036-3be14f6fdc86', '4af491f0-1a16-43d2-977e-c38883ac61f9', 'concept', 'Bài Học 4: Kết Hợp Nhịp Điệu', 'Bài học này sẽ giúp bạn kết hợp nhịp 4/4 với nhịp 3/4 để tạo ra âm nhạc độc đáo.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bản nhạc với sự kết hợp của nhiều nhịp.", "videoScript": "Hãy cùng nhau kết hợp nhịp 4/4 và 3/4 trong bài học hôm nay. Chúng ta sẽ thực hành từng đoạn và cảm nhận sự chuyển đổi.", "imageDescription": "Một bản nhạc có sự kết hợp giữa nhịp 4/4 và 3/4, cùng với các ký hiệu nhịp.", "videoDescription": "Video hướng dẫn về việc kết hợp các nhịp điệu khác nhau để tạo ra âm nhạc độc đáo."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:45:53.556523', '2026-01-27 12:45:53.556523', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách kết hợp nhiều nhịp điệu khác nhau.", "detailed": "Bài học này sẽ giúp bạn kết hợp nhịp 4/4 với nhịp 3/4 để tạo ra âm nhạc độc đáo.", "comprehensive": "Chúng ta sẽ học cách kết hợp nhịp điệu khác nhau để tạo ra một bản nhạc phong phú. Bạn sẽ thực hành việc chuyển đổi giữa các nhịp và cảm nhận sự khác biệt."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f787e80b-c675-409c-8e61-b38d10d7a969', '4af491f0-1a16-43d2-977e-c38883ac61f9', 'concept', 'Bài Học 5: Ứng Dụng Nhịp Điệu Trong Sáng Tác', 'Bài học này sẽ giúp bạn hiểu cách sử dụng nhịp điệu trong việc sáng tác một bản nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhạc sĩ đang sáng tác bên piano.", "videoScript": "Hôm nay, chúng ta sẽ cùng nhau ứng dụng nhịp điệu vào việc sáng tác. Bạn sẽ được hướng dẫn cách tạo ra một đoạn nhạc ngắn.", "imageDescription": "Một nhạc sĩ ngồi bên piano, ghi chú lại ý tưởng sáng tác của mình với các ký hiệu nhịp.", "videoDescription": "Video hướng dẫn về cách ứng dụng nhịp điệu trong sáng tác âm nhạc."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:45:53.560864', '2026-01-27 12:45:53.560864', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách ứng dụng nhịp điệu vào sáng tác.", "detailed": "Bài học này sẽ giúp bạn hiểu cách sử dụng nhịp điệu trong việc sáng tác một bản nhạc.", "comprehensive": "Chúng ta sẽ khám phá cách nhịp điệu ảnh hưởng đến cảm xúc của bản nhạc. Bạn sẽ thực hành sáng tác một đoạn nhạc ngắn với các nhịp điệu đã học."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('857b1592-e9b4-48f9-99c0-f9d03fedee8e', '14b4bd9b-8e53-4c36-a0a9-fe2dba7623ac', 'concept', 'Giới Thiệu Về Cấu Trúc Tác Phẩm Piano', 'Tác phẩm piano thường được chia thành nhiều phần khác nhau. Mỗi phần có chức năng riêng trong việc phát triển ý tưởng âm nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh mô tả cấu trúc âm nhạc với các phần như Intro, Verse, Chorus, và Outro.", "videoScript": "Chào mừng các bạn đến với bài học về cấu trúc tác phẩm piano. Hôm nay, chúng ta sẽ khám phá các phần chính của một tác phẩm âm nhạc.", "imageDescription": "Một sơ đồ thể hiện cấu trúc của một tác phẩm piano với các phần được đánh dấu.", "videoDescription": "Video giới thiệu về các phần cơ bản cấu thành tác phẩm piano, với ví dụ minh họa từ các tác phẩm nổi tiếng."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:46:22.069389', '2026-01-27 12:46:22.069389', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cấu trúc tác phẩm piano thường gồm các phần như Intro, Verse, Chorus và Outro.", "detailed": "Tác phẩm piano thường được chia thành nhiều phần khác nhau. Mỗi phần có chức năng riêng trong việc phát triển ý tưởng âm nhạc.", "comprehensive": "Cấu trúc tác phẩm piano có thể phức tạp với nhiều phần như Intro, Verse, Chorus, Bridge và Outro. Mỗi phần không chỉ có vai trò riêng mà còn tương tác với nhau để tạo nên một tổng thể hài hòa, giúp người nghe dễ dàng theo dõi và cảm nhận âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('2b47cbb6-b928-4f87-9a8c-c2cf814ef261', '14b4bd9b-8e53-4c36-a0a9-fe2dba7623ac', 'concept', 'Phân Tích Phần Intro Trong Tác Phẩm Piano', 'Phần Intro của một tác phẩm piano thường được sử dụng để giới thiệu chủ đề chính và thu hút sự chú ý của người nghe.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một bản nhạc piano với phần Intro được đánh dấu.", "videoScript": "Hãy cùng tìm hiểu về phần Intro trong tác phẩm piano. Đây là phần quan trọng giúp tạo ra ấn tượng ban đầu cho người nghe.", "imageDescription": "Một đoạn nhạc piano với phần Intro rõ ràng, thể hiện giai điệu và tiết tấu.", "videoDescription": "Video phân tích phần Intro trong các tác phẩm piano nổi tiếng và cách nó ảnh hưởng đến cảm nhận của người nghe."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:46:22.077705', '2026-01-27 12:46:22.077705', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phần Intro tạo không khí cho tác phẩm và chuẩn bị tâm lý cho người nghe.", "detailed": "Phần Intro của một tác phẩm piano thường được sử dụng để giới thiệu chủ đề chính và thu hút sự chú ý của người nghe.", "comprehensive": "Phần Intro trong một tác phẩm piano không chỉ đơn thuần là mở đầu mà còn là cơ hội để giới thiệu giai điệu và không khí của tác phẩm. Nó tạo nên các yếu tố cảm xúc mà người nghe sẽ trải nghiệm trong suốt tác phẩm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('956b5f55-260f-46f5-9e2f-20a66ac31896', '14b4bd9b-8e53-4c36-a0a9-fe2dba7623ac', 'concept', 'Tìm Hiểu Về Phần Verse Trong Tác Phẩm Piano', 'Verse là phần mà trong đó các ý tưởng âm nhạc được phát triển và mở rộng, thường lặp lại với những biến thể.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa phần Verse trong một bản nhạc piano.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu về phần Verse trong tác phẩm piano và những cách mà nó giúp phát triển giai điệu.", "imageDescription": "Một đoạn nhạc piano với phần Verse được hiển thị rõ ràng, mô tả sự phát triển của chủ đề.", "videoDescription": "Video khám phá vai trò của phần Verse trong các tác phẩm piano và cách nó tạo ra sự kết nối giữa các phần khác."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:46:22.081476', '2026-01-27 12:46:22.081476', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Phần Verse thường là nơi phát triển chủ đề chính của tác phẩm.", "detailed": "Verse là phần mà trong đó các ý tưởng âm nhạc được phát triển và mở rộng, thường lặp lại với những biến thể.", "comprehensive": "Phần Verse trong một tác phẩm piano đóng vai trò rất quan trọng trong việc phát triển chủ đề âm nhạc. Đây là nơi mà các giai điệu được phát triển và lặp lại, thường tạo ra sự hồi hộp và dẫn dắt người nghe đến phần Chorus."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('23984e93-95b3-41c4-8a74-31c7fb3c9d49', '14b4bd9b-8e53-4c36-a0a9-fe2dba7623ac', 'concept', 'Khám Phá Phần Chorus Trong Tác Phẩm Piano', 'Chorus thường là phần có giai điệu bắt tai, dễ nhớ, và thường được lặp lại nhiều lần trong tác phẩm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh minh họa phần Chorus trong một bản nhạc piano.", "videoScript": "Hãy cùng khám phá phần Chorus trong tác phẩm piano, nơi mà giai điệu thăng hoa và dễ dàng ghi nhớ.", "imageDescription": "Một đoạn nhạc piano với phần Chorus được làm nổi bật, thể hiện giai điệu bắt tai.", "videoDescription": "Video trình bày về phần Chorus trong các tác phẩm piano và tầm quan trọng của nó trong việc tạo nên cảm xúc cho người nghe."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:46:22.085762', '2026-01-27 12:46:22.085762', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Chorus là phần nổi bật nhất của tác phẩm, dễ nhớ và thường lặp lại.", "detailed": "Chorus thường là phần có giai điệu bắt tai, dễ nhớ, và thường được lặp lại nhiều lần trong tác phẩm.", "comprehensive": "Phần Chorus trong một tác phẩm piano không chỉ mang tính giải trí mà còn có vai trò quan trọng trong việc tạo nên điểm nhấn cho tác phẩm. Đây là phần mà người nghe thường ghi nhớ nhất, vì nó thường có giai điệu mạnh mẽ và dễ thuộc lòng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('bde3d8f6-246c-4c69-b29e-c4709269936d', '65c63bd1-a319-41b5-b27a-249545b94cc4', 'concept', 'Thực Hành Nhịp 4/4', 'Nhịp 4/4 là nhịp phổ biến nhất trong âm nhạc. Bài học này sẽ giúp bạn làm quen với việc đếm và chơi trong nhịp này.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bảng nhạc với nhịp 4/4 và các nốt nhạc.", "videoScript": "Hôm nay, chúng ta sẽ tập trung vào nhịp 4/4. Tôi sẽ hướng dẫn các bạn cách đếm và chơi các nốt nhạc trong nhịp này.", "imageDescription": "Một bảng nhạc điển hình với nhịp 4/4 và các nốt nhạc được đánh dấu.", "videoDescription": "Video hướng dẫn thực hành nhịp 4/4 trên piano."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:01.509764', '2026-01-27 12:52:01.509764', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách thực hành nhịp 4/4 trên piano.", "detailed": "Nhịp 4/4 là nhịp phổ biến nhất trong âm nhạc. Bài học này sẽ giúp bạn làm quen với việc đếm và chơi trong nhịp này.", "comprehensive": "Nhịp 4/4 được sử dụng rộng rãi trong nhiều thể loại nhạc. Chúng ta sẽ thực hành các bài tập cụ thể để cải thiện khả năng phối hợp tay và cảm nhận nhịp điệu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('9b37e9a0-7a4d-4c53-b54f-9573785b9acb', '14b4bd9b-8e53-4c36-a0a9-fe2dba7623ac', 'concept', 'Tổng Kết Cấu Trúc Tác Phẩm Piano', 'Cấu trúc tác phẩm piano tạo nên nhịp điệu và sự phát triển của âm nhạc thông qua các phần như Intro, Verse, Chorus và Outro.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh tổng quát về cấu trúc tác phẩm piano với các phần được đánh dấu.", "videoScript": "Trong bài học cuối cùng này, chúng ta sẽ tổng kết lại cấu trúc tác phẩm piano và cách mà từng phần kết hợp với nhau.", "imageDescription": "Một sơ đồ tổng thể của cấu trúc tác phẩm piano, thể hiện mối liên hệ giữa các phần.", "videoDescription": "Video tổng kết về cấu trúc tác phẩm piano, phân tích mối liên hệ và vai trò của từng phần trong tác phẩm."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:46:22.089078', '2026-01-27 12:46:22.089078', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cấu trúc tác phẩm piano là sự kết hợp của nhiều phần khác nhau.", "detailed": "Cấu trúc tác phẩm piano tạo nên nhịp điệu và sự phát triển của âm nhạc thông qua các phần như Intro, Verse, Chorus và Outro.", "comprehensive": "Tổng kết về cấu trúc tác phẩm piano cho thấy rằng mỗi phần đều có vai trò và chức năng riêng, từ Intro cho đến Outro. Sự kết hợp khéo léo giữa các phần này tạo nên một tác phẩm hoàn chỉnh, mang đến cho người nghe những trải nghiệm âm nhạc phong phú."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('229f9b41-f796-4ce1-8827-7f8c8b0bb49b', '65c63bd1-a319-41b5-b27a-249545b94cc4', 'concept', 'Nhịp 3/4 và Valse', 'Nhịp 3/4 thường được sử dụng trong điệu valse. Bài học này sẽ giúp bạn cảm nhận và thực hành nhịp 3/4.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một điệu valse trên piano.", "videoScript": "Chào mừng đến với bài học về nhịp 3/4. Hôm nay, chúng ta sẽ học cách chơi điệu valse trên piano.", "imageDescription": "Một bức tranh thể hiện một nghệ sĩ chơi piano trong lúc nhảy điệu valse.", "videoDescription": "Video hướng dẫn thực hành nhịp 3/4 và điệu valse."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:01.513234', '2026-01-27 12:52:01.513234', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khám phá nhịp 3/4 qua điệu valse.", "detailed": "Nhịp 3/4 thường được sử dụng trong điệu valse. Bài học này sẽ giúp bạn cảm nhận và thực hành nhịp 3/4.", "comprehensive": "Nhịp 3/4 mang đến cảm giác nhẹ nhàng và uyển chuyển, đặc biệt trong điệu valse. Chúng ta sẽ phân tích cấu trúc nhịp và thực hành các bài valse nổi tiếng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('ed49e291-c6e1-4b5c-8076-53055e34ec73', '65c63bd1-a319-41b5-b27a-249545b94cc4', 'concept', 'Kết Hợp Nhịp 2/4', 'Nhịp 2/4 có thể được kết hợp với nhiều nhịp khác để tạo nên sự đa dạng trong âm nhạc. Bài học này sẽ giúp bạn làm quen với nhịp 2/4.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Sơ đồ nhịp 2/4 và các kết hợp với nhịp khác.", "videoScript": "Hôm nay, chúng ta sẽ khám phá nhịp 2/4 và cách kết hợp nó với các nhịp khác. Hãy chuẩn bị cho một buổi thực hành thú vị!", "imageDescription": "Một sơ đồ thể hiện nhịp 2/4 và các cách kết hợp với nhịp khác.", "videoDescription": "Video hướng dẫn về nhịp 2/4 và cách kết hợp nhịp."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:01.516919', '2026-01-27 12:52:01.516919', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Tìm hiểu về nhịp 2/4 và cách kết hợp với các nhịp khác.", "detailed": "Nhịp 2/4 có thể được kết hợp với nhiều nhịp khác để tạo nên sự đa dạng trong âm nhạc. Bài học này sẽ giúp bạn làm quen với nhịp 2/4.", "comprehensive": "Nhịp 2/4 thường được sử dụng trong các bản nhạc vui tươi. Chúng ta sẽ thực hành kết hợp nhịp 2/4 với các nhịp khác để tạo ra những âm thanh phong phú."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('4bb2f711-f471-4272-8827-693b7ee7be54', '65c63bd1-a319-41b5-b27a-249545b94cc4', 'concept', 'Nhịp Điệu Trong Âm Nhạc Hiện Đại', 'Âm nhạc hiện đại thường sử dụng các nhịp phức tạp hơn. Bài học này sẽ giúp bạn hiểu và thực hành các nhịp điệu mới.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh các nhạc sĩ hiện đại đang chơi piano.", "videoScript": "Chào mừng đến với bài học về nhịp điệu trong âm nhạc hiện đại! Hôm nay, chúng ta sẽ khám phá những nhịp điệu thú vị trong các thể loại âm nhạc khác nhau.", "imageDescription": "Một bức tranh thể hiện các nhạc sĩ hiện đại chơi piano với nhiều nhịp điệu khác nhau.", "videoDescription": "Video hướng dẫn về nhịp điệu trong âm nhạc hiện đại trên piano."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:01.522133', '2026-01-27 12:52:01.522133', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Khám phá nhịp điệu trong âm nhạc hiện đại.", "detailed": "Âm nhạc hiện đại thường sử dụng các nhịp phức tạp hơn. Bài học này sẽ giúp bạn hiểu và thực hành các nhịp điệu mới.", "comprehensive": "Nhịp điệu trong âm nhạc hiện đại rất đa dạng và phong phú. Chúng ta sẽ phân tích cách sử dụng nhịp trong các thể loại như pop, rock và jazz."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('36910d4d-8154-4504-b671-4faa7f0f6c83', '30dc66e2-1493-4b2e-999d-6525979015b5', 'concept', 'Xác Định Mục Tiêu Tài Chính', 'Xác định mục tiêu tài chính bao gồm việc biết bạn muốn sống như thế nào khi về hưu, chi phí sinh hoạt dự kiến và các hoạt động mà bạn mong muốn thực hiện.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một bảng trắng với các ghi chú về mục tiêu tài chính.", "videoScript": "Video bắt đầu với hình ảnh của một người lập danh sách mục tiêu tài chính. Lời lồng ghép giải thích cách xác định các mục tiêu tài chính cụ thể.", "imageDescription": "Hình ảnh một bảng trắng với nhiều ghi chú về mục tiêu tài chính, thể hiện quá trình lập kế hoạch.", "videoDescription": "Video hướng dẫn cách xác định mục tiêu tài chính cho kế hoạch hưu trí."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:01:09.841619', '2026-01-27 13:01:09.841619', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn cần xác định rõ mục tiêu tài chính để có thể lập kế hoạch hưu trí hiệu quả.", "detailed": "Xác định mục tiêu tài chính bao gồm việc biết bạn muốn sống như thế nào khi về hưu, chi phí sinh hoạt dự kiến và các hoạt động mà bạn mong muốn thực hiện.", "comprehensive": "Mục tiêu tài chính không chỉ là số tiền bạn cần có khi về hưu, mà còn là việc xác định các ưu tiên như du lịch, chăm sóc sức khỏe, và hỗ trợ gia đình. Việc này sẽ giúp bạn có cái nhìn rõ ràng hơn về số tiền cần tiết kiệm hàng tháng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('76cec6a4-ac9a-4fbc-9c48-2954ef24ed70', 'baddd431-b7d7-4df8-9aac-5d93271035b3', 'concept', 'Giới thiệu về Piano', 'Piano là một nhạc cụ thuộc họ dây, có 88 phím, gồm phím trắng và phím đen. Nó được sử dụng rộng rãi trong nhiều thể loại âm nhạc.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một cây đàn piano grand với ánh sáng dịu dàng.", "videoScript": "Chào mừng các bạn đến với bài học đầu tiên về piano. Hôm nay, chúng ta sẽ khám phá lịch sử và cấu tạo của nhạc cụ này.", "imageDescription": "Một cây đàn piano grand sang trọng đặt trong một phòng khách ấm cúng.", "videoDescription": "Video giới thiệu về piano, lịch sử và cách nó trở thành một trong những nhạc cụ phổ biến nhất thế giới."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:46:52.87912', '2026-01-27 12:46:52.87912', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Piano là một nhạc cụ phím phổ biến, được nhiều người yêu thích.", "detailed": "Piano là một nhạc cụ thuộc họ dây, có 88 phím, gồm phím trắng và phím đen. Nó được sử dụng rộng rãi trong nhiều thể loại âm nhạc.", "comprehensive": "Piano, một nhạc cụ phím phổ biến, được phát minh vào đầu thế kỷ 18. Nó có 88 phím, bao gồm 52 phím trắng và 36 phím đen. Piano có khả năng phát ra âm thanh từ nhẹ nhàng đến mạnh mẽ, cho phép người chơi thể hiện nhiều cảm xúc khác nhau. Các thể loại nhạc như cổ điển, jazz, pop đều có thể được biểu diễn trên piano."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('948bc536-4eb2-44d9-8b91-fc81dee7b873', 'baddd431-b7d7-4df8-9aac-5d93271035b3', 'concept', 'Cách ngồi và tư thế chơi Piano', 'Tư thế ngồi đúng là điều cần thiết để có thể chơi piano hiệu quả. Bạn nên ngồi thẳng lưng, hai chân đặt trên sàn và tay ở đúng vị trí phím.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một người ngồi chơi piano với tư thế đúng.", "videoScript": "Trong bài học này, chúng ta sẽ tìm hiểu về tư thế ngồi đúng khi chơi piano để có thể đạt được hiệu suất tối ưu.", "imageDescription": "Một người chơi piano với tư thế ngồi thẳng lưng, tay đặt đúng vị trí trên phím.", "videoDescription": "Video hướng dẫn tư thế ngồi khi chơi piano, giúp người chơi có được tư thế đúng và thoải mái."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:46:52.888485', '2026-01-27 12:46:52.888485', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Ngồi đúng cách khi chơi piano rất quan trọng.", "detailed": "Tư thế ngồi đúng là điều cần thiết để có thể chơi piano hiệu quả. Bạn nên ngồi thẳng lưng, hai chân đặt trên sàn và tay ở đúng vị trí phím.", "comprehensive": "Để có thể chơi piano một cách hiệu quả và tránh chấn thương, bạn cần chú ý đến tư thế ngồi. Hãy ngồi thẳng lưng, để mắt nhìn thẳng về phía trước. Đặt hai chân vững trên sàn và đảm bảo rằng tay của bạn ở vị trí thoải mái, không quá cao cũng không quá thấp so với phím đàn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('d2526328-1c37-43a0-be15-fdd98e590a57', 'baddd431-b7d7-4df8-9aac-5d93271035b3', 'concept', 'Các phím và âm thanh cơ bản', 'Piano có 88 phím, chia thành phím trắng và phím đen. Mỗi phím trắng tương ứng với một nốt nhạc tự nhiên, trong khi phím đen là nốt nhạc tăng hoặc giảm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh phím đàn piano với các nốt nhạc ghi chú.", "videoScript": "Hôm nay, chúng ta sẽ khám phá các phím trên piano và cách chúng phát ra âm thanh. Hãy cùng tìm hiểu nhé!", "imageDescription": "Gần cận hình ảnh phím đàn piano với các nốt nhạc được ghi chú trên đó.", "videoDescription": "Video giảng dạy về các phím và âm thanh cơ bản trên piano, giúp người chơi nhận biết các nốt nhạc."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:46:52.891944', '2026-01-27 12:46:52.891944', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Có nhiều loại phím trên piano, mỗi phím phát ra một âm thanh khác nhau.", "detailed": "Piano có 88 phím, chia thành phím trắng và phím đen. Mỗi phím trắng tương ứng với một nốt nhạc tự nhiên, trong khi phím đen là nốt nhạc tăng hoặc giảm.", "comprehensive": "Piano bao gồm 88 phím, trong đó có 52 phím trắng và 36 phím đen. Mỗi phím trắng tương ứng với các nốt nhạc tự nhiên từ C đến B, trong khi các phím đen là các nốt nhạc tăng (C#, D#, F#, G#, A#) và giảm (Db, Eb, Gb, Ab, Bb). Việc hiểu rõ cách hoạt động của các phím sẽ giúp bạn chơi nhạc chính xác hơn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('af8632ed-ef7b-4460-aa8f-d0b0f11d3a84', 'baddd431-b7d7-4df8-9aac-5d93271035b3', 'concept', 'Cách đọc bản nhạc cơ bản', 'Bản nhạc thường được viết trên 5 dòng kẻ, mỗi dòng và khoảng cách giữa chúng tương ứng với một nốt nhạc cụ thể.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bản nhạc với các nốt nhạc rõ ràng.", "videoScript": "Trong bài học này, chúng ta sẽ học cách đọc bản nhạc cơ bản và hiểu cách các nốt nhạc được biểu diễn.", "imageDescription": "Bản nhạc với các nốt nhạc được sắp xếp rõ ràng trên 5 dòng kẻ.", "videoDescription": "Video hướng dẫn cách đọc bản nhạc cho người mới bắt đầu, giúp họ hiểu cách nhận diện các nốt nhạc."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:46:52.895123', '2026-01-27 12:46:52.895123', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đọc bản nhạc là kỹ năng quan trọng khi chơi piano.", "detailed": "Bản nhạc thường được viết trên 5 dòng kẻ, mỗi dòng và khoảng cách giữa chúng tương ứng với một nốt nhạc cụ thể.", "comprehensive": "Đọc bản nhạc là một kỹ năng thiết yếu cho người chơi piano. Bản nhạc được viết trên 5 dòng kẻ, với mỗi dòng và khoảng cách giữa chúng đại diện cho các nốt nhạc khác nhau. Có hai khóa nhạc chính: khóa Sol (G) và khóa Fa (F). Người chơi cần hiểu cách nhận diện các nốt nhạc và cách chúng tương ứng với các phím trên đàn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7850adc4-483e-4981-a4a8-d4d1e9ab9670', 'baddd431-b7d7-4df8-9aac-5d93271035b3', 'concept', 'Thực hành các hợp âm cơ bản', 'Hợp âm được tạo thành từ ba nốt nhạc trở lên, thường được chơi cùng lúc để tạo ra âm thanh đầy đặn.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh bàn tay chơi hợp âm trên đàn piano.", "videoScript": "Trong bài học này, chúng ta sẽ thực hành các hợp âm cơ bản trên piano. Hãy cùng nhau khám phá!", "imageDescription": "Bàn tay của một người đang chơi hợp âm trên đàn piano.", "videoDescription": "Video hướng dẫn thực hành các hợp âm cơ bản trên piano, giúp người chơi làm quen với các âm thanh và cảm xúc khác nhau."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:46:52.899697', '2026-01-27 12:46:52.899697', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm là một phần quan trọng trong âm nhạc.", "detailed": "Hợp âm được tạo thành từ ba nốt nhạc trở lên, thường được chơi cùng lúc để tạo ra âm thanh đầy đặn.", "comprehensive": "Hợp âm là một phần không thể thiếu trong âm nhạc. Một hợp âm thường được tạo thành từ ba nốt nhạc trở lên và có thể được chơi cùng lúc để tạo ra âm thanh phong phú. Các hợp âm cơ bản như hợp âm trưởng và hợp âm thứ sẽ là nền tảng cho nhiều bản nhạc. Việc thực hành các hợp âm giúp người chơi phát triển kỹ năng và sự nhạy bén trong việc chuyển đổi giữa các nốt nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('cf0ae9b6-c09d-46db-b78f-0851876d3d12', '05e95475-8353-4a5e-b9cb-e481c1e73870', 'concept', 'Cấu Trúc Cơ Bản Của Một Bản Nhạc', 'Cấu trúc của một bản nhạc bao gồm phần giới thiệu, phần thân bài với các đoạn nhạc khác nhau, và phần kết thúc. Mỗi phần có thể mang một cảm xúc riêng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một bản nhạc với các phần được đánh dấu rõ ràng.", "videoScript": "Trong video này, chúng ta sẽ khám phá cấu trúc cơ bản của một bản nhạc, bao gồm giới thiệu, thân bài và kết thúc.", "imageDescription": "Một bản nhạc piano với các phần giới thiệu, thân bài và kết thúc được phân chia rõ ràng.", "videoDescription": "Video này hướng dẫn người xem hiểu cấu trúc cơ bản của một bản nhạc piano."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:23.426083', '2026-01-27 12:52:23.426083', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Một bản nhạc thường có ba phần chính: giới thiệu, thân bài và kết thúc.", "detailed": "Cấu trúc của một bản nhạc bao gồm phần giới thiệu, phần thân bài với các đoạn nhạc khác nhau, và phần kết thúc. Mỗi phần có thể mang một cảm xúc riêng.", "comprehensive": "Một bản nhạc thường được chia thành ba phần chính: giới thiệu, thân bài và kết thúc. Phần giới thiệu thường thiết lập giai điệu, phần thân bài phát triển ý tưởng âm nhạc với các đoạn khác nhau, và phần kết thúc giúp quay trở lại cảm xúc ban đầu hoặc tạo một kết thúc ấn tượng."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('779a478d-43c2-46cb-bb2f-e4025a459bc2', '05e95475-8353-4a5e-b9cb-e481c1e73870', 'concept', 'Phân Tích Giai Điệu và Hợp Âm', 'Giai điệu là dòng nhạc chính mà người nghe nhớ đến, trong khi hợp âm cung cấp nền tảng và hỗ trợ cho giai điệu đó.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh bàn phím piano với giai điệu và hợp âm được chỉ rõ.", "videoScript": "Chúng ta sẽ phân tích cách giai điệu và hợp âm tương tác với nhau để tạo ra một bản nhạc hoàn chỉnh.", "imageDescription": "Một bàn phím piano với các nốt giai điệu và hợp âm được đánh dấu để người học dễ dàng hình dung.", "videoDescription": "Video này sẽ giúp người xem hiểu rõ hơn về vai trò của giai điệu và hợp âm trong âm nhạc."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:23.433899', '2026-01-27 12:52:23.433899', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Giai điệu là phần chính của bản nhạc, hợp âm hỗ trợ tạo nền cho giai điệu.", "detailed": "Giai điệu là dòng nhạc chính mà người nghe nhớ đến, trong khi hợp âm cung cấp nền tảng và hỗ trợ cho giai điệu đó.", "comprehensive": "Giai điệu là phần nổi bật của bản nhạc, thường được chơi bằng tay phải trên piano. Hợp âm, thường được chơi bằng tay trái, tạo ra nền tảng hài hòa, hỗ trợ cho giai điệu và tạo cảm xúc cho bản nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('f521b363-f45d-4433-bf02-4b053e8249ad', '05e95475-8353-4a5e-b9cb-e481c1e73870', 'concept', 'Nhịp Điệu và Tempo', 'Nhịp điệu là cách sắp xếp các nốt nhạc theo thời gian, và tempo là tốc độ của nhịp điệu đó.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh biểu đồ nhịp điệu với các chỉ số tempo.", "videoScript": "Trong video này, chúng ta sẽ tìm hiểu về nhịp điệu và cách nó ảnh hưởng đến cách chơi nhạc trên piano.", "imageDescription": "Một biểu đồ nhịp điệu với các chỉ số tempo khác nhau để minh họa cho người học.", "videoDescription": "Video này giải thích cách nhịp điệu và tempo làm phong phú thêm bản nhạc."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:23.437591', '2026-01-27 12:52:23.437591', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhịp điệu xác định cách các nốt nhạc được sắp xếp theo thời gian.", "detailed": "Nhịp điệu là cách sắp xếp các nốt nhạc theo thời gian, và tempo là tốc độ của nhịp điệu đó.", "comprehensive": "Nhịp điệu là yếu tố chính quyết định cách thức sắp xếp các nốt nhạc trong một bản nhạc, trong khi tempo chỉ định tốc độ mà nhạc được chơi. Sự kết hợp giữa nhịp điệu và tempo tạo nên sự sống động và cảm xúc cho bản nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('05403d8f-63d5-4f60-9292-49883c3cafad', '05e95475-8353-4a5e-b9cb-e481c1e73870', 'concept', 'Phân Tích Cảm Xúc Trong Âm Nhạc', 'Mỗi bản nhạc có thể truyền tải những cảm xúc khác nhau thông qua giai điệu, hợp âm và nhịp điệu.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh một nhạc trưởng chỉ huy dàn nhạc với biểu cảm cảm xúc.", "videoScript": "Trong video này, chúng ta sẽ phân tích cách âm nhạc truyền tải cảm xúc thông qua các yếu tố khác nhau.", "imageDescription": "Một nhạc trưởng đang chỉ huy dàn nhạc với biểu cảm thể hiện cảm xúc của bản nhạc.", "videoDescription": "Video này khám phá cách mà âm nhạc có thể thể hiện nhiều cảm xúc khác nhau."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:52:23.441393', '2026-01-27 12:52:23.441393', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Âm nhạc có thể truyền đạt nhiều cảm xúc khác nhau.", "detailed": "Mỗi bản nhạc có thể truyền tải những cảm xúc khác nhau thông qua giai điệu, hợp âm và nhịp điệu.", "comprehensive": "Âm nhạc là một ngôn ngữ mạnh mẽ có khả năng truyền tải cảm xúc. Giai điệu, hợp âm và nhịp điệu phối hợp với nhau để tạo ra những cảm xúc khác nhau, từ vui vẻ, buồn bã đến kịch tính. Việc phân tích cảm xúc trong âm nhạc giúp người chơi hiểu rõ hơn về ý nghĩa của bản nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b35efc98-9eac-4149-9887-7ba7c7cca814', '3763eff5-2899-4712-b5de-a91cac6c1b48', 'concept', 'Giới thiệu về Piano', 'Piano là một nhạc cụ phím với 88 phím, bao gồm phím trắng và phím đen. Nó được phát minh vào đầu thế kỷ 18 và đã trở thành một trong những nhạc cụ phổ biến nhất trong âm nhạc cổ điển và hiện đại.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một chiếc piano grand trong một căn phòng sáng ánh sáng.", "videoScript": "Chào mừng đến với bài học đầu tiên về piano. Hôm nay, chúng ta sẽ khám phá lịch sử và cấu tạo của nhạc cụ tuyệt vời này.", "imageDescription": "Một chiếc piano grand màu đen bóng với ánh sáng chiếu từ cửa sổ, tạo nên một không gian ấm cúng.", "videoDescription": "Video giới thiệu về piano, bao gồm lịch sử, cấu tạo và ứng dụng trong âm nhạc."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:47:26.353622', '2026-01-27 12:47:26.353622', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Piano là một nhạc cụ phím được sử dụng rộng rãi trong nhiều thể loại âm nhạc.", "detailed": "Piano là một nhạc cụ phím với 88 phím, bao gồm phím trắng và phím đen. Nó được phát minh vào đầu thế kỷ 18 và đã trở thành một trong những nhạc cụ phổ biến nhất trong âm nhạc cổ điển và hiện đại.", "comprehensive": "Piano là một trong những nhạc cụ phím quan trọng nhất trong lịch sử âm nhạc. Được phát minh bởi Bartolomeo Cristofori vào khoảng năm 1700, piano có khả năng tạo ra âm thanh mạnh mẽ và êm dịu, tùy thuộc vào lực nhấn của ngón tay. Nó bao gồm 88 phím, trong đó có 52 phím trắng và 36 phím đen, cho phép người chơi thể hiện nhiều giai điệu và hợp âm phức tạp."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b233ee51-6a5c-4e54-9855-30c1cdb3cc4c', '3763eff5-2899-4712-b5de-a91cac6c1b48', 'concept', 'Cấu trúc âm thanh của Piano', 'Khi phím piano được nhấn, nó kích hoạt một búa gõ vào dây thép bên trong, tạo ra âm thanh. Âm thanh này được khuếch đại bởi vỏ gỗ của piano.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Cận cảnh bên trong một chiếc piano, cho thấy các dây thép và búa.", "videoScript": "Hãy cùng tìm hiểu cách mà piano tạo ra âm thanh. Ở đây, chúng ta sẽ xem xét các bộ phận bên trong và cách chúng hoạt động.", "imageDescription": "Một bức ảnh chi tiết bên trong piano cho thấy cấu trúc dây thép và các búa, thể hiện cách âm thanh được tạo ra.", "videoDescription": "Video giải thích cách thức hoạt động của piano, bao gồm cấu trúc bên trong và quy trình tạo ra âm thanh."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:47:26.362553', '2026-01-27 12:47:26.362553', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Piano phát ra âm thanh nhờ vào các dây thép bên trong.", "detailed": "Khi phím piano được nhấn, nó kích hoạt một búa gõ vào dây thép bên trong, tạo ra âm thanh. Âm thanh này được khuếch đại bởi vỏ gỗ của piano.", "comprehensive": "Âm thanh của piano được tạo ra bởi một hệ thống phức tạp gồm các dây thép được căng chặt bên trong thân đàn. Khi người chơi nhấn phím, một búa gõ vào dây, tạo ra âm thanh. Các dây này được thiết kế để tạo ra các tần số khác nhau, cho phép piano phát ra âm thanh từ thấp đến cao. Vỏ gỗ của piano giúp khuếch đại âm thanh, mang lại âm sắc phong phú và đầy đặn."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('1aa4a786-99b2-4c0c-bb91-f46dd8a6bfc7', '3763eff5-2899-4712-b5de-a91cac6c1b48', 'concept', 'Kỹ thuật chơi Piano cơ bản', 'Kỹ thuật chơi piano bao gồm việc kiểm soát ngón tay, vị trí tay và sử dụng lực nhấn đúng. Người mới bắt đầu nên bắt đầu với các bài tập đơn giản để làm quen với âm thanh.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người chơi piano đang thực hành với tư thế đúng.", "videoScript": "Hôm nay, chúng ta sẽ khám phá các kỹ thuật cơ bản để chơi piano. Hãy cùng nhau thực hành những điều cần lưu ý khi bắt đầu.", "imageDescription": "Một người chơi piano ngồi với tư thế đúng, tay đặt trên phím và tập trung vào việc chơi.", "videoDescription": "Video hướng dẫn kỹ thuật chơi piano cơ bản cho người mới bắt đầu, bao gồm tư thế và cách nhấn phím."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:47:26.366136', '2026-01-27 12:47:26.366136', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Người chơi piano cần biết cách đặt tay và nhấn phím đúng cách.", "detailed": "Kỹ thuật chơi piano bao gồm việc kiểm soát ngón tay, vị trí tay và sử dụng lực nhấn đúng. Người mới bắt đầu nên bắt đầu với các bài tập đơn giản để làm quen với âm thanh.", "comprehensive": "Để chơi piano hiệu quả, người chơi cần chú ý đến vị trí tay và cách nhấn phím. Điều này bao gồm việc giữ ngón tay thẳng, sử dụng lực nhấn phù hợp và di chuyển tay một cách linh hoạt trên phím. Các bài tập cơ bản sẽ giúp người mới bắt đầu phát triển kỹ năng và sự tự tin trong việc chơi nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b152a43c-eeda-43a2-a2e0-1311d3b0a890', '30dc66e2-1493-4b2e-999d-6525979015b5', 'concept', 'Các Khoản Tiết Kiệm Cần Thiết', 'Các khoản tiết kiệm cần thiết bao gồm quỹ hưu trí, tài khoản tiết kiệm, và các khoản đầu tư khác. Bạn nên xác định số tiền cần tiết kiệm hàng tháng để đạt được mục tiêu tài chính.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang tính toán chi phí và tiết kiệm trên máy tính.", "videoScript": "Video mở đầu với cảnh một người đang tính toán các khoản chi phí. Lời lồng ghép hướng dẫn cách xác định các khoản tiết kiệm cần thiết cho hưu trí.", "imageDescription": "Hình ảnh một người đang ngồi trước máy tính, tính toán các chi phí và khoản tiết kiệm cần thiết cho kế hoạch hưu trí.", "videoDescription": "Video chỉ ra các khoản tiết kiệm cần thiết cho kế hoạch hưu trí và cách quản lý chúng hiệu quả."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:01:09.844926', '2026-01-27 13:01:09.844926', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Bạn cần biết mình cần tiết kiệm bao nhiêu tiền cho hưu trí.", "detailed": "Các khoản tiết kiệm cần thiết bao gồm quỹ hưu trí, tài khoản tiết kiệm, và các khoản đầu tư khác. Bạn nên xác định số tiền cần tiết kiệm hàng tháng để đạt được mục tiêu tài chính.", "comprehensive": "Để có một kế hoạch hưu trí hiệu quả, bạn cần tính toán kỹ càng các khoản tiết kiệm cần thiết. Điều này bao gồm việc lập ngân sách hàng tháng, xác định tỷ lệ tiết kiệm phù hợp và xem xét các lựa chọn đầu tư như cổ phiếu, trái phiếu và quỹ tương hỗ."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('74cf8c47-c943-4608-bb65-a79059f6e826', '3763eff5-2899-4712-b5de-a91cac6c1b48', 'concept', 'Hợp âm cơ bản trên Piano', 'Hợp âm cơ bản trên piano thường bao gồm ba nốt: nốt gốc, nốt thứ ba và nốt thứ năm. Những hợp âm này là nền tảng cho nhiều bài hát.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Bảng hợp âm piano với các nốt được đánh dấu.", "videoScript": "Bây giờ, chúng ta sẽ học về các hợp âm cơ bản trên piano. Hãy cùng nhau xem qua từng hợp âm và cách chơi của chúng.", "imageDescription": "Một bảng hợp âm piano minh họa các nốt của các hợp âm cơ bản như C, G, Am, F.", "videoDescription": "Video giới thiệu các hợp âm cơ bản trên piano, bao gồm cách chơi và ứng dụng trong âm nhạc."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:47:26.380752', '2026-01-27 12:47:26.380752', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Hợp âm là sự kết hợp của ba nốt nhạc hoặc hơn.", "detailed": "Hợp âm cơ bản trên piano thường bao gồm ba nốt: nốt gốc, nốt thứ ba và nốt thứ năm. Những hợp âm này là nền tảng cho nhiều bài hát.", "comprehensive": "Hợp âm là một phần quan trọng trong việc chơi piano. Hợp âm cơ bản bao gồm các loại như hợp âm trưởng, hợp âm thứ và hợp âm giảm. Mỗi hợp âm có cấu trúc riêng, thường bao gồm ba nốt: nốt gốc, nốt thứ ba và nốt thứ năm. Việc hiểu và thực hành các hợp âm này sẽ giúp người chơi xây dựng nền tảng vững chắc cho việc chơi nhạc sau này."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('7ad9811e-1504-4082-9c51-f927c1356cd4', '3763eff5-2899-4712-b5de-a91cac6c1b48', 'concept', 'Thực hành và ứng dụng Piano', 'Người chơi nên thực hành hàng ngày để cải thiện kỹ năng. Có thể bắt đầu với các bài hát đơn giản và dần dần nâng cao độ khó.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người chơi piano đang thực hành với niềm đam mê.", "videoScript": "Trong bài học cuối cùng, chúng ta sẽ bàn về tầm quan trọng của việc thực hành và cách để nâng cao kỹ năng chơi piano của bạn.", "imageDescription": "Một người chơi piano chăm chú thực hành với biểu cảm tập trung và đam mê.", "videoDescription": "Video khuyến khích người chơi piano thực hành thường xuyên và đưa ra các mẹo để cải thiện kỹ năng."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:47:26.385465', '2026-01-27 12:47:26.385465', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Thực hành là rất quan trọng để cải thiện kỹ năng chơi piano.", "detailed": "Người chơi nên thực hành hàng ngày để cải thiện kỹ năng. Có thể bắt đầu với các bài hát đơn giản và dần dần nâng cao độ khó.", "comprehensive": "Thực hành đều đặn là chìa khóa để trở thành một người chơi piano giỏi. Người chơi nên dành thời gian mỗi ngày để luyện tập, bắt đầu với các bài hát đơn giản và dần dần chuyển sang các tác phẩm phức tạp hơn. Bên cạnh đó, việc ghi chú lại tiến bộ và tham gia vào các buổi biểu diễn nhỏ cũng giúp nâng cao kỹ năng và tự tin hơn trong việc chơi nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('a6086468-0362-41d9-9f10-9057280e5158', '082bbac3-b520-4a73-9a00-7a6778e697db', 'concept', 'Bài học 1: Thẩm âm tác phẩm ''Clair de Lune''', 'Phân tích từng phần của ''Clair de Lune'' để cảm nhận âm sắc và giai điệu. Tập trung vào cảm xúc mà tác phẩm truyền tải.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một bản nhạc ''Clair de Lune'' với các ghi chú nổi bật.", "videoScript": "Một video hướng dẫn thẩm âm ''Clair de Lune'', giải thích cách nhận biết các yếu tố âm nhạc.", "imageDescription": "Bản nhạc ''Clair de Lune'' với các ghi chú và hợp âm được làm nổi bật.", "videoDescription": "Video thẩm âm ''Clair de Lune'', giúp người xem hiểu rõ hơn về cách cảm nhận giai điệu và hòa âm."}', 0, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:47:52.283467', '2026-01-27 12:47:52.283467', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Học cách nhận biết giai điệu và hòa âm của ''Clair de Lune''.", "detailed": "Phân tích từng phần của ''Clair de Lune'' để cảm nhận âm sắc và giai điệu. Tập trung vào cảm xúc mà tác phẩm truyền tải.", "comprehensive": "Khám phá sâu sắc từng nốt nhạc trong ''Clair de Lune''. Nghiên cứu cấu trúc hòa âm, cách chuyển đổi giữa các hợp âm và cách tạo ra cảm xúc trong âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('73377b44-e31c-46df-8650-1d3a78df12d0', '082bbac3-b520-4a73-9a00-7a6778e697db', 'concept', 'Bài học 2: Thẩm âm tác phẩm ''Fur Elise''', 'Phân tích các mô hình nhịp điệu và giai điệu của ''Fur Elise''. Tìm hiểu về các biến thể trong tác phẩm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của một bản nhạc ''Fur Elise'' với các phần được chú thích.", "videoScript": "Video hướng dẫn thẩm âm ''Fur Elise'', phân tích các giai điệu và nhịp điệu đặc trưng.", "imageDescription": "Bản nhạc ''Fur Elise'' với các phần giai điệu và nhịp điệu được phân tích.", "videoDescription": "Video thẩm âm ''Fur Elise'', giúp người xem hiểu rõ về các yếu tố âm nhạc trong tác phẩm."}', 1, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:47:52.291313', '2026-01-27 12:47:52.291313', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhận biết các giai điệu chính trong ''Fur Elise''.", "detailed": "Phân tích các mô hình nhịp điệu và giai điệu của ''Fur Elise''. Tìm hiểu về các biến thể trong tác phẩm.", "comprehensive": "Khám phá sâu sắc ''Fur Elise'', từ cấu trúc giai điệu đến các yếu tố nhịp điệu. Học cách phân tích và cảm nhận các biến thể âm nhạc."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('c5edfc5f-b263-4189-83fd-77f59a8e234f', '082bbac3-b520-4a73-9a00-7a6778e697db', 'concept', 'Bài học 3: Thẩm âm tác phẩm ''Nocturne Op. 9 No. 2''', 'Nghiên cứu cấu trúc và cảm xúc trong ''Nocturne Op. 9 No. 2'', tìm hiểu sự kết hợp giữa giai điệu và hòa âm.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh của bản nhạc ''Nocturne Op. 9 No. 2'' với các ghi chú được làm nổi bật.", "videoScript": "Video hướng dẫn thẩm âm ''Nocturne Op. 9 No. 2'', giải thích các yếu tố âm nhạc trong tác phẩm.", "imageDescription": "Bản nhạc ''Nocturne Op. 9 No. 2'' với các ghi chú và hợp âm được phân tích.", "videoDescription": "Video thẩm âm ''Nocturne Op. 9 No. 2'', giúp người xem cảm nhận được các yếu tố hòa âm và giai điệu."}', 2, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:47:52.295176', '2026-01-27 12:47:52.295176', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Cảm nhận giai điệu nhẹ nhàng trong ''Nocturne Op. 9 No. 2''.", "detailed": "Nghiên cứu cấu trúc và cảm xúc trong ''Nocturne Op. 9 No. 2'', tìm hiểu sự kết hợp giữa giai điệu và hòa âm.", "comprehensive": "Khám phá các yếu tố âm nhạc trong ''Nocturne Op. 9 No. 2''. Phân tích sâu sắc về nhịp điệu, hòa âm và cảm xúc tác phẩm."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e9c8d610-27e6-4655-ae6e-bcfef74bae1b', '7fa5b8b2-f70f-4e5f-bb08-e02636b929ec', 'concept', 'Da Hỗn Hợp: Cách Nhận Biết và Chăm Sóc', 'Da hỗn hợp thường có vùng chữ T (trán, mũi, cằm) dầu, trong khi phần má lại khô hơn. Cần chọn sản phẩm phù hợp cho từng vùng.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh sản phẩm chăm sóc da hỗn hợp.", "videoScript": "Hôm nay, chúng ta sẽ tìm hiểu về da hỗn hợp và cách chăm sóc cho loại da này. Hãy cùng khám phá nhé!", "imageDescription": "Một bộ sản phẩm chăm sóc da hỗn hợp bao gồm sữa rửa mặt và kem dưỡng ẩm cho từng vùng.", "videoDescription": "Video hướng dẫn chăm sóc da hỗn hợp với các sản phẩm và thói quen phù hợp."}', 4, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:18:44.234619', '2026-01-27 13:18:44.234619', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Da hỗn hợp có phần da dầu và phần da khô.", "detailed": "Da hỗn hợp thường có vùng chữ T (trán, mũi, cằm) dầu, trong khi phần má lại khô hơn. Cần chọn sản phẩm phù hợp cho từng vùng.", "comprehensive": "Da hỗn hợp là một trong những loại da phổ biến nhất. Để chăm sóc da hỗn hợp, hãy sử dụng sản phẩm tẩy trang nhẹ nhàng, kem dưỡng ẩm cho vùng khô và sản phẩm kiểm soát dầu cho vùng da dầu. Đừng quên duy trì độ ẩm cho toàn bộ khuôn mặt."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('e9556d5a-da4f-4c7c-ba7c-8f561a271782', '082bbac3-b520-4a73-9a00-7a6778e697db', 'concept', 'Bài học 4: Thẩm âm tác phẩm ''Prelude in C Major''', 'Phân tích các hợp âm và cách chúng kết hợp với nhau trong ''Prelude in C Major''.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Hình ảnh bản nhạc ''Prelude in C Major'' với các hợp âm được chú thích.", "videoScript": "Video hướng dẫn thẩm âm ''Prelude in C Major'', giải thích về cấu trúc hợp âm và nhịp điệu.", "imageDescription": "Bản nhạc ''Prelude in C Major'' với các hợp âm và cấu trúc nhịp điệu được phân tích.", "videoDescription": "Video thẩm âm ''Prelude in C Major'', giúp người xem hiểu rõ hơn về sự kết hợp giữa các hợp âm và giai điệu."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 12:47:52.299507', '2026-01-27 12:47:52.299507', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Nhận biết các hợp âm trong ''Prelude in C Major''.", "detailed": "Phân tích các hợp âm và cách chúng kết hợp với nhau trong ''Prelude in C Major''.", "comprehensive": "Khám phá sâu sắc các hợp âm và cấu trúc của ''Prelude in C Major''. Học cách thẩm âm và cảm nhận sự hòa quyện giữa giai điệu và nhịp điệu."}');
INSERT INTO public.content_items (id, "nodeId", type, title, content, media, "order", rewards, "quizData", "createdAt", "updatedAt", format, difficulty, "richContent", status, "contributionGuide", "contributorId", "contributedAt", "textVariants") VALUES ('b2976f62-3146-490d-9eab-82f4f66d2448', '30dc66e2-1493-4b2e-999d-6525979015b5', 'concept', 'Lựa Chọn Các Kênh Đầu Tư', 'Lựa chọn các kênh đầu tư phù hợp sẽ giúp bạn tăng trưởng tài sản cho hưu trí. Bạn có thể xem xét cổ phiếu, trái phiếu, quỹ tương hỗ, và bất động sản.', '{"imageUrl": "https://placehold.co/800x600/1a1a2e/ffffff?text=Cần+hình+ảnh", "videoUrl": "", "imagePrompt": "Một người đang xem xét các biểu đồ đầu tư.", "videoScript": "Video bắt đầu với cảnh một người nghiên cứu các kênh đầu tư khác nhau. Lời lồng ghép giải thích các lựa chọn đầu tư và cách lựa chọn phù hợp.", "imageDescription": "Hình ảnh một người đang chăm chú xem xét các biểu đồ đầu tư, thể hiện sự quan tâm đến việc lựa chọn kênh đầu tư.", "videoDescription": "Video hướng dẫn cách lựa chọn các kênh đầu tư cho kế hoạch hưu trí."}', 3, '{"xp": 15, "coin": 3}', NULL, '2026-01-27 13:01:09.848285', '2026-01-27 13:01:09.848285', 'text', 'medium', NULL, 'published', NULL, NULL, NULL, '{"simple": "Đầu tư là một phần quan trọng trong kế hoạch hưu trí.", "detailed": "Lựa chọn các kênh đầu tư phù hợp sẽ giúp bạn tăng trưởng tài sản cho hưu trí. Bạn có thể xem xét cổ phiếu, trái phiếu, quỹ tương hỗ, và bất động sản.", "comprehensive": "Việc lựa chọn các kênh đầu tư đòi hỏi bạn phải hiểu rõ về từng loại hình đầu tư, mức độ rủi ro và lợi nhuận tiềm năng. Bạn cần cân nhắc giữa các lựa chọn như cổ phiếu, trái phiếu, quỹ đầu tư và bất động sản, đồng thời lập kế hoạch phân bổ tài sản hợp lý."}');
